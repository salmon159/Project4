<FlatfileSchema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation='flatfileschema.xsd'>
   <ParserDefaults  DefaultEscapeCharacter="\" DefaultPadCharacter="#" RecordIdStartPosition="1" RecordIdEndPosition="7">
      <FileHeader FileHeaderId="000" FileHeaderStartPos="1" FileHeaderLength="3"/>
      <FileTrailer FileTrailerId="666" FileTrailerStartPos="1" FileTrailerLength="3"/>
   </ParserDefaults>

   <Root Name="Order">
        <Header Name="OrderHeader" RecordName="ORDRHDR" MinOccurrence="1" MaxOccurrence="1"/>
	<Terminal Name="ContactInfo" RecordName="CONTACT" MinOccurrence="1" MaxOccurrence="1"/>
	<Terminal Name="HeaderInstructions" RecordName="HDRINST" MinOccurrence="0" MaxOccurrence="1000"/>
        <Sequence Name="Orderlines" RecordName="ORDRDTL" MinOccurrence="1" MaxOccurrence="10000">
     	   <Terminal Name="LineInstructions" RecordName="LNINSTN" MinOccurrence="0" MaxOccurrence="1000"/>
	</Sequence>
	<Terminal Name="OrderTrailer" RecordName="ORDRTRL" MinOccurrence="1" MaxOccurrence="1"/>
   </Root>

   <RecordDefinitions>
      <Record RecordId="ORDRHDR" Name="ORDRHDR" XMLName="ORDHDR">
         <!-- All dates truncated to 8 characters -->
         <Field Name="CARRSVC" StartPosition="8" EndPosition="47" XMLName="carrier_service"/>
         <Field Name="CORPCD" StartPosition="48" EndPosition="71" XMLName="corporation_code"/>
         <Field Name="CARRCHGACCT" StartPosition="72" EndPosition="111" XMLName="cust_carrier_charge_account_no"/>
         <Field Name="DELCD" StartPosition="112" EndPosition="151" XMLName="delivery_code"/>
         <Field Name="DELDT" StartPosition="152" EndPosition="159" XMLName="delivery_date"/>
         <Field Name="ORDCRTDT" StartPosition="160" EndPosition="167" XMLName="order_create_date"/>
         <Field Name="ORDNO" StartPosition="168" EndPosition="207" XMLName="order_no"/>
         <Field Name="ORDTYP" StartPosition="208" EndPosition="227" XMLName="order_type"/>
         <Field Name="ORGSHPDT" StartPosition="228" EndPosition="235" XMLName="orig_ship_date"/>
         <Field Name="SHPTAD1" StartPosition="236" EndPosition="305" XMLName="ship_to_addr1"/>
         <Field Name="SHPTAD2" StartPosition="306" EndPosition="375" XMLName="ship_to_addr2"/>
         <Field Name="SHPTAD3" StartPosition="376" EndPosition="445" XMLName="ship_to_addr3"/>
         <Field Name="SHPTAD4" StartPosition="446" EndPosition="515" XMLName="ship_to_addr4"/>
         <Field Name="SHPTCTY" StartPosition="516" EndPosition="550" XMLName="ship_to_city"/>
         <Field Name="SHPTCC" StartPosition="551" EndPosition="585" XMLName="ship_to_country_code"/>
         <Field Name="SHPTCUSID" StartPosition="586" EndPosition="620" XMLName="ship_to_cust_id"/>
         <Field Name="SHPTCUSNM" StartPosition="621" EndPosition="660" XMLName="ship_to_customer_name"/>
         <Field Name="SHPTST" StartPosition="661" EndPosition="695" XMLName="ship_to_state"/>
         <Field Name="SHPTZC" StartPosition="696" EndPosition="730" XMLName="ship_to_zip_code"/>
         <Field Name="TRMSCD" StartPosition="731" EndPosition="750" XMLName="terms_code"/>
      </Record>
      <Record RecordId="CONTACT" Name="CONTACT" XMLName="ContactInfo">
         <Field Name="ADDR1" StartPosition="8" EndPosition="77" XMLName="addr1"/>
         <Field Name="ADDR2" StartPosition="78" EndPosition="147" XMLName="addr2"/>
         <Field Name="ADDR3" StartPosition="148" EndPosition="217" XMLName="addr3"/>
         <Field Name="ADDR4" StartPosition="218" EndPosition="287" XMLName="addr4"/>
         <Field Name="CITY" StartPosition="288" EndPosition="322" XMLName="city"/>
         <Field Name="CC" StartPosition="323" EndPosition="357" XMLName="country_code"/>
         <Field Name="LSTNM" StartPosition="358" EndPosition="397" XMLName="customer_name"/>
         <Field Name="ST" StartPosition="398" EndPosition="432" XMLName="state"/>
         <Field Name="ZC" StartPosition="433" EndPosition="467" XMLName="zip_code"/>
      </Record>
      <Record RecordId="HDRINST" Name="HDRINST" XMLName="HdrInstrctn">
      <!-- Instruction Text truncated to 100 characters; Sterling stores 4000 chars -->
         <Field Name="HDINSTTXT" StartPosition="8" EndPosition="107" XMLName="instruction_text"/>
         <Field Name="HDINSTTYP" StartPosition="108" EndPosition="117" XMLName="instruction_type"/>
      </Record>
      <Record RecordId="ORDRDTL" Name="ORDRDTL" XMLName="ORDDTL">
         <!-- All dates truncated to 8 characters -->
         <Field Name="CARRCD" StartPosition="8" EndPosition="47" XMLName="carrier_code"/>
         <Field Name="POLNNO" StartPosition="48" EndPosition="57" XMLName="customer_po_line_no"/>
         <Field Name="PONO" StartPosition="58" EndPosition="157" XMLName="customer_po_no"/>
         <Field Name="CUSSKU" StartPosition="158" EndPosition="197" XMLName="customer_sku"/>
         <Field Name="DEPTCD" StartPosition="198" EndPosition="217" XMLName="department_code"/>
         <Field Name="ITMID" StartPosition="218" EndPosition="257" XMLName="item_id"/>
         <Field Name="LNPRC" StartPosition="258" EndPosition="272" XMLName="line_price"/>
         <Field Name="NONSHPQTY" StartPosition="273" EndPosition="286" XMLName="non_shippable_qty"/>
         <Field Name="ORDPRMLN" StartPosition="287" EndPosition="291" XMLName="order_prime_line"/>
         <Field Name="ORDSUBLN" StartPosition="292" EndPosition="296" XMLName="order_sub_line"/>
         <Field Name="REQSHPDT" StartPosition="297" EndPosition="304" XMLName="orig_req_ship_date"/>
         <Field Name="PCKTYP" StartPosition="305" EndPosition="324" XMLName="pack_type"/>
         <Field Name="PRDTCLS" StartPosition="325" EndPosition="334" XMLName="product_class"/>
         <Field Name="QTYORDRD" StartPosition="335" EndPosition="348" XMLName="qty_ordered"/>
         <Field Name="SHPTGRCD" StartPosition="349" EndPosition="388" XMLName="ship_together_code"/>
         <Field Name="SHPQTY" StartPosition="389" EndPosition="402" XMLName="shippable_qty"/>
         <Field Name="WRHS" StartPosition="403" EndPosition="426" XMLName="whse"/>
      </Record>
      <Record RecordId="LNINSTN" Name="LNINSTN" XMLName="LnInstrctn">
      <!-- Instruction Text truncated to 100 characters; Sterling stores 4000 chars -->
         <Field Name="LNINSTTXT" StartPosition="8" EndPosition="107" XMLName="instruction_text"/>
         <Field Name="LNINSTTYP" StartPosition="108" EndPosition="117" XMLName="instruction_type"/>
      </Record>
      <Record RecordId="ORDRTRL" Name="ORDRTRL" XMLName="OrderTrailer">
         <Field Name="TRLDTA" StartPosition="8" EndPosition="71" XMLName="trailer_data"/>
         <Field Name="WHS" StartPosition="72" EndPosition="95" XMLName="whse"/>
      </Record>
   </RecordDefinitions>
</FlatfileSchema>
