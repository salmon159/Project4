                <AdditionalAddresses hasAttributes="Y">
                    <AdditionalAddress hasAttributes="Y">
                        <PersonInfo hasAttributes="Y"/>
                    </AdditionalAddress>
                </AdditionalAddresses>
                <PriceInfo hasAttributes="Y"/>
                <PriceInfoInEnterpriseCurrency hasAttributes="Y"/>
