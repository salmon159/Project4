<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/YFS/getItemListForOrdering/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/YFS/getItemListForOrdering/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="YFS_getItemListForOrdering_input.xsd" yfc:rootElement="Item">
        <xsd:appinfo source="yfc:structure">
            <Item hasAttributes="Y">
                <ServiceType hasAttributes="Y"/>
                <CustomerInformation hasAttributes="Y"/>
                <ShipToAddress hasAttributes="Y"/>
                <ShipNodes>
                    <ShipNode hasAttributes="Y"/>
                </ShipNodes>
                <ItemAssociationTypeList hasAttributes="Y">
                    <ItemAssociationType hasAttributes="Y"/>
                </ItemAssociationTypeList>
                <ItemLocaleList>
                    <ItemLocale hasAttributes="Y">
                        <PrimaryInformation hasAttributes="Y"/>
                    </ItemLocale>
                </ItemLocaleList>
                <ItemServiceAssoc hasAttributes="Y"/>
                <PrimaryInformation hasAttributes="Y"/>
                <InventoryParameters hasAttributes="Y"/>
                <ClassificationCodes hasAttributes="Y"/>
                <ItemAliasList>
                    <ItemAlias hasAttributes="Y"/>
                </ItemAliasList>
                <ItemExclusionList>
                    <ItemExclusion hasAttributes="Y"/>
                </ItemExclusionList>
                <AdditionalAttributeList>
                    <AdditionalAttribute hasAttributes="Y"/>
                </AdditionalAttributeList>
                <CategoryFilter hasAttributes="Y"/>
                <Components>
                    <Component hasAttributes="Y"/>
                </Components>
                <LanguageDescriptionList>
                    <LanguageDescription hasAttributes="Y"/>
                </LanguageDescriptionList>
                <InventoryTagAttributes hasAttributes="Y"/>
                <ItemAttributeGroupTypeList>
                    <ItemAttributeGroupType hasAttributes="Y"/>
                </ItemAttributeGroupTypeList>
                <OrderBy>
                    <Attribute hasAttributes="Y"/>
                </OrderBy>
            </Item>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="Item" type="yfc:ItemXSDType">
        <xsd:annotation yfc:tableName="YFS_ITEM">
            <xsd:documentation>The table stores product information.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="ItemXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ComplexQuery"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ServiceType"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CustomerInformation"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipToAddress"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipNodes"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemAssociationTypeList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemLocaleList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemServiceAssoc"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="PrimaryInformation">
                <xsd:annotation yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The table stores product information.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:attribute name="AssumeInfiniteInventory"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="ASSUME_INFINITE_INVENTORY" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates whether an item is assumed to be always available. Default value: N.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ColorCode"
                        type="yfctype:Text-40" use="optional">
                        <xsd:annotation yfc:columnName="COLOR_CODE" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This field describes color code for the item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ConfiguredModelKey"
                        type="yfctype:Key" use="optional">
                        <xsd:annotation
                            yfc:columnName="CONFIGURED_MODEL_KEY" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The key of the configured model for a configurable item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="CostCurrency"
                        type="yfctype:Currency" use="optional">
                        <xsd:annotation yfc:columnName="COST_CURRENCY"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The currency in which the item's cost is defined.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="CostCurrencyQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="CountryOfOrigin"
                        type="yfctype:Country" use="optional">
                        <xsd:annotation
                            yfc:columnName="COUNTRY_OF_ORIGIN"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The item's country of origin/manufacture.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="CountryOfOriginQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="DefaultProductClass"
                        type="yfctype:ProductClass" use="optional">
                        <xsd:annotation
                            yfc:columnName="DEFAULT_PRODUCT_CLASS"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The default product class of an item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="DefaultProductClassQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="Description"
                        type="yfctype:Text-500" use="optional">
                        <xsd:annotation yfc:columnName="DESCRIPTION"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The description of the item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="DescriptionQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="FromCostCurrency"
                        type="yfctype:Currency" use="optional"/>
                    <xsd:attribute name="FromCountryOfOrigin"
                        type="yfctype:Country" use="optional"/>
                    <xsd:attribute name="FromDefaultProductClass"
                        type="yfctype:ProductClass" use="optional"/>
                    <xsd:attribute name="FromDescription"
                        type="yfctype:Text-500" use="optional"/>
                    <xsd:attribute name="FromIsAirShippingAllowed"
                        type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="FromIsForwardingAllowed"
                        type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="FromIsFreezerRequired"
                        type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="FromIsHazmat"
                        type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="FromIsProcurementAllowed"
                        type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="FromItemType"
                        type="yfctype:Text-40" use="optional"/>
                    <xsd:attribute name="FromKitCode"
                        type="yfctype:EnumKitCode" use="optional"/>
                    <xsd:attribute name="FromManufacturerItem"
                        type="yfctype:Text-100" use="optional"/>
                    <xsd:attribute name="FromManufacturerItemDesc"
                        type="yfctype:Text-255" use="optional"/>
                    <xsd:attribute name="FromManufacturerName"
                        type="yfctype:Text-50" use="optional"/>
                    <xsd:attribute name="FromMasterCatalogID"
                        type="yfctype:ID-40" use="optional"/>
                    <xsd:attribute name="FromMaxOrderQuantity"
                        type="yfctype:Quantity" use="optional"/>
                    <xsd:attribute name="FromMinOrderQuantity"
                        type="yfctype:Quantity" use="optional"/>
                    <xsd:attribute name="FromMinimumCapacityQuantity"
                        type="yfctype:Quantity" use="optional"/>
                    <xsd:attribute name="FromPricingQuantityConvFactor"
                        type="yfctype:Quantity" use="optional"/>
                    <xsd:attribute name="FromPricingQuantityStrategy"
                        type="yfctype:Text-20" use="optional"/>
                    <xsd:attribute name="FromPricingUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="FromPricingUOMStrategy"
                        type="yfctype:Text-20" use="optional"/>
                    <xsd:attribute name="FromProductLine"
                        type="yfctype:Text-100" use="optional"/>
                    <xsd:attribute name="FromServiceTypeID"
                        type="yfctype:Text-24" use="optional"/>
                    <xsd:attribute
                        name="FromShipmentConsolidationWindow"
                        type="yfctype:NumberOfDays" use="optional"/>
                    <xsd:attribute name="FromShortDescription"
                        type="yfctype:Text-200" use="optional"/>
                    <xsd:attribute name="FromStatus"
                        type="yfctype:EnumShortStatus" use="optional"/>
                    <xsd:attribute name="FromUnitCost"
                        type="yfctype:UnitPrice" use="optional"/>
                    <xsd:attribute name="FromUnitHeight"
                        type="yfctype:Dimension" use="optional"/>
                    <xsd:attribute name="FromUnitHeightUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="FromUnitLength"
                        type="yfctype:Dimension" use="optional"/>
                    <xsd:attribute name="FromUnitLengthUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="FromUnitWeight"
                        type="yfctype:Dimension" use="optional"/>
                    <xsd:attribute name="FromUnitWeightUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="FromUnitWidth"
                        type="yfctype:Dimension" use="optional"/>
                    <xsd:attribute name="FromUnitWidthUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="IsAirShippingAllowed"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_AIR_SHIPPING_ALLOWED"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>Indicates if item can be shipped by air. Default value: Y.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsAirShippingAllowedQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="IsConfigurable"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation yfc:columnName="IS_CONFIGURABLE" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates that the item is a configurable item. A configurable item can either be a bundle or a physical kit. A configurable bundle item offers a number of different options from which a customer can select before buying the product. Components for a bundle are not specified in the catalog as the actual components will be determined by the user when an order is placed. A physical kit must always be pre-configured.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsDeliveryAllowed"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_DELIVERY_ALLOWED" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates whether the delivery method DEL is allowed for item. Default value: N.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsForwardingAllowed"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_FORWARDING_ALLOWED"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates if the item can be forwarded for zone skipping. Default value: Y.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsForwardingAllowedQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="IsFreezerRequired"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_FREEZER_REQUIRED"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>Indicates if item requires freezer for transportation. Default value: N.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsFreezerRequiredQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="IsHazmat" type="yfctype:Flag" use="optional">
                        <xsd:annotation yfc:columnName="IS_HAZMAT"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>Indicates if item is hazardous material.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsHazmatQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="IsModelItem"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation yfc:columnName="IS_MODEL_ITEM" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates that the item is an item with variations (formerly known as a model item). An item with variations represents a group of items with similar set of attributes.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsParcelShippingAllowed"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_PARCEL_SHIPPING_ALLOWED" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates whether the item can be shipped through parcel service. Default value: Y.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsPickupAllowed"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_PICKUP_ALLOWED" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates whether the delivery method PICK is allowed for item. Default value: N.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsPreConfigured"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_PRE_CONFIGURED" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates that the configurable item is pre-configured with selected options. If a bundle is pre-configured, the bundle or physical components that represent the selected option will be available in the catalog. For a pre-configured physical kit, only the physical kit will be available as the item in the catalog.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsProcurementAllowed"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_PROCUREMENT_ALLOWED"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates if the item can be procured for zone skipping. Default value: Y.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsProcurementAllowedQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="IsReturnService"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_RETURN_SERVICE" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates whether this provided or delivery service item is to be associated only to a return document.  Default value: N.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsReturnable"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation yfc:columnName="IS_RETURNABLE" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates whether this product item is returnable. Default value: N.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsShippingAllowed"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_SHIPPING_ALLOWED" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates whether the delivery method SHP is allowed for item. Default value: N.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsStandaloneService"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_STANDALONE_SERVICE" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates whether a PS Item is standalone and does not require associations with product lines for it to be included on an order line.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="IsSubOnOrderAllowed"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="IS_SUB_ON_ORDER_ALLOWED" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>Indicates if during scheduling an order, substitution is allowed or not. If Y, schedule will automatically attempt to substitute an item with preferred or regular substitute.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ItemType"
                        type="yfctype:Text-40" use="optional">
                        <xsd:annotation yfc:columnName="ITEM_TYPE"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The generic type of an item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ItemTypeQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="KitCode"
                        type="yfctype:EnumKitCode" use="optional">
                        <xsd:annotation yfc:columnName="KIT_CODE"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This field indicates the kit code for this item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="KitCodeQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="ManufacturerItem"
                        type="yfctype:Text-100" use="optional">
                        <xsd:annotation
                            yfc:columnName="MANUFACTURER_ITEM"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The identifier used by a manufacturer to identify an item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ManufacturerItemDesc"
                        type="yfctype:Text-255" use="optional">
                        <xsd:annotation
                            yfc:columnName="MANUFACTURER_ITEM_DESC"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The description of an item, from a manufacturer's view point.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ManufacturerItemDescQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="ManufacturerItemQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="ManufacturerName"
                        type="yfctype:Text-50" use="optional">
                        <xsd:annotation
                            yfc:columnName="MANUFACTURER_NAME"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The manufacturer of the item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ManufacturerNameQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="MasterCatalogID"
                        type="yfctype:ID-40" use="optional">
                        <xsd:annotation
                            yfc:columnName="MASTER_CATALOG_ID"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The master catalog to which an item belongs.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="MasterCatalogIDQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="MaxOrderQuantity"
                        type="yfctype:Quantity" use="optional">
                        <xsd:annotation
                            yfc:columnName="MAX_ORDER_QUANTITY"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The maximum order quantity.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="MaxOrderQuantityQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="MinOrderQuantity"
                        type="yfctype:Quantity" use="optional">
                        <xsd:annotation
                            yfc:columnName="MIN_ORDER_QUANTITY"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The minimum order quantity.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="MinOrderQuantityQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="MinimumCapacityQuantity"
                        type="yfctype:Quantity" use="optional">
                        <xsd:annotation
                            yfc:columnName="MINIMUM_CAPACITY_QUANTITY"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This is the minimum capacity quantity which will be assumed when  the inventory quantity of the line to arrive at the capacity quantity when the strategy is to arrive at the capacity quantity by association with product line.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="MinimumCapacityQuantityQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="ModelItemUnitOfMeasure"
                        type="yfctype:UOM" use="optional">
                        <xsd:annotation yfc:columnName="MODEL_ITEM_UOM" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This field describes the unit of measure of the item with variations.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="NumSecondarySerials"
                        type="yfctype:Digit" use="optional">
                        <xsd:annotation
                            yfc:columnName="NUM_SECONDARY_SERIALS" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The number of secondary serial numbers applicable for a serialized item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="PricingQuantityConvFactor"
                        type="yfctype:Quantity" use="optional">
                        <xsd:annotation
                            yfc:columnName="PRICING_QUANTITY_CONV_FACTOR"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The pricing quantity conversion factor. This conversion factor is multiplied with the inventory quantity of the line to arrive at this pricing quantity.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute
                        name="PricingQuantityConvFactorQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="PricingQuantityStrategy"
                        type="yfctype:Text-20" use="optional">
                        <xsd:annotation
                            yfc:columnName="PRICING_QUANTITY_STRATEGY"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This field determines how the pricing quantity is arrived at when different from the inventory quantity.   Valid values: IQTY - Based on the inventory quantity.  Default value: IQTY.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="PricingQuantityStrategyQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="PricingUOM" type="yfctype:UOM" use="optional">
                        <xsd:annotation yfc:columnName="PRICING_UOM"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The pricing unit of measure, if different from the inventory unit of measure.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="PricingUOMQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="PricingUOMStrategy"
                        type="yfctype:Text-20" use="optional">
                        <xsd:annotation
                            yfc:columnName="PRICING_UOM_STRATEGY"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This field determines how the pricing unit of measure is defaulted for the item. It could be same as the inventory UOM or could be different.  Valid values: INV - Same as the inventory UOM; ALT - Different from the inventory UOM.  Default value: INV.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="PricingUOMStrategyQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="ProductLine"
                        type="yfctype:Text-100" use="optional">
                        <xsd:annotation yfc:columnName="PRODUCT_LINE"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The product line associated with the item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ProductLineQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="RequiresProdAssociation"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation
                            yfc:columnName="REQUIRES_PROD_ASSOCIATION" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>IThis flag indicates whether the item must be associated to an order line having a group code of PROD. Applicable for service items which must be associated to a Product order line.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ServiceTypeID"
                        type="yfctype:Text-24" use="optional">
                        <xsd:annotation yfc:columnName="SERVICE_TYPE_ID"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The service type associated to this service item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ServiceTypeIDQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="ShipmentConsolidationWindow" use="optional">
                        <xsd:annotation
                            yfc:columnName="SHP_CONSOL_WINDOW"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>Indicates the number of days an item can be delayed after it becomes available to be consolidated to a single shipment. Used by scheduling when shipment/cost optimization is considered.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                            <xsd:union>
                                <xsd:simpleType>
                                    <xsd:restriction base="yfctype:NumberOfDays"/>
                                </xsd:simpleType>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:union>
                        </xsd:simpleType>
                    </xsd:attribute>
                    <xsd:attribute
                        name="ShipmentConsolidationWindowQryType"
                        type="yfctype:NumberQryType" use="optional"/>
                    <xsd:attribute name="ShortDescription"
                        type="yfctype:Text-200" use="optional">
                        <xsd:annotation
                            yfc:columnName="SHORT_DESCRIPTION"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The short description of the item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ShortDescriptionQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="SizeCode"
                        type="yfctype:Text-40" use="optional">
                        <xsd:annotation yfc:columnName="SIZE_CODE" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This field describes the size code of the item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="Status"
                        type="yfctype:EnumShortStatus" use="optional">
                        <xsd:annotation yfc:columnName="STATUS"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This field indicates the status of an item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="StatusQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="TaxableFlag"
                        type="yfctype:Flag" use="optional">
                        <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>This flag indicates whether an item is taxable.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ToCostCurrency"
                        type="yfctype:Currency" use="optional"/>
                    <xsd:attribute name="ToCountryOfOrigin"
                        type="yfctype:Country" use="optional"/>
                    <xsd:attribute name="ToDefaultProductClass"
                        type="yfctype:ProductClass" use="optional"/>
                    <xsd:attribute name="ToDescription"
                        type="yfctype:Text-500" use="optional"/>
                    <xsd:attribute name="ToIsAirShippingAllowed"
                        type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="ToIsForwardingAllowed"
                        type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="ToIsFreezerRequired"
                        type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="ToIsHazmat" type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="ToIsProcurementAllowed"
                        type="yfctype:Flag" use="optional"/>
                    <xsd:attribute name="ToItemType"
                        type="yfctype:Text-40" use="optional"/>
                    <xsd:attribute name="ToKitCode"
                        type="yfctype:EnumKitCode" use="optional"/>
                    <xsd:attribute name="ToManufacturerItem"
                        type="yfctype:Text-100" use="optional"/>
                    <xsd:attribute name="ToManufacturerItemDesc"
                        type="yfctype:Text-255" use="optional"/>
                    <xsd:attribute name="ToManufacturerName"
                        type="yfctype:Text-50" use="optional"/>
                    <xsd:attribute name="ToMasterCatalogID"
                        type="yfctype:ID-40" use="optional"/>
                    <xsd:attribute name="ToMaxOrderQuantity"
                        type="yfctype:Quantity" use="optional"/>
                    <xsd:attribute name="ToMinOrderQuantity"
                        type="yfctype:Quantity" use="optional"/>
                    <xsd:attribute name="ToMinimumCapacityQuantity"
                        type="yfctype:Quantity" use="optional"/>
                    <xsd:attribute name="ToPricingQuantityConvFactor"
                        type="yfctype:Quantity" use="optional"/>
                    <xsd:attribute name="ToPricingQuantityStrategy"
                        type="yfctype:Text-20" use="optional"/>
                    <xsd:attribute name="ToPricingUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="ToPricingUOMStrategy"
                        type="yfctype:Text-20" use="optional"/>
                    <xsd:attribute name="ToProductLine"
                        type="yfctype:Text-100" use="optional"/>
                    <xsd:attribute name="ToServiceTypeID"
                        type="yfctype:Text-24" use="optional"/>
                    <xsd:attribute name="ToShipmentConsolidationWindow"
                        type="yfctype:NumberOfDays" use="optional"/>
                    <xsd:attribute name="ToShortDescription"
                        type="yfctype:Text-200" use="optional"/>
                    <xsd:attribute name="ToStatus"
                        type="yfctype:EnumShortStatus" use="optional"/>
                    <xsd:attribute name="ToUnitCost"
                        type="yfctype:UnitPrice" use="optional"/>
                    <xsd:attribute name="ToUnitHeight"
                        type="yfctype:Dimension" use="optional"/>
                    <xsd:attribute name="ToUnitHeightUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="ToUnitLength"
                        type="yfctype:Dimension" use="optional"/>
                    <xsd:attribute name="ToUnitLengthUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="ToUnitWeight"
                        type="yfctype:Dimension" use="optional"/>
                    <xsd:attribute name="ToUnitWeightUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="ToUnitWidth"
                        type="yfctype:Dimension" use="optional"/>
                    <xsd:attribute name="ToUnitWidthUOM"
                        type="yfctype:UOM" use="optional"/>
                    <xsd:attribute name="UnitCost"
                        type="yfctype:UnitPrice" use="optional">
                        <xsd:annotation yfc:columnName="UNIT_COST"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The cost of one unit of an item.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UnitCostQryType"
                        type="yfctype:NumberQryType" use="optional"/>
                    <xsd:attribute name="UnitHeight"
                        type="yfctype:Dimension" use="optional">
                        <xsd:annotation yfc:columnName="UNIT_HEIGHT"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The height of one unit of item quantity.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UnitHeightQryType"
                        type="yfctype:NumberQryType" use="optional"/>
                    <xsd:attribute name="UnitHeightUOM"
                        type="yfctype:UOM" use="optional">
                        <xsd:annotation yfc:columnName="UNIT_HEIGHT_UOM"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The unit of measure in which item height is measured.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UnitHeightUOMQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="UnitLength"
                        type="yfctype:Dimension" use="optional">
                        <xsd:annotation yfc:columnName="UNIT_LENGTH"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The length of one unit of item quantity.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UnitLengthQryType"
                        type="yfctype:NumberQryType" use="optional"/>
                    <xsd:attribute name="UnitLengthUOM"
                        type="yfctype:UOM" use="optional">
                        <xsd:annotation yfc:columnName="UNIT_LENGTH_UOM"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The unit of measure in which an item length is measured.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UnitLengthUOMQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="UnitWeight"
                        type="yfctype:Dimension" use="optional">
                        <xsd:annotation yfc:columnName="UNIT_WEIGHT"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The weight of one unit of item quantity.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UnitWeightQryType"
                        type="yfctype:NumberQryType" use="optional"/>
                    <xsd:attribute name="UnitWeightUOM"
                        type="yfctype:UOM" use="optional">
                        <xsd:annotation yfc:columnName="UNIT_WEIGHT_UOM"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The unit of measure in which the item weight is measured.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UnitWeightUOMQryType"
                        type="yfctype:StringQryType" use="optional"/>
                    <xsd:attribute name="UnitWidth"
                        type="yfctype:Dimension" use="optional">
                        <xsd:annotation yfc:columnName="UNIT_WIDTH"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The width of one unit of item quantity.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UnitWidthQryType"
                        type="yfctype:NumberQryType" use="optional"/>
                    <xsd:attribute name="UnitWidthUOM"
                        type="yfctype:UOM" use="optional">
                        <xsd:annotation yfc:columnName="UNIT_WIDTH_UOM"
                            yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                            <xsd:documentation>The unit of measure in which an item's width is measured.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UnitWidthUOMQryType"
                        type="yfctype:StringQryType" use="optional"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:InventoryParameters"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ClassificationCodes"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemAliasList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemExclusionList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AdditionalAttributeList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CategoryFilter"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Components"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:LanguageDescriptionList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:InventoryTagAttributes"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemAttributeGroupTypeList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderBy"/>
        </xsd:all>
        <xsd:attribute name="AuthenticationKey" type="yfctype:Key" use="optional">
            <xsd:annotation>
                <xsd:documentation>Not used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CallingOrganizationCode"
            type="yfctype:OrgCode" use="required">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
					The organization code that the API is being invoked for.  This organization will be used to retrieve pricing data and availability.
					<Validation>
                            <Condition>
                                <code>CallingOrganizationCode</code> is not passed.
						</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>CallingOrganizationCode</code> does not identify a valid organization.</Condition>
                            <Error
                                Description="YFS:Invalid organization code" ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CanUseAsServiceTool" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="CAN_USE_AS_SERVICE_TOOL" yfc:tableName="YFS_ITEM">
                <xsd:documentation>This flag indicates whether this item is configured to be a service tool.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Currency" type="yfctype:Currency" use="optional">
            <xsd:annotation yfc:columnName="CURRENCY" yfc:tableName="YFS_PRICE_PROGRAM_DEFN">
                <xsd:documentation>
					The currency to return prices in.  If not passed, this is defaulted to the currency of the <code>CallingOrganizationCode</code>'s primary enterprise's locale.
				</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DeliveryMethod" type="yfctype:String" use="optional"/>
        <xsd:attribute name="DemandType" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="DEMAND_TYPE" yfc:tableName="YFS_INVENTORY_DEMAND_TYPE">
                <xsd:documentation>
					The demand type to be used when determining availability for items.  If not passed, <code>OPEN_ORDER</code> demand type is assumed.
				</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DisplayLocalizedFieldInLocale"
            type="yfctype:Locale" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						The locale for which output should be returned in. Locale should be passed in the standard format of "language_country_variant" (e.g., "fr_FR", "en_US_CA", etc.).
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DistributionRuleId"
            type="yfctype:DistributionRuleId" use="optional">
            <xsd:annotation>
                <xsd:documentation>The distribution rule ID to be used to determine the node(s) that 
				will be used for availability checks.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="EntitlementDate" type="yfctype:TimeStamp" use="optional">
            <xsd:annotation>
                <xsd:documentation>The date to be used when reading entitlements.  If not passed, this date defaults to the current date.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExcludeChildItemsOfModelItems"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
			This flag indicates whether child items of an item with variations should be returned in the search results.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExcludeItemsInPriceList"
            type="yfctype:String" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>Items within this Pricelist will be excluded from the search results.</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExecuteQueries" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
					Indicates that query-based associations should be executed, and a list of matching items should be returned under the <code>Association</code> element.  If this flag is passed as <code>
                        <b>N</b>
                    </code>, query-based associations will not be returned.<br/>
					The default value is <code>
                        <b>N</b>
                    </code>.
				</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FromGlobalItemID"
            type="yfctype:GlobalItemID" use="optional"/>
        <xsd:attribute name="FromItemGroupCode" type="yfctype:Text-20" use="optional"/>
        <xsd:attribute name="FromItemID" type="yfctype:ItemID" use="optional"/>
        <xsd:attribute name="FromUnitOfMeasure" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="FulfillmentType" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="FULFILLMENT_TYPE" yfc:tableName="YFS_SOURCING_RULE_HDR">
                <xsd:documentation>
					The fulfillment type to be used when determining availability for items.  If not passed, the <code>CallingOrganizationCode</code>'s primary enterprise's default distribution rule will be used.
				</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="GetAvailabilityFromCache" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
			This flag indicates whether the available quantity for the item must be computed or obtained from cache. When <code>GetAvailabilityFromCache</code> is not passed or is passed as <code>N</code>, the output for the <code>Availability</code> element will only contain the attribute <code>CurrentAvailableQty</code>, if passed in the template.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="yfctype:Flag">
                    <xsd:enumeration value="Y"/>
                    <xsd:enumeration value="N"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="GetUnpublishedItems" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>Indicates if unpublished items needs to be listed. The default value is <code>
                        <b>N</b>
                    </code>.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="GlobalItemID" type="yfctype:GlobalItemID" use="optional">
            <xsd:annotation yfc:columnName="GLOBAL_ITEM_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                <xsd:documentation>Unique Global identifier used to cross reference an item with another catalog organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="GlobalItemIDQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IgnoreInvalidItems" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						This flag indicates whether invalid items should be returned in the search results.
						Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
						Default value: <b>
                            <code>Y</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IgnoreIsSoldSeparately" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						Set this flag to consider all items regardlessly of the <code>IsSoldSeparately</code> flag.
						Valid values: <b>
                            <code>Y</code>
                        </b>, <b>
                            <code>NULL</code>
                        </b>, or <b>
                            <code>N</code>
                        </b>
						Default value: <b>
                            <code>NULL</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IgnoreOrdering" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
				By default, the output is ordered by <code>ORGANIZATION_CODE</code>, <code>ITEM_ID</code>, and <code>UNIT_OF_MEASURE</code>. Pass <code>IgnoreOrdering</code> as <b>
                            <code>Y</code>
                        </b> to omit ordering.
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InheritAttributesFromClassification"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>If the InheritAttributesFromClassification flag is passed as Y and the value of the item attribute is not defined at item level, the system will try to find the value from the classification definition for the ClassificationPurposeCode - 'Inherited Item Attributes'.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsForOrdering" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
			This flag indicates whether the item search is for ordering purpose. It must be set to <code>Y</code> to apply customer entitlements.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>Y</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemGroupCode" type="yfctype:Text-20" use="optional">
            <xsd:annotation yfc:columnName="ITEM_GROUP_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                <xsd:documentation>
                    <div>
					Valid values: <b>
                            <code>PS</code>
                        </b> - Provided Service, <b>
                            <code>DS</code>
                        </b> - Delivery Service, <b>
                            <code>PROD</code>
                        </b> - Product Item
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemGroupCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
            <xsd:annotation yfc:columnName="ITEM_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                <xsd:documentation>The unique identifier for the item.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemIDQryType" type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="MaximumRecords" type="yfctype:Count" use="optional">
            <xsd:annotation>
                <xsd:documentation>Maximum records requested for output. Defaults to 5000.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationCode" type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
					The <code>OrganizationCode</code> attribute can be passed to override the catalog organization.  By default, the <code>CallingOrganizationCode</code>'s catalog organization code is used.  The catalog organization code corresponds to the <code>OrganizationCode</code> attribute of the items, and will be used to query the YFS_ITEM table.
				</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PriceProgramKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="PRICE_PROGRAM_KEY" yfc:tableName="YFS_PRICE_PROGRAM">
                <xsd:documentation>
					The key of the price program to use to compute prices.  If not passed, this will be defaulted to the <code>CallingOrganizationCode</code>'s default price program if specified.
					<br/>In order to compute prices, the <code>PriceProgramKey</code> or <code>PriceProgramName</code> and <code>CallingOrganizationCode</code> must be passed, if the <code>CallingOrganizationCode</code> does not define a default price program.
				</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PriceProgramName" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="PRICE_PROGRAM_NAME" yfc:tableName="YFS_PRICE_PROGRAM">
                <xsd:documentation>
					The name of the price program to use to compute prices.  This value is used along with <code>CallingOrganizationCode</code> to identify a price program.  If not passed, this will be defaulted to the <code>CallingOrganizationCode</code>'s default price program if specified.
					<br/>In order to compute prices, the <code>PriceProgramKey</code> or <code>PriceProgramName</code> and <code>CallingOrganizationCode</code> must be passed, if the <code>CallingOrganizationCode</code> does not define a default price program.
				</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PricingDate" type="yfctype:TimeStamp" use="optional">
            <xsd:annotation>
                <xsd:documentation>The date to be used when computing prices.  If not passed, this date defaults to the current date.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PurposeForDisplayClassification"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="CLASSIFICATION_PURPOSE_CODE" yfc:tableName="YFS_CLASSIFICATION_PURPOSE">
                <xsd:documentation>
					If passed, the corresponding classification path of each item for the specified purpose will be returned in the output as <code>DisplayClassification</code> on the <code>Item</code>'s <code>PrimaryInformation</code> element.  The value passed as <code>PurposeForDisplayClassification</code> should correspond to a valid <code>ClassificationPurposeCode</code> for a classification purpose in the system.  A list of valid <code>ClassificationPurposeCode</code> values can be obtained from the <code>getClassificationPurposeList</code> API.
				</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RequiresDGOverride" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="REQUIRES_DG_OVERRIDE" yfc:tableName="YFS_ITEM">
                <xsd:documentation>This flag indicates if the Default Distribution Rule has to be overridden for real time availability monitoring.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SellerOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
					The organization code of the Seller Organization.
					<Validation>
                            <Condition>
                                <code>SellerOrganizationCode</code> does not identify a valid organization.</Condition>
                            <Error
                                Description="YFS:Invalid organization code" ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShowAttributesWithDifferentValuesOnly"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
					Flag indicating that only attributes that have different values for different items should be returned when calling the API for item comparison.
					Valid values: <b>
                        <code>Y</code>
                    </b> or <b>
                        <code>N</code>
                    </b>
						Default value: <b>
                        <code>N</code>
                    </b>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShowFinalSupersededItemOnly"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>When set to <code>Y</code>, only the final valid superseded item will be returned as the superseded item.
				<br/>Default: <code>N</code>
                    <br/>Valid values: <code>Y</code>, <code>N</code>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShowKeyItemAttributesOnly"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
					Flag indicating that only key item attributes should be returned when the API is called to get items for comparison.
					Valid values: <b>
                        <code>Y</code>
                    </b> or <b>
                        <code>N</code>
                    </b>
						Default value: <b>
                        <code>N</code>
                    </b>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="StartItemKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ITEM_KEY" yfc:tableName="YFS_ITEM">
                <xsd:documentation>To start from a particular Item key.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SubCatalogOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="SUB_CAT_ORG_CODE" yfc:tableName="YFS_ITEM">
                <xsd:documentation>The sub-catalog organization that maintains the item definition.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ToGlobalItemID" type="yfctype:GlobalItemID" use="optional"/>
        <xsd:attribute name="ToItemGroupCode" type="yfctype:Text-20" use="optional"/>
        <xsd:attribute name="ToItemID" type="yfctype:ItemID" use="optional"/>
        <xsd:attribute name="ToUnitOfMeasure" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
            <xsd:annotation yfc:columnName="UOM"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                <xsd:documentation>The unit of measure for item quantity.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="UnitOfMeasureQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="UserOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
						Not used by <code>getItemListForOrdering</code>.  <code>CallingOrganizationCode</code> should be used instead.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="ServiceType">
        <xsd:annotation yfc:tableName="YFS_SERVICE_TYPE">
            <xsd:documentation>This table stores service types that are used to group service items.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="IsInStoreService" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_IN_STORE_SERVICE" yfc:tableName="YFS_SERVICE_TYPE">
                    <xsd:documentation>This flag indicates whether the service item can be serviced at store location.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CustomerInformation">
        <xsd:annotation yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
            <xsd:documentation>This is used to provide information about the customer for which items are being retrieved when entitlements need to be applied. CustomerID is used to get 
				customer information from the system. If both CustomerID and CustomerType are not populated, CustomerType is
				defaulted to <code>03 - Anonymous</code>. All other attributes are used as an override to apply corresponding entitlements. </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BuyerUserId"
                type="yfctype:ContactUserId" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>An identifier of the customer. This is a pass through field available to user exits.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerContactID" type="yfctype:ID-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>An identifier of the customer. This is a pass through field available to user exits.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerID" type="yfctype:CustomerId" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ID" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>The identifier of the customer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerLevel" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_LEVEL" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>Level of customer membership (Gold, Tin, Platinum, etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerType" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_TYPE" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>
                        <div> The type of customer.
							<br/>Valid Values:
							<br/>01 - for B2B customer
							<br/>02 - for B2C customer
							<br/>03 - for anonymous customer 
							<br/>04 - for everyone
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Region" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="REGION_KEY" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>
                        <div> Region to which entitlement rule is assigned. </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RelationshipType"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="RELATIONSHIP_TYPE" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>The type of the relationship that exists between an organization and an enterprise (distributor, retailer, reseller, etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Vertical" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="VERTICAL" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>The vertical customer belongs to.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ShipToAddress">
        <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
            <xsd:documentation>The address to be used when computing prices.  This address will be used to determine a pricing region.  Additionally, this address will be used when determining availability.  If this address is not passed, no region information will be available to determine pricing or availability.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AlternateEmailID"
                type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="City" type="yfctype:City" use="optional">
                <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The city in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The country in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ErrorTxt" type="yfctype:ErrorText" use="optional">
                <xsd:annotation yfc:columnName="ERROR_TXT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The first name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HttpUrl" type="yfctype:URL" use="optional">
                <xsd:annotation yfc:columnName="HTTP_URL" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The URL of the person or organization referred to in this contact information.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsCommercialAddress"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="JobTitle" type="yfctype:Title-40" use="optional">
                <xsd:annotation yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The job title of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The last name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonInfoKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PERSON_INFO_KEY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary key for the YFS_PERSON_INFO table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PreferredShipAddress"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PREFERRED_SHIP_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The preferred ship address over and above the address in this record. Pointer to another record of this type in this table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="State" type="yfctype:State" use="optional">
                <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The state in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                <xsd:annotation yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UseCount" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="USE_COUNT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VerificationStatus"
                type="yfctype:ShortStatus" use="optional">
                <xsd:annotation yfc:columnName="VERIFICATION_STATUS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ShipNodes">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipNode"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ShipNode">
        <xsd:annotation yfc:tableName="YFS_SHIP_NODE">
            <xsd:documentation>
						This element can be used to indicate a specific set of nodes that <code>getItemListForOrdering</code> should use when determining availability.
					</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Node" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="SHIP_NODE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The unique identifier for the ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemAssociationTypeList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ItemAssociationType"/>
            </xsd:sequence>
            <xsd:attribute name="MaximumRecords" type="yfctype:Count" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Maximum records requested for output. Defaults to 1000.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderBy" type="yfctype:Text-32" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Name of the attribute to use for sorting associations. Presently, value is defaulted to <code>Priority</code>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemAssociationType">
        <xsd:complexType>
            <xsd:attribute name="Type" type="yfctype:String" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemLocaleList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemLocale"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemLocale">
        <xsd:annotation yfc:tableName="YFS_ITEM_LOCALE">
            <xsd:documentation>The table is used to store localized information for the related item in different locales.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="PrimaryInformation">
                    <xsd:annotation yfc:tableName="YFS_ITEM_LOCALE">
                        <xsd:documentation>The table is used to store localized information for the related item in different locales.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="Description"
                            type="yfctype:Text-500" use="optional">
                            <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_ITEM_LOCALE">
                                <xsd:documentation>The description of the item.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShortDescription"
                            type="yfctype:Text-200" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHORT_DESCRIPTION" yfc:tableName="YFS_ITEM_LOCALE">
                                <xsd:documentation>The short description of the item.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_ITEM_LOCALE">
                    <xsd:documentation>The country associated with the locale.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Language" type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="LANGUAGE" yfc:tableName="YFS_ITEM_LOCALE">
                    <xsd:documentation>The language associated with the locale.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Variant" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="VARIANT" yfc:tableName="YFS_ITEM_LOCALE">
                    <xsd:documentation>The variant associated with the locale.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemServiceAssoc">
        <xsd:annotation yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
            <xsd:documentation>All valid provided service items and delivery service associated to an item are maintained in this table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ServiceItemGroupCode"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_ITEM_GROUP_CODE" yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
                    <xsd:documentation>ItemGroupCode for the service or delivery item.  Valid values are PS - Provided Service, DS - Delivery Service.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceItemId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_ITEM_ID" yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
                    <xsd:documentation>Provided service or delivery service item associated as option when selling the parent item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceOrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation
                    yfc:columnName="SERVICE_ORGANIZATION_CODE" yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
                    <xsd:documentation>Organization code of Service.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_UOM" yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
                    <xsd:documentation>UOM of the provided service or delivery service item associated to the parent item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="InventoryParameters">
        <xsd:annotation yfc:tableName="YFS_ITEM">
            <xsd:documentation>The table stores product information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ATPRule" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ATP_RULE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The available to promise rule to be used for the item to determine availability.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ATPRuleQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="AdvanceNotificationTime" use="optional">
                <xsd:annotation
                    yfc:columnName="ADVANCE_NOTIFICATION_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The advance notification time for this item. Communication to ship the order to shipping node/supplier is made at most this many days before the expected shipment date. Used during the release process. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="AdvanceNotificationTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="CapacityQuantityStrategy"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation
                    yfc:columnName="CAPACITY_QUANTITY_STRATEGY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field determines how the capacity quantity is arrived at for provided and delivery services from the inventory quantity.   Valid values: ORD - Based on the ordered service quantity; ASSOC - Based on the service and product association.  Default value: ORD.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CapacityQuantityStrategyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="DefaultExpirationDays"
                type="yfctype:NumberOfDays" use="optional">
                <xsd:annotation yfc:columnName="DEFAULT_EXPIRATION_DAYS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The default for number of days after which an item would expire.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DefaultExpirationDaysQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="FromATPRule" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromAdvanceNotificationTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromCapacityQuantityStrategy"
                type="yfctype:Text-20" use="optional"/>
            <xsd:attribute name="FromDefaultExpirationDays"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromFutureSafetyFactorPercentage"
                type="yfctype:Percentage" use="optional"/>
            <xsd:attribute name="FromInventoryMonitorRule"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromLeadTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromMaximumNotificationTime"
                type="yfctype:NumberOfHours" use="optional"/>
            <xsd:attribute name="FromMinNotificationTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromNodeLevelInventoryMonitorRule"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromOnhandSafetyFactorPercentage"
                type="yfctype:Percentage" use="optional"/>
            <xsd:attribute name="FromOnhandSafetyFactorQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="FromProcessingTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromTimeSensitive" type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FutureSafetyFactorPercentage" use="optional">
                <xsd:annotation
                    yfc:columnName="FUTURE_SAFETY_FACTOR_PCT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The future safety factor percentage of this item.  The FutureSafetyFactorPercentage will be excluded from the actual future availability of the item if the scheduling rule, node type, and supply type are configured to apply future safety factor.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Percentage"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FutureSafetyFactorPercentageQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="InventoryMonitorRule"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="INVENTORY_MONITOR_RULE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The monitoring rule that will be used by the availability monitor.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InventoryMonitorRuleQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="IsFifoTracked" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FIFO_TRACKED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is FIFO tracked. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsItemBasedAllocationAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_IBA_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the item is allowed for Item Based Allocation process.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsSerialTracked" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_SERIAL_TRACKED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is to be serial tracked in inventory.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LeadTime" use="optional">
                <xsd:annotation yfc:columnName="LEAD_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The lead time for getting the product delivered. Purchase order placed today will arrive after this many days at the warehouse. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LeadTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="MaximumNotificationTime" use="optional">
                <xsd:annotation yfc:columnName="MAX_NOTIFICATION_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The maximum notification time for this item. Communication to ship the order to shipping node/supplier is made at most this many shipping calendar hours before the expected shipment date. Used during the release process. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfHours"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MaximumNotificationTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="MinNotificationTime" use="optional">
                <xsd:annotation yfc:columnName="MIN_NOTIFICATION_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The minimum notification time for this item. Communication to ship the order should be made at least this many days before the expected shipment date. Used to determine the earliest ship date on the line. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MinNotificationTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="NodeLevelInventoryMonitorRule"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="NODE_LEVEL_INV_MONITOR_RULE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The monitoring rule that will be used by the real-time availability monitor for node-level monitoring.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NodeLevelInventoryMonitorRuleQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OnhandSafetyFactorPercentage" use="optional">
                <xsd:annotation
                    yfc:columnName="ONHAND_SAFETY_FACTOR_PCT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The on-hand safety factor percentage of this item.  The OnhandSafetyFactorPercentage will be excluded from the actual on-hand availability of the item if the scheduling rule, node type, and supply type are configured to apply on-hand safety factor.   OnhandSafetyFactorPercentage will be used only when OnhandSafetyFactorQuantity is not specified.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Percentage"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OnhandSafetyFactorPercentageQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="OnhandSafetyFactorQuantity" use="optional">
                <xsd:annotation
                    yfc:columnName="ONHAND_SAFETY_FACTOR_QTY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The on-hand safety factor quantity of this item.  The OnhandSafetyFactorQuantity will be excluded from the actual on-hand availability of the item if the scheduling rule, node type, and supply type are configured to apply on-hand safety factor.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Quantity"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OnhandSafetyFactorQuantityQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ProcessingTime"
                type="yfctype:NumberOfDays" use="optional">
                <xsd:annotation yfc:columnName="PROCESSING_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Define processing time for items in days such as special services.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProcessingTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="SerializedFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SERIALIZED_FLAG" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is serialized.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TagControlFlag"
                type="yfctype:EnumTagControlFlag" use="optional">
                <xsd:annotation yfc:columnName="TAG_CONTROL_FLAG" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field indicates whether an item is tag controlled. Inventory module uses this flag to determine if additional inventory attributes need to be stored for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TimeSensitive" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TIME_SENSITIVE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indiactes whether the item is time-sensitive. Inventory module uses this to determine if inventory is dated. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TimeSensitiveQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToATPRule" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToAdvanceNotificationTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToCapacityQuantityStrategy"
                type="yfctype:Text-20" use="optional"/>
            <xsd:attribute name="ToDefaultExpirationDays"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToFutureSafetyFactorPercentage"
                type="yfctype:Percentage" use="optional"/>
            <xsd:attribute name="ToInventoryMonitorRule"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToLeadTime" type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToMaximumNotificationTime"
                type="yfctype:NumberOfHours" use="optional"/>
            <xsd:attribute name="ToMinNotificationTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToNodeLevelInventoryMonitorRule"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToOnhandSafetyFactorPercentage"
                type="yfctype:Percentage" use="optional"/>
            <xsd:attribute name="ToOnhandSafetyFactorQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="ToProcessingTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToTimeSensitive" type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="UseUnplannedInventory"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="USE_UNPLANNED_INV" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the item is allowed to use unplanned inventory. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
