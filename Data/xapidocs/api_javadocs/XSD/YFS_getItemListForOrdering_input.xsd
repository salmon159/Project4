    <xsd:element name="ItemAliasList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ItemAlias"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemAlias">
        <xsd:annotation yfc:tableName="YFS_ITEM_ALIAS">
            <xsd:documentation>This table is used to cross-reference item id of one organization to another. It also stores all aliases for an item. One of the aliases can be designated as a "cross-referencing" alias.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AliasName" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ALIAS_NAME" yfc:tableName="YFS_ITEM_ALIAS">
                    <xsd:documentation>Alias type. For example, UPC, EAN, MFG. CODE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AliasValue" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="ALIAS_VALUE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_ALIAS">
                    <xsd:documentation>Alias value for the corresponding alias name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AliasValueQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromAliasValue" type="yfctype:Text-100" use="optional"/>
            <xsd:attribute name="ToAliasValue" type="yfctype:Text-100" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemExclusionList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ItemExclusion"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemExclusion">
        <xsd:annotation yfc:tableName="YFS_ITEM_EXCLUSION">
            <xsd:documentation>Used to store exclusion codes associated with an item. Can be used to store list of countries, customers etc that are excluded from ordering an item. This element is not enabled for complex query.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ExclusionCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="EXCLUSION_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_EXCLUSION">
                    <xsd:documentation>Exclusion code for an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExclusionCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromExclusionCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToExclusionCode" type="yfctype:Text-40" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalAttributeList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:AdditionalAttribute"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalAttribute">
        <xsd:annotation yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
            <xsd:documentation>Stores dynamic attributes attached with an entity. Currently used to store additional attributes for item and category entities.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DoubleValue" use="optional">
                <xsd:annotation yfc:columnName="DOUBLE_VALUE" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>The value of the assigned attribute whose data type is 'DECIMAL'.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Number"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FromValue" type="yfctype:Text-2000" use="optional"/>
            <xsd:attribute name="IntegerValue" use="optional">
                <xsd:annotation yfc:columnName="INTEGER_VALUE" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>The value of the assigned attribute whose data type is 'INTEGER'.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Number20"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsInherited" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_INHERITED" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>This field indicates whether the assignment of attribute value as additional attribute was based on inheritance. For ParentTable='YFS_CATEGORY', a value of 'Y' indicates that the assignment is based on inheritance of the value assigned to the upper level category. For ParentTable='YFS_ITEM', a value of 'Y' indicates that the assignment is based on inheritance of the value assigned to the category to which the item belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Name" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="NAME" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>Name of the dynamic attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ToValue" type="yfctype:Text-2000" use="optional"/>
            <xsd:attribute name="Value" type="yfctype:Text-2000" use="optional">
                <xsd:annotation yfc:columnName="VALUE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>Value of the attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ValueQryType"
                type="yfctype:StringQryType" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CategoryFilter">
        <xsd:annotation yfc:tableName="YFS_CATEGORY">
            <xsd:documentation>Used to filter items based on category. This element is not enabled for complex query.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CategoryPath" type="yfctype:Text-800" use="optional">
                <xsd:annotation yfc:columnName="CATEGORY_PATH"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CATEGORY">
                    <xsd:documentation>The path for the category.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CategoryPathQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromCategoryPath"
                type="yfctype:Text-800" use="optional"/>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_CATEGORY">
                    <xsd:documentation>The code for the organization this category is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ToCategoryPath" type="yfctype:Text-800" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Components">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Component"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Component">
        <xsd:annotation yfc:tableName="YFS_KIT_ITEM">
            <xsd:documentation>A Component Definition.  In order to correctly identify a component item, either <code>ComponentOrganizationCode</code>, <code>ComponentItemID</code> and <code>ComponentUnitOfMeasure</code> (if not blank) must be passed, or <code>ComponentItemKey</code> must be passed.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ComponentItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Along with ComponentOrganizationCode and ComponentUnitOfMeasure, it identifies a component item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ComponentItemKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ITEM_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Component item's primary key</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ComponentItemKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ComponentOrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Organization that maintains catalog.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ComponentUnitOfMeasure"
                type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Unit of measure for the Component Item</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FromComponentItemKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToComponentItemKey" type="yfctype:Key" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LanguageDescriptionList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:LanguageDescription"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LanguageDescription">
        <xsd:annotation yfc:tableName="YFS_LANGUAGE_DESCRIPTION">
            <xsd:documentation>This table stores description in different languages.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="FromLocaleCode" type="yfctype:Locale" use="optional"/>
            <xsd:attribute name="LocaleCode" type="yfctype:Locale" use="optional">
                <xsd:annotation yfc:columnName="LOCALE_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_LANGUAGE_DESCRIPTION">
                    <xsd:documentation>Locale for which the extended description is stored.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LocaleCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToLocaleCode" type="yfctype:Locale" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="InventoryTagAttributes">
        <xsd:annotation yfc:tableName="YFS_ITEM_TAG">
            <xsd:documentation>This table stores and defines the Tag Identifier and Tag Descriptor attributes for Tag Controlled Items.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BatchNo" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Identifier or a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 02 - Use as Tag Identifier, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BatchNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromBatchNo" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromItemTagKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromLotAttribute1"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromLotAttribute2"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromLotAttribute3"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromLotKeyReference"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromLotNumber" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromManufacturingDate"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromRevisionNo" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ItemTagKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ITEM_TAG_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Primary key attribute of YFS_ITEM_TAG.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemTagKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotAttribute1" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE1"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute1QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotAttribute2" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE2"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute2QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotAttribute3" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE3"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute3QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotKeyReference" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_KEY_REFERENCE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotKeyReferenceQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotNumber" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_NUMBER"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Identifier or a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 02 - Use as Tag Identifier, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotNumberQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ManufacturingDate"
                type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_MANUFACTURE_DATE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturingDateQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="RevisionNo" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="REVISION_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Identifier or a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 02 - Use as Tag Identifier, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RevisionNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToBatchNo" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToItemTagKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToLotAttribute1" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToLotAttribute2" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToLotAttribute3" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToLotKeyReference"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToLotNumber" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToManufacturingDate"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToRevisionNo" type="yfctype:Text-2" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemAttributeGroupTypeList">
        <xsd:annotation>
            <xsd:documentation>
                <div>
				A list of ItemAttributeGroupType can be passed to be used as filter criteria to control the item attribute group types to return in the output XML.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ItemAttributeGroupType"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemAttributeGroupType">
        <xsd:annotation yfc:tableName="YFS_ITEM_ATTR_GROUP_TYPE">
            <xsd:documentation>This table stores item attribute group types.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ClassificationPurposeCode"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation
                    yfc:columnName="CLASSIFICATION_PURPOSE_CODE" yfc:tableName="YFS_ITEM_ATTR_GROUP_TYPE">
                    <xsd:documentation>The classification purpose this group type is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemAttributeGroupType"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ITEM_ATTR_GROUP_TYPE" yfc:tableName="YFS_ITEM_ATTR_GROUP_TYPE">
                    <xsd:documentation>The group type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderBy">
        <xsd:annotation>
            <xsd:documentation>
                <div>
				If OrderBy element is passed, items returned by the API are sorted in the same order as Attribute elements passed.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:Attribute"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Attribute">
        <xsd:annotation yfc:tableName="YFS_ATTRIBUTE">
            <xsd:documentation>This table stores attribute definitions. Attributes represent characteristics of entities and can be assigned to an attribute group.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Desc" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Sort in decending order.
						Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
						Default value: <b>
                            <code>N</code>
                        </b>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Name" type="yfctype:Text-32" use="required">
                <xsd:annotation>
                    <xsd:documentation>Name of the attribute to use for sorting the results.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ComplexQuery">
        <xsd:complexType>
            <xsd:sequence maxOccurs="unbounded" minOccurs="1">
                <xsd:choice>
                    <xsd:element maxOccurs="1" minOccurs="1" name="And" type="yfc:ComplexQueryAnd"/>
                    <xsd:element maxOccurs="1" minOccurs="1" name="Or" type="yfc:ComplexQueryOr"/>
                    <xsd:element maxOccurs="1" minOccurs="1" name="Exp" type="yfc:ComplexQueryExp"/>
                </xsd:choice>
            </xsd:sequence>
            <xsd:attribute name="Operator" use="optional">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value=""/>
                        <xsd:enumeration value="AND"/>
                        <xsd:enumeration value="And"/>
                        <xsd:enumeration value="and"/>
                        <xsd:enumeration value="OR"/>
                        <xsd:enumeration value="Or"/>
                        <xsd:enumeration value="or"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="ComplexQueryAnd">
        <xsd:sequence maxOccurs="unbounded" minOccurs="1">
            <xsd:choice>
                <xsd:element maxOccurs="1" minOccurs="1" name="And" type="yfc:ComplexQueryAnd"/>
                <xsd:element maxOccurs="1" minOccurs="1" name="Or" type="yfc:ComplexQueryOr"/>
                <xsd:element maxOccurs="1" minOccurs="1" name="Exp" type="yfc:ComplexQueryExp"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ComplexQueryOr">
        <xsd:sequence maxOccurs="unbounded" minOccurs="1">
            <xsd:choice>
                <xsd:element maxOccurs="1" minOccurs="1" name="And" type="yfc:ComplexQueryAnd"/>
                <xsd:element maxOccurs="1" minOccurs="1" name="Or" type="yfc:ComplexQueryOr"/>
                <xsd:element maxOccurs="1" minOccurs="1" name="Exp" type="yfc:ComplexQueryExp"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ComplexQueryExp">
        <xsd:attribute name="Name" type="xsd:string" use="required"/>
        <xsd:attribute name="Value" type="xsd:string" use="optional"/>
        <xsd:attribute name="QryType" use="optional">
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value="EQ"/>
                    <xsd:enumeration value="LIKE"/>
                    <xsd:enumeration value="FLIKE"/>
                    <xsd:enumeration value="NE"/>
                    <xsd:enumeration value="GE"/>
                    <xsd:enumeration value="LE"/>
                    <xsd:enumeration value="GT"/>
                    <xsd:enumeration value="LT"/>
                    <xsd:enumeration value="ISNULL"/>
                    <xsd:enumeration value="NOTNULL"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
</xsd:schema>
