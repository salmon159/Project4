                                    type="yfctype:UserId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CONTACT_USER" yfc:tableName="YFS_PARTICIPANT_NOTE">
                                    <xsd:documentation>The user who made the contact.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute
                                    name="CustomerSatIndicator"
                                    type="yfctype:Text-255" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_SAT_INDICATOR" yfc:tableName="YFS_PARTICIPANT_NOTE">
                                    <xsd:documentation>The customer satisfaction level.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="NoteText"
                                    type="yfctype:Text-2000" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="NOTE_TEXT" yfc:tableName="YFS_PARTICIPANT_NOTE">
                                    <xsd:documentation>The detailed notes for this entity.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Operation"
                                    type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					 Valid value is <b>
                                    <code>Delete</code>
                                    </b> or blank.  If not provided the information will be created or modified.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Priority"
                                    type="yfctype:PriorityWithDecimal" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRIORITY" yfc:tableName="YFS_PARTICIPANT_NOTE">
                                    <xsd:documentation>Priority of the note. 1 is High, 0 is Low.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="ReasonCode"
                                    type="yfctype:ReasonCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_PARTICIPANT_NOTE">
                                    <xsd:documentation>The reason for which the notes are logged. Like "CustomerContact" etc. The values will be selected from common code of code type "NOTES_REASON".</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Tranid"
                                    type="yfctype:TranId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TRANID" yfc:tableName="YFS_PARTICIPANT_NOTE">
                                    <xsd:documentation>Transaction for which these notes are applicable.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="VisibleToAll"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="VISIBLE_TO_ALL" yfc:tableName="YFS_PARTICIPANT_NOTE">
                                    <xsd:documentation>This flag determines whether the note applied should be visible to all users. This field is used for filtering. No logic is currently applied to it. Default: Y.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>
                                <div>
                                    <code>Reset</code> flag clears all previously recorded notes and replaces them with the new notes passed in the input during this api call.<br/>
							Valid Values: <code>Y</code>, <code>N</code>
                                    <br/>Default Value: <code>N</code>
                                </div>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CustomerCurrencyList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CustomerAnalytics"/>
        </xsd:all>
        <xsd:attribute name="BuyerOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="BUYER_ORGANIZATION_CODE" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>
                    <div>
                        <code>BuyerOrganizationCode</code> attribute must be passed when creating a customer representing an existing organization. If <code>BuyerOrganizationCode</code> is passed, the API will update the organization with information provided in <code>BuyerOrganization</code> element.   If <code>BuyerOrganizationCode</code> is not passed, the API will create a new organization with information provided in <code>BuyerOrganization</code> element.
				<Validation>
                            <Condition>Organization does not exist.
					</Condition>
                            <Error Description="Invalid Organization." ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>BuyerOrganizationCode</code> attribute is not passed when creating a customer representing an existing organization.
					</Condition>
                            <Error
                                Description="Record already exists in the database." ErrorCode="YFC0001"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CanConsumeSupplementalCapacity"
            type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="CAN_CONSUME_SUPPL_CAPACITY" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>This flag indicates whether the customer can be scheduled against the supplemental capacity.  Default value is N.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CommitmentTime" use="optional">
            <xsd:annotation yfc:columnName="COMMITMENT_TIME" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>Hours taken to send commitment to the customer.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:NumberOfHours"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="CustomerClassificationCode"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation
                yfc:columnName="CUSTOMER_CLASSIFICATION_CODE" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>The classification code of customer.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerID" type="yfctype:CustomerId" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_ID" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>
                    <div>
				Identifier for a customer.  If <code>CustomerID</code> is not provided and Customer is a business customer, it will be defaulted as <code>BuyerOrganizationCode</code>.  If <code>CustomerID</code> is not provided and Customer is a consumer customer, it will be defaulted as consumer's <code>BillingPersonInfo's PersonID</code>.
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_KEY" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>The primary key for the YFS_CUSTOMER table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerLevel" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_LEVEL" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>Indicates a level a customer has achieved.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerRewardsNo" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_REWARDS_NO" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>The identifier of the customer's rewards number</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerType"
            type="yfctype:EnumCustomerType" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_TYPE" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>
                    <div>
				Type of customer.  Valid values are: 01-Business, 02-Consumer.  Default value is 01.  If <code>CustomerType</code> is <b>
                            <code>01</code>
                        </b>, <code>BuyerOrganization</code> information is required and <code>Consumer</code> element will be ignored.  If <code>CustomerType</code> is <b>
                            <code>02</code>
                        </b>, <code>Consumer</code> element is required and <code>BuyerOrganization</code> information will be ignored.
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExternalCustomerID"
            type="yfctype:CustomerId" use="optional">
            <xsd:annotation yfc:columnName="EXTERNAL_CUSTOMER_ID" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>The external identifier for the customer.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FuncAckTime" use="optional">
            <xsd:annotation yfc:columnName="FUNC_ACK_TIME" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>Hours taken to send functional acknowledgement to the customer.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:NumberOfHours"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="MaxAssignRepsOrTeams"
            type="yfctype:NumberOfAssignment" use="optional">
            <xsd:annotation yfc:columnName="MAX_ASSIGN_REPS_OR_TEAMS" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>
                    <div>
				The maximum number of <code>Teams</code> or <code>Users</code> that this <code>Customer</code> can be assigned to. Only direct assignments to the <code>Customer</code> are taken into consideration to validate the maximum assignments.
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>Operation to perform
					<br/>Valid Values: 
					<br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
					<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
					<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
					<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
					<br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationCode" type="yfctype:OrgCode" use="required">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>
                    <div>
				Organization code of organization that defines this customer. Organization code is mandatory while creating a customer.  Organization code is mandatory when modifying a customer, if the Customer Key is not passed.
				<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not provided to create the customer.
					</Condition>
                            <Error Description="Missing attribute." ErrorCode="YFS10460"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> passed is not the Customer Master Organization.
					</Condition>
                            <Error
                                Description="Invalid Customer Master Organization." ErrorCode="YSC91_0007"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RegisteredDate" type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="REGISTERED_DATE" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>The date and time when the user was registered or when registration goes into effect. Default: current timestamp.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RelationshipType" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="RELATIONSHIP_TYPE" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>Indicates the type of a customer.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SendsAsn" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="SENDS_ASN" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>Indicates whether the  customer has to be sent an advance ship notice.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SendsCommitment" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="SENDS_COMMITMENT" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>Indicates whether the  customer has to be sent commitment against a purchase order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SendsFuncAck" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="SENDS_FUNC_ACK" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>Indicates whether the customer  has to be sent functional acknowledgment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SlotPreferenceType"
            type="yfctype:EnumSlotPreferenceType" use="optional">
            <xsd:annotation yfc:columnName="SLOT_PREFERENCE_TYPE" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>
                    <div>
				Slot preference type of the customer.  Valid values are: <code>
                            <b>HARD</b>
                        </code> - Only the preferred slots are to be considered.  <code>
                            <b>SOFT</b>
                        </code> - Other slots will also be considered.  <code>
                            <b>NONE</b>
                        </code> - No preferences.  Default value: <code>
                            <b>NONE</b>
                        </code>
                        <Validation>
                            <Condition>
                                <code>SlotPreferenceType</code> is not a valid value.
					</Condition>
                            <Error
                                Description="Invalid SlotPreferenceType." ErrorCode="YCP73_00022"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>SlotPreferenceType</code> is <code>
                                    <b>NONE</b>
                                </code> and <code>CustomerSlotPreferenceList</code> is passed.
					</Condition>
                            <Error
                                Description="Cannot pass CustomerSlotPreferenceList for NONE SlotPreferenceType." ErrorCode="YCP73_00023"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Status" type="yfctype:EnumCustomerStatus" use="optional">
            <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>
                    <div>
						Indicates the status of a customer.  Valid values are: 10-Active, 20-On Hold, 30-Inactive.  
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SyncTS" use="optional">
            <xsd:annotation yfc:columnName="SYNCTS" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>The date and time the customer was synchronized with an external system. If attribute is passed in the input without a value, the attribute will be updated to system date and time.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:TimeStamp"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="Vertical" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="VERTICAL" yfc:tableName="YFS_CUSTOMER">
                <xsd:documentation>Indicates a vertical of a customer.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="ParentCustomer">
        <xsd:annotation yfc:tableName="YFS_CUSTOMER">
            <xsd:documentation>
                <div>
					The parent customer of this customer.  
					<Validation>
                        <Condition>If <code>ParentCustomer</code> is provided, the following condition must be true.
						<ul>
                                <li>It must be a valid existing customer.</li>
                                <li>It must be of the same CustomerType.</li>
                                <li>It must belong to the same organization as the child.</li>
                            </ul>
                        </Condition>
                        <Error Description="Invalid Parent Customer." ErrorCode="YCP77_00001"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CustomerID" type="yfctype:CustomerId" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ID" yfc:tableName="YFS_CUSTOMER">
                    <xsd:documentation>The identifier for the customer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_CUSTOMER">
                    <xsd:documentation>The organization code of organization that defines this customer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="BuyerOrganization">
        <xsd:annotation yfc:tableName="YFS_ORGANIZATION">
            <xsd:documentation>
                <div>
				Identify the organization or node which customer represents.  <code>BuyerOrganization</code> is required if customer is a business customer. If the Organization represented by <code>BuyerOrganization</code> already exists, pass BuyerOrganizationCode attribute with <code>Customer</code>. 
				<Validation>
                        <Condition>If <code>BuyerOrganization</code> is not provided for creating the customer.
					</Condition>
                        <Error Description="Missing element in XML." ErrorCode="YFS10507"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Node"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CorporatePersonInfo"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="ContactPersonInfo">
                    <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
                        <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="AddressID"
                            type="yfctype:Text-256" use="optional">
                            <xsd:annotation yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>A user specified label for an address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine1"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 1 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine2"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 2 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine3"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 3 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine4"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 4 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine5"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 5 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine6"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 6 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AlternateEmailID"
                            type="yfctype:EMailID" use="optional">
                            <xsd:annotation
                                yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Beeper"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="City" type="yfctype:City" use="optional">
                            <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The city in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Company"
                            type="yfctype:Company" use="optional">
                            <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The company this person is associated with.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Country"
                            type="yfctype:Country" use="optional">
                            <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The country in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DayFaxNo"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DayPhone"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Department"
                            type="yfctype:Department" use="optional">
                            <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EMailID"
                            type="yfctype:EMailID" use="optional">
                            <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ErrorTxt"
                            type="yfctype:ErrorText" use="optional">
                            <xsd:annotation yfc:columnName="ERROR_TXT" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>This field is currently not used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EveningFaxNo"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EveningPhone"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="FirstName"
                            type="yfctype:PersonName" use="optional">
                            <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The first name of the person.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HttpUrl" type="yfctype:URL" use="optional">
                            <xsd:annotation yfc:columnName="HTTP_URL" yfc:tableName="YFS_PERSON_INFO">
