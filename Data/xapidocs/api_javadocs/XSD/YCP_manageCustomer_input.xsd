            </xsd:attribute>
            <xsd:attribute name="FridayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="FRIDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Friday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MondayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="MONDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Monday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					 Valid value is <b>
                                <code>Delete</code>
                            </b> or blank.  If not provided the information will be created or modified.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SaturdayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SATURDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Saturday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StartTime" type="yfctype:Time" use="optional">
                <xsd:annotation yfc:columnName="START_TIME" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The start time of the preferred slot.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SundayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SUNDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Sunday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ThursdayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="THURSDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Thursday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TuesdayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TUESDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Tuesday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="WednesdayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="WEDNESDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Wednesday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CustomerSchedulingPreferences">
        <xsd:annotation yfc:tableName="YFS_CUSTOMER_SCH_PREFERENCE">
            <xsd:documentation>This table stores customer preferences for scheduling orders.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CancelOrderOnBackorder" use="optional">
                <xsd:annotation
                    yfc:columnName="CANCEL_ORDER_ON_EXCP_FLAG" yfc:tableName="YFS_CUSTOMER_SCH_PREFERENCE">
                    <xsd:documentation>This flag indicates whether the order needs to be cancelled in case of inventory shortage.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsDropShipAllowed" use="optional">
                <xsd:annotation yfc:columnName="IS_DROP_SHIP_ALLOWED" yfc:tableName="YFS_CUSTOMER_SCH_PREFERENCE">
                    <xsd:documentation>This flag indicates whether it is allowed to schedule against the node that requires drop ship chained order creation.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsLineShipComplete" use="optional">
                <xsd:annotation yfc:columnName="IS_LINE_SHIP_COMPLETE" yfc:tableName="YFS_CUSTOMER_SCH_PREFERENCE">
                    <xsd:documentation>This flag indicates whether the complete line needs to be shipped.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsLineShipSingleNode" use="optional">
                <xsd:annotation
                    yfc:columnName="IS_LINE_SHIP_SINGLE_NODE" yfc:tableName="YFS_CUSTOMER_SCH_PREFERENCE">
                    <xsd:documentation>This flag indicates whether the order line needs to be shipped from a single ship node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsShipComplete" use="optional">
                <xsd:annotation yfc:columnName="IS_SHIP_COMPLETE" yfc:tableName="YFS_CUSTOMER_SCH_PREFERENCE">
                    <xsd:documentation>This flag indicates whether the complete order needs to be shipped.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsShipSingleNode" use="optional">
                <xsd:annotation yfc:columnName="IS_SHIP_SINGLE_NODE" yfc:tableName="YFS_CUSTOMER_SCH_PREFERENCE">
                    <xsd:documentation>This flag indicates whether the order needs to be shipped from a single ship node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsSubstitutionAllowed" use="optional">
                <xsd:annotation yfc:columnName="IS_SUBSTITUTION_ALLOWED" yfc:tableName="YFS_CUSTOMER_SCH_PREFERENCE">
                    <xsd:documentation>This flag indicates whether an item can be substituted if inventory is not available.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OptimizationType"
                type="yfctype:EnumOptimizationType" use="optional">
                <xsd:annotation yfc:columnName="OPTIMIZATION_TYPE" yfc:tableName="YFS_CUSTOMER_SCH_PREFERENCE">
                    <xsd:documentation>This value indicates indicates what scheduling will first optimize on, for this customer.  If not specified scheduling rule will be used to determine optimization type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CustomerContactList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:CustomerContact"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CustomerContact">
        <xsd:annotation yfc:tableName="YFS_CUSTOMER_CONTACT">
            <xsd:documentation>This table stores contact information of a customer.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="CustomerAdditionalAddressList">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="CustomerAdditionalAddress">
                                <xsd:annotation yfc:tableName="YFS_CUSTOMER_ADDNL_ADDRESS">
                                    <xsd:documentation>
                                    <div>Defines an additional address for a customer. <br/>
						Pass either <code>CustomerAdditionalAddressID</code> or <code>CustomerAdditionalAddressKey</code> to modify an existing customer additional address reocrd, otherwise a new record will be created. <br/>
						Attribute values in the <code>PersonInfo</code> element will be defaulted from <code>CustomerContact</code> if not passed in the input. 
						<Validation>
                                    <Condition>An address already exists as with either <code>IsDefaultShipTo</code>, <code>IsDefaultBillTo</code>, or <code>IsDefaultSoldTo</code> set as <code>Y</code> for this user and customer combination.</Condition>
                                    <Error
                                    Description="There can only be one default BillTo, ShipTo, and SoldTo address for a given combination of User and Customer." ErrorCode="YSC80_0001"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="PersonInfo">
                                    <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="AddressID"
                                    type="yfctype:Text-256" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>A user specified label for an address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AddressLine1"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AddressLine2"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AddressLine3"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AddressLine4"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AddressLine5"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AddressLine6"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AlternateEmailID"
                                    type="yfctype:EMailID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Beeper"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="City"
                                    type="yfctype:City" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The city in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Company"
                                    type="yfctype:Company" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Country"
                                    type="yfctype:Country" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The country in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DayFaxNo"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DayPhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Department"
                                    type="yfctype:Department" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="EMailID"
                                    type="yfctype:EMailID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ErrorTxt"
                                    type="yfctype:ErrorText" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ERROR_TXT" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="EveningFaxNo"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="EveningPhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="FirstName"
                                    type="yfctype:PersonName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The first name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="HttpUrl"
                                    type="yfctype:URL" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="HTTP_URL" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The URL of the person or organization referred to in this contact information.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsCommercialAddress"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="JobTitle"
                                    type="yfctype:Title-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The job title of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="LastName"
                                    type="yfctype:PersonName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The last name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Latitude" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LATITUDE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The latitude of this location.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Longitude" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LONGITUDE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The longitude of this location.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MiddleName"
                                    type="yfctype:MiddleName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MobilePhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OtherPhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PersonID"
                                    type="yfctype:PersonId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PreferredShipAddress"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PREFERRED_SHIP_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The preferred ship address over and above the address in this record. Pointer to another record of this type in this table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="State"
                                    type="yfctype:State" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The state in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Suffix"
                                    type="yfctype:Suffix" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Title"
                                    type="yfctype:Title-10" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UseCount"
                                    type="yfctype:Count" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USE_COUNT" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="VerificationStatus"
                                    type="yfctype:ShortStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="VERIFICATION_STATUS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ZipCode"
                                    type="yfctype:ZipCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    <xsd:attribute name="AddressType"
                                    type="yfctype:Text-24" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_TYPE" yfc:tableName="YFS_CUSTOMER_ADDNL_ADDRESS">
                                    <xsd:documentation>The type of address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CustomerAdditionalAddressID"
                                    type="yfctype:Text-24" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_ADDNL_ADDRESS_ID" yfc:tableName="YFS_CUSTOMER_ADDNL_ADDRESS">
                                    <xsd:documentation>The unique identifier for a customer additional address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="IsBillTo"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_BILL_TO" yfc:tableName="YFS_CUSTOMER_ADDNL_ADDRESS">
                                    <xsd:documentation>Indicates if an address is used as a billing address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsDefaultBillTo"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_DEFAULT_BILL_TO" yfc:tableName="YFS_CUSTOMER_ADDNL_ADDRESS">
                                    <xsd:documentation>Indicates if an address is a default billing address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsDefaultShipTo"
