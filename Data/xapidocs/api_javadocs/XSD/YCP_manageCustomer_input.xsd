            <xsd:attribute name="Usertype" type="yfctype:EnumUserType" use="optional">
                <xsd:annotation yfc:columnName="USERTYPE" yfc:tableName="YFS_USER">
                    <xsd:documentation>This field indicates the user type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="QueueSubscriptionList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:QueueSubscription"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="QueueSubscription">
        <xsd:annotation yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
            <xsd:documentation>Maintains Subscription of users to various queues. Users may subscribe to more than one Queue.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="QueueKey" type="yfctype:Key" use="required">
                <xsd:annotation yfc:columnName="QUEUE_KEY" yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                    <xsd:documentation>A Queue for which subscription is maintained.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="QueueSubscriptionKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="QUEUE_SUBSCRIPTION_KEY" yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                    <xsd:documentation>Primary key attribute of YFS_QUEUE_SUBSCRIPTION.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ResourcePermissionList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ResourcePermission"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ResourcePermission">
        <xsd:annotation yfc:tableName="YFS_RESOURCE_PERMISSION">
            <xsd:documentation>Permissions to various resources in the system are maintained here. Permissions are maintained at a user group level.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ResourceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="RESOURCE_KEY" yfc:tableName="YFS_RESOURCE_PERMISSION">
                    <xsd:documentation>A permission controlled resource.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Rights" type="yfctype:ShorterNumber" use="optional">
                <xsd:annotation yfc:columnName="RIGHTS" yfc:tableName="YFS_RESOURCE_PERMISSION">
                    <xsd:documentation>Low level access rights to this object, represented as a bit set.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UserKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="USER_KEY" yfc:tableName="YFS_RESOURCE_PERMISSION">
                    <xsd:documentation>A user that is given permission to access a resource.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="UserGroupLists">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:UserGroupList"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="UserGroupList">
        <xsd:annotation yfc:tableName="YFS_USER_GROUP_LIST">
            <xsd:documentation>Captures group membership of various users.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="UserGroupListKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="USER_GROUP_LIST_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                    <xsd:documentation>Primary key attribute of YFS_USER_GROUP_LIST.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UsergroupId" type="yfctype:String" use="optional"/>
            <xsd:attribute name="UsergroupKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="USERGROUP_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                    <xsd:documentation>A group which maintains users with membership to the group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AuthAnswerList">
        <xsd:annotation yfc:tableName="PLT_AUTH_ANSWER">
            <xsd:documentation>The table is used for storing answers.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:AuthAnswer"/>
            </xsd:sequence>
            <xsd:attribute name="Reset" type="yfctype:String" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AuthAnswer">
        <xsd:annotation yfc:tableName="PLT_AUTH_ANSWER">
            <xsd:documentation>
                <div> The table is used for storing answers.
							<Validation>
                        <Condition>If invalid authorization question is answered</Condition>
                        <Error
                            Description="Incorrect question being answered." ErrorCode="YCP0459"/>
                    </Validation>
                    <Validation>
                        <Condition>If blank authorization answer is provided</Condition>
                        <Error Description="Blank authorization answer." ErrorCode="YCP0460"/>
                    </Validation>
                    <Validation>
                        <Condition>If incorrect authorization answer is provided</Condition>
                        <Error
                            Description="Incorrect authorization answer." ErrorCode="YCP0461"/>
                    </Validation>
                    <Validation>
                        <Condition>If blank authorization question key and question text are provided</Condition>
                        <Error
                            Description="Both authorization question key and question text are blank." ErrorCode="PLT0508"/>
                    </Validation>
                    <Validation>
                        <Condition>If there is no authorization question set up for the user.</Condition>
                        <Error
                            Description="No authorization question associated with the user." ErrorCode="PLT0509"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Answer" type="yfctype:Answer" use="optional">
                <xsd:annotation yfc:columnName="ANSWER" yfc:tableName="PLT_AUTH_ANSWER">
                    <xsd:documentation>Answer for the question.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AuthAnswerKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="AUTH_ANSWER_KEY" yfc:tableName="PLT_AUTH_ANSWER">
                    <xsd:documentation>Primary key attribute of PLT_AUTH_ANSWER.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AuthQuestionKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="AUTH_QUESTION_KEY" yfc:tableName="PLT_AUTH_ANSWER">
                    <xsd:documentation>Question Key</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="QuestionText" type="yfctype:Question" use="optional">
                <xsd:annotation yfc:columnName="QUESTION_TEXT" yfc:tableName="PLT_AUTH_ANSWER">
                    <xsd:documentation>Custom question for an user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AnswerSets">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:AnswerSet"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AnswerSet">
        <xsd:annotation yfc:tableName="YFS_ANSWER_SET_MASTER">
            <xsd:documentation>Stores the relationship between a set of answers and the entities which provided these answers.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Answers"/>
            </xsd:all>
            <xsd:attribute name="Action" type="yfctype:Text-10" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							Specify <code>Action</code> = <code>
                                <b>REMOVE</b>
                            </code> to remove the entity. Leave <code>Action</code> blank or don't include it for modifying or creating a new entity.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AnswerSetMasterKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ANSWER_SET_MASTER_KEY" yfc:tableName="YFS_ANSWER_SET_MASTER">
                    <xsd:documentation>Primary key of answer set master.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="QuestionType" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="QUESTION_TYPE" yfc:tableName="YFS_ANSWER_SET_MASTER">
                    <xsd:documentation>
                        <div>The question type that this answer set is for. 
							<Validation>
                                <Condition>
                                    <code>QuestionType</code> not passed.
									</Condition>
                                <Error
                                    Description="Mandatory attributes for answer set are missing." ErrorCode="YCP73_00005"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Answers">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Answer"/>
            </xsd:sequence>
            <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Indicates if all existing child node definition in the database need to be scrapped and have only these definitions supplied in the XML.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Answer">
        <xsd:annotation yfc:tableName="YFS_ANSWER_MASTER">
            <xsd:documentation>Stores the relationship between a set of answers and the entities which provided these answers.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Action" type="yfctype:Text-10" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								Specify <code>Action</code> = <code>
                                <b>REMOVE</b>
                            </code> to remove the entity. Leave <code>Action</code> blank or don't include it for modifying or creating a new entity.
								</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AnswerMasterKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ANSWER_MASTER_KEY" yfc:tableName="YFS_ANSWER_MASTER">
                    <xsd:documentation>Primary key of answer master.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AnswerSetMasterKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ANSWER_SET_MASTER_KEY" yfc:tableName="YFS_ANSWER_MASTER">
                    <xsd:documentation>The answer set for this answer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AnswerValue" type="yfctype:Text-1000" use="optional">
                <xsd:annotation yfc:columnName="ANSWER_VALUE" yfc:tableName="YFS_ANSWER_MASTER">
                    <xsd:documentation>
                        <div>The answer for the question.
									<Validation>
                                <Condition>
                                    <code>AnswerValue</code> passed should be a valid answer for the question it belongs to.</Condition>
                                <Error
                                    Description="Answer is not one of the valid pre-defined answer options." ErrorCode="YCP73_00007"/>
                                <Error
                                    Description="Answer for this question should be an integer number." ErrorCode="YCP73_00020"/>
                                <Error
                                    Description="Answer for this question should be a decimal number." ErrorCode="YCP73_00021"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="QuestionID" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="QUESTION_ID" yfc:tableName="YFS_ANSWER_MASTER">
                    <xsd:documentation>
                        <div>"Identifier for the question that this answer is for.
									<Validation>
                                <Condition>
                                    <code>QuestionID</code> should be valid.</Condition>
                                <Error
                                    Description="Question does not exist in the database." ErrorCode="YCP73_00006"/>
                                <Error
                                    Description="Question should only be answered when parent answer option is selected." ErrorCode="YCP73_00009"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CustomerCurrencyList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:CustomerCurrency"/>
            </xsd:sequence>
            <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Indicates if all existing child node definition in the database need to be deleted and replace with the provided child nodes.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CustomerCurrency">
        <xsd:annotation yfc:tableName="YFS_CUSTOMER_CURRENCY">
            <xsd:documentation>This table stores available currencies for a customer.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Currency" type="yfctype:Currency" use="optional">
                <xsd:annotation yfc:columnName="CURRENCY" yfc:tableName="YFS_CUSTOMER_CURRENCY">
                    <xsd:documentation>Currency Code.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerCurrencyKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_CURRENCY_KEY" yfc:tableName="YFS_CUSTOMER_CURRENCY">
                    <xsd:documentation>Primary key attribute of YFS_CUSTOMER_CURRENCY.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsDefaultCurrency" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_DEFAULT_CURRENCY" yfc:tableName="YFS_CUSTOMER_CURRENCY">
                    <xsd:documentation>
                        <div>Indicates if a currency is the default customer currency. 
								<Validation>
                                <Condition>
                                    <code>IsDefaultCurrency</code> not passed.
									</Condition>
                                <Error
                                    Description="A customer cannot have multiple default currencies." ErrorCode="YSC83_0007"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								Valid value is <b>
                                <code>Delete</code>
                            </b> or blank.  If not provided the information will be created or modified.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CustomerAnalytics">
        <xsd:annotation yfc:tableName="YFS_CUSTOMER_ANALYTICS">
            <xsd:documentation>The table stores the customer information like customer grade information for a customer, from an external BI system. This table is meant for either direct data upload through custom BI system or can also be updated using the managecustomer api. If this table is inserted or updated through direct db update (ETL), no events will be raised.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CustomerAnalyticsKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ANALYTICS_KEY" yfc:tableName="YFS_CUSTOMER_ANALYTICS">
                    <xsd:documentation>The primary key of the YFS_CUSTOMER_ANALYTICS table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GradeCode" type="yfctype:GradeCode" use="optional">
                <xsd:annotation yfc:columnName="GRADE_CODE" yfc:tableName="YFS_CUSTOMER_ANALYTICS">
                    <xsd:documentation>The grade code assigned for the range values between MINIMUM_RATING and MAXIMUM_RATING.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GradeDescription"
                type="yfctype:Description" use="optional">
                <xsd:annotation yfc:columnName="GRADE_DESCRIPTION" yfc:tableName="YFS_CUSTOMER_ANALYTICS">
                    <xsd:documentation>The description for the grade code.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
