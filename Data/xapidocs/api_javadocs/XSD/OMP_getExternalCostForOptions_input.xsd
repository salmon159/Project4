                                    <xsd:documentation>
                                    <div>Assignment key of the supporting choice.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Capacity"
                                    type="yfctype:Capacity" use="optional">
