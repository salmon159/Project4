                                    <xsd:documentation>
                                    <div>Capacity for <code>ShipDate</code> at <code>ShipNode</code>.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CapacityConsumed"
                                    type="yfctype:Capacity" use="optional">
