                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="VerificationStatus"
                                    type="yfctype:ShortStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="VERIFICATION_STATUS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ZipCode"
                                    type="yfctype:ZipCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="QueueSubscriptionList">
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="QueueSubscription">
                                    <xsd:annotation yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                                    <xsd:documentation>Maintains Subscription of users to various queues. Users may subscribe to more than one Queue.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="QueueKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="QUEUE_KEY" yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                                    <xsd:documentation>A Queue for which subscription is maintained.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="QueueSubscriptionKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="QUEUE_SUBSCRIPTION_KEY" yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                                    <xsd:documentation>Primary key attribute of YFS_QUEUE_SUBSCRIPTION.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute name="Reset"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Indicates if all existing child node
								definition in the database need to be
								scrapped and have only these definitions
								supplied in the XML.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="UserGroupLists">
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="UserGroupList">
                                    <xsd:annotation yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>
                                    <div>
                                        Captures group membership of various users. Either <code>UsergroupKey</code> or <code>UsergroupId</code> must be passed to uniquely identify the UserGroup to which the user needs to belong.						
                                        </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="UserGroupListKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USER_GROUP_LIST_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>Primary key attribute of YFS_USER_GROUP_LIST.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UsergroupId"
                                    type="yfctype:UserGroup" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USERGROUP_ID" yfc:tableName="YFS_USER_GROUP">
                                    <xsd:documentation>
                                    <div>
                                    <Validation>
                                    <Condition>Invalid <code>UsergroupId</code> passed.
                                                                </Condition>

                                    <Error
                                    Description="Invalid Usergroup." ErrorCode="YCP0152"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UsergroupKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USERGROUP_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>A group which maintains users with membership to the group.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute name="Reset"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Indicates if all existing child node
								definition in the database need to be
								scrapped and have only these definitions
								supplied in the XML.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="UserDelegateList">
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="UserDelegate">
                                    <xsd:annotation yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>The table defines the delegate users assigned by a user for a delegation purpose so that the delegates can act on behalf of them.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="ActiveFlag"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACTIVE_FLAG" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>This flag indicates whether the user delegation is active.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DelegateUserID"
                                    type="yfctype:LoginId-50" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DELEGATE_USER_ID" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>Indicates delegate who has been assigned to act on behalf of the user.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DelegationPurpose"
                                    type="yfctype:EnumDelegationPurpose" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DELEGATION_PURPOSE" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>Indicates the purpose for which user has been delegated. The only valid value is 'QUOTE_APPROVAL' which means the delegation is for approval of a quote</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="EffectiveEndDate" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EFFECTIVE_END_DATE" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>The date that this user delegation is applicable to.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Date"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="EffectiveStartDate" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EFFECTIVE_START_DATE" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>The date that this user delegation is applicable from.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Date"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsPrimaryDelegate"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_PRIMARY_DELEGATE" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>This flag indicates whether the delegate user id is the primary delegate.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Operation"
                                    type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							 Valid value is <b>
                                    <code>Delete</code>
                                    </b> or blank.  If not provided the information will be created or modified.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute name="Reset"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Indicates if all existing child node definition in the database need to be deleted and replace with the provided child nodes.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                        <xsd:attribute name="Activateflag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="ACTIVATEFLAG" yfc:tableName="YFS_USER">
                                <xsd:documentation>This flag indicates whether a user is active.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BillingaddressKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="BILLINGADDRESS_KEY" yfc:tableName="YFS_USER">
                                <xsd:documentation>The billing address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BusinessKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="BUSINESS_KEY" yfc:tableName="YFS_USER">
                                <xsd:documentation>The business associated with this user. Identifies a ship node when filled.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ChangePassNext"
                            type="yfctype:ChangePassNext" use="optional">
                            <xsd:annotation
                                yfc:columnName="CHANGE_PASS_NEXT" yfc:tableName="YFS_USER">
                                <xsd:documentation>Flag to change password on next login. Valid values are: 0 (for no) and 1 (for yes).</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ContactaddressKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="CONTACTADDRESS_KEY" yfc:tableName="YFS_USER">
                                <xsd:documentation>The contact address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CreatorOrganizationKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREATOR_ORGANIZATION_KEY" yfc:tableName="YFS_USER">
                                <xsd:documentation>The organization that created the user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DataSecurityGroupId"
                            type="yfctype:DataSecurityGroupId" use="optional">
                            <xsd:annotation
                                yfc:columnName="DATA_SECURITY_GROUP_ID" yfc:tableName="YFS_USER">
                                <xsd:documentation>Data Security Group Id.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExistingPassword"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The users password before execution of the
							api.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExistingPasswordRequired"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>Indicates if the existing password is
							required. Possible values are Y or N</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Imagefile"
                            type="yfctype:FileName" use="optional">
                            <xsd:annotation yfc:columnName="IMAGEFILE" yfc:tableName="YFS_USER">
                                <xsd:documentation>The image associated with a user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Localecode"
                            type="yfctype:Locale" use="optional">
                            <xsd:annotation yfc:columnName="LOCALECODE" yfc:tableName="YFS_USER">
                                <xsd:documentation>
                                    <div>
                                    <Validation>
                                    <Condition>
									If LocaleCode is invalid or not passed
								</Condition>
                                    <Error
                                    Description="LocaleCode is a mandatory field" ErrorCode="YFS10364"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Loginid"
                            type="yfctype:LoginId" use="required">
                            <xsd:annotation yfc:columnName="LOGINID" yfc:tableName="YFS_USER">
                                <xsd:documentation>The login ID for the user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Longdesc"
                            type="yfctype:Description-255" use="optional">
                            <xsd:annotation yfc:columnName="LONGDESC" yfc:tableName="YFS_USER">
                                <xsd:documentation>The long description for the user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="MenuId"
                            type="yfctype:Identifier" use="optional">
                            <xsd:annotation yfc:columnName="MENU_ID" yfc:tableName="YFS_USER">
                                <xsd:documentation>The menu ID associated with the user. This is used to display the menus in various applications to which this user has access.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NoteKey" type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="NOTE_KEY" yfc:tableName="YFS_USER">
                                <xsd:documentation>A generic note for the user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrganizationKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORGANIZATION_KEY" yfc:tableName="YFS_USER">
                                <xsd:documentation>The organization to which a user belongs.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Password"
                            type="yfctype:Password" use="optional">
                            <xsd:annotation yfc:columnName="PASSWORD" yfc:tableName="YFS_USER">
                                <xsd:documentation>The password for the user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PreferenceKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="PREFERENCE_KEY" yfc:tableName="YFS_USER">
                                <xsd:documentation>Not used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SessionTimeout"
                            type="yfctype:SessionTimeOut" use="optional">
                            <xsd:annotation
                                yfc:columnName="SESSION_TIMEOUT" yfc:tableName="YFS_USER">
                                <xsd:documentation>Number of seconds of inactivity before the session expires. If this value is negative or zero the session timeout is picked up from web.xml</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SuperUser"
                            type="yfctype:SuperUser" use="optional">
                            <xsd:annotation yfc:columnName="SUPER_USER" yfc:tableName="YFS_USER">
                                <xsd:documentation>Access Level of User. Valid values are: 0 (for no) and 1 (for yes).</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Theme"
                            type="yfctype:PresentationTheme" use="optional">
                            <xsd:annotation yfc:columnName="THEME" yfc:tableName="YFS_USER">
                                <xsd:documentation>The presentation theme for a user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="UserKey" type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="USER_KEY" yfc:tableName="YFS_USER">
                                <xsd:documentation>The primary key for the YFS_USER table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="UsergroupKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="USERGROUP_KEY" yfc:tableName="YFS_USER">
                                <xsd:documentation>The user group associated with this user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Username"
                            type="yfctype:UserName" use="optional">
                            <xsd:annotation yfc:columnName="USERNAME" yfc:tableName="YFS_USER">
                                <xsd:documentation>The name of the user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Usertype"
                            type="yfctype:EnumUserType" use="optional">
                            <xsd:annotation yfc:columnName="USERTYPE" yfc:tableName="YFS_USER">
                                <xsd:documentation>This field indicates the user type.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Indicates if all existing child node definitions
						in the database need to be scrapped and have
						only these definitions supplied in the XML.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="UserPrinterPreferences">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:UserPrinterPreference"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="UserPrinterPreference">
        <xsd:annotation yfc:tableName="YFS_USER_PRINTER_PREFERENCE">
            <xsd:documentation>This entity defines the printers to be used for different documents based on attributes such as User, UserGroup etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					 Valid value is <b>
                                <code>Delete</code>
                            </b> or Manage.  If not provided the information will be created or modified.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrinterKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PRINTER_KEY" yfc:tableName="YFS_USER_PRINTER_PREFERENCE">
                    <xsd:documentation>Primary key attribute of YFS_DEVICE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UserId" type="yfctype:LoginId" use="optional">
                <xsd:annotation yfc:columnName="USER_ID" yfc:tableName="YFS_USER_PRINTER_PREFERENCE">
                    <xsd:documentation>Unique key of YFS_USER.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UsergroupId" type="yfctype:UserGroup" use="optional">
                <xsd:annotation yfc:columnName="USERGROUP_ID" yfc:tableName="YFS_USER_PRINTER_PREFERENCE">
                    <xsd:documentation>Unique key of YFS_USER_GROUP.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
