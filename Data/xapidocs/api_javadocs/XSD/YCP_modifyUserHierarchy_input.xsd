                    <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                        <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                            <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="UseCount" type="yfctype:Count" use="optional">
                        <xsd:annotation yfc:columnName="USE_COUNT" yfc:tableName="YFS_PERSON_INFO">
                            <xsd:documentation>This field is currently not used.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="VerificationStatus"
                        type="yfctype:ShortStatus" use="optional">
                        <xsd:annotation
                            yfc:columnName="VERIFICATION_STATUS" yfc:tableName="YFS_PERSON_INFO">
                            <xsd:documentation>This field is currently not used.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                        <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                            <xsd:documentation>The zip code in the address.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="QueueSubscriptionList">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="QueueSubscription">
                            <xsd:annotation yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                                <xsd:documentation>Maintains Subscription of users to various queues. Users may subscribe to more than one Queue.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:attribute name="QueueKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="QUEUE_KEY" yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                                    <xsd:documentation>A Queue for which subscription is maintained.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute
                                    name="QueueSubscriptionKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="QUEUE_SUBSCRIPTION_KEY" yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                                    <xsd:documentation>Primary key attribute of YFS_QUEUE_SUBSCRIPTION.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="Localecode"
                        type="yfctype:String" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>
                                <div>
                                    <Validation>
                                    <Condition>
									Record does not exist in database
								</Condition>
                                    <Error
                                    Description="Invalid Localecode" ErrorCode="YFS10145"/>
                                    </Validation>
                                </div>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>Indicates if all existing child node definitions
						in the database need to be scrapped and have
						only these definitions supplied in the XML.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ResourcePermissionList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AuthAnswerList"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="UserGroupLists">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="UserGroupList">
                            <xsd:annotation yfc:tableName="YFS_USER_GROUP_LIST">
                                <xsd:documentation>
                                    <div>
                                        Captures group membership of various users. Either <code>UsergroupKey</code> or <code>UsergroupId</code> must be passed to uniquely identify the UserGroup to which the user needs to belong.						
                                        </div>
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:attribute name="IsPrimary"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_PRIMARY" yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>Indicates primary user group of the user</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="UserGroupListKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USER_GROUP_LIST_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>Primary key attribute of YFS_USER_GROUP_LIST.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="UsergroupId"
                                    type="yfctype:UserGroup" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USERGROUP_ID" yfc:tableName="YFS_USER_GROUP">
                                    <xsd:documentation>
                                    <div>
                                    <Validation>
                                    <Condition>Invalid <code>UsergroupId</code> passed.
                                                                </Condition>
                                    <Error
                                    Description="Invalid Usergroup." ErrorCode="YCP0152"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="UsergroupKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USERGROUP_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>A group which maintains users with membership to the group.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>Indicates if all existing child node definitions
						in the database need to be scrapped and have
						only these definitions supplied in the XML.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:SubUser"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:UserPrinterPreferences"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="UserDelegateList">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="UserDelegate">
                            <xsd:annotation yfc:tableName="YFS_USER_DELEGATE">
                                <xsd:documentation>The table defines the delegate users assigned by a user for a delegation purpose so that the delegates can act on behalf of them.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:attribute name="ActiveFlag"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACTIVE_FLAG" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>This flag indicates whether the user delegation is active.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="DelegateUserID"
                                    type="yfctype:LoginId-50" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DELEGATE_USER_ID" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>Indicates delegate who has been assigned to act on behalf of the user.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="DelegationPurpose"
                                    type="yfctype:EnumDelegationPurpose" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DELEGATION_PURPOSE" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>Indicates the purpose for which user has been delegated. The only valid value is 'QUOTE_APPROVAL' which means the delegation is for approval of a quote</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="EffectiveEndDate" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EFFECTIVE_END_DATE" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>The date that this user delegation is applicable to.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Date"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                </xsd:attribute>
                                <xsd:attribute name="EffectiveStartDate" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EFFECTIVE_START_DATE" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>The date that this user delegation is applicable from.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Date"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                </xsd:attribute>
                                <xsd:attribute name="IsPrimaryDelegate"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_PRIMARY_DELEGATE" yfc:tableName="YFS_USER_DELEGATE">
                                    <xsd:documentation>This flag indicates whether the delegate user id is the primary delegate.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Operation"
                                    type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					 Valid value is <b>
                                    <code>Delete</code>
                                    </b> or blank.  If not provided the information will be created or modified.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>Indicates if all existing child node definition in the database need to be deleted and replace with the provided child nodes.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
        </xsd:all>
        <xsd:attribute name="Activateflag" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="ACTIVATEFLAG" yfc:tableName="YFS_USER">
                <xsd:documentation>This flag indicates whether a user is active.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BillingaddressKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="BILLINGADDRESS_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>The billing address.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BusinessKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="BUSINESS_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>The business associated with this user. Identifies a ship node when filled.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ChangePassNext"
            type="yfctype:ChangePassNext" use="optional">
            <xsd:annotation yfc:columnName="CHANGE_PASS_NEXT" yfc:tableName="YFS_USER">
                <xsd:documentation>Flag to change password on next login. Valid values are: 0 (for no) and 1 (for yes).</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ContactaddressKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CONTACTADDRESS_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>The contact address.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CreatorOrganizationKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CREATOR_ORGANIZATION_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>The organization that created the user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DataSecurityGroupId"
            type="yfctype:DataSecurityGroupId" use="optional">
            <xsd:annotation yfc:columnName="DATA_SECURITY_GROUP_ID" yfc:tableName="YFS_USER">
                <xsd:documentation>Data Security Group Id.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DepartmentCode" type="yfctype:Text-20" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
                        <Validation>
                            <Condition>
								The department code is invalid or the
								organization does not contain the
								department
							</Condition>
                            <Error
                                Description="The Department code does not exist for the organization." ErrorCode="YCP0256"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DisplayUserID" type="yfctype:Text-150" use="optional">
            <xsd:annotation yfc:columnName="DISPLAY_USER_ID" yfc:tableName="YFS_USER">
                <xsd:documentation>Indicates Display User's ID which is used for login in to the system.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="EnterpriseCode" type="yfctype:Text-24" use="optional">
            <xsd:annotation yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_USER">
                <xsd:documentation>The Customer Master Organization Code of the User's Organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExistingPassword" type="yfctype:Text-100" use="optional">
            <xsd:annotation>
                <xsd:documentation>The users password before execution of the api.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExistingPasswordRequired"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>Indicates if the existing password is required.
					Possible values are Y or N</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Imagefile" type="yfctype:FileName" use="optional">
            <xsd:annotation yfc:columnName="IMAGEFILE" yfc:tableName="YFS_USER">
                <xsd:documentation>The image associated with a user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsCustomer" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_CUSTOMER" yfc:tableName="YFS_USER">
                <xsd:documentation>
                    <div>
					This flag indicates if the user is a customer in the system or not.This flag must be set to <code>Y</code> if the user is a customer contact user. The only valid value for this attribute is <code>Y</code>. This flag will be set to <code>Y</code> only if a valid customer contact exist in the system.
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsSupervisor" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_SUPERVISOR" yfc:tableName="YFS_USER">
                <xsd:documentation>
                    <div>
				This flag indicates if the user is a supervisor or not.  
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Localecode" type="yfctype:Locale" use="optional">
            <xsd:annotation yfc:columnName="LOCALECODE" yfc:tableName="YFS_USER">
                <xsd:documentation>The locale of a user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Loginid" type="yfctype:LoginId" use="required">
            <xsd:annotation yfc:columnName="LOGINID" yfc:tableName="YFS_USER">
                <xsd:documentation>
                    <div>
                        <Validation>
                            <Condition>
									The Loginid specified does not exist
								</Condition>
                            <Error
                                Description="Invalid User identifiers passed" ErrorCode="YCP0084"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Longdesc" type="yfctype:Description-255" use="optional">
            <xsd:annotation yfc:columnName="LONGDESC" yfc:tableName="YFS_USER">
                <xsd:documentation>The long description for the user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="MaxCustomerAssignments"
            type="yfctype:NumberOfAssignment" use="optional">
            <xsd:annotation yfc:columnName="MAX_CUST_ASSIGN" yfc:tableName="YFS_USER">
                <xsd:documentation>
                    <div>
				This indicates maximum number of customers that can be assigned to this user.  
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="MenuId" type="yfctype:Identifier" use="optional">
            <xsd:annotation yfc:columnName="MENU_ID" yfc:tableName="YFS_USER">
                <xsd:documentation>The menu ID associated with the user. This is used to display the menus in various applications to which this user has access.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NoteKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="NOTE_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>A generic note for the user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>
                    <div>
                        <Validation>
                            <Condition>
									The destination organization does not have a valid data security group
								</Condition>
                            <Error
                                Description="Cannot move user because a valid data security group could not be found" ErrorCode="YCP0256"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ParentUserKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="PARENT_USER_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>The parent user associated with this user. May be used to capture reporting hierarchy. A user has only one parent but may have many children.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Password" type="yfctype:Password" use="optional">
            <xsd:annotation yfc:columnName="PASSWORD" yfc:tableName="YFS_USER">
                <xsd:documentation>The password for the user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PasswordPolicyId"
            type="yfctype:PasswordPolicyId" use="optional">
            <xsd:annotation yfc:columnName="PASSWORD_POLICY_ID" yfc:tableName="YFS_USER">
                <xsd:documentation>The Id of the Password Policy a user has been assigned to.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PasswordPolicyKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="PWD_POLICY_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>Primary key attribute of PLT_PWD_POLICY.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PreferenceKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="PREFERENCE_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>Not used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SessionTimeout"
            type="yfctype:SessionTimeOut" use="optional">
            <xsd:annotation yfc:columnName="SESSION_TIMEOUT" yfc:tableName="YFS_USER">
                <xsd:documentation>Number of seconds of inactivity before the session expires. If this value is negative or zero the session timeout is picked up from web.xml</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SuperUser" type="yfctype:SuperUser" use="optional">
            <xsd:annotation yfc:columnName="SUPER_USER" yfc:tableName="YFS_USER">
                <xsd:documentation>Access Level of User. Valid values are: 0 (for no) and 1 (for yes).</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Theme" type="yfctype:PresentationTheme" use="optional">
            <xsd:annotation yfc:columnName="THEME" yfc:tableName="YFS_USER">
                <xsd:documentation>The presentation theme for a user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="UserKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="USER_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>The primary key for the YFS_USER table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="UsergroupKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="USERGROUP_KEY" yfc:tableName="YFS_USER">
                <xsd:documentation>The user group associated with this user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Username" type="yfctype:UserName" use="optional">
            <xsd:annotation yfc:columnName="USERNAME" yfc:tableName="YFS_USER">
                <xsd:documentation>The name of the user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Usertype" type="yfctype:EnumUserType" use="optional">
            <xsd:annotation yfc:columnName="USERTYPE" yfc:tableName="YFS_USER">
                <xsd:documentation>This field indicates the user type.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="ResourcePermissionList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ResourcePermission"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ResourcePermission">
        <xsd:annotation yfc:tableName="YFS_RESOURCE_PERMISSION">
            <xsd:documentation>Permissions to various resources in the system are maintained here. Permissions are maintained at a user group level.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ResourceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="RESOURCE_KEY" yfc:tableName="YFS_RESOURCE_PERMISSION">
                    <xsd:documentation>A permission controlled resource.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Rights" type="yfctype:ShorterNumber" use="optional">
                <xsd:annotation yfc:columnName="RIGHTS" yfc:tableName="YFS_RESOURCE_PERMISSION">
                    <xsd:documentation>Low level access rights to this object, represented as a bit set.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UserKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="USER_KEY" yfc:tableName="YFS_RESOURCE_PERMISSION">
                    <xsd:documentation>A user that is given permission to access a resource.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AuthAnswerList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AuthAnswer"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AuthAnswer">
        <xsd:annotation yfc:tableName="PLT_AUTH_ANSWER">
            <xsd:documentation>The table is used for storing answers.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Answer" type="yfctype:Answer" use="optional">
                <xsd:annotation yfc:columnName="ANSWER" yfc:tableName="PLT_AUTH_ANSWER">
                    <xsd:documentation>Answer for the question.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AuthAnswerKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="AUTH_ANSWER_KEY" yfc:tableName="PLT_AUTH_ANSWER">
                    <xsd:documentation>Primary key attribute of PLT_AUTH_ANSWER.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AuthQuestionKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="AUTH_QUESTION_KEY" yfc:tableName="PLT_AUTH_ANSWER">
                    <xsd:documentation>Question Key</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="QuestionText" type="yfctype:Question" use="optional">
                <xsd:annotation yfc:columnName="QUESTION_TEXT" yfc:tableName="PLT_AUTH_ANSWER">
                    <xsd:documentation>Custom question for an user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SubUser">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="User">
                    <xsd:annotation yfc:tableName="YFS_USER">
                        <xsd:documentation>This table stores information on users in the system.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="ContactPersonInfo">
