<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/VAS/getWorkOrderAppointmentList/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/VAS/getWorkOrderAppointmentList/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation
        yfc:fileName="VAS_getWorkOrderAppointmentList_input.xsd" yfc:rootElement="WorkOrderAppointment">
        <xsd:appinfo source="yfc:structure">
            <WorkOrderAppointment hasAttributes="Y">
                <WorkOrder hasAttributes="Y">
                    <Order hasAttributes="Y"/>
                </WorkOrder>
            </WorkOrderAppointment>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="WorkOrderAppointment" type="yfc:WorkOrderAppointmentXSDType">
        <xsd:annotation yfc:tableName="YFS_WORK_ORDER_APPT">
            <xsd:documentation>This table stores information about appointments taken on the work order. Used only for PS/DS work orders.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="WorkOrderAppointmentXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:WorkOrder"/>
        </xsd:all>
        <xsd:attribute name="AdditionalEquipmentUsed"
            type="yfctype:Text-200" use="optional">
            <xsd:annotation yfc:columnName="ADDITIONAL_EQUIPMENT_USED"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>This field indicates items that were used, which were not originally requested.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AdditionalEquipmentUsedQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="AlertSales" type="yfctype:Text-200" use="optional">
            <xsd:annotation yfc:columnName="ALERT_SALES"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>This field indicates that the sales person has to be notified and entered information is send to the sales person.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AlertSalesQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="AlertScheduler" type="yfctype:Text-200" use="optional">
            <xsd:annotation yfc:columnName="ALERT_SCHEDULER"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>This field indicates that the scheduler has to be notified and entered information is send to the scheduler.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AlertSchedulerQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="AllocatedQuantity" type="yfctype:Capacity" use="optional">
            <xsd:annotation yfc:columnName="ALLOCATED_QUANTITY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>The capacity allocated for the service or delivery line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AllocatedQuantityQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="ApptSeq" type="yfctype:LineNo" use="optional">
            <xsd:annotation yfc:columnName="APPT_SEQ"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>This number denotes the position in the sequence when the current appointment would be executed.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ApptSeqQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="ApptStatus" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="APPT_STATUS"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>The appointment status. Will be in synch with the service order line. Used in case of provided service work order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ApptStatusQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="AttentionRequired" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="ATTENTION_REQUIRED"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>This flag indicates that some problems exist with work order and it has to be looked at.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AttentionRequiredQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ConsiderCurrentSlot" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:qryTypeSupported="Y">
                <xsd:documentation>
                    <div>
					The <code>ConsiderCurrentSlot</code> flag indicates that system should consider the available capacity of the current slots, which the current timestamp falls under.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ConsiderCurrentSlotQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ExecutionBy" type="yfctype:UserId" use="optional">
            <xsd:annotation yfc:columnName="EXECUTION_BY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>The technician executing the work order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExecutionByQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ExecutionComments" type="yfctype:Text-1500" use="optional">
            <xsd:annotation yfc:columnName="EXECUTION_COMMENTS"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>The execution comments.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExecutionCommentsQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ExecutionDuration"
            type="yfctype:NumberOfHours" use="optional">
            <xsd:annotation yfc:columnName="EXECUTION_DURATION"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>The amount of time took to perform the service.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExecutionDurationQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="ExecutionEndTime" use="optional">
            <xsd:annotation yfc:columnName="EXECUTION_END_TIME"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>The last date when the execution of the service was attempted. This can correspond to either a successful or failed attempt.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:TimeStamp"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ExecutionEndTimeQryType"
            type="yfctype:DateQryType" use="optional"/>
        <xsd:attribute name="ExecutionStartTime" use="optional">
            <xsd:annotation yfc:columnName="EXECUTION_START_TIME"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>The last date when the execution of the service was attempted. This can correspond to either a successful or failed attempt.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:TimeStamp"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ExecutionStartTimeQryType"
            type="yfctype:DateQryType" use="optional"/>
        <xsd:attribute name="ExpectedApptEndDate" use="optional">
            <xsd:annotation yfc:columnName="EXPECTED_APPT_END_DATE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>The expected end date of the appointment. Populated during route planning of provided service work order.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:TimeStamp"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ExpectedApptEndDateQryType"
            type="yfctype:DateQryType" use="optional"/>
        <xsd:attribute name="ExpectedApptStartDate" use="optional">
            <xsd:annotation yfc:columnName="EXPECTED_APPT_START_DATE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_APPT">
                <xsd:documentation>The expected start date of the appointment.Populated during route planning of provided service work order.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:TimeStamp"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ExpectedApptStartDateQryType"
            type="yfctype:DateQryType" use="optional"/>
        <xsd:attribute name="FromAdditionalEquipmentUsed"
            type="yfctype:Text-200" use="optional"/>
        <xsd:attribute name="FromAlertSales" type="yfctype:Text-200" use="optional"/>
        <xsd:attribute name="FromAlertScheduler" type="yfctype:Text-200" use="optional"/>
        <xsd:attribute name="FromAllocatedQuantity"
