        <xsd:attribute name="ToItemsNotUsed" type="yfctype:Text-200" use="optional"/>
        <xsd:attribute name="ToMaximumRecords" type="yfctype:Count" use="optional"/>
        <xsd:attribute name="ToNonInventoryItemsReturned"
            type="yfctype:Text-200" use="optional"/>
