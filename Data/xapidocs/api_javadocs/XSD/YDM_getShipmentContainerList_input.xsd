        <xsd:attribute name="ToContainerHeight" type="yfctype:Dimension" use="optional"/>
        <xsd:attribute name="ToContainerHeightUOM" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="ToContainerLength" type="yfctype:Dimension" use="optional"/>
        <xsd:attribute name="ToContainerLengthUOM" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="ToContainerNetWeight"
