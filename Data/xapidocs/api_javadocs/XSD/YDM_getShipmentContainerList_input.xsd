        <xsd:attribute name="ToIsHazmat" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsManifested" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsPackProcessComplete"
            type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsReceived" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToLoadKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToOversizedFlag" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToParentContainerGroup"
            type="yfctype:EnumContainerGroup" use="optional"/>
        <xsd:attribute name="ToParentContainerKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToParentContainerNo" type="yfctype:ID-40" use="optional"/>
        <xsd:attribute name="ToRequiredNoOfReturnLabels"
            type="yfctype:Count" use="optional"/>
        <xsd:attribute name="ToRoutingCode" type="yfctype:Text-20" use="optional"/>
        <xsd:attribute name="ToSCAC" type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="ToShipmentContainerKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToShipmentKey" type="yfctype:Key" use="optional"/>
