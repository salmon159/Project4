            </xsd:attribute>
            <xsd:attribute name="LastNameQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MiddleNameQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="MOBILE_PHONE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MobilePhoneQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="OTHER_PHONE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OtherPhoneQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                <xsd:annotation yfc:columnName="PERSON_ID"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonIDQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PersonInfoKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PERSON_INFO_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary key for the YFS_PERSON_INFO table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonInfoKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PreferredShipAddress"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PREFERRED_SHIP_ADDRESS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The preferred ship address over and above the address in this record. Pointer to another record of this type in this table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PreferredShipAddressQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="State" type="yfctype:State" use="optional">
                <xsd:annotation yfc:columnName="STATE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The state in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StateQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                <xsd:annotation yfc:columnName="SUFFIX"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SuffixQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                <xsd:annotation yfc:columnName="TAX_GEO_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxGeoCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                <xsd:annotation yfc:columnName="TITLE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TitleQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToAddressLine1" type="yfctype:Address" use="optional"/>
            <xsd:attribute name="ToAddressLine2" type="yfctype:Address" use="optional"/>
            <xsd:attribute name="ToAddressLine3" type="yfctype:Address" use="optional"/>
            <xsd:attribute name="ToAddressLine4" type="yfctype:Address" use="optional"/>
            <xsd:attribute name="ToAddressLine5" type="yfctype:Address" use="optional"/>
            <xsd:attribute name="ToAddressLine6" type="yfctype:Address" use="optional"/>
            <xsd:attribute name="ToAlternateEmailID"
                type="yfctype:EMailID" use="optional"/>
            <xsd:attribute name="ToBeeper" type="yfctype:PhoneNo" use="optional"/>
            <xsd:attribute name="ToCity" type="yfctype:City" use="optional"/>
            <xsd:attribute name="ToCompany" type="yfctype:Company" use="optional"/>
            <xsd:attribute name="ToCountry" type="yfctype:Country" use="optional"/>
            <xsd:attribute name="ToDayFaxNo" type="yfctype:PhoneNo" use="optional"/>
            <xsd:attribute name="ToDayPhone" type="yfctype:PhoneNo" use="optional"/>
            <xsd:attribute name="ToDepartment" type="yfctype:Department" use="optional"/>
            <xsd:attribute name="ToEMailID" type="yfctype:EMailID" use="optional"/>
            <xsd:attribute name="ToErrorTxt" type="yfctype:ErrorText" use="optional"/>
            <xsd:attribute name="ToEveningFaxNo" type="yfctype:PhoneNo" use="optional"/>
            <xsd:attribute name="ToEveningPhone" type="yfctype:PhoneNo" use="optional"/>
            <xsd:attribute name="ToFirstName" type="yfctype:PersonName" use="optional"/>
            <xsd:attribute name="ToHttpUrl" type="yfctype:URL" use="optional"/>
            <xsd:attribute name="ToIsCommercialAddress"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToJobTitle" type="yfctype:Title-40" use="optional"/>
            <xsd:attribute name="ToLastName" type="yfctype:PersonName" use="optional"/>
            <xsd:attribute name="ToMiddleName" type="yfctype:MiddleName" use="optional"/>
            <xsd:attribute name="ToMobilePhone" type="yfctype:PhoneNo" use="optional"/>
            <xsd:attribute name="ToOtherPhone" type="yfctype:PhoneNo" use="optional"/>
            <xsd:attribute name="ToPersonID" type="yfctype:PersonId" use="optional"/>
            <xsd:attribute name="ToPersonInfoKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToPreferredShipAddress"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToState" type="yfctype:State" use="optional"/>
            <xsd:attribute name="ToSuffix" type="yfctype:Suffix" use="optional"/>
            <xsd:attribute name="ToTaxGeoCode" type="yfctype:GeoCode" use="optional"/>
            <xsd:attribute name="ToTitle" type="yfctype:Title-10" use="optional"/>
            <xsd:attribute name="ToUseCount" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="ToVerificationStatus"
                type="yfctype:ShortStatus" use="optional"/>
            <xsd:attribute name="ToZipCode" type="yfctype:ZipCode" use="optional"/>
            <xsd:attribute name="UseCount" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="USE_COUNT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UseCountQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="VerificationStatus"
                type="yfctype:ShortStatus" use="optional">
                <xsd:annotation yfc:columnName="VERIFICATION_STATUS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VerificationStatusQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                <xsd:annotation yfc:columnName="ZIP_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ZipCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LoadShipments">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:LoadShipment"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LoadShipment">
        <xsd:annotation yfc:tableName="YFS_LOAD_SHIPMENT">
            <xsd:documentation>Stores shipments that belong to a load. In addition, it stores the pick up and drop off point for a shipment during load delivery. This is created and updated by the manageLoad API.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PickupStop"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:DropoffStop"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="Shipment">
                    <xsd:annotation yfc:tableName="YFS_SHIPMENT">
                        <xsd:documentation>Stores shipment information. A shipment is a collection of order lines which will be shipped as a unit. One or more orders from a buyer organization may be grouped into the same shipment to minimize transportation costs.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="ActualDeliveryDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="ACTUAL_DELIVERY_DATE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The date on which the shipment was actually delivered at the buyer organization.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ActualFreightCharge"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="ACTUAL_FREIGHT_CHARGE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The actual freight charge for transporting the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ActualShipmentDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="ACTUAL_SHIPMENT_DATE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The actual shipment date. This is updated by the confirmShipment API.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AppointmentNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="APPOINTMENT_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>A number which helps the ship node and the carrier identify the pick up details and any relevant details about the shipment delivery.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AssignedToUserId"
                            type="yfctype:LoginId" use="optional">
                            <xsd:annotation
                                yfc:columnName="ASSIGNED_TO_USER_ID" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The backroom pick user who is currently working on this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BolNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="BOL_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The bill of lading number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BuyerMarkForNodeId"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="BUYER_MARK_FOR_NODE_ID" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>Mark For Node as identified by the buyer for eg: it could be a person,store,DC.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BuyerOrganizationCode"
                            type="yfctype:OrgCode" use="optional">
                            <xsd:annotation
                                yfc:columnName="BUYER_ORGANIZATION_CODE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The buyer organization to which this shipment belongs.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BuyerReceivingNodeId"
                            type="yfctype:ShipNode" use="optional">
                            <xsd:annotation
                                yfc:columnName="BUYER_RECEIVING_NODE_ID" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>Receiving node as identified by the buyer for eg:Store,DC Number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CarrierAccountNo"
                            type="yfctype:AccountNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTCARRIER_ACCOUNT_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The account number of the customer or buying organization with the carrier. This is used for arriving at the billing account number. See YFS_SHIPMENT_CONTAINER.carrier_bill_account.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CarrierServiceCode"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The carrier service availed for transporting this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CarrierType"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="CARRIER_TYPE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The type of carrier. This field is denormalized from YFS_SCAC_AND_SERVICE for the scac and carrier service code on the shipment. Typical values are PARCEL, LTL, and so forth.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CommercialValue"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="COMMERCIAL_VALUE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The commercial or declared value of the shipment for insurance purposes.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Currency"
                            type="yfctype:Currency" use="optional">
                            <xsd:annotation yfc:columnName="CURRENCY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The currency corresponding to the shipment charges.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DeliveryMethod"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="DELIVERY_METHOD" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The delivery method for the Product Line.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DeliveryPlanKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="DELIVERY_PLAN_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The delivery plan to which this shipment belongs.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DestinationZone"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="DESTINATION_ZONE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This field is not currently used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DocumentType"
                            type="yfctype:ID-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>A classification of data on which a particular set of business processes must be applied. In this case, it happens to be the document type of the order document - basically the document type of the orders comprising the shipment. A document type derived from the base document type 0001 - ORDER.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DownLoadCount"
                            type="yfctype:Count" use="optional">
                            <xsd:annotation
                                yfc:columnName="DOWNLOAD_COUNT" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The number of times that the shipment information has been downloaded from the user interface.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EnterpriseCode"
                            type="yfctype:OrgCode" use="optional">
                            <xsd:annotation
                                yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The enterprise to which this shipment belongs.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExpectedDeliveryDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXPECTED_DELIVERY_DATE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The date on which the shipment is expected to be delivered at the buyer organization.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExpectedShipmentDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXPECTED_SHIPMENT_DATE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The shipment date as planned or arrived at in the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExportTaxpayerID"
                            type="yfctype:ID-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXPORT_TAXPAYER_ID" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The tax payer ID of the shipping organization if the shipment is meant for export.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="FreightTerms"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="FREIGHT_TERMS" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The entity that pays the freight. Freight Terms determines whether a shipment is Pre Paid, 3rd Party Billing or Collect Billing.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="FromAddressKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="FROM_ADDRESS_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The from address key that is associated with this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="FromAppointment"
                            type="yfctype:Date" use="optional">
                            <xsd:annotation
                                yfc:columnName="FROM_APPOINTMENT" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The time after which an appointment can be taken by the ship node with the carrier.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HasNodeExceptions"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="HAS_NODE_EXCEPTIONS" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This flag indicates whether there are any exceptions at the node. Typically, these exceptions are due to insufficient inventory at the node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HasOtherShipments"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="HAS_OTHER_SHIPMENTS" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This field is not currently used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HazardousMaterialFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="HAZARDOUS_MATERIAL_FLAG" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This flag indicates whether the shipment carries an item that is marked hazardous.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HeldForConfirmation"
                            type="yfctype:String" use="optional"/>
                        <xsd:attribute name="HoldLocation"
                            type="yfctype:LocationId" use="optional">
                            <xsd:annotation
                                yfc:columnName="HOLD_LOCATION" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>Deposit location of picked items of shipment which are waiting for customer pick up in a store. If there are multiple locations, then those should be updated in a comma separated string. Primarily used in store application for backroom picking.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ITDate" type="yfctype:Date" use="optional">
                            <xsd:annotation yfc:columnName="IT_DATE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The date on which the IT number was stamped.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ITNo"
                            type="yfctype:NText-30" use="optional">
                            <xsd:annotation yfc:columnName="IT_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The license number given by government or customs for shipments going to bonded warehouses.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ManifestKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="MANIFEST_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>A number assigned on the system, if the CARRIER_TYPE of the shipment is PARCEL. A shipment assigned a particular manifest number is picked up by the carrier pick up personnel for purposes of shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ManifestNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="MANIFEST_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>An attribute used to store manifest number assigned by carrier server. ManifestKey on shipment is where the actual manifesting related information is stored for the manifesting process in the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NextAlertTs"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="NEXT_ALERT_TS" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The timestamp for the next alert. This is a computed field, which is used internally by the shipment monitor to proactively identify when the shipment could potentially be realerted on a business exception as per the monitor rules setup.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NumOfCartons"
                            type="yfctype:ContainerCount" use="optional">
                            <xsd:annotation
                                yfc:columnName="NUM_OF_CARTONS" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The number of cartons in the shipment. This field is only an approximation and not accurate depiction of the number of containers in a shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NumOfPallets"
                            type="yfctype:ContainerCount" use="optional">
                            <xsd:annotation
                                yfc:columnName="NUM_OF_PALLETS" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The number of pallets in the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderAvailableOnSystem"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_AVAILABLE_ON_SYSTEM" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This flag indicates whether the order is available on the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderHeaderKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The order header key that is associated with this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="ORDER_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The order number of the order for the shipment while creating with SingleOrder.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderReleaseKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_RELEASE_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The order release key that is associated with this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OriginZone"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="ORIGIN_ZONE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This field is not currently used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PackListType"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation
                                yfc:columnName="PACKLIST_TYPE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The packlist type. One of the attributes used in shipment consolidation.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ParentShipmentKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="PARENT_SHIPMENT_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This field is not currently used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PickListNo"
                            type="yfctype:DocumentNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="PICK_LIST_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>A number that indicates a group of shipments which will be picked together. A picklist is a document used for picking. It is generated for a ship node across one or more shipments and contains information on the shipment lines and the quantity for each line that is to be picked. Picklist is created by the printPickList API.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PickticketNo"
                            type="yfctype:VarcharID-24" use="optional">
                            <xsd:annotation
                                yfc:columnName="PICKTICKET_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>A number assigned to group related shipments under one head. This is input by the user.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PipelineKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="PIPELINE_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The pipeline to which this shipment belongs. This is determined by the createShipment API based on the document type and several other key attributes of the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PodNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="POD_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The proof of delivery number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ProNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="PRO_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The progressive number assigned by a carrier to track a shipment (usually for LTL shipments).</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ProfileID"
                            type="yfctype:ShipmentProfileID" use="optional">
                            <xsd:annotation yfc:columnName="PROFILE_ID" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The profile_id of the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReceivingNode"
                            type="yfctype:ShipNode" use="optional">
                            <xsd:annotation
                                yfc:columnName="RECEIVING_NODE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The node that receives the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReleaseNo"
                            type="yfctype:Count" use="optional">
                            <xsd:annotation yfc:columnName="RELEASE_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The release number of the OrderRelease for the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestedDeliveryDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQUESTED_DELIVERY_DATE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The date on which the shipment must be delivered at the buyer organization.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestedShipmentDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQUESTED_SHIPMENT_DATE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The shipment date as requested by the buyer organization.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SCAC"
                            type="yfctype:OrgCode" use="optional">
                            <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The carrier by which this shipment will be transported.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SealNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="SEAL_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The seal number on the load.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SellerOrganizationCode"
                            type="yfctype:OrgCode" use="optional">
                            <xsd:annotation
                                yfc:columnName="SELLER_ORGANIZATION_CODE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The seller organization to which the shipment belongs.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipMode"
                            type="yfctype:ShipMode" use="optional">
                            <xsd:annotation yfc:columnName="SHIP_MODE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>An attribute used to identify the mode of shipment. Typical values are TL - Truck Load, LTL - Less Than Truck Load, etc. This attribute can also be used to store any of your own categorizations.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipNode"
                            type="yfctype:ShipNode" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The ship node where the shipment was created.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipVia"
                            type="yfctype:Text-10" use="optional">
                            <xsd:annotation yfc:columnName="SHIP_VIA" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The means of shipment. This is usually Land, Air, Sea, or Ocean.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentClosedFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_CLOSED_FLAG" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This flag indicates whether the shipment is closed. This is a computed field, which is used internally as a way to logically mark data for purge so that transactions do not look into this data. This data is later purged by the purge agent at preset intervals. This is updated by the close shipment agent.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentConfirmUpdatesDone"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_CONFIRM_UPDATES_DONE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This flag indicates whether the shipment confirm updates have been done. This is an internal field, which is used by the confirmShipment API to identify if shipment confirmation updates have already been performed on the shipment. Since there are various ways through which shipment confirmation updates could happen -- receiveIntransitUpdates, closeManifest, or confirmShipment -- it is required that there be an attribute that identifies if the shipment confirmation has indeed been done.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="ShipmentConsolidationGroupId"
                            type="yfctype:VarcharShipNode" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_CONSOL_GROUP_ID" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>It is a consolidation group id  that is used as a standard do not mix constraint.Releases belonging to same shipment consolidation group may consolidate to same shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentContainerizedFlag"
                            type="yfctype:EnumShipmentContainerizedFlag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_CONTAINERIZED_FLAG" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This is an internal field that indicates the containerization state of the shipment. The values being: 01 - not containerized, 02 - containerization in progress and 03 - indicating complete containerization. This is updated by all shipment APIs that containerize or uncontainerize. APIs include createShipment, packShipment, unpackShipment, confirmShipment and changeShipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentDeliverUpdatesDone"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_DELIVER_UPDATES_DONE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This internal field is used by the receiveIntransitUpdates API to identify if delivery updates have already been done. Delivery updates are possible by explicitly invoking the deliverShipment API.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The primary key for the YFS_SHIPMENT table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentNo"
                            type="yfctype:DocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="SHIPMENT_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The shipment number. This is a part of the unique key or business key.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentPlannedFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_PLANNED_FLAG" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This flag indicates whether the shipment is planned. This is a computed field. When every leg of the shipment from source to destination is included into loads, the shipment is said to be planned. This is updated by the manageLoad API.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentType"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_TYPE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The user-defined shipment type. This is used to indicate the composition of the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Status"
                            type="yfctype:ShortStatus" use="optional">
                            <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The status of the shipment. As various transactions in the shipment's pipeline act upon the shipment, the shipment status changes. Typical values are 1100 - Created, 1200 - Being Picked, 1400 - Shipped.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="StatusDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="STATUS_DATE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The date and time when the last status change took place.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ToAddressKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="TO_ADDRESS_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The to address key that is associated with this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ToAppointment"
                            type="yfctype:Date" use="optional">
                            <xsd:annotation
                                yfc:columnName="TO_APPOINTMENT" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The time before which an appointment could be taken by the ship node with the carrier.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalActualCharge"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_ACTUAL_CHARGE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The actual charge for transporting the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalEstimatedCharge"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_ESTIMATED_CHARGE" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The estimated charge for transporting the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalVolume"
                            type="yfctype:Dimension" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_VOLUME" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The total volume of the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalVolumeUOM"
                            type="yfctype:UOM" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_VOLUME_UOM" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The unit of measure for the units of volume specified in TotalVolume attribute of this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalWeight"
                            type="yfctype:Dimension" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_WEIGHT" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The total weight of the shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalWeightUOM"
                            type="yfctype:UOM" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_WEIGHT_UOM" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The unit of measure for the units of weight specified in Total_Weight attribute of this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TrackingNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="TRACKING_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>This field is not currently used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TrailerNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="TRAILER_NO" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The trailer number by which the shipment leaves the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="WorkOrderApptKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="WORK_ORDER_APPT_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The work order appt key that is associated with this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="WorkOrderKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="WORK_ORDER_KEY" yfc:tableName="YFS_SHIPMENT">
                                <xsd:documentation>The work order key that is associated with this shipment.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PickupStop">
        <xsd:annotation yfc:tableName="YFS_LOAD_STOP">
            <xsd:documentation>Stores information about a load stop. A load stop is identified by a stop node -  which is a receiving or shipping node on the system, or a stop address in cases where stop nodes do correspond to a receiving node which is not maintained on the system. A load stop is created and managed by the manageLoad API. Certain time related attributes such as actual arrival and departure dates, and the stop reached attribute are updated by the receiveIntransitUpdates API.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ActualArrivalDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="ACTUAL_ARRIVAL_DATE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The actual arrival date and time of the load at this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ActualDepartureDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="ACTUAL_DEPARTURE_DATE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The actual departure date and time of the load from this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DockLocationId"
                type="yfctype:LocationId" use="optional">
                <xsd:annotation yfc:columnName="DOCK_LOCATION_ID" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The identifier of the dock location for the load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedArrivalDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_ARRIVAL_DATE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The date and time at which the load is expected to arrive at the load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedDepartureDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_DEPARTURE_DATE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The date and time at which the load is expected to depart from the load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LoadKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="LOAD_KEY" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The load for which stops are maintained.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LoadStopKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="LOAD_STOP_KEY" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The primary key for the YFS_LOAD_STOP table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopAddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="STOP_ADDRESS_KEY" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The stop address key. A stop may be identified alternatively by a stop address. This is typically the case when receiving or destination nodes are not maintained on the system.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="STOP_NODE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The stop node. A stop node corresponds to a receiving or ship node on the system. The information of such nodes are maintained in YFS_SHIP_NODE entity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopReached" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="STOP_REACHED" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>This flag indicate whether updates have been received for this load stop, and is updated by the receiveIntransitUpdates API.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopSequenceNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="STOP_SEQUENCE_NO" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The stop sequence number that determines the load travel path. The origin stop is assigned the sequence no. -1 and the destination stop is assigned the sequence no. 99999. All intermediate stops may be sequenced by the user to fall within this range. Stop Type attribute of the stop determines whether a stop is the origin, destination or an intermediate stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopType" type="yfctype:EnumStopType" use="optional">
                <xsd:annotation yfc:columnName="STOP_TYPE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The type of the load stop. There are three types of load stops: O - corresponds to the origin stop, D - corresponds to the destination stop and I - for an intermediate stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalVolumeIn" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_VOLUME_IN" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The total volume of the load as it arrives at this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalVolumeOut"
                type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_VOLUME_OUT" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The total volume of the load as it departs from this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalWeightIn" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_WEIGHT_IN" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The total weight of the load as it arrives at this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalWeightOut"
                type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_WEIGHT_OUT" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The total weight of the load as it departs from this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VolumeUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="VOLUME_UOM" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The unit of measure for the units of volume specified in Total_Volume_In and Total_Volume_Out attributes of this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="WeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="WEIGHT_UOM" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The unit of measure for the units of weight specified in Total_Weight_In and Total_Weight_Out attributes of this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="isHistory" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Indicates if data is fetched from the regular table or the history table. Valid values are Y and N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="DropoffStop">
        <xsd:annotation yfc:tableName="YFS_LOAD_STOP">
            <xsd:documentation>Stores information about a load stop. A load stop is identified by a stop node -  which is a receiving or shipping node on the system, or a stop address in cases where stop nodes do correspond to a receiving node which is not maintained on the system. A load stop is created and managed by the manageLoad API. Certain time related attributes such as actual arrival and departure dates, and the stop reached attribute are updated by the receiveIntransitUpdates API.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ActualArrivalDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="ACTUAL_ARRIVAL_DATE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The actual arrival date and time of the load at this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ActualDepartureDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="ACTUAL_DEPARTURE_DATE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The actual departure date and time of the load from this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DockLocationId"
                type="yfctype:LocationId" use="optional">
                <xsd:annotation yfc:columnName="DOCK_LOCATION_ID" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The identifier of the dock location for the load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedArrivalDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_ARRIVAL_DATE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The date and time at which the load is expected to arrive at the load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedDepartureDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_DEPARTURE_DATE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The date and time at which the load is expected to depart from the load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LoadKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="LOAD_KEY" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The load for which stops are maintained.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LoadStopKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="LOAD_STOP_KEY" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The primary key for the YFS_LOAD_STOP table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopAddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="STOP_ADDRESS_KEY" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The stop address key. A stop may be identified alternatively by a stop address. This is typically the case when receiving or destination nodes are not maintained on the system.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="STOP_NODE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The stop node. A stop node corresponds to a receiving or ship node on the system. The information of such nodes are maintained in YFS_SHIP_NODE entity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopReached" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="STOP_REACHED" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>This flag indicate whether updates have been received for this load stop, and is updated by the receiveIntransitUpdates API.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopSequenceNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="STOP_SEQUENCE_NO" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The stop sequence number that determines the load travel path. The origin stop is assigned the sequence no. -1 and the destination stop is assigned the sequence no. 99999. All intermediate stops may be sequenced by the user to fall within this range. Stop Type attribute of the stop determines whether a stop is the origin, destination or an intermediate stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StopType" type="yfctype:EnumStopType" use="optional">
                <xsd:annotation yfc:columnName="STOP_TYPE" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The type of the load stop. There are three types of load stops: O - corresponds to the origin stop, D - corresponds to the destination stop and I - for an intermediate stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalVolumeIn" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_VOLUME_IN" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The total volume of the load as it arrives at this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalVolumeOut"
                type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_VOLUME_OUT" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The total volume of the load as it departs from this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalWeightIn" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_WEIGHT_IN" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The total weight of the load as it arrives at this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TotalWeightOut"
                type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_WEIGHT_OUT" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The total weight of the load as it departs from this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VolumeUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="VOLUME_UOM" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The unit of measure for the units of volume specified in Total_Volume_In and Total_Volume_Out attributes of this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="WeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="WEIGHT_UOM" yfc:tableName="YFS_LOAD_STOP">
                    <xsd:documentation>The unit of measure for the units of weight specified in Total_Weight_In and Total_Weight_Out attributes of this load stop.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="isHistory" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Indicates if data is fetched from the regular table or the history table. Valid values are Y and N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ContainerActivities">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ContainerActivity"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ContainerActivity">
        <xsd:annotation yfc:tableName="YFS_CONTAINER_ACTIVITY">
            <xsd:documentation>Stores information about activities performed on a container.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ActivityLocation"/>
            </xsd:all>
            <xsd:attribute name="ActivityCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ACTIVITY_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>Identifier of an activity.For example PACK or TRANSPORTATION.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ActivityCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ActivityTimeStamp"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="ACTIVITY_TIME_STAMP"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>Activity timestamp.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ActivityTimeStampQryType"
                type="yfctype:DateQryType" use="optional"/>
            <xsd:attribute name="ActivityUserId" type="yfctype:UserId" use="optional">
                <xsd:annotation yfc:columnName="ACTIVITY_USERID"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The user who performed the activity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ActivityUserIdQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ContainerActivityKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="CONTAINER_ACTIVITITY_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The primary key for the YFS_CONTAINER_DETAILS table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContainerActivityKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromActivityCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromActivityTimeStamp"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="FromActivityUserId"
                type="yfctype:UserId" use="optional"/>
            <xsd:attribute name="FromContainerActivityKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromIsCommercialAddress"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FromIsException" type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FromNotes" type="yfctype:Text-2000" use="optional"/>
            <xsd:attribute name="FromShipmentContainerKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromTaxGeoCode" type="yfctype:GeoCode" use="optional"/>
            <xsd:attribute name="IsCommercialAddress"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_COMMERCIAL_ADDRESS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>
                        <div>
							Indicates whether the address is commercial, residential or none. Values of this field can be Y is Commercial address. N is Residential address. NULL is None. Default value is Null.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsCommercialAddressQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="IsException" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_EXCEPTION"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>This flag indicates whether the activity has an exception or not.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsExceptionQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="Notes" type="yfctype:Text-2000" use="optional">
                <xsd:annotation yfc:columnName="NOTES"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>General notes which includes reasons for any exceptions.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NotesQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ShipmentContainerKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_CONTAINER_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The shipment container key that is associated with this container details.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentContainerKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                <xsd:annotation yfc:columnName="TAX_GEO_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxGeoCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToActivityCode" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToActivityTimeStamp"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToActivityUserId" type="yfctype:UserId" use="optional"/>
            <xsd:attribute name="ToContainerActivityKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToIsCommercialAddress"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToIsException" type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToNotes" type="yfctype:Text-2000" use="optional"/>
            <xsd:attribute name="ToShipmentContainerKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToTaxGeoCode" type="yfctype:GeoCode" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ActivityLocation">
        <xsd:annotation yfc:tableName="YFS_CONTAINER_ACTIVITY">
            <xsd:documentation>Stores information about activities performed on a container.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ActivityLocationId"
                type="yfctype:LocationId" use="optional">
                <xsd:annotation yfc:columnName="ACTIVITY_LOCATION_ID" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The location within a node where the activity is performed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ActivityNodeKey"
                type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="ACTIVITY_NODE_KEY" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The node where the activity is performed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="City" type="yfctype:City" use="optional">
                <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The city in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Company" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The country in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The first name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsCommercialAddress"
                type="yfctype:String" use="optional"/>
            <xsd:attribute name="LastName" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The last name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="State" type="yfctype:State" use="optional">
                <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The state in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxGeoCode" type="yfctype:String" use="optional"/>
            <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_CONTAINER_ACTIVITY">
                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Wave">
        <xsd:annotation yfc:tableName="YFS_WAVE">
            <xsd:documentation>This table stores wave definitions.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Description"
                type="yfctype:FreeText-200" use="optional">
                <xsd:annotation yfc:columnName="DESCRIPTION"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>A brief description of the wave.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DescriptionQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>This field indicates the document type to which the wave belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentTypeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="EstimatedMinutes" use="optional">
                <xsd:annotation yfc:columnName="ESTIMATED_MINUTES"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>Indicates the number of minutes it is estimated that the wave would take to execute.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Count"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="EstimatedMinutesQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="FromDescription"
                type="yfctype:FreeText-200" use="optional"/>
            <xsd:attribute name="FromDocumentType"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromEstimatedMinutes"
                type="yfctype:Count" use="optional"/>
            <xsd:attribute name="FromNode" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromOverrideConcurrentWave"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FromOverrideReleaseDate"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FromPipelineKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromPriority" type="yfctype:Priority" use="optional"/>
            <xsd:attribute name="FromReasonCode"
                type="yfctype:ReasonCode" use="optional"/>
            <xsd:attribute name="FromReasonText"
                type="yfctype:ReasonText" use="optional"/>
            <xsd:attribute name="FromReleaseNoEarlierThan"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="FromReleaseNoLaterThan"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="FromRemainingMinutes"
                type="yfctype:Count" use="optional"/>
            <xsd:attribute name="FromSeqNo" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="FromShipmentGroupId"
                type="yfctype:ID-10" use="optional"/>
            <xsd:attribute name="FromWaveKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromWaveNo" type="yfctype:SequenceNo" use="optional"/>
            <xsd:attribute name="FromWaveStatus" type="yfctype:Status" use="optional"/>
            <xsd:attribute name="Node" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="NODE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>The primary key for the YFS_WAVE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OverrideConcurrentWave"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="OVERRIDE_CONCURRENT_WAVE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>This flag indicates whether the wave overrides the concurrent wave that is released. If 'Y' is selected, the Assign Pick Location (Release Wave) agent ignores the number of waves released concurrently that is defined for a specific shipment group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OverrideConcurrentWaveQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OverrideReleaseDate"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="OVERRIDE_RELEASE_DATE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>This flag indicates whether the wave overrides the wave release date. If 'Y' is selected, the Assign Pick Location (Release Wave) agent ignores the Release No Earlier Than and Release No Later Than date ranges.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OverrideReleaseDateQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PipelineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PIPELINE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>The pipeline key that is associated with the wave.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PipelineKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="Priority" type="yfctype:Priority" use="optional">
                <xsd:annotation yfc:columnName="PRIORITY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>This field indicates the priority to apply to the wave. All waves will be processed in the order of this priority.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PriorityQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="ReasonCode" type="yfctype:ReasonCode" use="optional">
                <xsd:annotation yfc:columnName="REASON_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>This field indicates the reason code for modifying the wave.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReasonCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReasonText" type="yfctype:ReasonText" use="optional">
                <xsd:annotation yfc:columnName="REASON_TEXT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>This field describes any additional information as to why the wave was modified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReasonTextQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReleaseNoEarlierThan" use="optional">
                <xsd:annotation yfc:columnName="RELEASE_NO_EARLIER_THAN"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>This field indicates that the wave is not released for picking before this date.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReleaseNoEarlierThanQryType"
                type="yfctype:DateQryType" use="optional"/>
            <xsd:attribute name="ReleaseNoLaterThan" use="optional">
                <xsd:annotation yfc:columnName="RELEASE_NO_LATER_THAN"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WAVE">
                    <xsd:documentation>This field indicates that the wave is not released for picking after this date.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReleaseNoLaterThanQryType"
                type="yfctype:DateQryType" use="optional"/>
            <xsd:attribute name="RemainingMinutes" use="optional">
                <xsd:annotation yfc:qryTypeSupported="Y">
                    <xsd:documentation>Indicates the time required for the planned tasks that are not yet complete.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Count"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
