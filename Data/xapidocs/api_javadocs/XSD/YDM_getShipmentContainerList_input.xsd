        <xsd:attribute name="ToContainerNetWeightUOM" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="ToContainerNo" type="yfctype:ContainerNo" use="optional"/>
        <xsd:attribute name="ToContainerScm" type="yfctype:UCC128" use="optional"/>
        <xsd:attribute name="ToContainerSeqNo" type="yfctype:Count" use="optional"/>
        <xsd:attribute name="ToContainerType" type="yfctype:Text-6" use="optional"/>
        <xsd:attribute name="ToContainerWidth" type="yfctype:Dimension" use="optional"/>
        <xsd:attribute name="ToContainerWidthUOM" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="ToCustomsValue" type="yfctype:Money" use="optional"/>
        <xsd:attribute name="ToDeclaredValue" type="yfctype:Money" use="optional"/>
        <xsd:attribute name="ToDimmedFlag" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToDiscountAmount" type="yfctype:Money" use="optional"/>
        <xsd:attribute name="ToExportLicenseExpDate" type="yfctype:Date" use="optional"/>
