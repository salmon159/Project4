        <xsd:attribute name="ToBarcodeDiscount" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToBasicFreightCharge" type="yfctype:Money" use="optional"/>
        <xsd:attribute name="ToCODReturnTrackingNo"
