            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Ucc128codeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Zone" type="yfctype:Text-15" use="optional">
            <xsd:annotation yfc:columnName="ZONE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_CONTAINER">
                <xsd:documentation>This field is not currently used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ZoneQryType" type="yfctype:StringQryType" use="optional"/>
    </xsd:complexType>
    <xsd:element name="ContainerStatusAudits">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ContainerStatusAudit"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ContainerStatusAudit">
        <xsd:annotation yfc:tableName="YFS_CONTAINER_STS_AUDIT">
            <xsd:documentation>This table stores container status change audit information. Each time the container status changes because of a business process, an audit is posted along with the status, timestamp, reason code and text, user ID and program ID. All transactions in the container pipeline in fact will have entries in this table upon completion of a transaction.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ContainerKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CONTAINER_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_STS_AUDIT">
                    <xsd:documentation>Container for which a status change audit is written.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContainerKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ContainerStatusAuditKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="CONTAINER_STATUS_AUDIT_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_STS_AUDIT">
                    <xsd:documentation>Primary key attribute of YFS_CONTAINER_STATUS_AUDIT table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContainerStatusAuditKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromContainerKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromContainerStatusAuditKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromNewStatus"
                type="yfctype:ShortStatus" use="optional"/>
            <xsd:attribute name="FromNewStatusDate"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="FromOldStatus"
                type="yfctype:ShortStatus" use="optional"/>
            <xsd:attribute name="FromOldStatusDate"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="FromReasonCode"
                type="yfctype:ReasonCode" use="optional"/>
            <xsd:attribute name="FromReasonText"
                type="yfctype:ReasonText" use="optional"/>
            <xsd:attribute name="NewStatus" type="yfctype:ShortStatus" use="optional">
                <xsd:annotation yfc:columnName="NEW_STATUS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_STS_AUDIT">
                    <xsd:documentation>Status of the container after the status change.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NewStatusDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="NEW_STATUS_DATE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_STS_AUDIT">
                    <xsd:documentation>Status date recorded on the container at the most recent status change.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NewStatusDateQryType"
                type="yfctype:DateQryType" use="optional"/>
            <xsd:attribute name="NewStatusQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OldStatus" type="yfctype:ShortStatus" use="optional">
                <xsd:annotation yfc:columnName="OLD_STATUS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_STS_AUDIT">
                    <xsd:documentation>Status of the container before the status change.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OldStatusDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="OLD_STATUS_DATE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_STS_AUDIT">
                    <xsd:documentation>Status date as recorded on the container at the previous status change.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OldStatusDateQryType"
                type="yfctype:DateQryType" use="optional"/>
            <xsd:attribute name="OldStatusQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReasonCode" type="yfctype:ReasonCode" use="optional">
                <xsd:annotation yfc:columnName="REASON_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_STS_AUDIT">
                    <xsd:documentation>Reason code indicating the reason for the status change.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReasonCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReasonText" type="yfctype:ReasonText" use="optional">
                <xsd:annotation yfc:columnName="REASON_TEXT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CONTAINER_STS_AUDIT">
                    <xsd:documentation>Free-form reason text indicating any additional information related to the status change.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReasonTextQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToContainerKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToContainerStatusAuditKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToNewStatus" type="yfctype:ShortStatus" use="optional"/>
            <xsd:attribute name="ToNewStatusDate"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToOldStatus" type="yfctype:ShortStatus" use="optional"/>
            <xsd:attribute name="ToOldStatusDate"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToReasonCode" type="yfctype:ReasonCode" use="optional"/>
            <xsd:attribute name="ToReasonText" type="yfctype:ReasonText" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Tote">
        <xsd:annotation yfc:tableName="YFS_TOTE">
            <xsd:documentation>This table stores the tote id and the associated reference.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Node" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="NODE_KEY" yfc:tableName="YFS_TOTE">
                    <xsd:documentation>The node key that is associated with this user zone.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ToteId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TOTE_ID" yfc:tableName="YFS_TOTE">
                    <xsd:documentation>
                        <div> Identifier for tote.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Notes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Note"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Note">
        <xsd:annotation yfc:tableName="YFS_NOTES">
            <xsd:documentation>Generic table used to stores notes associated with various entities in the system.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:User"/>
            </xsd:all>
            <xsd:attribute name="AuditTransactionId"
                type="yfctype:TransactionID" use="optional">
                <xsd:annotation yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContactReference"
                type="yfctype:NotesContactReference" use="optional">
                <xsd:annotation yfc:columnName="CONTACT_REFERENCE" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>If the notes were entered because of customer contact by phone, the phone no can be stored in this column. If the contact is by email, email ID can be stored here.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContactTime" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="CONTACT_TIME" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>The time at which the customer contact occurred.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContactType" type="yfctype:CommonCode" use="optional">
                <xsd:annotation yfc:columnName="CONTACT_TYPE" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>The contact type like Phone call, email etc. The values will be selected from common code of code type "CONTACT_TYPE"</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContactUser" type="yfctype:UserId" use="optional">
                <xsd:annotation yfc:columnName="CONTACT_USER" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>The user who made the contact. The default value will the logged in user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerSatIndicator"
                type="yfctype:CustomerSatisfactionLevel" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_SAT_INDICATOR" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>The customer satisfaction level.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NoteText" type="yfctype:NoteText" use="optional">
                <xsd:annotation yfc:columnName="NOTE_TEXT" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>Detailed Notes.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Priority"
                type="yfctype:PriorityWithDecimal" use="optional">
                <xsd:annotation yfc:columnName="PRIORITY" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>Priority of the note. 1 is High, 0 is Low.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReasonCode" type="yfctype:ReasonCode" use="optional">
                <xsd:annotation yfc:columnName="REASON_CODE" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>The reason for which the notes are logged. Like "CustomerContact" etc. The values will be selected from common code of code type "NOTES_REASON"</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SequenceNo" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="SEQUENCE_NO" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>The sequence of the note.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Tranid" type="yfctype:TranId" use="optional">
                <xsd:annotation yfc:columnName="TRANID" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>Transaction for which these notes are applicable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Tranname" type="yfctype:Text-50" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Specifies the name of the Transaction</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VisibleToAll" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="VISIBLE_TO_ALL" yfc:tableName="YFS_NOTES">
                    <xsd:documentation>This flag determines whether the note applied should be visible to all users. This field is used for filtering. No logic is currently applied to it.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="User">
        <xsd:annotation yfc:tableName="YFS_USER">
            <xsd:documentation>This table stores information on users in the system.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Username" type="yfctype:UserName" use="optional">
                <xsd:annotation yfc:columnName="USERNAME" yfc:tableName="YFS_USER">
                    <xsd:documentation>The name of the user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Usertype" type="yfctype:EnumUserType" use="optional">
                <xsd:annotation yfc:columnName="USERTYPE" yfc:tableName="YFS_USER">
                    <xsd:documentation>This field indicates the user type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ShipmentLines">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipmentLine"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ShipmentLine">
        <xsd:annotation yfc:tableName="YFS_SHIPMENT_LINE">
            <xsd:documentation>Stores information about shipment lines. Shipment lines are created using createShipment or changeShipment API.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="Instructions">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="Instruction">
                                <xsd:annotation yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Generic table used to store instructions associated with various entities in the system.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute
                                    name="FromInstructionDetailKey"
                                    type="yfctype:Key" use="optional"/>
                                    <xsd:attribute
                                    name="FromInstructionText"
                                    type="yfctype:InstructionText" use="optional"/>
                                    <xsd:attribute
                                    name="FromInstructionType"
                                    type="yfctype:InstructionType" use="optional"/>
                                    <xsd:attribute
                                    name="FromInstructionURL"
                                    type="yfctype:URL" use="optional"/>
                                    <xsd:attribute
                                    name="FromInstructionUsage"
                                    type="yfctype:InstructionUsage" use="optional"/>
                                    <xsd:attribute
                                    name="FromReferenceKey"
                                    type="yfctype:Key" use="optional"/>
                                    <xsd:attribute name="FromSequenceNo"
                                    type="yfctype:Count" use="optional"/>
                                    <xsd:attribute name="FromTableName"
                                    type="yfctype:TableName" use="optional"/>
                                    <xsd:attribute
                                    name="InstructionDetailKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_DETAIL_KEY"
                                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Primary key attribute of YFS_INSTRUCTION_DETAIL table that identifies an instruction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InstructionDetailKeyQryType"
                                    type="yfctype:StringQryType" use="optional"/>
                                    <xsd:attribute
                                    name="InstructionText"
                                    type="yfctype:InstructionText" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_TEXT"
                                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Free-form text indicating any additional information related to the instruction type or otherwise.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InstructionTextQryType"
                                    type="yfctype:StringQryType" use="optional"/>
                                    <xsd:attribute
                                    name="InstructionType"
                                    type="yfctype:InstructionType" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_TYPE"
                                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Categorization of instructions so that they can be grouped together for reference. For example, you may have instructions of type Pick, Pack, and Ship for an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InstructionTypeQryType"
                                    type="yfctype:StringQryType" use="optional"/>
                                    <xsd:attribute name="InstructionURL"
                                    type="yfctype:URL" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_URL"
                                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>The URL relating to and/or containing the instructions.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InstructionURLQryType"
                                    type="yfctype:StringQryType" use="optional"/>
                                    <xsd:attribute
                                    name="InstructionUsage"
                                    type="yfctype:InstructionUsage" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_USAGE"
                                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Another way of categorization of instructions, over and above the instruction type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InstructionUsageQryType"
                                    type="yfctype:StringQryType" use="optional"/>
                                    <xsd:attribute name="ReferenceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_KEY"
                                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Link to the entity key for which the instruction is stored. For example, if this record stores order header level instructions, then this attribute stores the value of order header key.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReferenceKeyQryType"
                                    type="yfctype:StringQryType" use="optional"/>
                                    <xsd:attribute name="SequenceNo"
                                    type="yfctype:Count" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SEQUENCE_NO"
                                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>You could have an instruction exceeding the limits of a single record. Sequence number is used to provide a way to store a link between such instructions.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SequenceNoQryType"
                                    type="yfctype:NumberQryType" use="optional"/>
                                    <xsd:attribute name="TableName"
                                    type="yfctype:TableName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TABLE_NAME"
                                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Identifies the entity for which this instruction is stored. For example, if this record stores order header instructions, then you will see this field reading YFS_ORDER_HEADER.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TableNameQryType"
                                    type="yfctype:StringQryType" use="optional"/>
                                    <xsd:attribute
                                    name="ToInstructionDetailKey"
                                    type="yfctype:Key" use="optional"/>
                                    <xsd:attribute
                                    name="ToInstructionText"
                                    type="yfctype:InstructionText" use="optional"/>
                                    <xsd:attribute
                                    name="ToInstructionType"
                                    type="yfctype:InstructionType" use="optional"/>
                                    <xsd:attribute
                                    name="ToInstructionURL"
                                    type="yfctype:URL" use="optional"/>
                                    <xsd:attribute
                                    name="ToInstructionUsage"
                                    type="yfctype:InstructionUsage" use="optional"/>
                                    <xsd:attribute name="ToReferenceKey"
                                    type="yfctype:Key" use="optional"/>
                                    <xsd:attribute name="ToSequenceNo"
                                    type="yfctype:Count" use="optional"/>
                                    <xsd:attribute name="ToTableName"
