    <xsd:annotation
        yfc:fileName="YCD_getAlternateStoreAvailability_output.xsd" yfc:rootElement="AlternateStores">
        <xsd:appinfo source="yfc:structure">
            <AlternateStores hasAttributes="Y">
                <NodeList>
                    <Node hasAttributes="Y">
                        <Availability hasAttributes="Y"/>
                        <OwnerOrganization hasAttributes="Y"/>
                        <Organization hasAttributes="Y"/>
                        <ShipNodePersonInfo hasAttributes="Y"/>
                        <ContactPersonInfo hasAttributes="Y"/>
                        <ItemShipNodeList>
                            <ItemShipNode hasAttributes="Y"/>
                        </ItemShipNodeList>
                        <CanShipToNodeTypes>
                            <CanShipToNodeType hasAttributes="Y"/>
                        </CanShipToNodeTypes>
                    </Node>
                </NodeList>
                <OrderLines>
                    <OrderLine hasAttributes="Y">
                        <Item hasAttributes="Y">
                            <PrimaryInformation hasAttributes="Y"/>
                        </Item>
                        <AvailabilityList>
                            <Availability hasAttributes="Y">
                                <Node hasAttributes="Y">
                                    <OwnerOrganization hasAttributes="Y"/>
                                    <Organization hasAttributes="Y"/>
                                    <ShipNodePersonInfo hasAttributes="Y"/>
                                    <ContactPersonInfo hasAttributes="Y"/>
                                    <ItemShipNodeList>
                                    <ItemShipNode hasAttributes="Y"/>
                                    </ItemShipNodeList>
                                    <CanShipToNodeTypes>
                                    <CanShipToNodeType hasAttributes="Y"/>
                                    </CanShipToNodeTypes>
                                </Node>
                            </Availability>
                        </AvailabilityList>
                    </OrderLine>
                </OrderLines>
            </AlternateStores>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="AlternateStores" type="yfc:AlternateStoresXSDType"/>
    <xsd:complexType name="AlternateStoresXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:NodeList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLines"/>
        </xsd:all>
        <xsd:attribute name="OrganizationCode" type="yfctype:OrgCode" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>The Organization provided in the input to the API.</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="NodeList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="Node">
                    <xsd:annotation yfc:tableName="YFS_SHIP_NODE">
                        <xsd:documentation>This table stores receiving node, ship node, and distribution centers in the system for various organizations.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="Availability">
                                <xsd:complexType>
                                    <xsd:attribute name="AvailableDate"
                                    type="yfctype:Date" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							This will be returned when <code>Mode="01"</code> or <code>Mode="03"</code> when there in sufficient inventory available, and in <code>Mode="02"</code> when there is only future inventory available at the node.
						</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="IsAvailable"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							This flag will consistently display if the requested item is available at the provided node or not regardless of the API used to derive the information.
						</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsFutureAvailability"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							If the availability is for the future, this will be returned as "Y" else "N" and is currently only supported in <code>Mode="02"</code>.
						</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ShipNode"
                                    type="yfctype:ShipNode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIP_NODE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The unique identifier for the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="OwnerOrganization">
