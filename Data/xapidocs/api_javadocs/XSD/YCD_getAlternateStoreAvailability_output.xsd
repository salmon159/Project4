                                    yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The state in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Suffix"
                                    type="yfctype:Suffix" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TaxGeoCode"
                                    type="yfctype:GeoCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Title"
                                    type="yfctype:Title-10" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UseCount"
                                    type="yfctype:Count" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USE_COUNT" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="VerificationStatus"
                                    type="yfctype:ShortStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="VERIFICATION_STATUS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ZipCode"
                                    type="yfctype:ZipCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="ItemShipNodeList">
                                    <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="ItemShipNode">
                                    <xsd:annotation yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>This table stores distribution group information for an organization. This information is used during any inventory operation that involves distribution groups.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="CostCurrency"
                                    type="yfctype:Currency" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="COST_CURRENCY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Inventorytype"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="INVENTORYTYPE" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ItemId"
                                    type="yfctype:ItemID" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ITEM_ID" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The unique identifier for the sourced item. "ALL" indicates all items.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ItemType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ITEM_TYPE" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The only valid value for this field is currently "ALL", which indicates all items.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ItemshipnodeKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ITEMSHIPNODE_KEY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The primary key of the YFS_ITEM_SHIP_NODE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OwnerKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="OWNER_KEY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The owner organization that is associated with this distribution group.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Priority"
                                    type="yfctype:PriorityWithDecimal" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRIORITY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The priority for this distribution.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipNodeItem"
                                    type="yfctype:ItemID" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_NODE_ITEM" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The item as it is identified at the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipnodeKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The ship node associated with a given item ID, or with all items if ITEM_ID is set to "ALL".</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UnitCost"
                                    type="yfctype:UnitPrice" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="UNIT_COST" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="CanShipToNodeTypes">
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="CanShipToNodeType">
                                    <xsd:annotation yfc:tableName="YFS_NODE_CAN_SHIP_TO_NT">
                                    <xsd:documentation>This table contains node types that ship node can ship to.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="NodeTypeID"
                                    type="yfctype:Identifier" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="NODE_TYPE_ID" yfc:tableName="YFS_NODE_CAN_SHIP_TO_NT">
                                    <xsd:documentation>The unique identifier for the node type that the node can ship to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    <xsd:attribute
                                    name="AcceptanceRequired"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACCEPTANCE_REQUIRED" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether acceptance is required by the ship node before processing orders for shipping.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ActivateFlag"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACTIVATE_FLAG" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the ship node is active.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AgentCriteriaGroup"
                                    type="yfctype:AgentCriteriaGroup" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AGENT_CRITERIA_GROUP" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The agent criteria group that is used to classify nodes.  This value can be accepted by WMS agents which will only perform their tasks on the nodes with matching node transactional velocity value. Valid values are: LOW, HIGH and common code values defined by hub.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AllowGiftWrap"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ALLOW_GIFT_WRAP" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates that the node allows gift wrapping.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CanShipToAllNodes"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CAN_SHIP_TO_ALL_NODES" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the node can ship to all node/node types.  Entries in YFS_NODE_CAN_SHIP_TO_NT table will be ignored if flag is Y.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CanShipToOtherAddresses"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CAN_SHIP_TO_OTHER_ADDRESSES" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the node can ship to other addresses.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ContactAddressKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CONTACT_ADDRESS_KEY" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The contact address that is associated with this ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CurrentBolNumber"
                                    type="yfctype:Counter" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CURRENT_BOL_NUMBER" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The last generated BOL number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DefaultDeclaredValue"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DEFAULT_DECLARED_VALUE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The default value the ship node may declare for its shipments.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Description"
                                    type="yfctype:Description" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DESCRIPTION" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>the description of the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DistanceFromShipToAddress"
                                    type="yfctype:Number-22" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
				It is returned in <code>DistanceToConsiderUOM</code> specified in input xml. If <code>DistanceToConsiderUOM</code> is blank then <code>DistanceFromShipToAddress</code> is in MILE.
			</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DistanceUOM"
                                    type="yfctype:UOM" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Specifies the UOM for <code>DistanceFromShipToAddress</code>.
				</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ExportLicenseExpDate"
                                    type="yfctype:Date" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXPORT_LICENSE_EXP_DATE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The date by which an export license given to a ship node expires.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ExportLicenseNo"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXPORT_LICENSE_NO" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The export license number given to the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ExportTaxpayerId"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXPORT_TAXPAYER_ID" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The identifier of the tax payer for the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="GLN"
                                    type="yfctype:GlobalLocationNumber" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="GLN" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The global location number defined for a node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="GenerateBolNumber"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="GENERATE_BOL_NUMBER" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the BOL number needs to be generated for this ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="HasEffectiveNodeLevelInventoryControls"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								Having value 'Y' indicates that at least one Node level Inventory control is applicable on the Node.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IdentifiedByParentAs"
                                    type="yfctype:ShipNode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IDENTIFIED_BY_PARENT_AS" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The store or distribution center identifier assigned by the parent organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InterfaceSubType"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INTERFACE_SUB_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>A sub-classification of the interface type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InterfaceType"
                                    type="yfctype:Text-20" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INTERFACE_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This field indicates the manner in which orders are passed to a ship node for servicing ( "WMS_YANTRA","YFX","EVENTS" ).</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InventoryTracked"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVENTORY_TRACKED" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the ship node tracks inventory. Valid values are 'Y' or 'N'. Default Value is 'Y'.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Inventorytype"
                                    type="yfctype:EnumInventoryType" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVENTORYTYPE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This field indicates whether the ship node tracks inventory. The value is defaulted to 'TRACK' if the flag InventoryTracked is set to 'Y' and it is defaulted to 'INFINITE' if flag InventoryTracked is set to 'N'.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsItemBasedAllocationAllowed"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_IBA_ALLOWED" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the node is allowed for Item-Based Allocation process.  Valid values: Y/N.  Default value: Y.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Latitude" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LATITUDE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The latitude of this ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="Localecode"
                                    type="yfctype:Locale" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LOCALECODE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The locale of the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Longitude" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LONGITUDE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The longitude of this ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MaintainInventoryCost"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MAINTAIN_INVENTORY_COST" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the ship node maintains inventory cost.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="NodeOrgCode"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="NODE_ORG_CODE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The organization identifier that this ship node corresponds to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="NodeType"
                                    type="yfctype:NodeType" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="NODE_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The type of the node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OwnerKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OWNER_KEY" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The owner organization that is associated with this ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OwnerType"
                                    type="yfctype:OwnerType" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OWNER_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PicklistType"
                                    type="yfctype:Text-20" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PICKLIST_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The type of pick-list that this ship node requires for picking.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReceiptProcessingTime"
                                    type="yfctype:NumberOfHours" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="RECEIPT_PROCESSING_TIME" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The time in hours required to process an incoming procurement (purchase or transfer) order after receiving it.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReceiptProcessingTimeForForwarding"
                                    type="yfctype:NumberOfHours" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="RCPT_PRCSSNG_TIME_FOR_FRWRDNG" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The time in hours required to process an incoming shipment that is being forwarded to this node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ReceivingNode"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="RECEIVING_NODE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the ship node can perform receiving operations. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RequireTransferAcceptance"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REQUIRE_TRANSFER_ACCEPTANCE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the order line will need to wait for acceptance, if this node is used as procure from node on the line.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReturnCenterFlag"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="RETURN_CENTER_FLAG" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the ship node is a return center.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ReturnsNode"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="RETURNS_NODE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the ship node can perform reverse logistic operations. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SerialTrackedInShipping"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SERIAL_TRCKD_IN_SHIPPING" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether to track Serial information during shipping. Valid values are Y and N.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SerialTrackedOnTransfer"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SERIAL_TRCKD_ON_TRANSFER" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether to track Serial information on Transfer. Valid values are Y and N.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SerialTrackedReceiving"
                                    type="yfctype:ProgramID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SERIAL_TRCKD_RECEIVING" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether to track Serial information during receiving.This receiveing includes return receiving. Valid values are REC(for receiving) and RET(for only returns).</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="SerialTracking"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SERIAL_TRACKING" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the node requires serial tracking in inventory. Supported values are Y and N. This attribute comes into play only when the catalog signifies that the serial number requires tracking in inventory. However, a node has a choice to override the inventory tracking preference to indicate that it is not needed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ShipNode"
                                    type="yfctype:ShipNode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIP_NODE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The unique identifier for the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ShipNodeAddressKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIP_NODE_ADDRESS_KEY" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The address that is associated with this ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ShipNodeClass"
                                    type="yfctype:ShipNodeClass" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIP_NODE_CLASS" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The class of the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ShipnodeKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The primary key for the YFS_SHIP_NODE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ShipnodeType"
                                    type="yfctype:ShipnodeType" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIPNODE_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The type of the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ShippingNode"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIPPING_NODE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the ship node can perform shipping operations. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="StoreInvConfig"
                                    type="yfctype:Text-2" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="STORE_INV_CONFIG" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The type of the Store Inventory Config. Null or '00' means not SIM Enabled Store. '01' means SIM-enabled Store and Inventory IS Store managed. '02' means SIM-enabled Store but Inventory IS NOT Store managed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="SupplierKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SUPPLIER_KEY" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The supplier who owns the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ThreePlNode"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="THREE_PL_NODE" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This flag indicates whether the ship node is a 3PL node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TimeDiff"
                                    type="yfctype:Number" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TIME_DIFF" yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>The time difference between the ship node and the place where the database is hosted from.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="AvailableDate"
                            type="yfctype:Date" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
					This will be returned when <code>Mode="01"</code> or <code>Mode="03"</code> when there in sufficient inventory available, and in <code>Mode="02"</code> when there is only future inventory available at the node.
				</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AvailableQty"
                            type="yfctype:Quantity" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
					This will be returned when <code>Mode="02"</code> and will identify the on hand inventory if no AvailableDate is return, otherwise this will be the future inventory for that date.
				</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IsAvailable"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
					This flag will consistently display if the requested item is available at the provided node or not regardless of the API used to derive the information.
				</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IsFutureAvailability"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
					If the availability is for the future, this will be returned as "Y" else "N" and is currently only supported in <code>Mode="02"</code>.
				</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipNode"
                            type="yfctype:ShipNode" use="optional">
                            <xsd:annotation yfc:columnName="SHIP_NODE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The unique identifier for the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
