                                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Department"
                                    type="yfctype:Department" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EMailID"
                                    type="yfctype:EMailID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ErrorTxt"
                                    type="yfctype:ErrorText" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ERROR_TXT" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EveningFaxNo"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EveningPhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="FirstName"
                                    type="yfctype:PersonName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The first name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HttpUrl"
                                    type="yfctype:URL" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="HTTP_URL" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The URL of the person or organization referred to in this contact information.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsCommercialAddress"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="JobTitle"
                                    type="yfctype:Title-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The job title of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="LastName"
                                    type="yfctype:PersonName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The last name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Latitude" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LATITUDE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The latitude of this location.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="Longitude" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LONGITUDE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The longitude of this location.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="MiddleName"
                                    type="yfctype:MiddleName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="MobilePhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OtherPhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PersonID"
                                    type="yfctype:PersonId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PersonInfoKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PERSON_INFO_KEY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The primary key for the YFS_PERSON_INFO table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PreferredShipAddress"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PREFERRED_SHIP_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The preferred ship address over and above the address in this record. Pointer to another record of this type in this table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="State"
                                    type="yfctype:State" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The state in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Suffix"
                                    type="yfctype:Suffix" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TaxGeoCode"
                                    type="yfctype:GeoCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Title"
                                    type="yfctype:Title-10" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="UseCount"
                                    type="yfctype:Count" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USE_COUNT" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="VerificationStatus"
                                    type="yfctype:ShortStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="VERIFICATION_STATUS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ZipCode"
                                    type="yfctype:ZipCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="ItemShipNodeList">
                                <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="ItemShipNode">
                                    <xsd:annotation yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>This table stores distribution group information for an organization. This information is used during any inventory operation that involves distribution groups.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="CostCurrency"
                                    type="yfctype:Currency" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COST_CURRENCY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Inventorytype"
                                    type="yfctype:Text-20" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVENTORYTYPE" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ItemId"
                                    type="yfctype:ItemID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ITEM_ID" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The unique identifier for the sourced item. "ALL" indicates all items.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ItemType"
                                    type="yfctype:Text-20" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ITEM_TYPE" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The only valid value for this field is currently "ALL", which indicates all items.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ItemshipnodeKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ITEMSHIPNODE_KEY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The primary key of the YFS_ITEM_SHIP_NODE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OwnerKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OWNER_KEY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The owner organization that is associated with this distribution group.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Priority"
                                    type="yfctype:PriorityWithDecimal" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRIORITY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The priority for this distribution.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ShipNodeItem"
                                    type="yfctype:ItemID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIP_NODE_ITEM" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The item as it is identified at the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ShipnodeKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>The ship node associated with a given item ID, or with all items if ITEM_ID is set to "ALL".</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UnitCost"
                                    type="yfctype:UnitPrice" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="UNIT_COST" yfc:tableName="YFS_ITEM_SHIP_NODE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="CanShipToNodeTypes">
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="CanShipToNodeType">
                                    <xsd:annotation yfc:tableName="YFS_NODE_CAN_SHIP_TO_NT">
                                    <xsd:documentation>This table contains node types that ship node can ship to.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="NodeTypeID"
                                    type="yfctype:Identifier" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="NODE_TYPE_ID" yfc:tableName="YFS_NODE_CAN_SHIP_TO_NT">
                                    <xsd:documentation>The unique identifier for the node type that the node can ship to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                        <xsd:attribute name="AcceptanceRequired"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="ACCEPTANCE_REQUIRED" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether acceptance is required by the ship node before processing orders for shipping.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ActivateFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="ACTIVATE_FLAG" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the ship node is active.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AgentCriteriaGroup"
                            type="yfctype:AgentCriteriaGroup" use="optional">
                            <xsd:annotation
                                yfc:columnName="AGENT_CRITERIA_GROUP" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The agent criteria group that is used to classify nodes.  This value can be accepted by WMS agents which will only perform their tasks on the nodes with matching node transactional velocity value. Valid values are: LOW, HIGH and common code values defined by hub.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AllowGiftWrap"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="ALLOW_GIFT_WRAP" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates that the node allows gift wrapping.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CanShipToAllNodes"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="CAN_SHIP_TO_ALL_NODES" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the node can ship to all node/node types.  Entries in YFS_NODE_CAN_SHIP_TO_NT table will be ignored if flag is Y.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CanShipToOtherAddresses"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="CAN_SHIP_TO_OTHER_ADDRESSES" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the node can ship to other addresses.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ContactAddressKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="CONTACT_ADDRESS_KEY" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The contact address that is associated with this ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CurrentBolNumber"
                            type="yfctype:Counter" use="optional">
                            <xsd:annotation
                                yfc:columnName="CURRENT_BOL_NUMBER" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The last generated BOL number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DefaultDeclaredValue"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="DEFAULT_DECLARED_VALUE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The default value the ship node may declare for its shipments.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Description"
                            type="yfctype:Description" use="optional">
                            <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>the description of the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DistanceFromShipToAddress"
                            type="yfctype:Number-22" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
					It is returned in <code>DistanceToConsiderUOM</code> specified in input xml. If <code>DistanceToConsiderUOM</code> is blank then <code>DistanceFromShipToAddress</code> is in MILE.
				</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DistanceUOM"
                            type="yfctype:UOM" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						Specifies the UOM for <code>DistanceFromShipToAddress</code>.
					</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExportLicenseExpDate"
                            type="yfctype:Date" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXPORT_LICENSE_EXP_DATE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The date by which an export license given to a ship node expires.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExportLicenseNo"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXPORT_LICENSE_NO" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The export license number given to the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExportTaxpayerId"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXPORT_TAXPAYER_ID" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The identifier of the tax payer for the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="GLN"
                            type="yfctype:GlobalLocationNumber" use="optional">
                            <xsd:annotation yfc:columnName="GLN" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The global location number defined for a node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="GenerateBolNumber"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="GENERATE_BOL_NUMBER" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the BOL number needs to be generated for this ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="HasEffectiveNodeLevelInventoryControls"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
								Having value 'Y' indicates that at least one Node level Inventory control is applicable on the Node.
							</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IdentifiedByParentAs"
                            type="yfctype:ShipNode" use="optional">
                            <xsd:annotation
                                yfc:columnName="IDENTIFIED_BY_PARENT_AS" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The store or distribution center identifier assigned by the parent organization.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InterfaceSubType"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="INTERFACE_SUB_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>A sub-classification of the interface type.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InterfaceType"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation
                                yfc:columnName="INTERFACE_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This field indicates the manner in which orders are passed to a ship node for servicing ( "WMS_YANTRA","YFX","EVENTS" ).</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InventoryTracked"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="INVENTORY_TRACKED" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the ship node tracks inventory. Valid values are 'Y' or 'N'. Default Value is 'Y'.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Inventorytype"
                            type="yfctype:EnumInventoryType" use="optional">
                            <xsd:annotation
                                yfc:columnName="INVENTORYTYPE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This field indicates whether the ship node tracks inventory. The value is defaulted to 'TRACK' if the flag InventoryTracked is set to 'Y' and it is defaulted to 'INFINITE' if flag InventoryTracked is set to 'N'.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="IsItemBasedAllocationAllowed"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="IS_IBA_ALLOWED" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the node is allowed for Item-Based Allocation process.  Valid values: Y/N.  Default value: Y.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Latitude" use="optional">
                            <xsd:annotation yfc:columnName="LATITUDE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The latitude of this ship node.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="Localecode"
                            type="yfctype:Locale" use="optional">
                            <xsd:annotation yfc:columnName="LOCALECODE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The locale of the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Longitude" use="optional">
                            <xsd:annotation yfc:columnName="LONGITUDE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The longitude of this ship node.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:DecimalWithNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="MaintainInventoryCost"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="MAINTAIN_INVENTORY_COST" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the ship node maintains inventory cost.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NodeOrgCode"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="NODE_ORG_CODE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The organization identifier that this ship node corresponds to.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NodeType"
                            type="yfctype:NodeType" use="optional">
                            <xsd:annotation yfc:columnName="NODE_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The type of the node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OwnerKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="OWNER_KEY" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The owner organization that is associated with this ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OwnerType"
                            type="yfctype:OwnerType" use="optional">
                            <xsd:annotation yfc:columnName="OWNER_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This field is not currently used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PicklistType"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation
                                yfc:columnName="PICKLIST_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The type of pick-list that this ship node requires for picking.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReceiptProcessingTime"
                            type="yfctype:NumberOfHours" use="optional">
                            <xsd:annotation
                                yfc:columnName="RECEIPT_PROCESSING_TIME" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The time in hours required to process an incoming procurement (purchase or transfer) order after receiving it.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="ReceiptProcessingTimeForForwarding"
                            type="yfctype:NumberOfHours" use="optional">
                            <xsd:annotation
                                yfc:columnName="RCPT_PRCSSNG_TIME_FOR_FRWRDNG" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The time in hours required to process an incoming shipment that is being forwarded to this node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReceivingNode"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="RECEIVING_NODE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the ship node can perform receiving operations. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequireTransferAcceptance"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQUIRE_TRANSFER_ACCEPTANCE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the order line will need to wait for acceptance, if this node is used as procure from node on the line.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReturnCenterFlag"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="RETURN_CENTER_FLAG" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the ship node is a return center.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReturnsNode"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="RETURNS_NODE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the ship node can perform reverse logistic operations. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SerialTrackedInShipping"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SERIAL_TRCKD_IN_SHIPPING" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether to track Serial information during shipping. Valid values are Y and N.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SerialTrackedOnTransfer"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SERIAL_TRCKD_ON_TRANSFER" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether to track Serial information on Transfer. Valid values are Y and N.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SerialTrackedReceiving"
                            type="yfctype:ProgramID" use="optional">
                            <xsd:annotation
                                yfc:columnName="SERIAL_TRCKD_RECEIVING" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether to track Serial information during receiving.This receiveing includes return receiving. Valid values are REC(for receiving) and RET(for only returns).</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SerialTracking"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SERIAL_TRACKING" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the node requires serial tracking in inventory. Supported values are Y and N. This attribute comes into play only when the catalog signifies that the serial number requires tracking in inventory. However, a node has a choice to override the inventory tracking preference to indicate that it is not needed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipNode"
                            type="yfctype:ShipNode" use="optional">
                            <xsd:annotation yfc:columnName="SHIP_NODE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The unique identifier for the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipNodeAddressKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIP_NODE_ADDRESS_KEY" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The address that is associated with this ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipNodeClass"
                            type="yfctype:ShipNodeClass" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIP_NODE_CLASS" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The class of the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipnodeKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The primary key for the YFS_SHIP_NODE table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipnodeType"
                            type="yfctype:ShipnodeType" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPNODE_TYPE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The type of the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShippingNode"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPPING_NODE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the ship node can perform shipping operations. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="StoreInvConfig"
                            type="yfctype:Text-2" use="optional">
                            <xsd:annotation
                                yfc:columnName="STORE_INV_CONFIG" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The type of the Store Inventory Config. Null or '00' means not SIM Enabled Store. '01' means SIM-enabled Store and Inventory IS Store managed. '02' means SIM-enabled Store but Inventory IS NOT Store managed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SupplierKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="SUPPLIER_KEY" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The supplier who owns the ship node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ThreePlNode"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="THREE_PL_NODE" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>This flag indicates whether the ship node is a 3PL node.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TimeDiff"
                            type="yfctype:Number" use="optional">
                            <xsd:annotation yfc:columnName="TIME_DIFF" yfc:tableName="YFS_SHIP_NODE">
                                <xsd:documentation>The time difference between the ship node and the place where the database is hosted from.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLines">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderLine"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Item"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AvailabilityList"/>
            </xsd:all>
            <xsd:attribute name="GiftWrap" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_GIFT_WRAP" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates if the order line has to be gift wrapped.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LevelOfService"
                type="yfctype:LevelOfService" use="optional">
                <xsd:annotation yfc:columnName="LEVEL_OF_SERVICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>Indicates the level of service required for this order line.  Default is Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductClass"
                type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product class. This field is used as an inventory identifier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequiredQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
	Required Quantity for the order line.  Is mandatory for <code>Mode="03"</code>, is ignored for <code>Mode="02"</code>, and is used to override the OrderedQty on an OrderLine for <code>Mode="01"</code>.
</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Item">
        <xsd:annotation yfc:tableName="YFS_ITEM">
            <xsd:documentation>The table stores product information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PrimaryInformation"/>
            </xsd:all>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unique identifier for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unit of measure for item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PrimaryInformation">
        <xsd:annotation yfc:tableName="YFS_ITEM">
            <xsd:documentation>The table stores product information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AgeVerificationCode"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="AGE_VERIFICATION_CODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field indicates the age verification code for the item</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AllowGiftWrap" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ALLOW_GIFT_WRAP" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates that the item can be gift wrapped.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AssumeInfiniteInventory"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="ASSUME_INFINITE_INVENTORY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is assumed to be always available. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BundleFulfillmentMode"
                type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="BUNDLE_FULFILLMENT_MODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The way in which the bundle will be fulfilled. It can be Ship Together(01), Ship Independent(00), or Deliver Together(02) for Bundle Item. Default value is Ship Together(01) for Bundle Item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BundlePricingStrategy" use="optional">
                <xsd:annotation yfc:columnName="BUNDLE_PRICE_STRAT" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Determines how a bundle will be priced. A bundle can by priced by PARENT (only the parent's price is used), COMP (only the components' prices are used), or ALL (both parent's and components' prices are used). Default: PARENT.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:BundlePricingStrategy">
                        <xsd:enumeration value="PARENT"/>
                        <xsd:enumeration value="COMP"/>
                        <xsd:enumeration value="ALL"/>
                        <xsd:enumeration value=""/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="CapacityPerOrderedQty"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation
                    yfc:columnName="CAPACITY_PER_ORDERED_QTY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The capacity per ordered quantity.  Used to compute required capacity based on line ordered quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CapacityQuantityStrategy"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation
                    yfc:columnName="CAPACITY_QUANTITY_STRATEGY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field determines how the capacity quantity is arrived at for provided and delivery services from the inventory quantity.   Valid values: ORD - Based on the ordered service quantity; ASSOC - Based on the service and product association.  Default value: ORD.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ColorCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="COLOR_CODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field describes color code for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ComputedUnitCost"
                type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="COMPUTED_UNIT_COST" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The computed standard cost of the item. This will be set when createItem/ modifyItem API is called specifically with computeUnitCost flag as Y and item unit cost is calculated based on REPLACEMENT COST.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConfigurationKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CONFIGURATION_KEY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>A reference to the BOM XML if the item is pre-configured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConfigurationModelName"
                type="yfctype:Text-254" use="optional">
                <xsd:annotation yfc:columnName="CONFIG_MODEL_NAME" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The configuration model name for this item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConfiguredModelKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CONFIGURED_MODEL_KEY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The key of the configured model for a configurable item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CostCurrency" type="yfctype:Currency" use="optional">
                <xsd:annotation yfc:columnName="COST_CURRENCY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The currency in which the item's cost is defined.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CountryOfOrigin" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY_OF_ORIGIN" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The item's country of origin/manufacture.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreditWOReceipt" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="CREDIT_WO_RECEIPT" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates if credit may be provided with out receipts on return of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DefaultProductClass"
                type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="DEFAULT_PRODUCT_CLASS" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The default product class of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Description" type="yfctype:Text-500" use="optional">
                <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplayItemDescription"
                type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>User defined description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EffectiveEndDate" use="optional">
                <xsd:annotation yfc:columnName="EFFECTIVE_END_DATE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Defines the end time when the item is valid.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Date"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="EffectiveStartDate" use="optional">
                <xsd:annotation yfc:columnName="EFFECTIVE_START_DATE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Defines the start time when the item is valid.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Date"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ExtendedDescription"
                type="yfctype:Text-2000" use="optional">
                <xsd:annotation yfc:columnName="EXTENDED_DESCRIPTION" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The extended multi-line description of the Item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExtendedDisplayDescription"
                type="yfctype:Text-500" use="optional">
                <xsd:annotation
                    yfc:columnName="EXTENDED_DISPLAY_DESCRIPTION" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Display description for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FixedCapacityQtyPerLine"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation
                    yfc:columnName="FIXED_CAPACITY_QTY_PER_LINE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The fixed capacity quantity will be assumed irrespective of the inventory quantity for the line shipped with this item when the strategy is fixed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FixedPricingQtyPerLine"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation
                    yfc:columnName="FIXED_PRICING_QTY_PER_LINE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The fixed pricing quanity per line. Fixed pricing quantity will be assumed irrespective of the inventory quantity for the line shipped with this item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ImageID" type="yfctype:ImageID" use="optional">
                <xsd:annotation yfc:columnName="IMAGE_ID" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Unique ID of the image in the external location.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ImageLabel" type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="IMAGE_LABEL" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Label of the image.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ImageLocation" type="yfctype:URL" use="optional">
                <xsd:annotation yfc:columnName="IMAGE_LOCATION" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>External location of the image.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InvoiceBasedOnActuals"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="INVOICE_BASED_ON_ACTUALS" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether invoicing is done based on actual weight to support catch weight items.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InvolvesSegmentChange"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="INVOLVES_SEGMENT_CHANGE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item performs segment change.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsAirShippingAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_AIR_SHIPPING_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates if item can be shipped by air. Default value: Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsConfigurable" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_CONFIGURABLE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates that the item is a configurable item. A configurable item can either be a bundle or a physical kit. A configurable bundle item offers a number of different options from which a customer can select before buying the product. Components for a bundle are not specified in the catalog as the actual components will be determined by the user when an order is placed. A physical kit must always be pre-configured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsDeliveryAllowed" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_DELIVERY_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the delivery method DEL is allowed for item. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsEligibleForShippingDiscount" use="optional">
                <xsd:annotation
                    yfc:columnName="IS_ELIGIBLE_FOR_SHIP_DISC" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Determines whether the item is eligible for shipping discounts. Default Value: Y.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value=""/>
                        <xsd:enumeration value="N"/>
                        <xsd:enumeration value="Y"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsForwardingAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FORWARDING_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates if the item can be forwarded for zone skipping. Default value: Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsFreezerRequired" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FREEZER_REQUIRED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates if item requires freezer for transportation. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsHazmat" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_HAZMAT" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates if item is hazardous material.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsModelItem" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_MODEL_ITEM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates that the item is an item with variations (formerly known as a model item). An item with variations represents a group of items with similar set of attributes.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsParcelShippingAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="IS_PARCEL_SHIPPING_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the item can be shipped through parcel service. Default value: Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPickupAllowed" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PICKUP_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the delivery method PICK is allowed for item. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPreConfigured" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PRE_CONFIGURED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates that the configurable item is pre-configured with selected options. If a bundle is pre-configured, the bundle or physical components that represent the selected option will be available in the catalog. For a pre-configured physical kit, only the physical kit will be available as the item in the catalog.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsProcurementAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PROCUREMENT_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates if the item can be procured for zone skipping. Default value: Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsReturnService" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_RETURN_SERVICE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether this provided or delivery service item is to be associated only to a return document.  Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsReturnable" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_RETURNABLE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether this product item is returnable. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsShippingAllowed" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_SHIPPING_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the delivery method SHP is allowed for item. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsSoldSeparately" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_SOLD_SEPARATELY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates if the item can be sold separately. If N, then the item must be part of a bundle or kit. Valid Values: 'Y' or 'N'.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsStandaloneService"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_STANDALONE_SERVICE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether a PS Item is standalone and does not require associations with product lines for it to be included on an order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsSubOnOrderAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_SUB_ON_ORDER_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates if during scheduling an order, substitution is allowed or not. If Y, schedule will automatically attempt to substitute an item with preferred or regular substitute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsValid" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>This flag indicates whether the item is valid.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ITEM_TYPE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The generic type of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Keywords" type="yfctype:Text-1000" use="optional">
                <xsd:annotation yfc:columnName="KEYWORDS" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>A field used to specify keywords to be associated with the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitCode" type="yfctype:EnumKitCode" use="optional">
                <xsd:annotation yfc:columnName="KIT_CODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field indicates the kit code for this item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItem"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The identifier used by a manufacturer to identify an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItemDesc"
                type="yfctype:Text-255" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM_DESC" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The description of an item, from a manufacturer's view point.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerName"
                type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_NAME" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The manufacturer of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MasterCatalogID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="MASTER_CATALOG_ID" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The master catalog to which an item belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MaxOrderQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="MAX_ORDER_QUANTITY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The maximum order quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MaxReturnWindow" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The number of days after which the return will not be accepted. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MinOrderQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="MIN_ORDER_QUANTITY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The minimum order quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MinReturnWindow" use="optional">
                <xsd:annotation yfc:columnName="MIN_RETURN_WINDOW" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The minimum number of days before which return may be accepted without any override. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MinimumCapacityQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation
                    yfc:columnName="MINIMUM_CAPACITY_QUANTITY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This is the minimum capacity quantity which will be assumed when  the inventory quantity of the line to arrive at the capacity quantity when the strategy is to arrive at the capacity quantity by association with product line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ModelItemUnitOfMeasure"
                type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="MODEL_ITEM_UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field describes the unit of measure of the item with variations.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NumSecondarySerials"
                type="yfctype:Digit" use="optional">
                <xsd:annotation yfc:columnName="NUM_SECONDARY_SERIALS" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The number of secondary serial numbers applicable for a serialized item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderingQuantityStrategy"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation
                    yfc:columnName="ORDERING_QUANTITY_STRATEGY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field determines how the ordered quantity is arrived at for a service line.  Valid values: ENT - Based on the entered value on the order line; PROD - Based on the associated product line.  Default value: ENT.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingQuantityConvFactor"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation
                    yfc:columnName="PRICING_QUANTITY_CONV_FACTOR" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The pricing quantity conversion factor. This conversion factor is multiplied with the inventory quantity of the line to arrive at this pricing quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingQuantityStrategy"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation
                    yfc:columnName="PRICING_QUANTITY_STRATEGY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field determines how the pricing quantity is arrived at when different from the inventory quantity.   Valid values: IQTY - Based on the inventory quantity.  Default value: IQTY.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="PRICING_UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The pricing unit of measure, if different from the inventory unit of measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingUOMStrategy"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PRICING_UOM_STRATEGY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field determines how the pricing unit of measure is defaulted for the item. It could be same as the inventory UOM or could be different.  Valid values: INV - Same as the inventory UOM; ALT - Different from the inventory UOM.  Default value: INV.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimaryEnterpriseCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="PRIMARY_ENTERPRISE_CODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The primary enterprise associated with an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimarySupplier" type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="PRIMARY_SUPPLIER" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The primary supplier. This will have the primary supplier organization code for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductLine" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_LINE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The product line associated with the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequiresProdAssociation"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="REQUIRES_PROD_ASSOCIATION" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>IThis flag indicates whether the item must be associated to an order line having a group code of PROD. Applicable for service items which must be associated to a Product order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReturnWindow" use="optional">
                <xsd:annotation yfc:columnName="RETURN_WINDOW" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The number of days after which return may not be accepted. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="RunQuantity" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="RUN_QUANTITY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The run quantity indicates the multiple of quantity in which a work order gets created. This field is valid only for service items.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SerializedFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SERIALIZED_FLAG" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is serialized.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceTypeID" type="yfctype:Text-24" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_TYPE_ID" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The service type associated to this service item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentConsolidationWindow" use="optional">
                <xsd:annotation yfc:columnName="SHP_CONSOL_WINDOW" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates the number of days an item can be delayed after it becomes available to be consolidated to a single shipment. Used by scheduling when shipment/cost optimization is considered.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ShortDescription"
                type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="SHORT_DESCRIPTION" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The short description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SizeCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SIZE_CODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field describes the size code of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Status" type="yfctype:EnumShortStatus" use="optional">
                <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field indicates the status of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is taxable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitCost" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_COST" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The cost of one unit of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitHeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="UNIT_HEIGHT" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The height of one unit of item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitHeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UNIT_HEIGHT_UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unit of measure in which item height is measured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitLength" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="UNIT_LENGTH" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The length of one unit of item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitLengthUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UNIT_LENGTH_UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unit of measure in which an item length is measured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitVolume" type="yfctype:Dimension" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Calculated volume of one unit of item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitVolumeUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Unit of measure in which the item volume is measured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitWeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="UNIT_WEIGHT" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The weight of one unit of item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitWeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UNIT_WEIGHT_UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unit of measure in which the item weight is measured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitWidth" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="UNIT_WIDTH" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The width of one unit of item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitWidthUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UNIT_WIDTH_UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unit of measure in which an item's width is measured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AvailabilityList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="Availability">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="Node">
                                <xsd:annotation yfc:tableName="YFS_SHIP_NODE">
                                    <xsd:documentation>This table stores receiving node, ship node, and distribution centers in the system for various organizations.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="OwnerOrganization">
                                    <xsd:annotation yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>This table stores attributes that are common to all oganizations. Organizations may play different roles in trade. For example,  Buyer, Seller, Enterprise, ShipNode.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="AccountWithHub"
                                    type="yfctype:ID-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACCOUNT_WITH_HUB" yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>The account with the hub.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AuthorityType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AUTHORITY_TYPE" yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>The nature of the authority that issued the tax exemption to the organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="BillingAddressKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="BILLING_ADDRESS_KEY" yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>The billing address that is associated with this organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CapacityOrganizationCode"
                                    type="yfctype:OrgCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CAPACITY_ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>The capacity organization for this organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CatalogOrganizationCode"
                                    type="yfctype:OrgCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CATALOG_ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>The organization that owns the catalog of this organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ContactAddressKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CONTACT_ADDRESS_KEY" yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>The contact address that is associated with this organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CorporateAddressKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CORPORATE_ADDRESS_KEY" yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>The corporate address that is associated with this organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CreatorOrganizationKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CREATOR_ORGANIZATION_KEY" yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>The organization that created this organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DunsNumber"
                                    type="yfctype:DunsNumber" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DUNS_NUMBER" yfc:tableName="YFS_ORGANIZATION">
                                    <xsd:documentation>The DUNS number of the organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InheritOrgConfig"
