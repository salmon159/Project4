<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/OMP/modifyChargeTransactionRequestMappingUE/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/OMP/modifyChargeTransactionRequestMappingUE/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation
        yfc:fileName="OMP_modifyChargeTransactionRequestMappingUE_input.xsd" yfc:rootElement="Order">
        <xsd:appinfo source="yfc:structure">
            <Order hasAttributes="Y">
                <SuggestedMapping>
                    <InvoiceList hasAttributes="Y">
                        <OrderInvoice hasAttributes="Y">
                            <OrderReleaseList>
                                <OrderRelease hasAttributes="Y"/>
                            </OrderReleaseList>
                            <ChargeTransactionRequest hasAttributes="Y"/>
                            <ChargeTransactionList>
                                <ChargeTransaction hasAttributes="Y">
                                    <PaymentMethod hasAttributes="Y"/>
                                </ChargeTransaction>
                            </ChargeTransactionList>
                        </OrderInvoice>
                    </InvoiceList>
                    <PresettledAmount hasAttributes="Y">
                        <ChargeTransactionRequestList>
                            <ChargeTransactionRequest hasAttributes="Y"/>
                        </ChargeTransactionRequestList>
                        <ChargeTransactionList>
                            <ChargeTransaction hasAttributes="Y">
                                <PaymentMethod hasAttributes="Y"/>
                            </ChargeTransaction>
                        </ChargeTransactionList>
                    </PresettledAmount>
                    <NegativeInvoiceList>
                        <OrderInvoice hasAttributes="Y">
                            <ChargeTransactionList>
                                <ChargeTransaction hasAttributes="Y">
                                    <PaymentMethod hasAttributes="Y"/>
                                </ChargeTransaction>
                            </ChargeTransactionList>
                        </OrderInvoice>
                    </NegativeInvoiceList>
                    <ChargeTransactionRequestList>
                        <ChargeTransactionRequest hasAttributes="Y">
                            <ChargeTransactionList>
                                <ChargeTransaction hasAttributes="Y">
                                    <PaymentMethod hasAttributes="Y"/>
                                </ChargeTransaction>
                            </ChargeTransactionList>
                        </ChargeTransactionRequest>
                    </ChargeTransactionRequestList>
                    <OrderRemainder hasAttributes="Y">
                        <ChargeTransactionList>
                            <ChargeTransaction hasAttributes="Y">
                                <PaymentMethod hasAttributes="Y"/>
                            </ChargeTransaction>
                        </ChargeTransactionList>
                    </OrderRemainder>
                </SuggestedMapping>
                <MappableObjects>
                    <InvoiceList>
                        <OrderInvoice hasAttributes="Y">
                            <OrderReleaseList>
                                <OrderRelease hasAttributes="Y"/>
                            </OrderReleaseList>
                        </OrderInvoice>
                    </InvoiceList>
                    <NegativeInvoiceList>
                        <OrderInvoice hasAttributes="Y"/>
                    </NegativeInvoiceList>
                    <PresettledAmount hasAttributes="Y"/>
                    <ChargeTransactionRequestList>
                        <ChargeTransactionRequest hasAttributes="Y"/>
                    </ChargeTransactionRequestList>
                    <OrderRemainder hasAttributes="Y"/>
                    <ChargeList>
                        <Charge hasAttributes="Y"/>
                    </ChargeList>
                    <AuthorizationList>
                        <Authorization hasAttributes="Y"/>
                    </AuthorizationList>
                    <PaymentMethodSequenceList>
                        <PaymentMethodSequence hasAttributes="Y">
                            <PaymentMethods>
                                <PaymentMethod hasAttributes="Y"/>
                            </PaymentMethods>
                        </PaymentMethodSequence>
                    </PaymentMethodSequenceList>
                </MappableObjects>
            </Order>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="Order" type="yfc:OrderXSDType">
        <xsd:annotation yfc:tableName="YFS_ORDER_HEADER">
            <xsd:documentation>This table stores all order documents in the system.  These orders can be further classified into various types of documents. For example, ORDERS, PLANNED ORDERS and any other future types that the system may provide.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="OrderXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:SuggestedMapping"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:MappableObjects"/>
        </xsd:all>
        <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The document type that is associated with this order. For example, Sales Order document (0001), Planned Order document (0002), Return Order document (0003) , and Purchase Order document (0005). This field determines the order's fulfillment cycle and controls the inventory and payment updates performed on the order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ENTERPRISE_KEY" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The enterprise that is associated with this order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The primary key for the YFS_ORDER_HEADER table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="ORDER_NO" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The number of the order, which can be passed during order creation or generated based on a system rule setting.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="SuggestedMapping">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="InvoiceList">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="1" name="OrderInvoice">
                                <xsd:annotation yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Stores order invoices raised against an order.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="OrderReleaseList">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							This list will be empty if the invoice is not linked to a shipment.
			        </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="OrderRelease">
                                    <xsd:annotation yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This table stores releases made against an order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="BillToID"
                                    type="yfctype:ID-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BILL_TO_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The identifier of the customer that this order is being billed to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BuyerMarkForNodeId"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BUYER_MARK_FOR_NODE_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Mark For Node as identified by the buyer for eg: it could be a person,store,DC.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BuyerOrganizationCode"
                                    type="yfctype:OrgCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BUYER_ORGANIZATION_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The code of the buyer organization. This must be a valid organization defined in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BuyerReceivingNodeId"
                                    type="yfctype:ShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BUYER_RECEIVING_NODE_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Receiving node as identified by the buyer for eg:Store,DC Number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CarrierAccountNo"
                                    type="yfctype:AccountNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTCARRIER_ACCOUNT_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The account number of the customer with the carrier.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CarrierServiceCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The carrier service code.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Createts"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREATETS" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Timestamp when the record was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Currency"
                                    type="yfctype:Currency" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CURRENCY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The currency in which the buyer will pay the seller.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustReqDeliveryDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUST_REQ_DELIVERY_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Customer Requested Delivery date. This date is the minimum value of the RequestedDeliveryDate on all the order lines which are a part of this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustReqShipDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUST_REQ_SHIP_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Customer Requested Ship date. This date is the minimum value of the RequestedShipDate on all the order lines which are a part of this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerPoNo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The PO number of the customer. For purchase orders, this can be used for storing the purchase order number of the purchasing system if that number is not already stored as part of order number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DeliveryCode"
                                    type="yfctype:DeliveryCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DELIVERY_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field indicates who will pay the freight. Application understands values such as SUPPLIER, ENTERPRISE, and MARKETPLACE to mean that each of these organizations will pay for the freight. You could however pass any other value which is a valid shipment charge type as understood by the carrier by which the shipment is transported. See YFS_SHIPMENT_CONTAINER.carrier_payment_type for additional information.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DeliveryMethod"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DELIVERY_METHOD" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The delivery method for the product line.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Division"
                                    type="yfctype:Division" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DIVISION" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The division in the buyer organization that placed the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DocumentType"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The type of order document used for this order release. For example,Sales Order document (0001), Planned Order document (0002), Return Order document (0003) , and Purchase Order document (0005). This field determines the order's fulfillment cycle and controls the inventory and payment updates performed on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="EnterpriseCode"
                                    type="yfctype:OrgCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ENTERPRISE_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The enterprise that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FOB"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FOB" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Indicates Freight Terms for the release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="GiftFlag"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="GIFT_FLAG" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether the order release has gift items.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="HoldFlag"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="HOLD_FLAG" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="HoldReasonCode"
                                    type="yfctype:HoldReasonCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="HOLD_REASON_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="LevelOfService"
                                    type="yfctype:LevelOfService" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="LEVEL_OF_SERVICE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Indicates the level of service required for this release.  Default is Null.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MarkForKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MARK_FOR_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The mark for address that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxOrderReleaseStatus"
                                    type="yfctype:Status" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Maximum order status amongst all records that belong to an order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxOrderReleaseStatusDesc"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Description of maximum order status amongst all records that belong to an order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MergeNode"
                                    type="yfctype:ShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MERGE_NODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The node that is used as either forwarding or merge location.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MinOrderReleaseStatus"
                                    type="yfctype:Status" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Minimum order status amongst all records that belong to an order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MinOrderReleaseStatusDesc"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Description of minimum order status amongst all records that belong to an order release</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Modifyts"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MODIFYTS" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Timestamp when the record was last modified.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MultipleStatusesExist"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>If order release is in more than one status then this flag will be returned as "Y".</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="NotificationReference"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="NOTIFICATION_REFERENCE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The data related to the notification type. If the notification type is E-mail, then the notification reference should hold the E-mail ID to which the notification mail must be sent.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="NotificationType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="NOTIFICATION_TYPE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field can be used to indicate a means in which the buyer may be notified when the order reaches various statuses, or in situations where the buyer input is required to resolve a business exception. For example, notification type could have values like E-mail, FAX, etc.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="NotifyAfterShipmentFlag"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="NOTIFY_AFTER_SHIPMENT_FLAG" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether the buyer must be notified after the order is shipped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The date on which the order was placed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The order header that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderName"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_NAME" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The name given to the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderReleaseKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_RELEASE_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The primary key for the YFS_ORDER_RELEASE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_TYPE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>An order classification attribute. This field can be used for reporting purposes or to build conditions while modeling your business process. Sterling Selling And fulfillment Suite system has no default logic based on this field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OtherCharges"
                                    type="yfctype:ComputedMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="OTHER_CHARGES" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The sum of the other charges at the release level.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PacklistType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PACKLIST_TYPE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The code indicating the type of packlist to be used for the line. Lines requiring different pack lists are not mixed in a single order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PersonalizeCode"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PERSONALIZE_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The product personalization preference code.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PickListNo"
                                    type="yfctype:DocumentNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PICK_LIST_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The pick list number generated, if using the Sterling Selling and Fulfillment Suite Console to pick, pack, and ship.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PriorityCode"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRIORITY_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The priority code of the order. External logic can be built using this field for prioritizing orders.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Purpose"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PURPOSE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is passed out to the Tax calculation user exits.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReceivingNode"
                                    type="yfctype:ShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="RECEIVING_NODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The node that should receive the shipment.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReleaseNo"
                                    type="yfctype:ShorterNumber" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="RELEASE_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The release number. Generated as a one up number against that release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReleaseSeqNo"
                                    type="yfctype:ShorterNumber" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="RELEASE_SEQ_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The release sequence number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReqCancelDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQ_CANCEL_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The requested cancel date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReqDeliveryDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQ_DELIVERY_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The requested delivery date.  It indicates the expected delivery date for the release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReqShipDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQ_SHIP_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The requested ship date.  It indicates the expected shipment date for the release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SCAC"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SCAC" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The carrier code.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SalesOrderNo"
                                    type="yfctype:VarcharDocumentNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SALES_ORDER_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The sales order that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ScacAndService"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Value of SCAC and Service code.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ScacAndServiceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Link to YFS_SCAC_AND_SERVICE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SellerOrganizationCode"
                                    type="yfctype:OrgCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SELLER_ORGANIZATION_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The code of the seller organization. This must be a valid organization defined in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipAdviceNo"
                                    type="yfctype:Text-50" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_ADVICE_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The ship advice number that uniquely identify the order release.  If the USE_NUMERIC_SHIP_ADVICE_NO rule is set to Y, the value will be a number.  Otherwise, the value will be the concatenated value of OrderNo and ReleaseNo.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipCompleteFlag"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_COMPLETE_FLAG" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether the order must be shipped only if it can be completely fulfilled.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipLineComplete"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_LINE_COMPLETE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether this order should be shipped only if complete lines can be fulfilled.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipNode"
                                    type="yfctype:ShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The ship node that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipNodeClass"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_NODE_CLASS" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipOrderComplete"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_ORDER_COMPLETE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether the order should be shipped only if it can be completely fulfilled.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipToId"
                                    type="yfctype:ID-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_TO_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The identifier of the customer that this order is being shipped to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipToKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_TO_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The ship to address that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipmentConsolidationGroupId"
                                    type="yfctype:VarcharShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIPMENT_CONSOL_GROUP_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>It is a consolidation group id  that is used as a standard do not mix constraint.Releases belonging to same shipment consolidation group may consolidate to same shipment.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Status"
                                    type="yfctype:Status" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Status of the order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SupplierCode"
                                    type="yfctype:NText-30" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUPPLIER_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SupplierName"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUPPLIER_NAME" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The name of the supplier who owns the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TaxpayerId"
                                    type="yfctype:ID-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TAXPAYER_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The identifier of the taxpayer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="WorkOrderApptKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="WORK_ORDER_APPT_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Link to the appointment on the associated work order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="WorkOrderKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="WORK_ORDER_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Link to the associated Work Order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="isHistory"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Indicates if data is fetched from the history table rather than the regular table. Valid values are Y, N or Void. Void implies N.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="ChargeTransactionRequest">
                                    <xsd:annotation yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>This table holds the ORDER_HEADER_KEY and the request id which make a set of unique groups for payment processing on the order.  Authorizations will be split such that each authorization is for only one request.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestId"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRAN_REQUEST_ID" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The unique identifier of the request in combination with the ORDER_HEADER_KEY.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRAN_REQUEST_KEY" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The primary key for the YFS_CHARGE_TRAN_REQUEST table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestSequence"
                                    type="yfctype:PossibleKey" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REQUEST_SEQUENCE" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The order in which the Groups should be authorized.  Records with REQUEST_SEQUENCE NOT NULL will be processed first; NULL Sequences will be ordered by CHARGE_TRAN_REQUEST_ID, ascending.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MaxRequestAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MAX_REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The maximum amount this unique identifier represents before distribution to another group begins to become Authorized.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The OrderHeaderKey that is associated with this charge tran request.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PaymentStatus"
                                    type="yfctype:AuthorizationPaymentStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_STATUS" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The Payment Status of the request.  This is computed in the same manner as the Order's Payment Status.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="ChargeTransactionList">
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="ChargeTransaction">
                                    <xsd:annotation yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This table stores information about financial transactions associated with order processing. Every action carried out on an order which may have an impact on financial components is logged in this table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:sequence>

                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="PaymentMethod">

                                    <xsd:annotation yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This table stores details about the customer's payment methods. The key to this table is posted in YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="AwaitingAuthInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total authorization amount that has not been
								sent to the payment processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AwaitingChargeInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total charge amount that has not been sent to
								the payment
								processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BillToKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BILL_TO_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The bill to address that is associated with this payment. Can be passed if different from the bill to address on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that is given back as cash for this payment method. This is not supported on a return order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeSequence"
                                    type="yfctype:ShorterNumber" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_SEQUENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The sequence in which the payment method is to be used for charging.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckNo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckReference"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_REFERENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque reference.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardExpDate"
                                    type="yfctype:CreditCardExpDate" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_EXP_DATE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The expiration date for the credit card.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card name.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CREDIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardType"
                                    type="yfctype:Text-32" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card type group.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerAccountNo"
                                    type="yfctype:AccountNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_ACCOUNT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CUSTOMER_ACCOUNT; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerPONo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The customer PO account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DebitCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup DEBIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCreditCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the credit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the credit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCustomerAccountNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CUST_ACCT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the customer account number that should be displayed.  If not passed, it is defaulted to be the last four digits of the customer account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayDebitCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the debit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the debit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPaymentReference1"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_PAYMENT_REF1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the payment reference 1 that should be displayed.  If not passed, it is defaulted to be the last four digits of the payment reference 1.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPrimaryAccountNo"
                                    type="yfctype:Text-4" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate display value of the primary
								identifier of this PaymentType, depending upon the
								PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplaySvcNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the stored value card that should be displayed.  If not passed, it is defaulted to be the last four digits of the stored value card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FirstName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The first name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FundsAvailable"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The funds available to be charged on this payment method as
									determined by the
									<code>YFSGetFundsAvailableUE</code>
									user exit, if it was invoked. If the order is created for a
									customer and the payment method is a valid customer payment
									method, it indicates the amount available for the customer
									account. If customer account is not maintained internally or if
									the MaxChargeLimit is not set on the customer payment method,
									this value is returned as 0.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="GetFundsAvailableUserExitInvoked"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									Indicates if the
									<code>YFSGetFundsAvailableUE</code>
									user exit was invoked for this payment method.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="IncompletePaymentType"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="INCOMPLETE_PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether the payment type is incomplete.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="LastName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="LAST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The last name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxChargeLimit"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MAX_CHARGE_LIMIT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The maximum amount that can be charged on this payment method. However, if UnlimitedCharges=Y, this amount will not be considered.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MiddleName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The middle name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary key for the YFS_PAYMENT table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference1"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 1. Additionally, this is the primary account number for payment methods that are defined under the PaymentTypeGroup OTHER; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference2"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE2" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 2.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference3"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE3" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 3.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference4"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE4" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 4. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference5"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE5" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 5. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference6"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE6" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 6. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference7"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE7" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 7. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference8"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE8" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 8. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference9"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE9" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 9. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTemplateType"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TEMPLATE_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This field indicates User Interfaces to use the template required to capture Payment Methods of this Payment Type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment type of this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTypeGroup"
                                    type="yfctype:CodeValue" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE_GROUP" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This filed indicates the payment type group, which will determine behavior.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PlannedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PLANNED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that will be refunded on this payment method. This field is set for only Return Orders indicating the refund amount.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PrimaryAccountNo"
                                    type="yfctype:Text-1000" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate primary identifier of this
								PaymentType, depending upon the PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Reason code for last update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedAuthAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_AUTH_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of authorizations sent to an
								external
								system and still
								awaiting response on this payment
								method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedChargeAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_CHARGE_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of charges sent to an external
								system and still
								awaiting response on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of refunds sent to an external
								system and still
								awaiting response on this payment method. This
								number will be
								positive. This total will no longer be included in
								the
								RequestedChargeAmount, but if an asynchronous refund is
								submitted
								in excess of this amount, the RequestedChargeAmount
								will
								be
								reduced for backwards compatibility.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendAnyMoreCharges"
                                    type="yfctype:Text-1" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_ANY_MORE_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Sets whether or not this payment method has been suspended. If the value is Y, then the PaymentMethod is suspended for charging, although refunds can still be made to this PaymentMethod. All open AUTHORIZATION and CHARGE records for positive amounts for this payment method are closed.  If the value is B then both charge and refund are suspended and all open AUTHORIZATION or CHARGE records are closed.  Default: N.  Valid Values: Y, B, or N.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendExpiredAuthReversal"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_EXP_AUTH_REVERSAL" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether or not the reversal of expired authorization has to be initiated for a payment method that has been suspended. If the value is set to Y, then the authorization reversal are not created even though Authorization Reversal Strategy is configured for the payment type. Please note the flag comes into action only when SuspendAnyMoreCharges is either set to Y or B. Valid Values are N or Y (Null value behaves similar to setting it to N ).</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SvcNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup STORED_VALUE_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAltRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_ALT_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that has been refunded to a new payment method based on this payment's payment type refund preferences.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAuthorized"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_AUTHORIZED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount auhorized on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalCharged"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_CHARGED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount charged on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total refunded amount on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UnlimitedCharges"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="UNLIMITED_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Indicates if there is a limit to the amount that can be charged on the payment method. If not passed, defaults to Y.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>

                                    <xsd:attribute
                                    name="AsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For Asynchronous requests only, this field holds the identifier to the transaction closed as asynchronous. This identifier will be used for recording the actual response back in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AuditTransactionID"
                                    type="yfctype:TransactionID" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AuthorizationExpirationDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_EXPIRATION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time until which the authorization specified by the authorization ID is applicable.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AuthorizationID"
                                    type="yfctype:Text-255" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For authorization requests, this field holds the identifier of the authorization that was made. For charge requests, this field holds the authroization ID to be used for charging the amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BookAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BOOK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that could be charged to the buyer if the order was fulfilled. At the time of fulfillment, a negative entry is made in this column and positive entry in the DEBIT_AMOUNT column.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CallForAuthorizationStatus"
                                    type="yfctype:CallForAuthStatus" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CALL_FOR_AUTH_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This column indicates the transaction is valid for a call for authorization and whether or not the authorization code was acquired.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount of cashback that has been charged using this transaction. Only valid for transactions of ChargeType CHARGE.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeTransactionKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRANSACTION_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The primary key for the YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeType"
                                    type="yfctype:ChargeCategory" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The type of charge that is being applied to the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CollectionDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="COLLECTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge was realized.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that was actually charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DebitAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DEBIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that should be charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DistributedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISTRIBUTED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field is used internally by the application to keep track of the amount which has been posted in the invoices as "collected".</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ExecutionDate" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="EXECUTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge request was processed.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:TimeStamp"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ForAsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FOR_ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Reference to AsyncRequestIdentifier, the actual transaction that was closed as async for which the response is now recorded in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="HoldAgainstBook"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="HOLD_AGAINST_BOOK" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates whether the request was created for a precharge or a payment method whose payment type has the ChargeInsteadOfAuth flag turned on.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="InPerson" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="IN_PERSON" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the transaction represents an in person, or card in hand, transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MappedAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount that is mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MappingFirm"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Indicates if it is firmly mapped charge transaction. The firmly mapped charge transaction should not be altered/missing in the UE output.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OfflineStatus" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="OFFLINE_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates the transaction attempted to reach the payment gateway within a UserExit, but the UserExit was unable to contact the gateway.  This may be updated to N by the user exit, and will be reset to null when the record moves to CLOSED status.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OpenAuthorizedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="OPEN_AUTHORIZED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is authorized for the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The order header that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderInvoiceKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The key to the invoice that was created for this record, if applicable. Invoices are created for shipments, returns, or debit and credit memos.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentEntryType"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_ENTRY_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Entry Type used to enter payment information into the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The payment method that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PostponedAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="POSTPONED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The total amount that does not require authorization or payment capture on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field can be used to store reason code for transaction update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is requested for the charge or authorization of record of types "REQUEST_CHARGE" and "REQUEST_AUTHORIZATION". Once the request is processed, the actual amount will be shown as "CreditAmount" or "OpenAuthorizedAmount", for charge and authorization requests respectively.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SettledAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SETTLED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field holds the amount for which a pre-settlement (settlement before invoice) is requested. Charge requests will be created for this amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Status"
                                    type="yfctype:EnumStatus2" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field indicates the status of this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TransactionDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MODIFYTS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Transaction Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TransferFromOhKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_FROM_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred from.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TransferToOhKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_TO_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UnusuableAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The unusable amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UserExitStatus"
                                    type="yfctype:Text-15" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="USER_EXIT_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The user exit call status of this record. The status will be "INVOKED" if the transaction fails during or after the user exit call, otherwise the value will be blank.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="VoidTransaction" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="VOID_TRANSACTION" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the Charge Transaction is either a Void request or submitted for a Void operation.  Valid values for a Void Charge Transaction are V (is a void request), R (is a forced refund request), C (closed error record, already recreated) and null. Valid value for a Charge Transaction that has been requested for Void operation is S (void submitted for this transaction).</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="V"/>

                                    <xsd:enumeration value="R"/>

                                    <xsd:enumeration value="S"/>

                                    <xsd:enumeration value="C"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute
                                    name="ActualFreightCharge"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACTUAL_FREIGHT_CHARGE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Actual freight charge or the charge for transporting the shipment from the origin  node to the destination node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AmountCollected"
                                    type="yfctype:ComputedMoney" use="required">
                                    <xsd:annotation
                                    yfc:columnName="AMOUNT_COLLECTED" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total amount which has been paid against this invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="BlindCreationAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>BlindCreationAmount is the amount that settlement will be attempted for without authorization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargedActualFreight"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGED_ACTUAL_FREIGHT" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>If shipping charges were charged on actual, this is set to 'Y'.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectedExternally"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COLLECTED_EXTERNALLY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Flag to indicate if invoice collection is happening externally in A/R system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectedThroughAR"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AMOUNT_PASSED_TO_AR" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Amount which was passed to A/R system for collection. This would typically be same as total amount unless there was some pre-payment on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DateInvoiced"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CREATETS" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Timestamp when the record was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DerivedFromOrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DERIVED_FROM_ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Link to order header from which this Invoice's Order is derived.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DocumentType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The document type. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EnterpriseCode"
                                    type="yfctype:OrgCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The enterprise code. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderCharges"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Header Charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderDiscount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Header discount amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TAX" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Tax amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InvoiceCreationReason"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_CREATION_REASON" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The reason for which the invoice is created. The system populates this field only for adjustment invoices.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InvoiceNo"
                                    type="yfctype:InvoiceNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Invoice number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InvoiceType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_TYPE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Classifies an invoice. For example, Orders, Returns, Debit Memo, Credit Memo, Info etc.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="LineSubTotal"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LINE_SUBTOTAL" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total of extended price for all the lines. This only includes the price * quantity information and excludes line charges and taxes.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MasterInvoiceNo"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MASTER_INVOICE_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Not used internally. Can be updated externally by custom logic to group invoices together.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderInvoiceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Primary key attribute of YFS_ORDER_INVOICE.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OrderNo"
                                    type="yfctype:VarcharDocumentNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The order number. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PreDistributedAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>PreDistributedAmount is the amount that is already collected for the invoice. This is fixed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Reference1"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_1" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Additional reference information.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RemainingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalAmount"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TOTAL_AMOUNT" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Grand total for the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalCharges"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalDiscount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total discount amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalHeaderCharges"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total header charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalRequiredMappingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalTax"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TOTAL_TAX" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total tax amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="isHistory"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Indicates if data is retrieved from the history table or the regular table. 
				<br/>Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                        <xsd:attribute name="NegativeInvoiceTotal"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The total of the negative invoices on the system that will distribute against positive invoices.
	            	If "Do Not Allow Debit And Credit Invoices To Settle Each Other" rule is enabled, then this will always be zero, and the NegativeInvoiceList will be populated instead.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="PresettledAmount">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
							PresettledAmount represents the SettledAmount on the order.
			        </div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="ChargeTransactionRequestList">
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="ChargeTransactionRequest">
                                    <xsd:annotation yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>This table holds the ORDER_HEADER_KEY and the request id which make a set of unique groups for payment processing on the order.  Authorizations will be split such that each authorization is for only one request.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestId"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRAN_REQUEST_ID" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The unique identifier of the request in combination with the ORDER_HEADER_KEY.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRAN_REQUEST_KEY" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The primary key for the YFS_CHARGE_TRAN_REQUEST table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestSequence"
                                    type="yfctype:PossibleKey" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REQUEST_SEQUENCE" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The order in which the Groups should be authorized.  Records with REQUEST_SEQUENCE NOT NULL will be processed first; NULL Sequences will be ordered by CHARGE_TRAN_REQUEST_ID, ascending.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MaxRequestAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MAX_REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The maximum amount this unique identifier represents before distribution to another group begins to become Authorized.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The OrderHeaderKey that is associated with this charge tran request.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PaymentStatus"
                                    type="yfctype:AuthorizationPaymentStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_STATUS" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The Payment Status of the request.  This is computed in the same manner as the Order's Payment Status.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PresettledMappedTotal"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>PresettledMappedTotal is how much is mapped to Presettled.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RemainingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="ChargeTransactionList">
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="ChargeTransaction">
                                    <xsd:annotation yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This table stores information about financial transactions associated with order processing. Every action carried out on an order which may have an impact on financial components is logged in this table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="PaymentMethod">
                                    <xsd:annotation yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This table stores details about the customer's payment methods. The key to this table is posted in YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="AwaitingAuthInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total authorization amount that has not been
								sent to the payment processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AwaitingChargeInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total charge amount that has not been sent to
								the payment
								processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BillToKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BILL_TO_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The bill to address that is associated with this payment. Can be passed if different from the bill to address on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that is given back as cash for this payment method. This is not supported on a return order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeSequence"
                                    type="yfctype:ShorterNumber" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_SEQUENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The sequence in which the payment method is to be used for charging.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckNo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckReference"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_REFERENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque reference.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardExpDate"
                                    type="yfctype:CreditCardExpDate" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_EXP_DATE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The expiration date for the credit card.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card name.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CREDIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardType"
                                    type="yfctype:Text-32" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card type group.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerAccountNo"
                                    type="yfctype:AccountNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_ACCOUNT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CUSTOMER_ACCOUNT; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerPONo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The customer PO account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DebitCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup DEBIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCreditCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the credit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the credit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCustomerAccountNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CUST_ACCT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the customer account number that should be displayed.  If not passed, it is defaulted to be the last four digits of the customer account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayDebitCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the debit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the debit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPaymentReference1"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_PAYMENT_REF1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the payment reference 1 that should be displayed.  If not passed, it is defaulted to be the last four digits of the payment reference 1.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPrimaryAccountNo"
                                    type="yfctype:Text-4" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate display value of the primary
								identifier of this PaymentType, depending upon the
								PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplaySvcNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the stored value card that should be displayed.  If not passed, it is defaulted to be the last four digits of the stored value card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FirstName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The first name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FundsAvailable"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The funds available to be charged on this payment method as
									determined by the
									<code>YFSGetFundsAvailableUE</code>
									user exit, if it was invoked. If the order is created for a
									customer and the payment method is a valid customer payment
									method, it indicates the amount available for the customer
									account. If customer account is not maintained internally or if
									the MaxChargeLimit is not set on the customer payment method,
									this value is returned as 0.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="GetFundsAvailableUserExitInvoked"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									Indicates if the
									<code>YFSGetFundsAvailableUE</code>
									user exit was invoked for this payment method.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="IncompletePaymentType"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="INCOMPLETE_PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether the payment type is incomplete.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="LastName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="LAST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The last name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxChargeLimit"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MAX_CHARGE_LIMIT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The maximum amount that can be charged on this payment method. However, if UnlimitedCharges=Y, this amount will not be considered.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MiddleName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The middle name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary key for the YFS_PAYMENT table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference1"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 1. Additionally, this is the primary account number for payment methods that are defined under the PaymentTypeGroup OTHER; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference2"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE2" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 2.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference3"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE3" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 3.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference4"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE4" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 4. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference5"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE5" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 5. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference6"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE6" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 6. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference7"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE7" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 7. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference8"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE8" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 8. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference9"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE9" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 9. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTemplateType"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TEMPLATE_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This field indicates User Interfaces to use the template required to capture Payment Methods of this Payment Type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment type of this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTypeGroup"
                                    type="yfctype:CodeValue" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE_GROUP" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This filed indicates the payment type group, which will determine behavior.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PlannedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PLANNED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that will be refunded on this payment method. This field is set for only Return Orders indicating the refund amount.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PrimaryAccountNo"
                                    type="yfctype:Text-1000" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate primary identifier of this
								PaymentType, depending upon the PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Reason code for last update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedAuthAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_AUTH_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of authorizations sent to an
								external
								system and still
								awaiting response on this payment
								method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedChargeAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_CHARGE_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of charges sent to an external
								system and still
								awaiting response on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of refunds sent to an external
								system and still
								awaiting response on this payment method. This
								number will be
								positive. This total will no longer be included in
								the
								RequestedChargeAmount, but if an asynchronous refund is
								submitted
								in excess of this amount, the RequestedChargeAmount
								will
								be
								reduced for backwards compatibility.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendAnyMoreCharges"
                                    type="yfctype:Text-1" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_ANY_MORE_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Sets whether or not this payment method has been suspended. If the value is Y, then the PaymentMethod is suspended for charging, although refunds can still be made to this PaymentMethod. All open AUTHORIZATION and CHARGE records for positive amounts for this payment method are closed.  If the value is B then both charge and refund are suspended and all open AUTHORIZATION or CHARGE records are closed.  Default: N.  Valid Values: Y, B, or N.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendExpiredAuthReversal"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_EXP_AUTH_REVERSAL" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether or not the reversal of expired authorization has to be initiated for a payment method that has been suspended. If the value is set to Y, then the authorization reversal are not created even though Authorization Reversal Strategy is configured for the payment type. Please note the flag comes into action only when SuspendAnyMoreCharges is either set to Y or B. Valid Values are N or Y (Null value behaves similar to setting it to N ).</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SvcNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup STORED_VALUE_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAltRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_ALT_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that has been refunded to a new payment method based on this payment's payment type refund preferences.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAuthorized"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_AUTHORIZED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount auhorized on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalCharged"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_CHARGED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount charged on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total refunded amount on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UnlimitedCharges"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="UNLIMITED_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Indicates if there is a limit to the amount that can be charged on the payment method. If not passed, defaults to Y.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute
                                    name="AsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For Asynchronous requests only, this field holds the identifier to the transaction closed as asynchronous. This identifier will be used for recording the actual response back in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AuditTransactionID"
                                    type="yfctype:TransactionID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AuthorizationExpirationDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_EXPIRATION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time until which the authorization specified by the authorization ID is applicable.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AuthorizationID"
                                    type="yfctype:Text-255" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For authorization requests, this field holds the identifier of the authorization that was made. For charge requests, this field holds the authroization ID to be used for charging the amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="BookAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="BOOK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that could be charged to the buyer if the order was fulfilled. At the time of fulfillment, a negative entry is made in this column and positive entry in the DEBIT_AMOUNT column.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CallForAuthorizationStatus"
                                    type="yfctype:CallForAuthStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CALL_FOR_AUTH_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This column indicates the transaction is valid for a call for authorization and whether or not the authorization code was acquired.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount of cashback that has been charged using this transaction. Only valid for transactions of ChargeType CHARGE.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRANSACTION_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The primary key for the YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeType"
                                    type="yfctype:ChargeCategory" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The type of charge that is being applied to the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectionDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COLLECTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge was realized.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CreditAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CREDIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that was actually charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DebitAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DEBIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that should be charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DistributedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DISTRIBUTED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field is used internally by the application to keep track of the amount which has been posted in the invoices as "collected".</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ExecutionDate" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXECUTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge request was processed.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:TimeStamp"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ForAsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="FOR_ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Reference to AsyncRequestIdentifier, the actual transaction that was closed as async for which the response is now recorded in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="HoldAgainstBook"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="HOLD_AGAINST_BOOK" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates whether the request was created for a precharge or a payment method whose payment type has the ChargeInsteadOfAuth flag turned on.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InPerson" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IN_PERSON" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the transaction represents an in person, or card in hand, transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MappedAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount that is mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OfflineStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OFFLINE_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates the transaction attempted to reach the payment gateway within a UserExit, but the UserExit was unable to contact the gateway.  This may be updated to N by the user exit, and will be reset to null when the record moves to CLOSED status.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OpenAuthorizedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OPEN_AUTHORIZED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is authorized for the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The order header that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderInvoiceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The key to the invoice that was created for this record, if applicable. Invoices are created for shipments, returns, or debit and credit memos.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PaymentEntryType"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_ENTRY_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Entry Type used to enter payment information into the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The payment method that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PostponedAmount" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="POSTPONED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The total amount that does not require authorization or payment capture on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field can be used to store reason code for transaction update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RequestAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is requested for the charge or authorization of record of types "REQUEST_CHARGE" and "REQUEST_AUTHORIZATION". Once the request is processed, the actual amount will be shown as "CreditAmount" or "OpenAuthorizedAmount", for charge and authorization requests respectively.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SettledAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SETTLED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field holds the amount for which a pre-settlement (settlement before invoice) is requested. Charge requests will be created for this amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Status"
                                    type="yfctype:EnumStatus2" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field indicates the status of this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TransactionDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MODIFYTS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Transaction Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TransferFromOhKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_FROM_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred from.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TransferToOhKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_TO_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UnusuableAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The unusable amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UserExitStatus"
                                    type="yfctype:Text-15" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USER_EXIT_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The user exit call status of this record. The status will be "INVOKED" if the transaction fails during or after the user exit call, otherwise the value will be blank.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="VoidTransaction" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="VOID_TRANSACTION" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the Charge Transaction is either a Void request or submitted for a Void operation.  Valid values for a Void Charge Transaction are V (is a void request), R (is a forced refund request), C (closed error record, already recreated) and null. Valid value for a Charge Transaction that has been requested for Void operation is S (void submitted for this transaction).</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="V"/>

                                    <xsd:enumeration value="R"/>

                                    <xsd:enumeration value="S"/>

                                    <xsd:enumeration value="C"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                        <xsd:attribute name="RemainingAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalRequiredMappingAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="NegativeInvoiceList">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="1" name="OrderInvoice">
                                <xsd:annotation yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Stores order invoices raised against an order.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="ChargeTransactionList">
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="ChargeTransaction">
                                    <xsd:annotation yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This table stores information about financial transactions associated with order processing. Every action carried out on an order which may have an impact on financial components is logged in this table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:sequence>

                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="PaymentMethod">

                                    <xsd:annotation yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This table stores details about the customer's payment methods. The key to this table is posted in YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="AwaitingAuthInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total authorization amount that has not been
								sent to the payment processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AwaitingChargeInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total charge amount that has not been sent to
								the payment
								processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BillToKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BILL_TO_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The bill to address that is associated with this payment. Can be passed if different from the bill to address on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that is given back as cash for this payment method. This is not supported on a return order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeSequence"
                                    type="yfctype:ShorterNumber" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_SEQUENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The sequence in which the payment method is to be used for charging.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckNo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckReference"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_REFERENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque reference.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardExpDate"
                                    type="yfctype:CreditCardExpDate" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_EXP_DATE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The expiration date for the credit card.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card name.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CREDIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardType"
                                    type="yfctype:Text-32" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card type group.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerAccountNo"
                                    type="yfctype:AccountNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_ACCOUNT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CUSTOMER_ACCOUNT; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerPONo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The customer PO account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DebitCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup DEBIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCreditCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the credit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the credit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCustomerAccountNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CUST_ACCT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the customer account number that should be displayed.  If not passed, it is defaulted to be the last four digits of the customer account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayDebitCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the debit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the debit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPaymentReference1"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_PAYMENT_REF1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the payment reference 1 that should be displayed.  If not passed, it is defaulted to be the last four digits of the payment reference 1.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPrimaryAccountNo"
                                    type="yfctype:Text-4" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate display value of the primary
								identifier of this PaymentType, depending upon the
								PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplaySvcNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the stored value card that should be displayed.  If not passed, it is defaulted to be the last four digits of the stored value card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FirstName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The first name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FundsAvailable"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The funds available to be charged on this payment method as
									determined by the
									<code>YFSGetFundsAvailableUE</code>
									user exit, if it was invoked. If the order is created for a
									customer and the payment method is a valid customer payment
									method, it indicates the amount available for the customer
									account. If customer account is not maintained internally or if
									the MaxChargeLimit is not set on the customer payment method,
									this value is returned as 0.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="GetFundsAvailableUserExitInvoked"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									Indicates if the
									<code>YFSGetFundsAvailableUE</code>
									user exit was invoked for this payment method.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="IncompletePaymentType"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="INCOMPLETE_PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether the payment type is incomplete.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="LastName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="LAST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The last name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxChargeLimit"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MAX_CHARGE_LIMIT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The maximum amount that can be charged on this payment method. However, if UnlimitedCharges=Y, this amount will not be considered.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MiddleName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The middle name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary key for the YFS_PAYMENT table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference1"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 1. Additionally, this is the primary account number for payment methods that are defined under the PaymentTypeGroup OTHER; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference2"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE2" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 2.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference3"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE3" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 3.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference4"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE4" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 4. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference5"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE5" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 5. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference6"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE6" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 6. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference7"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE7" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 7. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference8"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE8" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 8. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference9"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE9" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 9. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTemplateType"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TEMPLATE_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This field indicates User Interfaces to use the template required to capture Payment Methods of this Payment Type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment type of this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTypeGroup"
                                    type="yfctype:CodeValue" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE_GROUP" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This filed indicates the payment type group, which will determine behavior.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PlannedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PLANNED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that will be refunded on this payment method. This field is set for only Return Orders indicating the refund amount.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PrimaryAccountNo"
                                    type="yfctype:Text-1000" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate primary identifier of this
								PaymentType, depending upon the PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Reason code for last update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedAuthAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_AUTH_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of authorizations sent to an
								external
								system and still
								awaiting response on this payment
								method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedChargeAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_CHARGE_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of charges sent to an external
								system and still
								awaiting response on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of refunds sent to an external
								system and still
								awaiting response on this payment method. This
								number will be
								positive. This total will no longer be included in
								the
								RequestedChargeAmount, but if an asynchronous refund is
								submitted
								in excess of this amount, the RequestedChargeAmount
								will
								be
								reduced for backwards compatibility.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendAnyMoreCharges"
                                    type="yfctype:Text-1" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_ANY_MORE_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Sets whether or not this payment method has been suspended. If the value is Y, then the PaymentMethod is suspended for charging, although refunds can still be made to this PaymentMethod. All open AUTHORIZATION and CHARGE records for positive amounts for this payment method are closed.  If the value is B then both charge and refund are suspended and all open AUTHORIZATION or CHARGE records are closed.  Default: N.  Valid Values: Y, B, or N.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendExpiredAuthReversal"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_EXP_AUTH_REVERSAL" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether or not the reversal of expired authorization has to be initiated for a payment method that has been suspended. If the value is set to Y, then the authorization reversal are not created even though Authorization Reversal Strategy is configured for the payment type. Please note the flag comes into action only when SuspendAnyMoreCharges is either set to Y or B. Valid Values are N or Y (Null value behaves similar to setting it to N ).</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SvcNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup STORED_VALUE_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAltRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_ALT_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that has been refunded to a new payment method based on this payment's payment type refund preferences.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAuthorized"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_AUTHORIZED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount auhorized on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalCharged"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_CHARGED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount charged on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total refunded amount on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UnlimitedCharges"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="UNLIMITED_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Indicates if there is a limit to the amount that can be charged on the payment method. If not passed, defaults to Y.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>

                                    <xsd:attribute
                                    name="AsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For Asynchronous requests only, this field holds the identifier to the transaction closed as asynchronous. This identifier will be used for recording the actual response back in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AuditTransactionID"
                                    type="yfctype:TransactionID" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AuthorizationExpirationDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_EXPIRATION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time until which the authorization specified by the authorization ID is applicable.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AuthorizationID"
                                    type="yfctype:Text-255" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For authorization requests, this field holds the identifier of the authorization that was made. For charge requests, this field holds the authroization ID to be used for charging the amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BookAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BOOK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that could be charged to the buyer if the order was fulfilled. At the time of fulfillment, a negative entry is made in this column and positive entry in the DEBIT_AMOUNT column.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CallForAuthorizationStatus"
                                    type="yfctype:CallForAuthStatus" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CALL_FOR_AUTH_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This column indicates the transaction is valid for a call for authorization and whether or not the authorization code was acquired.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount of cashback that has been charged using this transaction. Only valid for transactions of ChargeType CHARGE.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeTransactionKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRANSACTION_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The primary key for the YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeType"
                                    type="yfctype:ChargeCategory" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The type of charge that is being applied to the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CollectionDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="COLLECTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge was realized.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that was actually charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DebitAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DEBIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that should be charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DistributedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISTRIBUTED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field is used internally by the application to keep track of the amount which has been posted in the invoices as "collected".</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ExecutionDate" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="EXECUTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge request was processed.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:TimeStamp"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ForAsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FOR_ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Reference to AsyncRequestIdentifier, the actual transaction that was closed as async for which the response is now recorded in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="HoldAgainstBook"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="HOLD_AGAINST_BOOK" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates whether the request was created for a precharge or a payment method whose payment type has the ChargeInsteadOfAuth flag turned on.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="InPerson" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="IN_PERSON" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the transaction represents an in person, or card in hand, transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MappedAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount that is mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OfflineStatus" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="OFFLINE_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates the transaction attempted to reach the payment gateway within a UserExit, but the UserExit was unable to contact the gateway.  This may be updated to N by the user exit, and will be reset to null when the record moves to CLOSED status.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OpenAuthorizedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="OPEN_AUTHORIZED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is authorized for the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The order header that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderInvoiceKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The key to the invoice that was created for this record, if applicable. Invoices are created for shipments, returns, or debit and credit memos.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentEntryType"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_ENTRY_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Entry Type used to enter payment information into the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The payment method that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PostponedAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="POSTPONED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The total amount that does not require authorization or payment capture on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field can be used to store reason code for transaction update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is requested for the charge or authorization of record of types "REQUEST_CHARGE" and "REQUEST_AUTHORIZATION". Once the request is processed, the actual amount will be shown as "CreditAmount" or "OpenAuthorizedAmount", for charge and authorization requests respectively.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SettledAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SETTLED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field holds the amount for which a pre-settlement (settlement before invoice) is requested. Charge requests will be created for this amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Status"
                                    type="yfctype:EnumStatus2" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field indicates the status of this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TransactionDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MODIFYTS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Transaction Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TransferFromOhKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_FROM_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred from.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TransferToOhKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_TO_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UnusuableAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The unusable amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UserExitStatus"
                                    type="yfctype:Text-15" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="USER_EXIT_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The user exit call status of this record. The status will be "INVOKED" if the transaction fails during or after the user exit call, otherwise the value will be blank.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="VoidTransaction" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="VOID_TRANSACTION" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the Charge Transaction is either a Void request or submitted for a Void operation.  Valid values for a Void Charge Transaction are V (is a void request), R (is a forced refund request), C (closed error record, already recreated) and null. Valid value for a Charge Transaction that has been requested for Void operation is S (void submitted for this transaction).</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="V"/>

                                    <xsd:enumeration value="R"/>

                                    <xsd:enumeration value="S"/>

                                    <xsd:enumeration value="C"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    <xsd:attribute
                                    name="ActualFreightCharge"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACTUAL_FREIGHT_CHARGE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Actual freight charge or the charge for transporting the shipment from the origin  node to the destination node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AmountCollected"
                                    type="yfctype:ComputedMoney" use="required">
                                    <xsd:annotation
                                    yfc:columnName="AMOUNT_COLLECTED" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total amount which has been paid against this invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargedActualFreight"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGED_ACTUAL_FREIGHT" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>If shipping charges were charged on actual, this is set to 'Y'.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectedExternally"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COLLECTED_EXTERNALLY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Flag to indicate if invoice collection is happening externally in A/R system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectedThroughAR"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AMOUNT_PASSED_TO_AR" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Amount which was passed to A/R system for collection. This would typically be same as total amount unless there was some pre-payment on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DateInvoiced"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CREATETS" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Timestamp when the record was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DerivedFromOrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DERIVED_FROM_ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Link to order header from which this Invoice's Order is derived.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DocumentType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The document type. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EnterpriseCode"
                                    type="yfctype:OrgCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The enterprise code. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderCharges"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Header Charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderDiscount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Header discount amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TAX" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Tax amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InvoiceCreationReason"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_CREATION_REASON" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The reason for which the invoice is created. The system populates this field only for adjustment invoices.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InvoiceNo"
                                    type="yfctype:InvoiceNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Invoice number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InvoiceType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_TYPE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Classifies an invoice. For example, Orders, Returns, Debit Memo, Credit Memo, Info etc.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="LineSubTotal"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LINE_SUBTOTAL" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total of extended price for all the lines. This only includes the price * quantity information and excludes line charges and taxes.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MasterInvoiceNo"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MASTER_INVOICE_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Not used internally. Can be updated externally by custom logic to group invoices together.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderInvoiceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Primary key attribute of YFS_ORDER_INVOICE.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OrderNo"
                                    type="yfctype:VarcharDocumentNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The order number. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Reference1"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_1" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Additional reference information.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RemainingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalAmount"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TOTAL_AMOUNT" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Grand total for the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalCharges"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalDiscount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total discount amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalHeaderCharges"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total header charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalRequiredMappingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalTax"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TOTAL_TAX" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total tax amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="isHistory"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Indicates if data is retrieved from the history table or the regular table. 
				<br/>Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ChargeTransactionRequestList">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
								This list excludes 
									(1) all ChargeTransactionRequests in the InvoiceList element, 
									(2) the ChargeTransactionRequests with RemainingAmount=0 in the PresettledAmount element, 
									(3) the ChargeTransactionRequests which are mapped to predistributed invoices. 
			        </div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ChargeTransactionRequest">
                                <xsd:annotation yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>This table holds the ORDER_HEADER_KEY and the request id which make a set of unique groups for payment processing on the order.  Authorizations will be split such that each authorization is for only one request.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="ChargeTransactionList">
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="ChargeTransaction">
                                    <xsd:annotation yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This table stores information about financial transactions associated with order processing. Every action carried out on an order which may have an impact on financial components is logged in this table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:sequence>

                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="PaymentMethod">

                                    <xsd:annotation yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This table stores details about the customer's payment methods. The key to this table is posted in YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="AwaitingAuthInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total authorization amount that has not been
								sent to the payment processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AwaitingChargeInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total charge amount that has not been sent to
								the payment
								processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BillToKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BILL_TO_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The bill to address that is associated with this payment. Can be passed if different from the bill to address on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that is given back as cash for this payment method. This is not supported on a return order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeSequence"
                                    type="yfctype:ShorterNumber" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_SEQUENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The sequence in which the payment method is to be used for charging.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckNo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckReference"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_REFERENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque reference.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardExpDate"
                                    type="yfctype:CreditCardExpDate" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_EXP_DATE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The expiration date for the credit card.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card name.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CREDIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardType"
                                    type="yfctype:Text-32" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card type group.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerAccountNo"
                                    type="yfctype:AccountNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_ACCOUNT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CUSTOMER_ACCOUNT; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerPONo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The customer PO account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DebitCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup DEBIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCreditCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the credit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the credit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCustomerAccountNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CUST_ACCT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the customer account number that should be displayed.  If not passed, it is defaulted to be the last four digits of the customer account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayDebitCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the debit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the debit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPaymentReference1"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_PAYMENT_REF1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the payment reference 1 that should be displayed.  If not passed, it is defaulted to be the last four digits of the payment reference 1.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPrimaryAccountNo"
                                    type="yfctype:Text-4" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate display value of the primary
								identifier of this PaymentType, depending upon the
								PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplaySvcNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the stored value card that should be displayed.  If not passed, it is defaulted to be the last four digits of the stored value card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FirstName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The first name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FundsAvailable"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The funds available to be charged on this payment method as
									determined by the
									<code>YFSGetFundsAvailableUE</code>
									user exit, if it was invoked. If the order is created for a
									customer and the payment method is a valid customer payment
									method, it indicates the amount available for the customer
									account. If customer account is not maintained internally or if
									the MaxChargeLimit is not set on the customer payment method,
									this value is returned as 0.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="GetFundsAvailableUserExitInvoked"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									Indicates if the
									<code>YFSGetFundsAvailableUE</code>
									user exit was invoked for this payment method.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="IncompletePaymentType"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="INCOMPLETE_PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether the payment type is incomplete.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="LastName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="LAST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The last name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxChargeLimit"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MAX_CHARGE_LIMIT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The maximum amount that can be charged on this payment method. However, if UnlimitedCharges=Y, this amount will not be considered.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MiddleName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The middle name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary key for the YFS_PAYMENT table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference1"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 1. Additionally, this is the primary account number for payment methods that are defined under the PaymentTypeGroup OTHER; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference2"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE2" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 2.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference3"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE3" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 3.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference4"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE4" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 4. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference5"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE5" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 5. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference6"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE6" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 6. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference7"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE7" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 7. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference8"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE8" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 8. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference9"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE9" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 9. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTemplateType"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TEMPLATE_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This field indicates User Interfaces to use the template required to capture Payment Methods of this Payment Type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment type of this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTypeGroup"
                                    type="yfctype:CodeValue" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE_GROUP" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This filed indicates the payment type group, which will determine behavior.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PlannedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PLANNED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that will be refunded on this payment method. This field is set for only Return Orders indicating the refund amount.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PrimaryAccountNo"
                                    type="yfctype:Text-1000" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate primary identifier of this
								PaymentType, depending upon the PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Reason code for last update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedAuthAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_AUTH_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of authorizations sent to an
								external
								system and still
								awaiting response on this payment
								method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedChargeAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_CHARGE_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of charges sent to an external
								system and still
								awaiting response on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of refunds sent to an external
								system and still
								awaiting response on this payment method. This
								number will be
								positive. This total will no longer be included in
								the
								RequestedChargeAmount, but if an asynchronous refund is
								submitted
								in excess of this amount, the RequestedChargeAmount
								will
								be
								reduced for backwards compatibility.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendAnyMoreCharges"
                                    type="yfctype:Text-1" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_ANY_MORE_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Sets whether or not this payment method has been suspended. If the value is Y, then the PaymentMethod is suspended for charging, although refunds can still be made to this PaymentMethod. All open AUTHORIZATION and CHARGE records for positive amounts for this payment method are closed.  If the value is B then both charge and refund are suspended and all open AUTHORIZATION or CHARGE records are closed.  Default: N.  Valid Values: Y, B, or N.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendExpiredAuthReversal"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_EXP_AUTH_REVERSAL" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether or not the reversal of expired authorization has to be initiated for a payment method that has been suspended. If the value is set to Y, then the authorization reversal are not created even though Authorization Reversal Strategy is configured for the payment type. Please note the flag comes into action only when SuspendAnyMoreCharges is either set to Y or B. Valid Values are N or Y (Null value behaves similar to setting it to N ).</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SvcNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup STORED_VALUE_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAltRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_ALT_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that has been refunded to a new payment method based on this payment's payment type refund preferences.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAuthorized"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_AUTHORIZED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount auhorized on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalCharged"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_CHARGED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount charged on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total refunded amount on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UnlimitedCharges"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="UNLIMITED_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Indicates if there is a limit to the amount that can be charged on the payment method. If not passed, defaults to Y.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>

                                    <xsd:attribute
                                    name="AsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For Asynchronous requests only, this field holds the identifier to the transaction closed as asynchronous. This identifier will be used for recording the actual response back in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AuditTransactionID"
                                    type="yfctype:TransactionID" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AuthorizationExpirationDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_EXPIRATION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time until which the authorization specified by the authorization ID is applicable.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AuthorizationID"
                                    type="yfctype:Text-255" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For authorization requests, this field holds the identifier of the authorization that was made. For charge requests, this field holds the authroization ID to be used for charging the amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BookAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BOOK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that could be charged to the buyer if the order was fulfilled. At the time of fulfillment, a negative entry is made in this column and positive entry in the DEBIT_AMOUNT column.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CallForAuthorizationStatus"
                                    type="yfctype:CallForAuthStatus" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CALL_FOR_AUTH_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This column indicates the transaction is valid for a call for authorization and whether or not the authorization code was acquired.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount of cashback that has been charged using this transaction. Only valid for transactions of ChargeType CHARGE.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeTransactionKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRANSACTION_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The primary key for the YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeType"
                                    type="yfctype:ChargeCategory" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The type of charge that is being applied to the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CollectionDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="COLLECTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge was realized.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that was actually charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DebitAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DEBIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that should be charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DistributedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISTRIBUTED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field is used internally by the application to keep track of the amount which has been posted in the invoices as "collected".</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ExecutionDate" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="EXECUTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge request was processed.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:TimeStamp"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ForAsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FOR_ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Reference to AsyncRequestIdentifier, the actual transaction that was closed as async for which the response is now recorded in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="HoldAgainstBook"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="HOLD_AGAINST_BOOK" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates whether the request was created for a precharge or a payment method whose payment type has the ChargeInsteadOfAuth flag turned on.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="InPerson" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="IN_PERSON" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the transaction represents an in person, or card in hand, transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MappedAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount that is mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OfflineStatus" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="OFFLINE_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates the transaction attempted to reach the payment gateway within a UserExit, but the UserExit was unable to contact the gateway.  This may be updated to N by the user exit, and will be reset to null when the record moves to CLOSED status.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OpenAuthorizedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="OPEN_AUTHORIZED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is authorized for the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The order header that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderInvoiceKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The key to the invoice that was created for this record, if applicable. Invoices are created for shipments, returns, or debit and credit memos.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentEntryType"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_ENTRY_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Entry Type used to enter payment information into the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The payment method that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PostponedAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="POSTPONED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The total amount that does not require authorization or payment capture on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field can be used to store reason code for transaction update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is requested for the charge or authorization of record of types "REQUEST_CHARGE" and "REQUEST_AUTHORIZATION". Once the request is processed, the actual amount will be shown as "CreditAmount" or "OpenAuthorizedAmount", for charge and authorization requests respectively.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SettledAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SETTLED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field holds the amount for which a pre-settlement (settlement before invoice) is requested. Charge requests will be created for this amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Status"
                                    type="yfctype:EnumStatus2" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field indicates the status of this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TransactionDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MODIFYTS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Transaction Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TransferFromOhKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_FROM_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred from.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TransferToOhKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_TO_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UnusuableAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The unusable amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UserExitStatus"
                                    type="yfctype:Text-15" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="USER_EXIT_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The user exit call status of this record. The status will be "INVOKED" if the transaction fails during or after the user exit call, otherwise the value will be blank.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="VoidTransaction" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="VOID_TRANSACTION" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the Charge Transaction is either a Void request or submitted for a Void operation.  Valid values for a Void Charge Transaction are V (is a void request), R (is a forced refund request), C (closed error record, already recreated) and null. Valid value for a Charge Transaction that has been requested for Void operation is S (void submitted for this transaction).</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="V"/>

                                    <xsd:enumeration value="R"/>

                                    <xsd:enumeration value="S"/>

                                    <xsd:enumeration value="C"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    <xsd:attribute
                                    name="BlindCreationAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>BlindCreationAmount is the amount that new authorization will be attempted for.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestId"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRAN_REQUEST_ID" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The unique identifier of the request in combination with the ORDER_HEADER_KEY.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRAN_REQUEST_KEY" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The primary key for the YFS_CHARGE_TRAN_REQUEST table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestSequence"
                                    type="yfctype:PossibleKey" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REQUEST_SEQUENCE" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The order in which the Groups should be authorized.  Records with REQUEST_SEQUENCE NOT NULL will be processed first; NULL Sequences will be ordered by CHARGE_TRAN_REQUEST_ID, ascending.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MaxRequestAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MAX_REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The maximum amount this unique identifier represents before distribution to another group begins to become Authorized.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The OrderHeaderKey that is associated with this charge tran request.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PaymentStatus"
                                    type="yfctype:AuthorizationPaymentStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_STATUS" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The Payment Status of the request.  This is computed in the same manner as the Order's Payment Status.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PresettledMappedTotal"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>PresettledMappedTotal is how much is mapped to Presettled.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RemainingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalRequiredMappingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="OrderRemainder">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
								The amount to authorize that does not have a ChargeTransactionRequest.  It will be zero if ChargeTransactionRequests are mandatory for authorizations.
			        </div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="ChargeTransactionList">
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="ChargeTransaction">
                                    <xsd:annotation yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This table stores information about financial transactions associated with order processing. Every action carried out on an order which may have an impact on financial components is logged in this table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="PaymentMethod">
                                    <xsd:annotation yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This table stores details about the customer's payment methods. The key to this table is posted in YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="AwaitingAuthInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total authorization amount that has not been
								sent to the payment processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AwaitingChargeInterfaceAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total charge amount that has not been sent to
								the payment
								processing system for processing yet.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BillToKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BILL_TO_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The bill to address that is associated with this payment. Can be passed if different from the bill to address on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that is given back as cash for this payment method. This is not supported on a return order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ChargeSequence"
                                    type="yfctype:ShorterNumber" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHARGE_SEQUENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The sequence in which the payment method is to be used for charging.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckNo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CheckReference"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CHEQUE_REFERENCE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The cheque reference.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardExpDate"
                                    type="yfctype:CreditCardExpDate" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_EXP_DATE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The expiration date for the credit card.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card name.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CREDIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CreditCardType"
                                    type="yfctype:Text-32" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREDIT_CARD_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The credit card type group.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerAccountNo"
                                    type="yfctype:AccountNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_ACCOUNT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CUSTOMER_ACCOUNT; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerPONo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The customer PO account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DebitCardNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup DEBIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCreditCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the credit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the credit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayCustomerAccountNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_CUST_ACCT_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the customer account number that should be displayed.  If not passed, it is defaulted to be the last four digits of the customer account number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayDebitCardNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the debit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the debit card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPaymentReference1"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_PAYMENT_REF1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the payment reference 1 that should be displayed.  If not passed, it is defaulted to be the last four digits of the payment reference 1.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplayPrimaryAccountNo"
                                    type="yfctype:Text-4" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate display value of the primary
								identifier of this PaymentType, depending upon the
								PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DisplaySvcNo"
                                    type="yfctype:Text-4" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DISPLAY_SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The four digits of the stored value card that should be displayed.  If not passed, it is defaulted to be the last four digits of the stored value card number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FirstName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The first name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FundsAvailable"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The funds available to be charged on this payment method as
									determined by the
									<code>YFSGetFundsAvailableUE</code>
									user exit, if it was invoked. If the order is created for a
									customer and the payment method is a valid customer payment
									method, it indicates the amount available for the customer
									account. If customer account is not maintained internally or if
									the MaxChargeLimit is not set on the customer payment method,
									this value is returned as 0.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="GetFundsAvailableUserExitInvoked"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									Indicates if the
									<code>YFSGetFundsAvailableUE</code>
									user exit was invoked for this payment method.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="IncompletePaymentType"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="INCOMPLETE_PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether the payment type is incomplete.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="LastName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="LAST_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The last name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxChargeLimit"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MAX_CHARGE_LIMIT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The maximum amount that can be charged on this payment method. However, if UnlimitedCharges=Y, this amount will not be considered.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MiddleName"
                                    type="yfctype:Text-300" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The middle name of the credit card holder.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary key for the YFS_PAYMENT table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference1"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE1" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 1. Additionally, this is the primary account number for payment methods that are defined under the PaymentTypeGroup OTHER; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference2"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE2" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 2.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference3"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE3" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 3.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference4"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE4" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 4. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference5"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE5" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 5. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference6"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE6" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 6. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference7"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE7" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 7. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference8"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE8" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 8. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentReference9"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_REFERENCE9" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment reference 9. This does not identify the payment method uniquely.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTemplateType"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TEMPLATE_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This field indicates User Interfaces to use the template required to capture Payment Methods of this Payment Type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The payment type of this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PaymentTypeGroup"
                                    type="yfctype:CodeValue" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE_GROUP" yfc:tableName="YFS_PAYMENT_TYPE">
                                    <xsd:documentation>This filed indicates the payment type group, which will determine behavior.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PlannedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PLANNED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that will be refunded on this payment method. This field is set for only Return Orders indicating the refund amount.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PrimaryAccountNo"
                                    type="yfctype:Text-1000" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The appropriate primary identifier of this
								PaymentType, depending upon the PaymentTypeGroup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Reason code for last update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedAuthAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_AUTH_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of authorizations sent to an
								external
								system and still
								awaiting response on this payment
								method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedChargeAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_CHARGE_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of charges sent to an external
								system and still
								awaiting response on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="RequestedRefundAmount" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount of refunds sent to an external
								system and still
								awaiting response on this payment method. This
								number will be
								positive. This total will no longer be included in
								the
								RequestedChargeAmount, but if an asynchronous refund is
								submitted
								in excess of this amount, the RequestedChargeAmount
								will
								be
								reduced for backwards compatibility.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendAnyMoreCharges"
                                    type="yfctype:Text-1" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_ANY_MORE_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Sets whether or not this payment method has been suspended. If the value is Y, then the PaymentMethod is suspended for charging, although refunds can still be made to this PaymentMethod. All open AUTHORIZATION and CHARGE records for positive amounts for this payment method are closed.  If the value is B then both charge and refund are suspended and all open AUTHORIZATION or CHARGE records are closed.  Default: N.  Valid Values: Y, B, or N.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SuspendExpiredAuthReversal"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUSPEND_EXP_AUTH_REVERSAL" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>This flag indicates whether or not the reversal of expired authorization has to be initiated for a payment method that has been suspended. If the value is set to Y, then the authorization reversal are not created even though Authorization Reversal Strategy is configured for the payment type. Please note the flag comes into action only when SuspendAnyMoreCharges is either set to Y or B. Valid Values are N or Y (Null value behaves similar to setting it to N ).</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SvcNo"
                                    type="yfctype:Text-1000" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SVC_NO" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup STORED_VALUE_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAltRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_ALT_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The amount that has been refunded to a new payment method based on this payment's payment type refund preferences.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalAuthorized"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_AUTHORIZED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount auhorized on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalCharged"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_CHARGED" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total amount charged on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TotalRefundedAmount"
                                    type="yfctype:Money" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TOTAL_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>The total refunded amount on this payment method.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UnlimitedCharges"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="UNLIMITED_CHARGES" yfc:tableName="YFS_PAYMENT">
                                    <xsd:documentation>Indicates if there is a limit to the amount that can be charged on the payment method. If not passed, defaults to Y.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute
                                    name="AsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For Asynchronous requests only, this field holds the identifier to the transaction closed as asynchronous. This identifier will be used for recording the actual response back in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AuditTransactionID"
                                    type="yfctype:TransactionID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AuthorizationExpirationDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_EXPIRATION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time until which the authorization specified by the authorization ID is applicable.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AuthorizationID"
                                    type="yfctype:Text-255" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AUTHORIZATION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>For authorization requests, this field holds the identifier of the authorization that was made. For charge requests, this field holds the authroization ID to be used for charging the amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="BookAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="BOOK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that could be charged to the buyer if the order was fulfilled. At the time of fulfillment, a negative entry is made in this column and positive entry in the DEBIT_AMOUNT column.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CallForAuthorizationStatus"
                                    type="yfctype:CallForAuthStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CALL_FOR_AUTH_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This column indicates the transaction is valid for a call for authorization and whether or not the authorization code was acquired.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CashBackAmount" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount of cashback that has been charged using this transaction. Only valid for transactions of ChargeType CHARGE.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRANSACTION_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The primary key for the YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeType"
                                    type="yfctype:ChargeCategory" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The type of charge that is being applied to the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectionDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COLLECTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge was realized.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CreditAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CREDIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that was actually charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DebitAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DEBIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that should be charged or refunded to the buyer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DistributedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DISTRIBUTED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field is used internally by the application to keep track of the amount which has been posted in the invoices as "collected".</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ExecutionDate" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXECUTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The date and time when the charge request was processed.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:TimeStamp"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ForAsyncRequestIdentifier"
                                    type="yfctype:Text-255" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="FOR_ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Reference to AsyncRequestIdentifier, the actual transaction that was closed as async for which the response is now recorded in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="HoldAgainstBook"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="HOLD_AGAINST_BOOK" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates whether the request was created for a precharge or a payment method whose payment type has the ChargeInsteadOfAuth flag turned on.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InPerson" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IN_PERSON" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the transaction represents an in person, or card in hand, transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MappedAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount that is mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OfflineStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OFFLINE_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates the transaction attempted to reach the payment gateway within a UserExit, but the UserExit was unable to contact the gateway.  This may be updated to N by the user exit, and will be reset to null when the record moves to CLOSED status.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="Y"/>

                                    <xsd:enumeration value="N"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OpenAuthorizedAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OPEN_AUTHORIZED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is authorized for the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The order header that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderInvoiceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The key to the invoice that was created for this record, if applicable. Invoices are created for shipments, returns, or debit and credit memos.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PaymentEntryType"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_ENTRY_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Entry Type used to enter payment information into the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PaymentKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The payment method that is associated with this charge transaction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PostponedAmount" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="POSTPONED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The total amount that does not require authorization or payment capture on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReasonCode"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field can be used to store reason code for transaction update.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RequestAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The amount that is requested for the charge or authorization of record of types "REQUEST_CHARGE" and "REQUEST_AUTHORIZATION". Once the request is processed, the actual amount will be shown as "CreditAmount" or "OpenAuthorizedAmount", for charge and authorization requests respectively.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SettledAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SETTLED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field holds the amount for which a pre-settlement (settlement before invoice) is requested. Charge requests will be created for this amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Status"
                                    type="yfctype:EnumStatus2" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This field indicates the status of this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TransactionDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MODIFYTS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>Transaction Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TransferFromOhKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_FROM_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred from.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TransferToOhKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TRANSFER_TO_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The Order Header Key of the order that funds are being transferred to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UnusuableAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The unusable amount.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UserExitStatus"
                                    type="yfctype:Text-15" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USER_EXIT_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>The user exit call status of this record. The status will be "INVOKED" if the transaction fails during or after the user exit call, otherwise the value will be blank.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="VoidTransaction" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="VOID_TRANSACTION" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                    <xsd:documentation>This flag indicates that the Charge Transaction is either a Void request or submitted for a Void operation.  Valid values for a Void Charge Transaction are V (is a void request), R (is a forced refund request), C (closed error record, already recreated) and null. Valid value for a Charge Transaction that has been requested for Void operation is S (void submitted for this transaction).</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Flag">

                                    <xsd:enumeration value="V"/>

                                    <xsd:enumeration value="R"/>

                                    <xsd:enumeration value="S"/>

                                    <xsd:enumeration value="C"/>

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                        <xsd:attribute name="BlindCreationAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>BlindCreationAmount is the amount that new authorization will be attempted for.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RemainingAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalRequiredMappingAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="MappableObjects">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="InvoiceList">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="1" name="OrderInvoice">
                                <xsd:annotation yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Stores order invoices raised against an order.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="OrderReleaseList">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								This list will be empty if the invoice is not linked to a shipment.
			        </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="OrderRelease">
                                    <xsd:annotation yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This table stores releases made against an order.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="BillToID"
                                    type="yfctype:ID-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BILL_TO_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The identifier of the customer that this order is being billed to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BuyerMarkForNodeId"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BUYER_MARK_FOR_NODE_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Mark For Node as identified by the buyer for eg: it could be a person,store,DC.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BuyerOrganizationCode"
                                    type="yfctype:OrgCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BUYER_ORGANIZATION_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The code of the buyer organization. This must be a valid organization defined in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="BuyerReceivingNodeId"
                                    type="yfctype:ShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="BUYER_RECEIVING_NODE_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Receiving node as identified by the buyer for eg:Store,DC Number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CarrierAccountNo"
                                    type="yfctype:AccountNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTCARRIER_ACCOUNT_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The account number of the customer with the carrier.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CarrierServiceCode"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The carrier service code.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Createts"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CREATETS" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Timestamp when the record was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Currency"
                                    type="yfctype:Currency" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CURRENCY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The currency in which the buyer will pay the seller.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustReqDeliveryDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUST_REQ_DELIVERY_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Customer Requested Delivery date. This date is the minimum value of the RequestedDeliveryDate on all the order lines which are a part of this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustReqShipDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUST_REQ_SHIP_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Customer Requested Ship date. This date is the minimum value of the RequestedShipDate on all the order lines which are a part of this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="CustomerPoNo"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The PO number of the customer. For purchase orders, this can be used for storing the purchase order number of the purchasing system if that number is not already stored as part of order number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DeliveryCode"
                                    type="yfctype:DeliveryCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DELIVERY_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field indicates who will pay the freight. Application understands values such as SUPPLIER, ENTERPRISE, and MARKETPLACE to mean that each of these organizations will pay for the freight. You could however pass any other value which is a valid shipment charge type as understood by the carrier by which the shipment is transported. See YFS_SHIPMENT_CONTAINER.carrier_payment_type for additional information.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DeliveryMethod"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DELIVERY_METHOD" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The delivery method for the product line.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Division"
                                    type="yfctype:Division" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DIVISION" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The division in the buyer organization that placed the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DocumentType"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The type of order document used for this order release. For example,Sales Order document (0001), Planned Order document (0002), Return Order document (0003) , and Purchase Order document (0005). This field determines the order's fulfillment cycle and controls the inventory and payment updates performed on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="EnterpriseCode"
                                    type="yfctype:OrgCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ENTERPRISE_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The enterprise that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="FOB"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="FOB" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Indicates Freight Terms for the release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="GiftFlag"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="GIFT_FLAG" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether the order release has gift items.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="HoldFlag"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="HOLD_FLAG" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="HoldReasonCode"
                                    type="yfctype:HoldReasonCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="HOLD_REASON_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="LevelOfService"
                                    type="yfctype:LevelOfService" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="LEVEL_OF_SERVICE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Indicates the level of service required for this release.  Default is Null.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MarkForKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MARK_FOR_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The mark for address that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxOrderReleaseStatus"
                                    type="yfctype:Status" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Maximum order status amongst all records that belong to an order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxOrderReleaseStatusDesc"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Description of maximum order status amongst all records that belong to an order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MergeNode"
                                    type="yfctype:ShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MERGE_NODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The node that is used as either forwarding or merge location.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MinOrderReleaseStatus"
                                    type="yfctype:Status" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Minimum order status amongst all records that belong to an order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MinOrderReleaseStatusDesc"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Description of minimum order status amongst all records that belong to an order release</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Modifyts"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="MODIFYTS" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Timestamp when the record was last modified.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MultipleStatusesExist"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>If order release is in more than one status then this flag will be returned as "Y".</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="NotificationReference"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="NOTIFICATION_REFERENCE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The data related to the notification type. If the notification type is E-mail, then the notification reference should hold the E-mail ID to which the notification mail must be sent.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="NotificationType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="NOTIFICATION_TYPE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field can be used to indicate a means in which the buyer may be notified when the order reaches various statuses, or in situations where the buyer input is required to resolve a business exception. For example, notification type could have values like E-mail, FAX, etc.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="NotifyAfterShipmentFlag"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="NOTIFY_AFTER_SHIPMENT_FLAG" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether the buyer must be notified after the order is shipped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The date on which the order was placed.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The order header that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderName"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_NAME" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The name given to the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderReleaseKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_RELEASE_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The primary key for the YFS_ORDER_RELEASE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OrderType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORDER_TYPE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>An order classification attribute. This field can be used for reporting purposes or to build conditions while modeling your business process. Sterling Selling And fulfillment Suite system has no default logic based on this field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="OtherCharges"
                                    type="yfctype:ComputedMoney" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="OTHER_CHARGES" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The sum of the other charges at the release level.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PacklistType"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PACKLIST_TYPE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The code indicating the type of packlist to be used for the line. Lines requiring different pack lists are not mixed in a single order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PersonalizeCode"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PERSONALIZE_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The product personalization preference code.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PickListNo"
                                    type="yfctype:DocumentNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PICK_LIST_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The pick list number generated, if using the Sterling Selling and Fulfillment Suite Console to pick, pack, and ship.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PriorityCode"
                                    type="yfctype:Text-20" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRIORITY_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The priority code of the order. External logic can be built using this field for prioritizing orders.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Purpose"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PURPOSE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is passed out to the Tax calculation user exits.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReceivingNode"
                                    type="yfctype:ShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="RECEIVING_NODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The node that should receive the shipment.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReleaseNo"
                                    type="yfctype:ShorterNumber" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="RELEASE_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The release number. Generated as a one up number against that release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReleaseSeqNo"
                                    type="yfctype:ShorterNumber" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="RELEASE_SEQ_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The release sequence number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReqCancelDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQ_CANCEL_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The requested cancel date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReqDeliveryDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQ_DELIVERY_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The requested delivery date.  It indicates the expected delivery date for the release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReqShipDate"
                                    type="yfctype:TimeStamp" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REQ_SHIP_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The requested ship date.  It indicates the expected shipment date for the release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SCAC"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SCAC" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The carrier code.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SalesOrderNo"
                                    type="yfctype:VarcharDocumentNo" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SALES_ORDER_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The sales order that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ScacAndService"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Value of SCAC and Service code.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ScacAndServiceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Link to YFS_SCAC_AND_SERVICE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SellerOrganizationCode"
                                    type="yfctype:OrgCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SELLER_ORGANIZATION_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The code of the seller organization. This must be a valid organization defined in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipAdviceNo"
                                    type="yfctype:Text-50" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_ADVICE_NO" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The ship advice number that uniquely identify the order release.  If the USE_NUMERIC_SHIP_ADVICE_NO rule is set to Y, the value will be a number.  Otherwise, the value will be the concatenated value of OrderNo and ReleaseNo.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipCompleteFlag"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_COMPLETE_FLAG" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether the order must be shipped only if it can be completely fulfilled.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipLineComplete"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_LINE_COMPLETE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether this order should be shipped only if complete lines can be fulfilled.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipNode"
                                    type="yfctype:ShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The ship node that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipNodeClass"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_NODE_CLASS" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipOrderComplete"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_ORDER_COMPLETE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This flag indicates whether the order should be shipped only if it can be completely fulfilled.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipToId"
                                    type="yfctype:ID-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_TO_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The identifier of the customer that this order is being shipped to.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipToKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIP_TO_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The ship to address that is associated with this order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipmentConsolidationGroupId"
                                    type="yfctype:VarcharShipNode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SHIPMENT_CONSOL_GROUP_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>It is a consolidation group id  that is used as a standard do not mix constraint.Releases belonging to same shipment consolidation group may consolidate to same shipment.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Status"
                                    type="yfctype:Status" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Status of the order release.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SupplierCode"
                                    type="yfctype:NText-30" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUPPLIER_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SupplierName"
                                    type="yfctype:Text-100" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="SUPPLIER_NAME" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The name of the supplier who owns the ship node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TaxpayerId"
                                    type="yfctype:ID-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="TAXPAYER_ID" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>The identifier of the taxpayer.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="WorkOrderApptKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="WORK_ORDER_APPT_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Link to the appointment on the associated work order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="WorkOrderKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="WORK_ORDER_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                                    <xsd:documentation>Link to the associated Work Order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="isHistory"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Indicates if data is fetched from the history table rather than the regular table. Valid values are Y, N or Void. Void implies N.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    <xsd:attribute
                                    name="ActualFreightCharge"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACTUAL_FREIGHT_CHARGE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Actual freight charge or the charge for transporting the shipment from the origin  node to the destination node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AmountCollected"
                                    type="yfctype:ComputedMoney" use="required">
                                    <xsd:annotation
                                    yfc:columnName="AMOUNT_COLLECTED" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total amount which has been paid against this invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargedActualFreight"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGED_ACTUAL_FREIGHT" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>If shipping charges were charged on actual, this is set to 'Y'.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectedExternally"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COLLECTED_EXTERNALLY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Flag to indicate if invoice collection is happening externally in A/R system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectedThroughAR"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AMOUNT_PASSED_TO_AR" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Amount which was passed to A/R system for collection. This would typically be same as total amount unless there was some pre-payment on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DateInvoiced"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CREATETS" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Timestamp when the record was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DerivedFromOrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DERIVED_FROM_ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Link to order header from which this Invoice's Order is derived.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DocumentType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The document type. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EnterpriseCode"
                                    type="yfctype:OrgCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The enterprise code. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderCharges"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Header Charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderDiscount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Header discount amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TAX" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Tax amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InvoiceCreationReason"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_CREATION_REASON" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The reason for which the invoice is created. The system populates this field only for adjustment invoices.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InvoiceNo"
                                    type="yfctype:InvoiceNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Invoice number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InvoiceType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_TYPE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Classifies an invoice. For example, Orders, Returns, Debit Memo, Credit Memo, Info etc.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="LineSubTotal"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LINE_SUBTOTAL" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total of extended price for all the lines. This only includes the price * quantity information and excludes line charges and taxes.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MasterInvoiceNo"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MASTER_INVOICE_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Not used internally. Can be updated externally by custom logic to group invoices together.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderInvoiceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Primary key attribute of YFS_ORDER_INVOICE.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OrderNo"
                                    type="yfctype:VarcharDocumentNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The order number. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Reference1"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_1" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Additional reference information.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RemainingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalAmount"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TOTAL_AMOUNT" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Grand total for the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalCharges"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalDiscount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total discount amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalHeaderCharges"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total header charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalRequiredMappingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalTax"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TOTAL_TAX" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total tax amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="isHistory"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Indicates if data is retrieved from the history table or the regular table. 
				<br/>Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="NegativeInvoiceList">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="1" name="OrderInvoice">
                                <xsd:annotation yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Stores order invoices raised against an order.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute
                                    name="ActualFreightCharge"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACTUAL_FREIGHT_CHARGE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Actual freight charge or the charge for transporting the shipment from the origin  node to the destination node.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AmountCollected"
                                    type="yfctype:ComputedMoney" use="required">
                                    <xsd:annotation
                                    yfc:columnName="AMOUNT_COLLECTED" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total amount which has been paid against this invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargedActualFreight"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGED_ACTUAL_FREIGHT" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>If shipping charges were charged on actual, this is set to 'Y'.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectedExternally"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COLLECTED_EXTERNALLY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Flag to indicate if invoice collection is happening externally in A/R system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CollectedThroughAR"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AMOUNT_PASSED_TO_AR" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Amount which was passed to A/R system for collection. This would typically be same as total amount unless there was some pre-payment on the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DateInvoiced"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CREATETS" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Timestamp when the record was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DerivedFromOrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DERIVED_FROM_ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Link to order header from which this Invoice's Order is derived.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DocumentType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The document type. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EnterpriseCode"
                                    type="yfctype:OrgCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The enterprise code. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderCharges"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Header Charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderDiscount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Header discount amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HeaderTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TAX" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Tax amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InvoiceCreationReason"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_CREATION_REASON" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The reason for which the invoice is created. The system populates this field only for adjustment invoices.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InvoiceNo"
                                    type="yfctype:InvoiceNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Invoice number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="InvoiceType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INVOICE_TYPE" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Classifies an invoice. For example, Orders, Returns, Debit Memo, Credit Memo, Info etc.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="LineSubTotal"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LINE_SUBTOTAL" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total of extended price for all the lines. This only includes the price * quantity information and excludes line charges and taxes.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MasterInvoiceNo"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MASTER_INVOICE_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Not used internally. Can be updated externally by custom logic to group invoices together.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrderInvoiceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Primary key attribute of YFS_ORDER_INVOICE.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OrderNo"
                                    type="yfctype:VarcharDocumentNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_NO" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>The order number. This field is a part of an order's unique key. The enterprise code and order number together uniquely identify an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Reference1"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_1" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Additional reference information.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RemainingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalAmount"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TOTAL_AMOUNT" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Grand total for the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalCharges"
                                    type="yfctype:ComputedMoney" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalDiscount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total discount amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalHeaderCharges"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Total header charge amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalRequiredMappingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TotalTax"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TOTAL_TAX" yfc:tableName="YFS_ORDER_INVOICE">
                                    <xsd:documentation>Total tax amount in the invoice.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="isHistory"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Indicates if data is retrieved from the history table or the regular table. 
				<br/>Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="PresettledAmount">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
							PresettledAmount represents the SettledAmount on the order.
		        	</div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="RemainingAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalRequiredMappingAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ChargeTransactionRequestList">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
							This list excludes the ChargeTransactionRequests which are mapped to predistributed invoices.
		        </div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ChargeTransactionRequest">
                                <xsd:annotation yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>This table holds the ORDER_HEADER_KEY and the request id which make a set of unique groups for payment processing on the order.  Authorizations will be split such that each authorization is for only one request.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestId"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRAN_REQUEST_ID" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The unique identifier of the request in combination with the ORDER_HEADER_KEY.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_TRAN_REQUEST_KEY" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The primary key for the YFS_CHARGE_TRAN_REQUEST table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeTransactionRequestSequence"
                                    type="yfctype:PossibleKey" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REQUEST_SEQUENCE" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The order in which the Groups should be authorized.  Records with REQUEST_SEQUENCE NOT NULL will be processed first; NULL Sequences will be ordered by CHARGE_TRAN_REQUEST_ID, ascending.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MaxRequestAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MAX_REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The maximum amount this unique identifier represents before distribution to another group begins to become Authorized.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The OrderHeaderKey that is associated with this charge tran request.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PaymentStatus"
                                    type="yfctype:AuthorizationPaymentStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_STATUS" yfc:tableName="YFS_CHARGE_TRAN_REQUEST">
                                    <xsd:documentation>The Payment Status of the request.  This is computed in the same manner as the Order's Payment Status.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RemainingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TotalRequiredMappingAmount"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="OrderRemainder">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
							The amount to authorize that does not have a ChargeTransactionRequest.  It will be zero if ChargeTransactionRequests are mandatory for authorizations.
		        	</div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="RemainingAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>RemainingAmount is how much is remaining to be needed after the current mapping.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalRequiredMappingAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The total amount to be mapped.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ChargeList"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AuthorizationList"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PaymentMethodSequenceList"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ChargeList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="Charge">
                    <xsd:annotation yfc:tableName="YFS_CHARGE_TRANSACTION">
                        <xsd:documentation>This table stores information about financial transactions associated with order processing. Every action carried out on an order which may have an impact on financial components is logged in this table.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="AsyncRequestIdentifier"
                            type="yfctype:Text-255" use="optional">
                            <xsd:annotation
                                yfc:columnName="ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>For Asynchronous requests only, this field holds the identifier to the transaction closed as asynchronous. This identifier will be used for recording the actual response back in the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuditTransactionID"
                            type="yfctype:TransactionID" use="optional">
                            <xsd:annotation
                                yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="AuthorizationExpirationDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="AUTHORIZATION_EXPIRATION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The date and time until which the authorization specified by the authorization ID is applicable.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthorizationID"
                            type="yfctype:Text-255" use="optional">
                            <xsd:annotation
                                yfc:columnName="AUTHORIZATION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>For authorization requests, this field holds the identifier of the authorization that was made. For charge requests, this field holds the authroization ID to be used for charging the amount.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AvailableToMap"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The amount available to map.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BookAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation yfc:columnName="BOOK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that could be charged to the buyer if the order was fulfilled. At the time of fulfillment, a negative entry is made in this column and positive entry in the DEBIT_AMOUNT column.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CallForAuthorizationStatus"
                            type="yfctype:CallForAuthStatus" use="optional">
                            <xsd:annotation
                                yfc:columnName="CALL_FOR_AUTH_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This column indicates the transaction is valid for a call for authorization and whether or not the authorization code was acquired.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CashBackAmount" use="optional">
                            <xsd:annotation
                                yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount of cashback that has been charged using this transaction. Only valid for transactions of ChargeType CHARGE.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="ChargeTransactionKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="CHARGE_TRANSACTION_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The primary key for the YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ChargeType"
                            type="yfctype:ChargeCategory" use="optional">
                            <xsd:annotation yfc:columnName="CHARGE_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The type of charge that is being applied to the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CollectionDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="COLLECTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The date and time when the charge was realized.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CreditAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREDIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that was actually charged or refunded to the buyer.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DebitAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="DEBIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that should be charged or refunded to the buyer.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DistributedAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="DISTRIBUTED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This field is used internally by the application to keep track of the amount which has been posted in the invoices as "collected".</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExecutionDate" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXECUTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The date and time when the charge request was processed.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:TimeStamp"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="ForAsyncRequestIdentifier"
                            type="yfctype:Text-255" use="optional">
                            <xsd:annotation
                                yfc:columnName="FOR_ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>Reference to AsyncRequestIdentifier, the actual transaction that was closed as async for which the response is now recorded in the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HoldAgainstBook"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="HOLD_AGAINST_BOOK" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This flag indicates whether the request was created for a precharge or a payment method whose payment type has the ChargeInsteadOfAuth flag turned on.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InPerson" use="optional">
                            <xsd:annotation yfc:columnName="IN_PERSON" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This flag indicates that the transaction represents an in person, or card in hand, transaction.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Flag">
                                    <xsd:enumeration value="Y"/>
                                    <xsd:enumeration value="N"/>
                                    <xsd:enumeration value=""/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="OfflineStatus" use="optional">
                            <xsd:annotation
                                yfc:columnName="OFFLINE_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This flag indicates the transaction attempted to reach the payment gateway within a UserExit, but the UserExit was unable to contact the gateway.  This may be updated to N by the user exit, and will be reset to null when the record moves to CLOSED status.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Flag">
                                    <xsd:enumeration value="Y"/>
                                    <xsd:enumeration value="N"/>
                                    <xsd:enumeration value=""/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="OpenAuthorizedAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="OPEN_AUTHORIZED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that is authorized for the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderHeaderKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The order header that is associated with this charge transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderInvoiceKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The key to the invoice that was created for this record, if applicable. Invoices are created for shipments, returns, or debit and credit memos.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentEntryType"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_ENTRY_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>Entry Type used to enter payment information into the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The payment method that is associated with this charge transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PostponedAmount" use="optional">
                            <xsd:annotation
                                yfc:columnName="POSTPONED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The total amount that does not require authorization or payment capture on the order.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="ReasonCode"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="REASON_CODE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This field can be used to store reason code for transaction update.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that is requested for the charge or authorization of record of types "REQUEST_CHARGE" and "REQUEST_AUTHORIZATION". Once the request is processed, the actual amount will be shown as "CreditAmount" or "OpenAuthorizedAmount", for charge and authorization requests respectively.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SettledAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="SETTLED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This field holds the amount for which a pre-settlement (settlement before invoice) is requested. Charge requests will be created for this amount.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Status"
                            type="yfctype:EnumStatus2" use="optional">
                            <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This field indicates the status of this record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TransactionDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="MODIFYTS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>Transaction Date.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TransferFromOhKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="TRANSFER_FROM_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The Order Header Key of the order that funds are being transferred from.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TransferToOhKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="TRANSFER_TO_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The Order Header Key of the order that funds are being transferred to.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="UserExitStatus"
                            type="yfctype:Text-15" use="optional">
                            <xsd:annotation
                                yfc:columnName="USER_EXIT_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The user exit call status of this record. The status will be "INVOKED" if the transaction fails during or after the user exit call, otherwise the value will be blank.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="VoidTransaction" use="optional">
                            <xsd:annotation
                                yfc:columnName="VOID_TRANSACTION" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This flag indicates that the Charge Transaction is either a Void request or submitted for a Void operation.  Valid values for a Void Charge Transaction are V (is a void request), R (is a forced refund request), C (closed error record, already recreated) and null. Valid value for a Charge Transaction that has been requested for Void operation is S (void submitted for this transaction).</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Flag">
                                    <xsd:enumeration value="V"/>
                                    <xsd:enumeration value="R"/>
                                    <xsd:enumeration value="S"/>
                                    <xsd:enumeration value="C"/>
                                    <xsd:enumeration value=""/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AuthorizationList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="Authorization">
                    <xsd:annotation yfc:tableName="YFS_CHARGE_TRANSACTION">
                        <xsd:documentation>This table stores information about financial transactions associated with order processing. Every action carried out on an order which may have an impact on financial components is logged in this table.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="AsyncRequestIdentifier"
                            type="yfctype:Text-255" use="optional">
                            <xsd:annotation
                                yfc:columnName="ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>For Asynchronous requests only, this field holds the identifier to the transaction closed as asynchronous. This identifier will be used for recording the actual response back in the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuditTransactionID"
                            type="yfctype:TransactionID" use="optional">
                            <xsd:annotation
                                yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="AuthorizationExpirationDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="AUTHORIZATION_EXPIRATION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The date and time until which the authorization specified by the authorization ID is applicable.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AuthorizationID"
                            type="yfctype:Text-255" use="optional">
                            <xsd:annotation
                                yfc:columnName="AUTHORIZATION_ID" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>For authorization requests, this field holds the identifier of the authorization that was made. For charge requests, this field holds the authroization ID to be used for charging the amount.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AvailableToMap"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The amount available to map.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BookAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation yfc:columnName="BOOK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that could be charged to the buyer if the order was fulfilled. At the time of fulfillment, a negative entry is made in this column and positive entry in the DEBIT_AMOUNT column.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CallForAuthorizationStatus"
                            type="yfctype:CallForAuthStatus" use="optional">
                            <xsd:annotation
                                yfc:columnName="CALL_FOR_AUTH_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This column indicates the transaction is valid for a call for authorization and whether or not the authorization code was acquired.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CashBackAmount" use="optional">
                            <xsd:annotation
                                yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount of cashback that has been charged using this transaction. Only valid for transactions of ChargeType CHARGE.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="ChargeTransactionKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="CHARGE_TRANSACTION_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The primary key for the YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ChargeType"
                            type="yfctype:ChargeCategory" use="optional">
                            <xsd:annotation yfc:columnName="CHARGE_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The type of charge that is being applied to the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CollectionDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation
                                yfc:columnName="COLLECTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The date and time when the charge was realized.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CreditAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREDIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that was actually charged or refunded to the buyer.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DebitAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="DEBIT_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that should be charged or refunded to the buyer.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DistributedAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="DISTRIBUTED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This field is used internally by the application to keep track of the amount which has been posted in the invoices as "collected".</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExecutionDate" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXECUTION_DATE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The date and time when the charge request was processed.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:TimeStamp"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="ForAsyncRequestIdentifier"
                            type="yfctype:Text-255" use="optional">
                            <xsd:annotation
                                yfc:columnName="FOR_ASYNC_REQUEST_IDENTIFIER" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>Reference to AsyncRequestIdentifier, the actual transaction that was closed as async for which the response is now recorded in the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="HoldAgainstBook"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="HOLD_AGAINST_BOOK" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This flag indicates whether the request was created for a precharge or a payment method whose payment type has the ChargeInsteadOfAuth flag turned on.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="InPerson" use="optional">
                            <xsd:annotation yfc:columnName="IN_PERSON" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This flag indicates that the transaction represents an in person, or card in hand, transaction.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Flag">
                                    <xsd:enumeration value="Y"/>
                                    <xsd:enumeration value="N"/>
                                    <xsd:enumeration value=""/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="OfflineStatus" use="optional">
                            <xsd:annotation
                                yfc:columnName="OFFLINE_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This flag indicates the transaction attempted to reach the payment gateway within a UserExit, but the UserExit was unable to contact the gateway.  This may be updated to N by the user exit, and will be reset to null when the record moves to CLOSED status.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Flag">
                                    <xsd:enumeration value="Y"/>
                                    <xsd:enumeration value="N"/>
                                    <xsd:enumeration value=""/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="OpenAuthorizedAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="OPEN_AUTHORIZED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that is authorized for the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderHeaderKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The order header that is associated with this charge transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderInvoiceKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_INVOICE_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The key to the invoice that was created for this record, if applicable. Invoices are created for shipments, returns, or debit and credit memos.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentEntryType"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_ENTRY_TYPE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>Entry Type used to enter payment information into the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The payment method that is associated with this charge transaction.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PostponedAmount" use="optional">
                            <xsd:annotation
                                yfc:columnName="POSTPONED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The total amount that does not require authorization or payment capture on the order.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:PlusMinusMoney"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="ReasonCode"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="REASON_CODE" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This field can be used to store reason code for transaction update.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQUEST_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The amount that is requested for the charge or authorization of record of types "REQUEST_CHARGE" and "REQUEST_AUTHORIZATION". Once the request is processed, the actual amount will be shown as "CreditAmount" or "OpenAuthorizedAmount", for charge and authorization requests respectively.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SettledAmount"
                            type="yfctype:PlusMinusMoney" use="optional">
                            <xsd:annotation
                                yfc:columnName="SETTLED_AMOUNT" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This field holds the amount for which a pre-settlement (settlement before invoice) is requested. Charge requests will be created for this amount.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Status"
                            type="yfctype:EnumStatus2" use="optional">
                            <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This field indicates the status of this record.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TransactionDate"
                            type="yfctype:TimeStamp" use="optional">
                            <xsd:annotation yfc:columnName="MODIFYTS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>Transaction Date.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TransferFromOhKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="TRANSFER_FROM_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The Order Header Key of the order that funds are being transferred from.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TransferToOhKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="TRANSFER_TO_OH_KEY" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The Order Header Key of the order that funds are being transferred to.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="UserExitStatus"
                            type="yfctype:Text-15" use="optional">
                            <xsd:annotation
                                yfc:columnName="USER_EXIT_STATUS" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>The user exit call status of this record. The status will be "INVOKED" if the transaction fails during or after the user exit call, otherwise the value will be blank.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="VoidTransaction" use="optional">
                            <xsd:annotation
                                yfc:columnName="VOID_TRANSACTION" yfc:tableName="YFS_CHARGE_TRANSACTION">
                                <xsd:documentation>This flag indicates that the Charge Transaction is either a Void request or submitted for a Void operation.  Valid values for a Void Charge Transaction are V (is a void request), R (is a forced refund request), C (closed error record, already recreated) and null. Valid value for a Charge Transaction that has been requested for Void operation is S (void submitted for this transaction).</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Flag">
                                    <xsd:enumeration value="V"/>
                                    <xsd:enumeration value="R"/>
                                    <xsd:enumeration value="S"/>
                                    <xsd:enumeration value="C"/>
                                    <xsd:enumeration value=""/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentMethodSequenceList">
        <xsd:annotation>
            <xsd:documentation>
                <div>
							Payment methods will be grouped by sequences, so this will be a list of lists of payment methods.
		        	</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PaymentMethodSequence"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentMethodSequence">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PaymentMethods"/>
            </xsd:all>
            <xsd:attribute name="SequenceNo" type="yfctype:String" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentMethods">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="PaymentMethod">
                    <xsd:annotation yfc:tableName="YFS_PAYMENT">
                        <xsd:documentation>This table stores details about the customer's payment methods. The key to this table is posted in YFS_CHARGE_TRANSACTION table.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute
                            name="AwaitingAuthInterfaceAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The total authorization amount that has not been
								sent to the payment processing system for processing yet.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="AwaitingChargeInterfaceAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The total charge amount that has not been sent to
								the payment
								processing system for processing yet.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="BillToKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="BILL_TO_KEY" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The bill to address that is associated with this payment. Can be passed if different from the bill to address on the order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CashBackAmount" use="optional">
                            <xsd:annotation
                                yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The amount that is given back as cash for this payment method. This is not supported on a return order.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="ChargeSequence"
                            type="yfctype:ShorterNumber" use="optional">
                            <xsd:annotation
                                yfc:columnName="CHARGE_SEQUENCE" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The sequence in which the payment method is to be used for charging.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CheckNo"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation yfc:columnName="CHEQUE_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The cheque number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CheckReference"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="CHEQUE_REFERENCE" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The cheque reference.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CreditCardExpDate"
                            type="yfctype:CreditCardExpDate" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREDIT_CARD_EXP_DATE" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The expiration date for the credit card.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CreditCardName"
                            type="yfctype:Text-300" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREDIT_CARD_NAME" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The credit card name.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CreditCardNo"
                            type="yfctype:Text-1000" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CREDIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CreditCardType"
                            type="yfctype:Text-32" use="optional">
                            <xsd:annotation
                                yfc:columnName="CREDIT_CARD_TYPE" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The credit card type group.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerAccountNo"
                            type="yfctype:AccountNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_ACCOUNT_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CUSTOMER_ACCOUNT; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CustomerPONo"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The customer PO account number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DebitCardNo"
                            type="yfctype:Text-1000" use="optional">
                            <xsd:annotation
                                yfc:columnName="DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup DEBIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DisplayCreditCardNo"
                            type="yfctype:Text-4" use="optional">
                            <xsd:annotation
                                yfc:columnName="DISPLAY_CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The four digits of the credit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the credit card number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DisplayCustomerAccountNo"
                            type="yfctype:Text-4" use="optional">
                            <xsd:annotation
                                yfc:columnName="DISPLAY_CUST_ACCT_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The four digits of the customer account number that should be displayed.  If not passed, it is defaulted to be the last four digits of the customer account number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DisplayDebitCardNo"
                            type="yfctype:Text-4" use="optional">
                            <xsd:annotation
                                yfc:columnName="DISPLAY_DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The four digits of the debit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the debit card number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DisplayPaymentReference1"
                            type="yfctype:Text-4" use="optional">
                            <xsd:annotation
                                yfc:columnName="DISPLAY_PAYMENT_REF1" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The four digits of the payment reference 1 that should be displayed.  If not passed, it is defaulted to be the last four digits of the payment reference 1.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DisplayPrimaryAccountNo"
                            type="yfctype:Text-4" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The appropriate display value of the primary
								identifier of this PaymentType, depending upon the
								PaymentTypeGroup.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DisplaySvcNo"
                            type="yfctype:Text-4" use="optional">
                            <xsd:annotation
                                yfc:columnName="DISPLAY_SVC_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The four digits of the stored value card that should be displayed.  If not passed, it is defaulted to be the last four digits of the stored value card number.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="FirstName"
                            type="yfctype:Text-300" use="optional">
                            <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The first name of the credit card holder.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="FundsAvailable"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
									The funds available to be charged on this payment method as
									determined by the
									<code>YFSGetFundsAvailableUE</code>
									user exit, if it was invoked. If the order is created for a
									customer and the payment method is a valid customer payment
									method, it indicates the amount available for the customer
									account. If customer account is not maintained internally or if
									the MaxChargeLimit is not set on the customer payment method,
									this value is returned as 0.
								</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute
                            name="GetFundsAvailableUserExitInvoked"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
									Indicates if the
									<code>YFSGetFundsAvailableUE</code>
									user exit was invoked for this payment method.
								</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IncompletePaymentType"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="INCOMPLETE_PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>This flag indicates whether the payment type is incomplete.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="LastName"
                            type="yfctype:Text-300" use="optional">
                            <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The last name of the credit card holder.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="MaxChargeLimit"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="MAX_CHARGE_LIMIT" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The maximum amount that can be charged on this payment method. However, if UnlimitedCharges=Y, this amount will not be considered.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="MiddleName"
                            type="yfctype:Text-300" use="optional">
                            <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The middle name of the credit card holder.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation yfc:columnName="PAYMENT_KEY" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The primary key for the YFS_PAYMENT table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentReference1"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_REFERENCE1" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment reference 1. Additionally, this is the primary account number for payment methods that are defined under the PaymentTypeGroup OTHER; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentReference2"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_REFERENCE2" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment reference 2.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentReference3"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_REFERENCE3" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment reference 3.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentReference4"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_REFERENCE4" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment reference 4. This does not identify the payment method uniquely.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentReference5"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_REFERENCE5" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment reference 5. This does not identify the payment method uniquely.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentReference6"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_REFERENCE6" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment reference 6. This does not identify the payment method uniquely.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentReference7"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_REFERENCE7" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment reference 7. This does not identify the payment method uniquely.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentReference8"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_REFERENCE8" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment reference 8. This does not identify the payment method uniquely.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentReference9"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_REFERENCE9" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment reference 9. This does not identify the payment method uniquely.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentTemplateType"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_TEMPLATE_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                                <xsd:documentation>This field indicates User Interfaces to use the template required to capture Payment Methods of this Payment Type.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentType"
                            type="yfctype:Text-20" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The payment type of this payment method.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PaymentTypeGroup"
                            type="yfctype:CodeValue" use="optional">
                            <xsd:annotation
                                yfc:columnName="PAYMENT_TYPE_GROUP" yfc:tableName="YFS_PAYMENT_TYPE">
                                <xsd:documentation>This filed indicates the payment type group, which will determine behavior.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PlannedRefundAmount" use="optional">
                            <xsd:annotation
                                yfc:columnName="PLANNED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The amount that will be refunded on this payment method. This field is set for only Return Orders indicating the refund amount.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="PrimaryAccountNo"
                            type="yfctype:Text-1000" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>The appropriate primary identifier of this
								PaymentType, depending upon the PaymentTypeGroup.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReasonCode"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation yfc:columnName="REASON_CODE" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>Reason code for last update.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestedAuthAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQUESTED_AUTH_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The total amount of authorizations sent to an
								external
								system and still
								awaiting response on this payment
								method.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestedChargeAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQUESTED_CHARGE_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The total amount of charges sent to an external
								system and still
								awaiting response on this payment method.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestedRefundAmount" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQUESTED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The total amount of refunds sent to an external
								system and still
								awaiting response on this payment method. This
								number will be
								positive. This total will no longer be included in
								the
								RequestedChargeAmount, but if an asynchronous refund is
								submitted
								in excess of this amount, the RequestedChargeAmount
								will
								be
								reduced for backwards compatibility.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="SuspendAnyMoreCharges"
                            type="yfctype:Text-1" use="optional">
                            <xsd:annotation
                                yfc:columnName="SUSPEND_ANY_MORE_CHARGES" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>Sets whether or not this payment method has been suspended. If the value is Y, then the PaymentMethod is suspended for charging, although refunds can still be made to this PaymentMethod. All open AUTHORIZATION and CHARGE records for positive amounts for this payment method are closed.  If the value is B then both charge and refund are suspended and all open AUTHORIZATION or CHARGE records are closed.  Default: N.  Valid Values: Y, B, or N.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SuspendExpiredAuthReversal"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="SUSPEND_EXP_AUTH_REVERSAL" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>This flag indicates whether or not the reversal of expired authorization has to be initiated for a payment method that has been suspended. If the value is set to Y, then the authorization reversal are not created even though Authorization Reversal Strategy is configured for the payment type. Please note the flag comes into action only when SuspendAnyMoreCharges is either set to Y or B. Valid Values are N or Y (Null value behaves similar to setting it to N ).</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SvcNo"
                            type="yfctype:Text-1000" use="optional">
                            <xsd:annotation yfc:columnName="SVC_NO" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup STORED_VALUE_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalAltRefundedAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_ALT_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The amount that has been refunded to a new payment method based on this payment's payment type refund preferences.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalAuthorized"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_AUTHORIZED" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The total amount auhorized on this payment method.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalCharged"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_CHARGED" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The total amount charged on this payment method.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="TotalRefundedAmount"
                            type="yfctype:Money" use="optional">
                            <xsd:annotation
                                yfc:columnName="TOTAL_REFUNDED_AMOUNT" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>The total refunded amount on this payment method.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="UnlimitedCharges"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="UNLIMITED_CHARGES" yfc:tableName="YFS_PAYMENT">
                                <xsd:documentation>Indicates if there is a limit to the amount that can be charged on the payment method. If not passed, defaults to Y.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
