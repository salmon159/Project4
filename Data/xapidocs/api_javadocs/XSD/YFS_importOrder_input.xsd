            </xsd:attribute>
            <xsd:attribute name="ReqCancelDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="REQ_CANCEL_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>The requested cancel date. This is defaulted from Order if it is not provided in the input XML.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReqDeliveryDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="REQ_DELIVERY_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>The requested delivery date.  It indicates the expected delivery date for the release.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReqShipDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="REQ_SHIP_DATE" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>The requested ship date.  It indicates the expected shipment date for the release.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SCAC" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>The carrier code.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceLineKey" type="yfctype:String" use="optional"/>
            <xsd:attribute name="ShipCompleteFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SHIP_COMPLETE_FLAG" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>This flag indicates whether the order must be shipped only if it can be completely fulfilled.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipLineComplete" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SHIP_LINE_COMPLETE" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>This flag indicates whether this order should be shipped only if complete lines can be fulfilled.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>The ship node that is associated with this order release.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipNodeClass" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SHIP_NODE_CLASS" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipOrderComplete" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SHIP_ORDER_COMPLETE" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>This flag indicates whether the order should be shipped only if it can be completely fulfilled.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipToId" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TO_ID" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>The identifier of the customer that this order is being shipped to.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierCode" type="yfctype:NText-30" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_CODE" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierName" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_NAME" yfc:tableName="YFS_ORDER_RELEASE">
                    <xsd:documentation>The name of the supplier who owns the ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineRelationships">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderLineRelationship"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineRelationship">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_RELATIONSHIP">
            <xsd:documentation>
                <div>
                    <code>OrderLineRelationship</code> contains the <code>ParentLine</code> and <code>ChildLine</code> elements which identify the Parent and Child line. Both <code>ParentLine</code> and <code>ChildLine</code> elements must be passed to uniquely identify the lines. If multiple OrderLineRelationships need to be made, multiple <code>OrderLineRelationship</code> nodes should be passed. Refer to <code>ParentLine</code> and <code>ChildLine</code> elements for further details.
			</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ParentLine"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ChildLine"/>
            </xsd:all>
            <xsd:attribute name="RelationshipType"
                type="yfctype:Text-40" use="required">
                <xsd:annotation yfc:columnName="RELATIONSHIP_TYPE" yfc:tableName="YFS_ORDER_LINE_RELATIONSHIP">
                    <xsd:documentation>
                        <div>
						The type of relationship between the parent and the child line.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ParentLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
				Specifies the parent and the child line being related. When a new relationship is created by passing the <code>OrderLineRelationship</code> the following validation is performed.
				<Validation>
                        <Condition> If either the parent line or the child line is not passed with the appropriate line identifiers the following error is displayed.
					</Condition>
                        <Error
                            Description="Invalid Line Passed in Relationship" ErrorCode="OMP79_0024"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY"
                    yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Primary key generated by the system. May be passed to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i>
                            <br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code> can be passed.
				<Validation>
                                <Condition>
                                    <code>OrderLineKey</code> must identify an existing OrderLine.
					</Condition>
                                <Error
                                    Description="Invalid Line Passed in Relationship" ErrorCode="OMP79_0024"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO"
                    yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				The order line number. Must be passed with the <code>SubLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
				<Validation>
                                <Condition>Must pass one of the following:
					<br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code>
                                    <br/>
                                    <i>or</i>
                                    <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code>.
					</Condition>
                                <Error Description="" ErrorCode=""/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO"
                    yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Must be passed with the <code>PrimeLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
				<Validation>
                                <Condition>Must pass one of the following:
					<br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code>
                                    <br/>
                                    <i>or</i>
                                    <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code>.
					</Condition>
                                <Error Description="" ErrorCode=" "/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
				A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in Line Associations. This value is not stored in the database. While creating a new line and associating it with another line, the same attribute must be passed in the <code>OrderLine</code> element and in this element to uniquely identify a line if <code>OrderLineKey</code> or <code>PrimeLineNo</code> and <code>SubLineNo</code> are not present.
				<Validation>
                                <Condition>Must pass one of the following:
					<br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code>
                                    <br/>
                                    <i>or</i>
                                    <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code>.
					</Condition>
                                <Error Description="" ErrorCode=" "/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ChildLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY"
                    yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Primary key generated by the system.
				<Validation>
                                <Condition>
                                    <code>OrderLineKey</code> must identify an existing <code>OrderLine</code>.
					</Condition>
                                <Error
                                    Description="Invalid Line Passed in Relationship" ErrorCode="OMP79_0024"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO"
                    yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				The order line number. Part of an order line's unique key.
				<br/>Must pass one of the following :
					<br/>The <code>PrimeLineNo</code>  and the <code>SubLineNo</code>
                            <i>or</i>
                            <code>OrderLineKey</code>
                            <i>or</i>
                            <code>TransactionalLineId</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO"
                    yfc:isConditional="Y" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
				Part of unique key of line.
					<br/>Must pass one of the following :
					<br/>The <code>PrimeLineNo</code>  and the <code>SubLineNo</code>
                            <i>or</i>
                            <code>OrderLineKey</code>
                            <i>or</i>
                            <code>TransactionalLineId</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
				A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in Line Associations. This value is not stored in the database. While creating a new line and associating it with another line, the same attribute must be passed in the <code>OrderLine</code> element as well in this element to uniquely identify a line if <code>OrderLineKey</code> or <code>PrimeLineNo</code> and <code>SubLineNo</code> are not present.
					<br/>Must pass one of the following :
					<br/>The <code>PrimeLineNo</code>  and the <code>SubLineNo</code>
                            <i>or</i>
                            <code>OrderLineKey</code>
                            <i>or</i>
                            <code>TransactionalLineId</code>.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLines">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" name="OrderLine">
                    <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
                        <xsd:documentation>
                            <div>
					Stores order line information, including item details and shipping information. Multiple lines can be created for a single order (YFS_ORDER_HEADER). Either <code>PipelineKey</code> or both <code>PipelineId</code> and <code>PipelineOwnerKey</code> must be provided.
						<Validation>
                                    <Condition>
							If either PipelineKey or both PipelineId and PipelineOwnerKey are missing from the input or have a void value:
							</Condition>
                                    <Error
                                    Description="Missing attribute in input" ErrorCode="YFS10460"/>
                                </Validation>
                                <Validation>
                                    <Condition>
							If PipelineKey is passed in the input, it is used as passed, without validating if the pipeline actually exists in the system. If PipelineKey is erroneous, there can be problems in further processing. However, this API will not perform any validations. PipelineKey is also stored in the ORDER_RELEASE_STATUS records of the line.
							</Condition>
                                </Validation>
                                <Validation>
                                    <Condition>
							If both PipelineId and PipelineOwnerKey are passed in the input, instead of PipelineKey, the API fetches the corresponding Pipeline object and obtains the PipelineKey. If the API does not find the pipeline:
							</Condition>
                                    <Error
                                    Description="YFS:No Record Found" ErrorCode="YFS10001"/>
                                </Validation>
                            </div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="1" ref="yfc:Item"/>
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineInvAttRequest"/>
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:KitLines"/>
                            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:LinePriceInfo"/>
                            <xsd:element maxOccurs="1" minOccurs="1" name="PersonInfoShipTo">
                                <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>
                                    <div>
						A generic table used to store relevant person infomation and addresses. Addresses are typically an organization address, ship node address, customer address, and so forth. People information is required in cases where person to person contact needs to be established (for example, taking appointments, or speaking during negotiations). The key of this table is posted in various tables to allow access to person and address information.<br>
						At least one attribute in this element must have a non-void value.</br>
						In addition, for <code>Order/OrderLines/OrderLine/PersonInfoShipTo</code>, this element must be present in both the order header and order line.
							<Validation>
                                    <Condition>
								If all attributes in element have a void value:
								</Condition>
                                    <Error
                                    Description="YFS: Blank Person Info" ErrorCode="YFS10391"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
								For Order/OrderLines/OrderLine/PersonInfoShipTo, if element absent in order header and order line:
								</Condition>
                                    <Error
                                    Description="YFS: Ship To Address not Found" ErrorCode="YFS10069"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="AddressID"
                                    type="yfctype:Text-256" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>A user specified label for an address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine1"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine2"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine3"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine4"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine5"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AddressLine6"
                                    type="yfctype:Address" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AlternateEmailID"
                                    type="yfctype:EMailID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Beeper"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="City"
                                    type="yfctype:City" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The city in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Company"
                                    type="yfctype:Company" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Country"
                                    type="yfctype:Country" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The country in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DayFaxNo"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DayPhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Department"
                                    type="yfctype:Department" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EMailID"
                                    type="yfctype:EMailID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EveningFaxNo"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="EveningPhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="FirstName"
                                    type="yfctype:PersonName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The first name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="HttpUrl"
                                    type="yfctype:URL" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="HTTP_URL" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The URL of the person or organization referred to in this contact information.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsCommercialAddress"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="JobTitle"
                                    type="yfctype:Title-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The job title of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="LastName"
                                    type="yfctype:PersonName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The last name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="MiddleName"
                                    type="yfctype:MiddleName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="MobilePhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OtherPhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
