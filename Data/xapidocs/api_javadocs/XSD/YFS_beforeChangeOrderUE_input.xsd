                                    type="yfctype:PersonName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The last name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MiddleName"
                                    type="yfctype:MiddleName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MobilePhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OtherPhone"
                                    type="yfctype:PhoneNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PersonID"
                                    type="yfctype:PersonId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="State"
                                    type="yfctype:State" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The state in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Suffix"
                                    type="yfctype:Suffix" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TaxGeoCode"
                                    type="yfctype:GeoCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Title"
                                    type="yfctype:Title-10" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ZipCode"
                                    type="yfctype:ZipCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute name="AddressType"
                                    type="yfctype:AddressType" use="required">
                                    <xsd:annotation
                                    yfc:columnName="ADDRESS_TYPE" yfc:tableName="YFS_ENTITY_ADDRESS">
                                    <xsd:documentation>Identifies the type of address. Part of unique key</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Schedules"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Dependency"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="OrderDates">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="OrderDate">
                                <xsd:annotation yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>This table stores custom and milestone dates for orders.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="ActualDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ACTUAL_DATE" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>Actual Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="CommittedDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COMMITTED_DATE" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>It is a data attribute to store committed dates from supplier.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DateTypeId"
                                    type="yfctype:ID-40" use="required">
                                    <xsd:annotation
                                    yfc:columnName="DATE_TYPE_ID" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>The unique identifier for a date type for which additional dates are stored.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ExpectedDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXPECTED_DATE" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>Expected Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="RequestedDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REQUESTED_DATE" yfc:tableName="YFS_ORDER_DATE">
                                    <xsd:documentation>Requested Date.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineInvAttRequest"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ProductItems"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="Awards">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="Award">
                                <xsd:annotation yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>
                                    <div>
					Awards that are created out of the promotions. A single promotion can result in to multiple awards. AwardId must be passed to identify the award.
			<Validation>
                                    <Condition>PromotionId must be passed and it must be a valid one to add the award</Condition>
                                    <Error
                                    Description="All awards must be associated to a promotion on the order" ErrorCode="OMP77_10003"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="PromotionReferences">
                                    <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="PromotionReference">
                                    <xsd:annotation yfc:tableName="YFS_PROMOTION_REFERENCE">
                                    <xsd:documentation>This table maintains references against applied promotion on order or order line. For example a unique Coupon barcode.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="IsUnique"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="IS_UNIQUE" yfc:tableName="YFS_PROMOTION_REFERENCE">
                                    <xsd:documentation>Specifies whether the reference is unique or not.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReferenceId"
                                    type="yfctype:VARCHAR2-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_ID" yfc:tableName="YFS_PROMOTION_REFERENCE">
                                    <xsd:documentation>Reference value within a particular promotion. For example a unique coupon barcode.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReferenceSequence" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_SEQUENCE" yfc:tableName="YFS_PROMOTION_REFERENCE">
                                    <xsd:documentation>Sequence number for each reference entry.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PrecisionDigits"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    <xsd:attribute name="Action"
                                    type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Action to perform on this <code>Award</code>.
					<br/>Default: <code>Null</code> - if not passed awards will be added if not exist or modified.
					<br/>Valid Values: 
						<br/>
                                    <b>
                                    <code>REMOVE</code>
                                    </b> - Removes the award.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardAmount"
                                    type="yfctype:ExternalAmount" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_AMOUNT" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The amount of discount or charge that was applied as a part of this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardApplied"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_APPLIED" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>A flag indicating if the award is applied. When ValidateReferences=Y the value can be 'P' meaning the award was partially applied</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardId"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_ID" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>A unique identifier for the award. If pricing is maintained within the system, this will be the PricingRuleName of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardQuantity" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_QUANTITY" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>Indicates the remaining free gift quantity available for the promotion.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Quantity"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardType"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_TYPE" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The type of award. If pricing is maintained within the system, this will be the RuleCategory of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeCategory"
                                    type="yfctype:ChargeCategory" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The charge category of the LineCharge or HeaderCharge for which this award was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeName"
                                    type="yfctype:ChargeName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The charge name of the LineCharge or HeaderCharge for which this award was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DenialReason"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DENIAL_REASON" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>If the award is denied, the reason for the denial.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Description"
                                    type="yfctype:Text-2000" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DESCRIPTION" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>A description for the award. If pricing is maintained within the system, this will be the Description of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsFreeGiftAward"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_FREE_GIFT_AWARD" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>Indicates that the award is associated with a free gift rule.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PosReasonCode"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="POS_REASON_CODE" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>Reason this promotion was applied.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionId"
                                    type="yfctype:PromoCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_ID" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>This identifies the promotion that is added to this order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="Promotions">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="Promotion">
                                <xsd:annotation yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>
                                    <div>
						Promotions that are associated to an order or any of its lines. PromotionId must be passed to identify the promotion.
						</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Action"
                                    type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
						Action to perform on this promotion.
						<br/>Default: <code>Null</code> - if not passed, promotion will be added if it doesn't exist or modified if it does exist.
						<br/>Valid Values: 
							<br/>
                                    <b>
                                    <code>CREATE</code>
                                    </b> - Add the specified promotion to the order.
							<br/>
                                    <b>
                                    <code>REMOVE</code>
                                    </b> - Removes the specified promotion from the order.
						<Validation>
                                    <Condition>If an award record is associated with this promotion, it may not be removed.</Condition>
                                    <Error
                                    Description="You are attempting to delete a Promotion associated with an award on the order." ErrorCode="OMP77_10004"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>If a promotion already exists, can't add the same promotion twice.</Condition>
                                    <Error
                                    Description="Record already exists in the database." ErrorCode="YFC0001"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DenialReason"
                                    type="yfctype:Text-200" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DENIAL_REASON" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>If the promotion is denied, the reason for the denial.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ExclusivityId"
                                    type="yfctype:VARCHAR2-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXCLUSIVITY_ID" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>ExclusivityID is used to group set of Promotions for mutual exclusivity. Exclusivity Type settings are applicable among Pricing Rules having same ExclusvityID.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ExclusivityType"
                                    type="yfctype:VARCHAR2-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXCLUSIVITY_TYPE" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>Exclusivity Type of the Promotion. Valid Values are: ALL, RULE_TYPE_ITEM, RULE_TYPE_ORDER, ONE_RULE_WITHIN_ID and GLOBAL_RULE</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="IsExternal"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_EXTERNAL" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>This flag specifies whether the promotion is external.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OverrideAdjustmentValue" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OVERRIDE_ADJUSTMENT_VALUE" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>For promotions, the overridden Adjustment value used in place of the configured Adjustment of the corresponding Pricing Rule Action.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Price"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OverrideMaxAdjustment" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OVERRIDE_MAX_ADJUSTMENT" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>
                                    <div> For Coupon promotions, the overridden maximum adjustment value for the coupon pricing rule. 
						</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="Priority" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRIORITY" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>This attribute is used to prioritize Promotion among a list of applicable Promotions with same Exclusivity settings. Higher the value of this attribute, higher the priority of Promotion, to be applied to an order/item.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:NUMBER-10"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PromotionApplied"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_APPLIED" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>A flag indicating if the promotion is applied. When ValidateReferences=Y the value can be 'P' meaning the promotion was partially applied.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PromotionCategory"
                                    type="yfctype:VarcharID-24" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_CATEGORY" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>Specifies category of external promotion. Valid values are ORDER,ITEM and SHIPPING</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionId"
                                    type="yfctype:PromoCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_ID" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>This identifies the promotion that is added to this order. This will be used as the CouponID when determining the price of the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionType"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_TYPE" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>The type of promotion. Like DISCOUNT, BOGO.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ValidateReferences"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>For Coupon promotions, if <code>'Y"</code> the references will be validated and only valid references will be used</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="Reset" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>Determines whether all Promotions, except those listed below, 
					should be removed.</div>
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Flag">
                                    <xsd:enumeration value="N"/>
                                    <xsd:enumeration value="Y"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="Notes">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="Note">
                                <xsd:annotation yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Generic table used to stores notes associated with various entities in the system.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute
                                    name="AuditTransactionId"
                                    type="yfctype:TransactionID" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AUDIT_TRANSACTION_ID" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The identifier of the order modification audit that caused this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ContactReference"
                                    type="yfctype:NotesContactReference" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CONTACT_REFERENCE" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>If the notes were entered because of customer contact by phone, the phone no can be stored in this column. If the contact is by email, email ID can be stored here.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactTime"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CONTACT_TIME" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The time at which the customer contact occurred.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactType"
                                    type="yfctype:CommonCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CONTACT_TYPE" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The contact type like Phone call, email etc. The values will be selected from common code of code type "CONTACT_TYPE"</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ContactUser"
                                    type="yfctype:UserId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CONTACT_USER" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The user who made the contact. The default value will the logged in user.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CustomerSatIndicator"
                                    type="yfctype:CustomerSatisfactionLevel" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CUSTOMER_SAT_INDICATOR" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The customer satisfaction level.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="NoteText"
                                    type="yfctype:NoteText" use="required">
                                    <xsd:annotation
                                    yfc:columnName="NOTE_TEXT" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Detailed Notes.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Operation"
                                    type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
                                    <p>Operation to perform on this record. Default: If not passed then API will try to either create or modify depending on the existence of the data.</p>
                                    <p>Valid Values: </p>
                                    <p>
                                    <b>
                                    <code>Create</code>
                                    </b> - Creates a new record.</p>
                                    <p>
                                    <b>
                                    <code>Modify</code>
                                    </b> - Change values in an existing record.</p>
                                    <p>
                                    <b>
                                    <code>Delete</code>
                                    </b> - Deletes an existing record.</p>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Priority"
                                    type="yfctype:PriorityWithDecimal" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRIORITY" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Priority of the note. 1 is High, 0 is Low.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ReasonCode"
                                    type="yfctype:ReasonCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REASON_CODE" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The reason for which the notes are logged. Like "CustomerContact" etc. The values will be selected from common code of code type "NOTES_REASON"</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="SequenceNo"
                                    type="yfctype:Count" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SEQUENCE_NO" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>The sequence of the note.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Tranid"
                                    type="yfctype:TranId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TRANID" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>Transaction for which these notes are applicable.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="VisibleToAll"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="VISIBLE_TO_ALL" yfc:tableName="YFS_NOTES">
                                    <xsd:documentation>This flag determines whether the note applied should be visible to all users. This field is used for filtering. No logic is currently applied to it.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:BundleParentLine"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="OrderHoldTypes">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
                                <br/> This element cannot be passed for orders with <code>pending changes</code>.
             </div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="OrderHoldType">
                                <xsd:annotation yfc:tableName="YFS_ORDER_HOLD_TYPE">
                                    <xsd:documentation>
                                    <div>
			Optional. Stores hold type information. Multiple <code>HoldType</code>s can be passed for a single order. These hold types will be applied in addition to hold types configured to be applied during create order.
			</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="HoldType"
                                    type="yfctype:CodeType" use="required">
                                    <xsd:annotation
                                    yfc:columnName="HOLD_TYPE" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                                    <xsd:documentation>
                                    <div>
					The code identifying the hold type to be applied on Order.
					<Validation>
                                    <Condition>
                                    <code>HoldType</code> must be defined in the configuration
						</Condition>
                                    <Error
                                    Description="Hold Type is not setup." ErrorCode="OMP11035"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ReasonText"
                                    type="yfctype:ReasonText" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REASON_TEXT" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                                    <xsd:documentation>The reason text associates to the order hold type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Status"
                                    type="yfctype:ShortStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="STATUS" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                                    <xsd:documentation>Status of the order hold type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineReservations"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderOverride"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AdditionalLinePrices"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="CustomAttributes">
                    <xsd:annotation yfc:tableName="YFS_ORDER_LINE_EXTENSION">
                        <xsd:documentation>This table stores dynamic extension attributes of order line entity.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType/>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLinePriceMatch"/>
            </xsd:all>
            <xsd:attribute name="Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						Action to perform on this <code>OrderLine</code>.
						<br/>Default: <code>Null</code> - if not passed, the <code>OrderLine</code> will be created if it doesn't exist or updated if it does exist.
                                <br/>Valid Values:
						<br/>
                            <b>
                                <code>CANCEL</code>
                            </b> - Tries to cancel the <code>OrderLine</code> if cancellable. If the quantity of a Service or Delivery line is reduce to zero by cancellation, the associations for that line are also removed. If the quantity is partially reduced, the association is not be removed. Not a valid action for draft orders.
                                For master orders, please refer to the <code>cancelMasterOrder</code> API for more detials.
						<br/>
                            <b>
                                <code>REMOVE</code>
                            </b> - Removes the entire <code>OrderLine</code>. This is allowed only for draft orders.
						<br/>
                            <b>
                                <code>CREATE</code>
                            </b> - Adds a new line.
						<br/>
                            <b>
                                <code>MODIFY</code>
                            </b> - Changes a line.
						<br/>
                            <b>
                                <code>REPLACE_BUNDLE</code>
                            </b> - This action should be passed only for existing bundle parent order lines.
					<Validation>
                                <Condition>
						Action REPLACE_BUNDLE is passed for order lines not having kit code as <code>BUNDLE</code>
                                </Condition>
                                <Error
                                    Description="Action REPLACE_BUNDLE can be passed only for bundle parent order lines." ErrorCode="OMP80_0042"/>
                            </Validation>
                            <Validation>
                                <Condition>
						Action REPLACE_BUNDLE is passed for new order lines
						</Condition>
                                <Error
                                    Description="Action REPLACE_BUNDLE can be passed only for existing bundle parent order lines." ErrorCode="OMP80_0043"/>
                            </Validation>
                            <Validation>
                                <Condition>
										Action Cancel is not allowed in pending mode when other pending changes exist.
									</Condition>
                                <Error
                                    Description="Cancellation is not allowed in pending mode when other pending changes exist." ErrorCode="YFS85_0020"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Action">
                        <xsd:enumeration value="CREATE"/>
                        <xsd:enumeration value="MODIFY"/>
                        <xsd:enumeration value="CANCEL"/>
                        <xsd:enumeration value="REMOVE"/>
                        <xsd:enumeration value="REPLACE_BUNDLE"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="AllocationDate" type="yfctype:Date" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                            The date when this line will be allocated. This is calculated at order creation time, based on the node processing time. This field is not used from version 50 onwards. The <code>ALLOCATION_LEAD_TIME</code> field is used instead.
                        </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BOMXML" type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The BOM XML to be used to create the order line and its components.
							<Validation>
                                <Condition>
                                    <code>BOMXML</code> is passed without passing Action as <code>REPLACE_BUNDLE</code>
                                </Condition>
                                <Error
                                    Description="Action REPLACE_BUNDLE required to when passing a new BOM XML." ErrorCode="OMP85_0060"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierAccountNo"
                type="yfctype:AccountNo" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_ACCOUNT_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The customers carrier account number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierServiceCode"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The carrier service code for this order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CommittedQuantity" use="optional">
                <xsd:annotation yfc:columnName="COMMITTED_QUANTITY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>It is a data attribute to store the quantity that is committed by supplier.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Number"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ConditionVariable1"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CONDITION_VARIABLE_1" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>A user defined variable that can be used for condition building in process modeling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConditionVariable2"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CONDITION_VARIABLE_2" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>A user defined variable that can be used for condition building in process modeling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConfigurationKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BOM_CONFIG_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is used by Sterling Multi-Channel Selling Solution so that Visual Modeler can be opened with existing options.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerLinePONo"
                type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_PO_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The buyer PO line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerPONo" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The buyer's PO number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeliveryCode"
                type="yfctype:DeliveryCode" use="optional">
                <xsd:annotation yfc:columnName="DELIVERY_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates who will pay the freight. Application understands values such as SUPPLIER, ENTERPRISE and MARKETPLACE to mean that each of these organizations will pay for the freight. You could however pass any other value which is a valid shipment charge type as understood by the carrier by which the shipment is transported. See YFS_SHIPMENT_CONTAINER.carrier_payment_type for additional information.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeliveryMethod" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DELIVERY_METHOD" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					Describes how product is delivered. Only valid for Product line having <code>ItemGroupCode</code> value of <b>
                                <code>PROD</code>
                            </b>.
					<br/>Valid Values:
					<br/>
                            <b>
                                <code>DEL</code>
                            </b> - Delivery
					<br/>
                            <b>
                                <code>SHP</code>
                            </b>- Ship
					<br/>
                            <b>
                                <code>CARRY</code>
                            </b>- Cash And Carry
					<br/>
                            <b>
                                <code>PICK</code>
                            </b>- Pickup by the user.
						<br/>Orderlines having Cash And Carry delivery method are ignored while scheduling the order.
						<br/>if <code>DeliveryMethod</code> is being changed from a delivery method that requires immediate settlement to one that does not (for example, from CARRY to SHIP), then <code>SettledQuantity</code> will be set to zero.
					<Validation>
                                <Condition>If <code>DeliveryMethod</code> is changed for an existing line, it must be a valid value.
						</Condition>
                                <Error
                                    Description="Invalid Delivery Method Passed" ErrorCode="OMP10020"/>
                            </Validation>
                            <br/>The DeliveryMethod cannot be modified if the <code>OrderLine</code> has been scheduled. If changing <code>DeliveryMethod</code> from <b>
                                <code>DEL</code>
                            </b> to a new value, there should be no association present with a delivery line.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DepartmentCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						This field can be used for line classification and reporting purposes. If department code is passed at order header but not passed at order line then value at order header is pushed to order line. If department code is changed at order header and previous value is same as value with Order line then new value of order header is pushed to order line. If department code is changed at order line and previous value is same as value with shipment line then new value of order line is pushed to shipment line.

						 </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DependencyShippingRule"
                type="yfctype:Text-2" use="optional">
                <xsd:annotation
                    yfc:columnName="DEPENDENCY_SHIPPING_RULE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
							Indicates how the <code>OrderLines</code> are dependent on the parent line. The <code>DependencyShippingRule</code> can only be specified for the parent line.
							<br/> If a <code>DependencyShippingRule</code> is specified on the parent line, it is defaulted to the same value on all the dependent lines. If a <code>DependencyShippingRule</code> is passed on any of the dependent lines, the value passed is ignored.
							<br/>Default: " "  (Null)
							<br/>Valid values: <b>
                                <code>01</code>
                            </b> - Ship together,<b>
                                <code>02</code>
                            </b> - Deliver together
							<br/>
                            <br/>If a <code>DependencyShippingRule</code> value is <b>
                                <code>02</code>
                            </b>, a <code>MergeNode</code> must be specified. Order release created for these lines have a <code>MergeNode</code> value, so shipments are  first made to the <code>MergeNode</code>, where they are consolidated for final shipment.
							<br/>
                            <br/>The <code>DependencyShippingRule</code> can only be specified for Product line. A Product line associated with a delivery line cannot be a part of any dependency shipping rule.
							 <Validation>
                                <Condition>If a <code>DependencyShippingRule</code> is passed for Delivery or Provided Service Line or an associated Product Line.
								 </Condition>
                                <Error
                                    Description="A Product Line with Dependency Shipping Rule cannot be associated with Delivery Service Line." ErrorCode="OMP10041"/>
                                <Error
                                    Description="Service Line cannot have Dependency Shipping Rule" ErrorCode="OMP10058"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>MergeNode</code> is not specified for parent line, and  <code>DependencyShippingRule</code> value is <b>
                                    <code>02</code>
                                    </b>.
								 </Condition>
                                <Error
                                    Description="Merge node is required for shipping rule deliver together" ErrorCode="YFS10443"/>
                            </Validation>
                            <Validation>
                                <Condition>Invalid value is passed.
								 </Condition>
                                <Error
                                    Description="Invalid dependency shipping rule" ErrorCode="YFS10450"/>
                            </Validation>
                            <br/>
                            <br/>Use a <code>ModificationType</code> of <code>OTHERS</code> to change this field.
								<br/>
                            <br/>If a <code>DependencyShippingRule</code> is being modified for a line that has dependent-lines associated with it, then the <code>DependencyShippingRule</code> on each of the dependent lines is also changed.
								<br/>If the <code>DependencyShippingRule</code> is being modified for a dependent line. the change is ignored. The child line retains the parent line's value.
								<br/>
                            <br/>Changing the <code>DependencyShippingRule</code> value  does not reset existing dependency attributes from any of the lines. For example, changing the <code>DependencyShippingRule</code> value from <b>
                                <code>02</code>
                            </b> to <b>
                                <code>01</code>
                            </b> does not remove the <code>MergeNode</code>  from any of the lines. Originally, the <code>MergeNode</code> was defaulted from the parent line.
								<br/>
                            <br/>Any <code>OrderLine</code> that is a part of an association cannot specify a <code>DependencytShippingRule</code>.
								 <br/>

                            <br/>Based on the <code>DependencyShippingRule</code>, some of attributes on dependent lines are defaulted from the parent line.  If the child line has a different value from the parent line, the child line's value is ignored, and the parent lines value is used.
							 <div
                                class="innerpanel" width="30%">
                                <table border="0" cellpadding="0"
                                    cellspacing="0" class="table">
                                    <tbody>
                                    <tr>
                                    <td class="tablecolumn">
                                    <b>Value</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ShippingRule</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ShipNode</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ReceivingNode</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ShipTo</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>MergeNode</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>RequiredShipDate</b>
                                    <br/>
                                    <b>RequiredDeliveryDate</b>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">01</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn"/>
                                    <td class="tablecolumn"/>
                                    <td class="tablecolumn"/>
                                    <td class="tablecolumn">defaults</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">02</td>
                                    <td class="tablecolumn"/>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">mandatory</td>
                                    <td class="tablecolumn"/>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DependentOnLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="DEPENDENT_ON_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order line key that this order line is associated with. This field will indicate which order line is the parent of the the order line for line dependencies.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DistributionRuleId"
                type="yfctype:DistributionRuleId" use="optional">
                <xsd:annotation yfc:columnName="DISTRIBUTION_RULE_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>the identifier of the distribution group that is used to determine the ship node(s) that will ship the order line. If not proviFded, the seller organization's default distribution rule ID is used. If provided, it must be a valid ID for the Seller organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EarliestScheduleDate" use="optional">
                <xsd:annotation yfc:columnName="EARLIEST_SCHEDULE_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The date before which the order line will not be picked up by scheduling. This is not supported on a return order.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FillQuantity" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="FILL_QUANTITY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The minimum order line quantity that must be fulfilled in an order release before it is shipped.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstIterationSeqNo" use="optional">
                <xsd:annotation yfc:columnName="FIRST_ITER_SEQ_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The first iteration sequence for which this line is applicable. If Null, it is assumed that the line applies to all sequences before LastIterationSeqNo. Default is Null.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:LongSequence"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FreightTerms" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="FOB" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>Indicates Freight Terms for the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FromStatus" type="yfctype:Status" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Pass if quantity has to be canceled from a specific status.
					<Validation>
                                <Condition>There is not enough quantity in this status
						</Condition>
                                <Error
                                    Description="YFS: Not Enough Quantity" ErrorCode="YFS10131"/>
                            </Validation>
                            <Validation>
                                <Condition>Should be a valid status for the order's process type. Status equivalent to Cancel - <code>9000</code>
						 cannot be specified.
						</Condition>
                                <Error Description="YFS:Invalid Status" ErrorCode="YFS10235"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FulfillmentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="FULFILLMENT_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The method of fulfilling the order line during scheduling. This field can be matched with the corresponding field in the YFS_SOURCING_RULE_HDR table and the appropriate sourcing rule can be selected.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FutureAvailabilityDate" use="optional">
                <xsd:annotation yfc:columnName="FUTURE_AVAIL_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>Thie field indicates when inventory for the product line will be available.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="GiftFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="GIFT_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether the order line is a gift item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GiftWrap" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_GIFT_WRAP" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates if the order line has to be gift wrapped.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HoldFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="HOLD_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag determines whether a hold has been placed on the order when certain hold type criteria are met. If order is on hold, default configuration will stop the shceduling and release process for the order. The flag is reset to 'N' when all the hold types have been resolved on the order and all of its order lines. Based on hold type configuration, certain transactions may not be allowed to process. Note that there is no validation made on the hold applied but this flag can be reset by the application based on hold types existed on the order. Default: N Valid Values: Y or N</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HoldReasonCode"
                type="yfctype:HoldReasonCode" use="optional">
                <xsd:annotation yfc:columnName="HOLD_REASON_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ImportLicenseExpDate"
                type="yfctype:Date" use="optional">
                <xsd:annotation yfc:columnName="IMPORT_LICENSE_EXP_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The expiration date of the import license number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ImportLicenseNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="IMPORT_LICENSE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The import license number to be used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InStorePaymentRequired"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="IN_STORE_PAYMENT_REQUIRED" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates if this order line will have its payment processed later. When set, the line's total will be added to the postponed amount. This will override tha value on the order header.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IntentionalBackorder"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="INTENTIONAL_BACKORDER" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					 			This flag indicates whether the order line should be backordered upon creation. If passed as 'Y', the line will be backordered even if inventory is available. If a reservation ID is passed on the order line and reservation exists for a current, past or future date, reservation will be consumed to block inventory for the backordered demand. In this scenario, demand created by the reservation ID is removed, and a backordered demand will be generated. No attempt is made to look for inventory if reservation is not available for the passed reservation ID. For draft orders, the reservation is consumed and the order line is created in created status. During confirmDraftOrder, this line goes in backordered status. This flag is ignored if reservation is mandatory. This flag should not be passed for bundle parent lines. If it is passed as 'Y' for bundle parent order line, the line is created in backorder status. It is not propagated to bundle components.
					 		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsFirmPredefinedNode"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FIRM_PREDEFINED_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					Indicates if the pre-defined node on the line is firm or not.  If IsFirmPredefinedNode is marked as <b>N</b>, then the node on the line is ignored during scheduling. If not passed, the default value for this Flag is <b>Y</b>. If <code>ProcureFromNode</code> is passed, this flag will always be set to <b>Y</b>. For a <b>DS</b> or a <code>PS</code> line so, it is defaulted to <b>Y</b>. If the ship node is not present on the line but the value passed is <b>Y</b>, then it is treated as <b>N</b> during scheduling.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsForwardingAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FORWARDING_ALLOWED" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether forwarding is allowed for the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsProcurementAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PROCUREMENT_ALLOWED" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether procurement is allowed for the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemGroupCode"
                type="yfctype:EnumItemGroupCode" use="optional">
                <xsd:annotation yfc:columnName="ITEM_GROUP_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates what kind of item is on the order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemNotExist" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ITEM_NOT_EXIST" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field defines approval condition when item not in file is found.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitCode" type="yfctype:EnumKitCode" use="optional">
                <xsd:annotation yfc:columnName="KIT_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The collection of items that form a unit. For example, a home theatre system is a unit consisting of kit components, such as a DVD player, TV, CD player, etc.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitQty" use="optional">
                <xsd:annotation yfc:columnName="KIT_QTY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The quantity that is used to compute ordered quantity for bundle component line. It can be specified only for bundle components. If specified ordered quantity will be set to KitQty*OrderedQty of the bundle parent.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Quantity"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LastIterationSeqNo" use="optional">
                <xsd:annotation yfc:columnName="LAST_ITER_SEQ_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The last iteration sequence for which this line is applicable. If Null, it is assumed to go on indefinitely. Default is Null.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:LongSequence"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LevelOfService"
                type="yfctype:LevelOfService" use="optional">
                <xsd:annotation yfc:columnName="LEVEL_OF_SERVICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>Indicates the level of service required for this order line.  Default is Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LineType" type="yfctype:LineType" use="optional">
                <xsd:annotation yfc:columnName="LINE_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The line type. This field can be used in process modeling for pipeline determination, or for doing conditional processing for this line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MergeNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="MERGE_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The node where two or more order lines that are to be delivered together should be merged before the final delivery. This field is required when the DependancyShippingRule is set to '02'.  Additionally this field will indicate forwarding location for zone skipping.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MinShipByDate" use="optional">
                <xsd:annotation yfc:columnName="MIN_SHIP_BY_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                        The preferred minimum ship by date of the available supply that order line can allocate against.  Available supply with ship by date earlier than the minimum ship by date will not be considered.  This applies to the order line with a valid time-sensitive item.  The <code>MinShipByDate</code> of the order line will also be used by all kit lines with valid time-sensitive item.
                        <Validation>
                                <Condition>If <code>MinShipByDate</code> is modified and some quantities of the order line have been scheduled.
                            </Condition>
                                <Error
                                    Description="Cannot modify MinShipByDate." ErrorCode="OMP73HF_00002"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderedQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="ORDERED_QTY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The quantity of the line item that has been ordered. If part of line is cancelled, this number is reduced appropriately. If an order line is returned, this number is not reduced.
            <br/> For a service line item which is used as a standalone service, <code>OrderedQty</code> is treated in the same manner as product lines and must be a non-zero positive number.
						<br/>The <code>OrderedQty</code> value passed on the service line is ignored, if ordered quantity computaion is not based on entered quantity.
						<Validation>
                                <Condition>If <code>SerialNo</code> is provided and <code>OrderedQty</code> is not equal to one.
							  </Condition>
                                <Error
                                    Description="OrderedQty must be equal to one because SerialNo is provided." ErrorCode="OMP11024"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>ReservationManadatory</code> is passed as 'Y' and added quantity cannot be reserved.
							  </Condition>
                                <Error
                                    Description="Cannot Reserve Quantity added to the line." ErrorCode="OMP11040_55SP1"/>
                            </Validation>
                            <Validation>
                                <Condition>Adding quantity on a bundle component is allowed for fixed quantity line or if it does not result in increase in bundle parent line quantity or if quantity is increased on all the components in ratio with bundle parent.
							  </Condition>
                                <Error
                                    Description="Bundle parent line quantity cannot be increased by its component." ErrorCode="OMP79_0019"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PackListType" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PACKLIST_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The code indicating the type of packlist to be used for the line. Lines requiring different pack lists are not mixed in a single order release.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonalizeCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PERSONALIZE_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The code for personalization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PickableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PICKABLE_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is used internally.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PipelineId" type="yfctype:Identifier" use="optional">
                <xsd:annotation yfc:columnName="PIPELINE_ID" yfc:tableName="YFS_PIPELINE">
                    <xsd:documentation>
                        <div>
					Pipeline id on order line. Before modifying the pipeline make sure all the statuses line is in are valid for the changed pipeline too. API does not check that statuses the line is in are valid for the changed pipeline.
					<Validation>
                                <Condition>
						It should be a valid pipeline for the process type order line belongs.
						</Condition>
                                <Error Description="Invalid pipeline id" ErrorCode="OMP11071"/>
                            </Validation>
                            <Validation>
                                <Condition>
						Order line is in a status that cannot be processed by any transaction in the new pipeline.
						</Condition>
                                <Error
                                    Description="Some of the line statuses can not be processed in new pipeline." ErrorCode="OMP11077"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProcureFromNode"
                type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="PROCURE_FROM_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					The node from which the order line should be procured if cannot source from the shipnode on the order line.
					<Validation>
                                <Condition>
						ProcureFromNode can be specified only if shipnode is passed.
						</Condition>
                                <Error
                                    Description="Cannot pass ProcureFromNode without Shipnode" ErrorCode="OMP10086"/>
                            </Validation>
                            <Validation>
                                <Condition>
						If Reservation is attempted from ProcureFromNode, then shipnode must have <code>Procure To Ship Allowed</code> Flag set to <b>Y</b>
                                </Condition>
                                <Error
                                    Description="Cannot pass ProcureFromNode when ShipNode cannot procure." ErrorCode="OMP11042_55SP1"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PromisedApptEndDate" use="optional">
                <xsd:annotation yfc:columnName="PROMISED_APPT_END_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					The date and time when the appointment ends for an <code>OrderLine</code>.
						<Validation>
                                <Condition>
                                    <code>PromisedApptEndDate</code> must be greater than or equal to <code>PromisedApptStartDate</code>.
							</Condition>
                                <Error
                                    Description="PromisedApptStartDate cannot be greater than PromisedApptEndDate" ErrorCode="OMP10056"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>PromisedApptEndDate</code> is passed, but  <code>PromisedApptStartDate</code> is not passed.
							</Condition>
                                <Error
                                    Description="Both PromisedApptStartDate and PromisedApptEndDate must be passed, if either one is passed." ErrorCode="OMP11047_55SP2"/>
                            </Validation>
                            <Validation>
                                <Condition>PromisedApptEndDate cannot be modified for product line once it is associated with work order.
							</Condition>
                                <Error
                                    Description="Cannot change PersonInfoShipTo, PromisedApptStartDate and PromisedApptEndDate for Product Line already associated with a work order." ErrorCode="OMP11050"/>
                            </Validation>
                            <Validation>
                                <Condition>PromisedApptEndDate cannot be modified for service if it is part of a complex work order (multi-appointment or appoinment against a service resource) or there are multiple service lines in the work order.
							</Condition>
                                <Error
                                    Description="Cannot change PersonInfoShipTo, PromisedApptStartDate and PromisedApptEndDate for Service Line associated with a complex work order" ErrorCode="OMP11051"/>
                            </Validation>
                            <Validation>
                                <Condition>If the date corresponds to a slot which cannot be used for appointment planning.
							</Condition>
                                <Error
                                    Description="Slot based Capacity Consumption cannot be done on slots that cannot be used for taking appointments." ErrorCode="INV80_002"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="PromisedApptStartDate" use="optional">
                <xsd:annotation
                    yfc:columnName="PROMISED_APPT_START_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>	The date and time when the appointment starts for an <code>OrderLine</code>.
					<br/> A change to <code>PromisedAppointmentDate</code> of a Delivery line also changes dates for all the associated Product lines.
						<Validation>
                                <Condition>
                                    <code>PromisedApptStartDate</code> cannot be after <code>PromisedApptEndDate</code>.
							</Condition>
                                <Error
                                    Description="PromisedApptStartDate cannot be greater than PromisedApptEndDate." ErrorCode="OMP10056"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>PromisedApptStartDate</code> is passed, but  <code>PromisedApptEndDate</code> is not passed.
							</Condition>
                                <Error
                                    Description="Both PromisedApptStartDate and PromisedApptEndDate must be passed, if either one is passed." ErrorCode="OMP11047_55SP2"/>
                            </Validation>
                            <Validation>
                                <Condition>PromisedApptStartDate cannot be modified for product line once it is associated with work order.
							</Condition>
                                <Error
                                    Description="Cannot change PersonInfoShipTo, PromisedApptStartDate and PromisedApptEndDate for Product Line already associated with a work order." ErrorCode="OMP11050"/>
                            </Validation>
                            <Validation>
                                <Condition>PromisedApptStartDate cannot be modified for service if it is part of a complex work order (multi-appointment or appoinment against a service resource) or there are multiple service lines in the work order.
							</Condition>
                                <Error
                                    Description="Cannot change PersonInfoShipTo, PromisedApptStartDate and PromisedApptEndDate for Service Line associated with a complex work order" ErrorCode="OMP11051"/>
                            </Validation>
                            <Validation>
                                <Condition>If the date corresponds to a slot which cannot be used for appointment planning.
							</Condition>
                                <Error
                                    Description="Slot based Capacity Consumption cannot be done on slots that cannot be used for taking appointments." ErrorCode="INV80_002"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Purpose" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="PURPOSE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is passed out in the Tax calculation user exits.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceivingNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="RECEIVING_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The node an order line is to be shipped to.<br>
						Note: When calling this API for a return order, this ReceivingNode attribute should contain the receiving_node ID associated with the "Ship From" node (the node from which the returned item is shipped).</br>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReqCancelDate" use="optional">
                <xsd:annotation yfc:columnName="REQ_CANCEL_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The requested cancel date. If not provided, this field is defaulted from the order header during order creation. Refer to the YFS_ORDER_HEADER table for more details.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReqDeliveryDate" use="optional">
                <xsd:annotation yfc:columnName="REQ_DELIVERY_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The requested delivery date. If not provided, this field is defaulted from the order header during order creation. Refer to the YFS_ORDER_HEADER table for more details.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReqShipDate" use="optional">
                <xsd:annotation yfc:columnName="REQ_SHIP_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The requested ship date. If not provided, this field is defaulted from the order header during order creation. Refer to the YFS_ORDER_HEADER table for more details.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReservationID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="RESERVATION_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						This attribute should be passed when a line, or quantity to the existing line, is added. This attribute is not applicable to the existing quantity. If this item was reserved before adding a line or a quantity to the line, pass a reservation ID to associate the line with the reservation. If a reservation ID is passed, and the reservation exists for the item, the reservation will be consumed to block inventory.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReservationMandatory" use="optional">
                <xsd:annotation yfc:columnName="RESERVATION_MANDATORY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether this line can be created without a reservation. If set to Y, creation of this line or any quantity addition to this line will be allowed only if the quantity can be reserved. If a ProcureFromNode is also present on the line, then the reservation will be carried out on the procure from node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="RetainAnswerSets" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>This flag indicates if upon ship-to address change, the answer sets for the old address should be copied to be used for the new address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReturnReason" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="RETURN_REASON" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The reason for returning the order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SCAC" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The carrier to be used for this line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndService" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An identifier for a service offered by a carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE_KEY" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Primary key attribute of YFS_SCAC_AND_SERVICE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Segment" type="yfctype:Segment" use="optional">
                <xsd:annotation yfc:columnName="SEGMENT" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The segment of the inventory.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SegmentType" type="yfctype:SegmentType" use="optional">
                <xsd:annotation yfc:columnName="SEGMENT_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The segment type of the inventory.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SerialNo" type="yfctype:SerialNo" use="optional">
                <xsd:annotation yfc:columnName="SERIAL_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                        Requested serial number of the item.
                        <Validation>
                                <Condition>If <code>SerialNo</code> is provided and <code>OrderedQty</code> is not equal to one.
                              </Condition>
                                <Error
                                    Description="OrderedQty must be equal to one because SerialNo is provided." ErrorCode="OMP11024"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>SerialNo</code> is provided but <code>ShipNode</code> is not.
                            </Condition>
                                <Error
                                    Description="Ship node is mandatory because SerialNo is provided." ErrorCode="OMP11025"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Shipping node from which this line should be shipped. If provided, all releases will be made against this node.<br>
						Note: When calling this API for a return order, this ShipNode attribute should contain the shipnode_key associated with the "Return To" node (the node to which the returned item is being sent).
						If ship node is not set to either the order or the orderline and the seller organization is a node then the seller organization's ship node will be set as the ship node for the Cash And Carry order line.</br>
                            <Validation>
                                <Condition>If <code>SerialNo</code> is provided, <code>ShipNode is mandatory</code>
                                </Condition>
                                <Error
                                    Description="Ship node is mandatory because SerialNo is provided." ErrorCode="OMP11025"/>
                            </Validation>
                            <Validation>
                                <Condition>Shipnode cannot be modified on service line once it becomes part of work order.
							</Condition>
                                <Error
                                    Description="Cannot change ship node on service line already associated with a work order." ErrorCode="OMP11049"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>ShipNode</code> is provided and <code>ReservationID</code> is provided to represent a capacity reservation for the service line and <code>ShipNode</code> is not the same as the node of the resource pool on the reservation.
							</Condition>
                                <Error
                                    Description="Node does not match with the node of the resource pool of the reservation." ErrorCode="OMP73_10001"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipToID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TO_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The ship to identifier.  If a customer definition representing the buyer organization exists within the system, the ShipToID can represent the CustomerID of the customer.  Otherwise, the ShipToID can represent the PersonID of the ship to address or the receiving node of the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipToKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TO_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                            	The ship to address that is associated with this order line. If not provided, it is defaulted from order header when needed.
                            	To remove shipping address from a line, pass <code>ShipToKey</code> as blank and pass <code>PersonInfoShipTo</code> element
                            	with all blank attributes or don't pass it.
                            </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipTogetherNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TOGETHER_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The sub line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Timezone" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TIMEZONE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                            <br/>Times from the database are converted into the timezone specified by this field by the UI. Used with order lines with an <code>ItemGroupCode</code> of <b>
                                <code>PS</code>
                            </b> - <i>Provided Service</i> or <b>
                                <code>DS</code>
                            </b> - <i>Delivery Service</i>.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ToStatus" type="yfctype:Status" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Pass if quantity has to be added to a specific status. It is only applicable to add quantity. It can also be specified while adding a line. Status must be less than <code>3200</code>
                            <Validation>
                                <Condition>Should be a valid status for the order's process type. Status equivalent to Cancel - <code>9000</code>
						 cannot be specified.
						</Condition>
                                <Error Description="YFS:Invalid Status" ErrorCode="YFS10235"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					A field used to identify <code>OrderLine</code>s when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in line associations. This value is transient, it is not stored in the database.
					<Validation>
                                <Condition>Must pass one of the following
						<br/>the <code>PrimaryLineNo</code>'s <code>OrderLineKey</code> and the <code>SubLineNo</code>
                                    <br/>or
						<br/>
                                    <code>TransactionalLineID</code>
                                </Condition>
                                <Error Description="" ErrorCode=""/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ValidateItem" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
				  Indicates if item needs to be validated against catalog. If passed, the configuration rule for validating the item will be overridden.
				  <br/>Default: <b>
                                <code>Null</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineTranQuantity">
        <xsd:complexType>
            <xsd:attribute name="FillQuantity" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						The minimum <code>OrderLine</code> quantity that must be fulfilled in an order release before it is shipped.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderedQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						The quantity of the the line item that has been ordered. If part of line is cancelled, this number is reduced appropriately. If an <code>OrderLine</code> is returned, this number is not reduced.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SettledQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						Quantity which should be settled before it is actually invoiced.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					  UOM on which all the transactional quantities are specified.
					  </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="DerivedFrom">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
Use this element to derive a new <code>OrderLine</code> from an existing <code>OrderLine</code>. If the passed order line is a bundle parent then all the PROD components that have specified <code>KitQty</code> are also pulled in the derived order. If components do not meet status criteria then error is thrown. Bundle-child relationship is maintained in the derived order if parent and components exist in the same derived order.				<br/>The existing <code>OrderLine</code> can be identified by using one of the following:
				<br/>- <code>OrderLineKey</code>
                    <br/>- <code>OrderHeaderKey</code>, <code>PrimeLineNo</code>, and  <code>SubLineNo</code>
                    <br/>- <code>OrderNo</code>, <code>DocumentType</code>, <code>EnterpriseCode</code>, <code>PrimeLineNo</code>,  and <code>SubLineNo</code>
                    <br/>
                    <br/>If the parent line cannot be identified, an error is thrown.
				<br/>To select only the part of the existing <code>OrderLine</code> associated with a particular release for
				derive a new  <code>OrderLine</code>, additional data is needed. In addition to the information to identify the existing  <code>OrderLine</code>, supply either the <code>OrderReleaseKey</code> or the <code>ReleaseNo</code> that  identifies the release.
				<br/>
                    <br/>Information is taken from the parent line, based on the DerivedOrderDefaultTemplate, stored in the YFS_DOCUMENT_TEMPLATE table. The DerivedOrderDefaultTemplate is chosen on the basis of the DocumentType of the child order.
				<br/>
                    <br/>If there are attributes passed in the  <code>changeOrder</code> input xml,  the values in the passed attributes are used for creating the new <code>OrderLine</code>. If passed line is a bundle parent line, derived lines are created for all the eligible components existing in the parent order. All the attributes which are passed to <code>OrderLine</code> and its children elements are applied to all the components that are created as part of creating derived line from a bundle parent. If <code>KitCode</code> and <code>Item</code> attributes are changed while creating the derived line from bundle parent, result may be unpredictable.
				<br/>Schedule information is never propagated from the parent line to the child line.
				<br/>
                    <br/>The <code>DerivedFromOrderLineKey</code>, <code>DerivedFromOrderHeaderKey</code> and, if applicable, <code>DerivedFromOrderReleaseKey</code> are set on the new <code>OrderLine</code>. The new <code>OrderLine</code> is validated, like all the other lines of the new order.
				<br/> A transaction is triggered. The transaction is based on the existing order's ProcessType, and the document type of the new order. This <b>IncludeInDerivedOrder<code>1</code>
                    </b> transaction changes the status of that quantity of the existing line which is equal to the OrderedQty of the child line. If the parent line does not have sufficient quantity to derive from, error is thrown.
				If <code>Action</code> is <code>CREATE</code>, the  <code>PrimeLineNo</code> is generated and <code>SubLineNo</code> is set to <b>
                        <code>1</code>
                    </b>. A starting number can be specified  in the configurator at each document type and enterprise level for <code>PrimeLineNo</code>. If a starting number is specified the <code>PrimeLineNo</code> will be generated starting from that number.

				<Validation>
                        <Condition>If action is not <b>
                                <code>Create</code>
                            </b>, then the <code>OrderLineKey</code> attribute OR the <code>PrimeLineNo</code> and <code>SubLineNo</code> attributes must be passed and must be valid.
					</Condition>
                        <Error Description="YFS: Invalid Line" ErrorCode="YFS10009"/>
                    </Validation>
                </div>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
                            </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_HEADER">
                    <xsd:documentation>The document type that is associated with this order. For example, Sales Order document (0001), Planned Order document (0002), Return Order document (0003) , and Purchase Order document (0005). This field determines the order's fulfillment cycle and controls the inventory and payment updates performed on the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional"/>
            <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order header that this order line is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderNo" type="yfctype:DocumentNo" use="optional"/>
            <xsd:attribute name="OrderReleaseKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReleaseNo" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The sub line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ChainedFrom">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
				Optional. Use this element only to chain a new <code>OrderLine</code> with an existing <code>OrderLine</code>.
				<br/>The existing <code>OrderLine</code> can be identified by using one of the following:
				<br/>- <code>OrderLineKey</code>
                    <br/>- <code>OrderHeaderKey</code>, <code>PrimeLineNo</code>, and  <code>SubLineNo</code>
                    <br/>- <code>OrderNo</code>, <code>DocumentType</code>, <code>EnterpriseCode</code>, <code>PrimeLineNo</code>,  and <code>SubLineNo</code>
                    <br/>
                    <br/>If the parent line cannot be identified, an error is thrown.
				<br/>
                    <br/>Information is taken from the parent line, based on the ChainedOrderOtherTemplate, stored in the YFS_DOCUMENT_TEMPLATE table. The ChainedOrderOtherTemplate is chosen on the basis of the DocumentType of the parent order.
				<br/>
                    <br/>If there are attributes passed in the  <code>createOrder</code> input xml,  the values in the passed attributes are used for creating the new <code>OrderLine</code>.
				<br/>Schedule information is never propagated from the parent line to the child line.
				<br/>
                    <br/>The <code>ChainedFromOrderLineKey</code> and <code>ChainedFromOrderHeaderKey</code> are set on the new <code>OrderLine</code>. The new <code>OrderLine</code> is validated, like all the other lines of the new order.
				<br/> A transaction is triggered. The transaction is based on the existing order's ProcessType, and the document type of the new order. This <b>IncludeInChainedOrder</b> transaction changes the status of that quantity of the existing line which is equal to the OrderedQty of the child line. If the parent line does not have sufficient quantity to chain from, error is thrown.
				<br/>Having different any of item identifying attributes (item id, product class, unit of measure) from parent line is allowed only when inventory is not maintained by seller organization and not by by buyer organization. Error is thrown if any of them maintains.
				<Validation>
                        <Condition>Item attributes different from parent line.
					</Condition>
                        <Error
                            Description="Item attributes on chained line cannot be different from parent line as inventory is maintained either by buyer organization or by seller organization." ErrorCode="OMP11078"/>
                    </Validation>
                </div>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
                            </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_HEADER">
                    <xsd:documentation>The document type that is associated with this order. For example, Sales Order document (0001), Planned Order document (0002), Return Order document (0003) , and Purchase Order document (0005). This field determines the order's fulfillment cycle and controls the inventory and payment updates performed on the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The enterprise that is associated with this order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order header that this order line is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderNo" type="yfctype:DocumentNo" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The order number that this order line is chained from.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The sub line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Item">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
				To modify item attributes. <code>ItemID</code>,<code>AlternateItemID</code>, <code>ProductClass</code> and <code>UnitOfMeasure</code> can be modified only for draft orders (when <code>DraftOrderFlag</code> for the order is set to <b>
                        <code>Y</code>
                    </b>). No inventory updates (demand/supply) take place on item attributes modifications. Item attributes are modified under modification type <code>OTHERS</code>.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AlternateItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_ITEM_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates the alternate ItemID, which can be passed instead of the actual ItemID. This is an alias for the ItemID.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BundleFulfillmentMode" use="optional">
                <xsd:annotation yfc:columnName="BUNDLE_FULFILLMENT_MODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The way in which the bundle will be fulfilled. It can be Ship Independent (00), Ship Together (01), or Deliver Together (02) for Bundle Item. Default value is based on the catalog. If the item is not in the catalog, then Ship Together(01) is defaulted for Bundle Item.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Text-2">
                        <xsd:enumeration value=""/>
                        <xsd:enumeration value="00"/>
                        <xsd:enumeration value="01"/>
                        <xsd:enumeration value="02"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="CostCurrency" type="yfctype:Currency" use="optional">
                <xsd:annotation yfc:columnName="COST_CURRENCY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The currency in which the unit cost is specified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CountryOfOrigin" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY_OF_ORIGIN" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The country of origin being requested.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ITEM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item ID for the buyer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerItemDesc"
                type="yfctype:Text-500" use="optional">
                <xsd:annotation
                    yfc:columnName="CUSTOMER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item description of the buyer organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ECCNNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ECCN_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The Export Commodity Control Number for a commodity contained in the shipment, as indicated on the Waybill.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HarmonizedCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="HARMONIZED_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The harmonized code, if applicable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ISBN" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ISBN" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The ISBN number, if applicable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemDesc" type="yfctype:Text-500" use="optional">
                <xsd:annotation yfc:columnName="ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The identifier for the item. The ITEM_ID, UOM, and PRODUCT_CLASS form the unique inventory identifier for an order line. The ITEM_ID, UOM and ORGANIZATION_CODE form the unique item identifier for the Product Management module.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemShortDesc" type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="ITEM_SHORT_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The short description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The weight of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT_UOM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit of measure used for weight.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItem"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item identifier for the manufacturer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItemDesc"
                type="yfctype:Text-254" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM_DESC" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item description for the manufacturer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerName"
                type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_NAME" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The name of the manufacturer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCClass" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="NMFC_CLASS" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The National Motor Freight Classification - A way of classifying goods for the purposes of transportation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="NMFC_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The National Motor Freight Classification - A way of classifying goods for the purposes of transportation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCDescription"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="NMFC_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The description of the NMFC. National Motor Freight Classification is a way of classifying goods for the purposes of transportation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductClass"
                type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product class. This field is used as an inventory identifier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductLine" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_LINE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product line for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScheduleBCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCHEDULE_B_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The Schedule B commodity codes - A 10-digit numeric codes used to identify products that are exported to other countries.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_ITEM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item ID of the supplier (if this is a PO or the line is being drop-shipped and the supplier is known upfront).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItemDesc"
                type="yfctype:ItemDesc" use="optional">
                <xsd:annotation
                    yfc:columnName="SUPPLIER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item description of the supplier (if this is a PO or line is being drop-shipped and the supplier is known upfront).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxProductCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TAX_PRODUCT_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product code classification for the purposes of taxation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UPCCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="UPC_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The UPC Code of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitCost" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_COST" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit cost of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit of measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineOptions">
        <xsd:annotation>
            <xsd:documentation>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
             </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLineOption"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineOption">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_OPTION">
            <xsd:documentation>
                <div>
				Describes the Options attached to a Delivery or a Provided Service Line. Options can only be specified for a Delivery or a Provided Service line.
				<br/> An option cannot be added or removed once the <code>OrderLine</code> has been scheduled.
				</div>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
                            </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Default:<b>
                                <code>MODIFY</code>
                            </b>
                            <br/>Valid Values:
					<br/>
                            <b>
                                <code>REMOVE</code>
                            </b> - Removes the entire <code>OrderLine</code> Option.  Set <code>ModificationType</code> to <code>RemoveOption</code>.
					<br/>
                            <b>
                                <code>CREATE</code>
                            </b> - Adds a new line Option.  <code>ModificationType</code>  to allow this change is <code>AddOption</code>.
					<br/>
                            <b>
                                <code>MODIFY</code>
                            </b> - Default value if nothing is passed. <code>OptionItemID</code> and <code>OptionUOM</code> cannot be changed even for a Draft order.
					<br/> An option cannot be added or removed once the <code>OrderLine</code> has been scheduled.
					<br/> If Action is <code>MODIFY</code> or <code>REMOVE</code>, both <code>OptionItemID</code> and <code>OptionUOM</code> must be passed to identify the <code>OrderLineOption</code>.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OptionDescription"
                type="yfctype:FreeText-200" use="optional">
                <xsd:annotation yfc:columnName="OPTION_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>Short Description for the Option added to the Item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OptionItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation yfc:columnName="OPTION_ITEM_ID" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>The unique identifier for the Option Item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OptionUOM" type="yfctype:UOM" use="required">
                <xsd:annotation yfc:columnName="OPTION_UOM" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>Unit Of Measure of the Option Item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="PRICING_UOM" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>For future use.  Option is currently priced based on ordering unit of measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitPrice" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_PRICE" yfc:tableName="YFS_ORDER_LINE_OPTION">
                    <xsd:documentation>Unit price for the Option.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineSourcingControls">
        <xsd:annotation>
            <xsd:documentation>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
             </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderLineSourcingCntrl"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineSourcingCntrl">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
            <xsd:documentation>
                <div>
            Describes the Sourcing Controls attached to a <code>OrderLine</code>.
            <br/>
                </div>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
                            </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Default:<b>
                                <code>MODIFY</code>
                            </b>
                            <br/>Valid Values:
					<br/>
                            <b>
                                <code>REMOVE</code>
                            </b> - Removes the entire <code>OrderLine</code> Sourcing Control.  Set <code>ModificationType</code> to <code>Sourcing Control Modifications</code>.
					<br/>
                            <b>
                                <code>CREATE</code>
                            </b> - Adds a new line sourcing control.  <code>ModificationType</code>  to allow this change is <code>Sourcing Control Modifications</code>.
          <br/>
                            <b>
                                <code>MODIFY</code>
                            </b> - Changes an existing line sourcing control.  <code>ModificationType</code>  to allow this change is <code>Sourcing Control Modifications</code>.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InventoryCheckCode"
                type="yfctype:EnumInvCheckCode" use="optional">
                <xsd:annotation yfc:columnName="INV_CHECK_CODE" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>If this attribute is populated, inventory availability for that node is overridden based on the value.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Node" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="NODE_KEY" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>The node for which either sourcing or procurement has been suppressed when either Suppress Sourcing or Suppress Procurement flags are set to Yes. NOTE: If Node is blank and SuppressNodeCapacity is Yes, then inventory node capacity will be ignored for all nodes of the OrderLine.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReasonText" type="yfctype:ReasonText" use="optional">
                <xsd:annotation yfc:columnName="REASON_TEXT" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>Free-form reason text indicating any additional information.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SuppressNodeCapacity" use="optional">
                <xsd:annotation yfc:columnName="SUPPRESS_NODE_CAPACITY" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>Flag determines whether to ignore inventory node capacity. Default: N. NOTE: If Node is blank and SuppressNodeCapacity is Yes, then inventory node capacity will be ignored for all nodes of the OrderLine.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="N"/>
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value=""/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="SuppressProcurement" use="optional">
                <xsd:annotation yfc:columnName="SUPPRESS_PROCUREMENT" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>Flag determines whether to ignore this Node for procurement consideration.  Node will not be used by scheduling as a procure from node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="SuppressShipNodeCapacityForProc" use="optional">
                <xsd:annotation
                    yfc:columnName="SUPPRESS_SHP_NODE_CAPCTY_PROC" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>Flag determines whether to ignore ship node's capacity consideration for procurement for this order line. Default: N. NOTE: Node should not be passed when SuppressShipNodeCapacityForProc is Yes.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="N"/>
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value=""/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="SuppressSourcing" use="optional">
                <xsd:annotation yfc:columnName="SUPPRESS_SOURCING" yfc:tableName="YFS_ORDER_LINE_SRC_CNTRL">
                    <xsd:documentation>Flag determines whether to ignore this Node for sourcing consideration.  Node will not be used by scheduling as a ship node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LinePriceInfo">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
				If the order is a draft order, <code>SettledQuantity</code> must not be passed.
				<Validation>
                        <Condition>A non-zero value passed for <code>SettledQuantity</code>
                        </Condition>
                        <Error
                            Description="Cannot specify non-zero SettledQuantity for line of draft order." ErrorCode="YFS10508"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DiscountPercentage"
                type="yfctype:Percentage" use="optional">
                <xsd:annotation yfc:columnName="DISCOUNT_PERCENTAGE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The discount percentage is the percentage decrease in price between the list and unit price for a given buyer. This value is calculated by subtracting the difference between the list price and unit price and divided by the list price.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsEligibleForShippingDiscount"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="IS_ELIGIBLE_FOR_SHIP_DISC" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag can override the configured value for the item. Otherwise it is the same as the configured value for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsLinePriceForInformationOnly"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PRICE_INFO_ONLY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
								This flag indicates whether price information passed on the line is for information only. If it is 'Y' then line total will not be considered in order total. If it is not passed or passed as 'N' then line total is considered in order total.
								If this flag is not passed for a static bundle (bundle defined in the catalog),
								the the flag will be set on the parent and its components based on the
								<code>BundlePricingStrategy</code> defined on the item.
								<br/>
                            <table border="0" cellpadding="0"
                                cellspacing="0" class="table">
                                <tr>
                                    <th class="tablecolumn">BundlePricingStrategy</th>
                                    <th class="tablecolumn">Bundle Parent Value</th>
                                    <th class="tablecolumn">Components Value</th>
                                </tr>
                                <tr>
                                    <td class="tablecolumn">COMP</td>
                                    <td class="tablecolumn">Y</td>
                                    <td class="tablecolumn">Null (or N)</td>
                                </tr>
                                <tr>
                                    <td class="tablecolumn">PARENT</td>
                                    <td class="tablecolumn">Null (or N)</td>
                                    <td class="tablecolumn">Y</td>
                                </tr>
                                <tr>
                                    <td class="tablecolumn">ALL</td>
                                    <td class="tablecolumn">Null (or N)</td>
                                    <td class="tablecolumn">Null (or N)</td>
                                </tr>
                            </table>
                            <Validation>
                                <Condition>If flag is changed after line is invoiced partially or fully.
									</Condition>
                                <Error
                                    Description="You cannot change the flag once anything on the line is invoiced." ErrorCode="OMP79_0022"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPriceLocked" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PRICE_LOCKED" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether UnitPrice passed at LinePriceInfo element must be used and price determination ignored, if price determination is configured and enabled. Valid values are 'Y' or 'N'. Default value is 'N'.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ListPrice" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="LIST_PRICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>the list price of the item. This is an informational field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RetailPrice" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="RETAIL_PRICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The retail price of the item. this is an informational field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SettledQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="SETTLED_QUANTITY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
	                        		The quantity which should be settled before it is actually invoiced.<br/>
                            <code>DeliveryMethod</code> takes higher precedence over <code>SettledQuantity</code>. In other words, if <code>DeliveryMethod</code> is being changed from a delivery method that requires immediate settlement to a 
	                        		one that does not (for example, from CARRY to SHIP), and <code>SettledQuantity</code> is set in the same call, then, the value passed as <code>SettledQuantity</code> will be
	                        		ignored and <code>SettledQuantity</code> will be set to zero.
                        		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxExemptionCertificate"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation
                    yfc:columnName="TAX_EXEMPTION_CERTIFICATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The Tax Exemption Certificate applied to this order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether this order line is taxable or not. This field is for informational purposes only. Currently, it is not used in any tax calculations. If not passed, this flag is defaulted to that of item in the catalog, if exists, else it is defaulted to "N".</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitPrice" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_PRICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit price for the order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineCharges">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:LineCharge"/>
            </xsd:sequence>
            <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>If reset is passed as Y, all the existing charges that are not passed as a child node will be set to zero.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineCharge">
        <xsd:annotation yfc:tableName="YFS_LINE_CHARGES">
            <xsd:documentation>This table stores any additional charges or discounts that can be added to an  order line.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeCategory"
                type="yfctype:ChargeCategory" use="required">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Link to YFS_CHARGE_CATEGORY.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Link to YFS_CHARGE_NAME.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargePerLine"
                type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="CHARGEPERLINE" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Fixed charge amount for this order/invoice line. When the line charge is associated with an order line, this amount will be invoiced on the first invoice for this order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargePerUnit" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="CHARGEPERUNIT" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Charge per unit for the item on this order/invoice line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE" yfc:tableName="YFS_LINE_CHARGES">
                    <xsd:documentation>Any additional reference related to this line charge.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineTaxes">
        <xsd:annotation>
            <xsd:documentation>
                <div>
						Pass this element to replace the tax information for an order line. To update the tax information for an order line, all applicable taxes (not just the modified or added tax) should be passed to the API. Tax which is not passed will be removed from the order line.
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:LineTax"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineTax">
        <xsd:annotation yfc:tableName="YFS_TAX_BREAKUP">
            <xsd:documentation>
                <div>
						Contains tax related information for various entities in the system.  If <code>ChargeCategory</code> and <code>ChargeName</code> are not provided, tax will be considered as a billable and positive charge.  Whether <code>YFSRecalculateLineTaxUE</code> is implemented or not, <code>ChargeCategory</code>, <code>Tax</code>, and <code>TaxPercentage</code> are required for the repricing logic.
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeCategory"
                type="yfctype:ChargeCategory" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
							The charge category for which tax is being charged. For example, Labeling or Handling.
							To specify the tax for the line price, use the charge category  <b>
                            <code>Price</code>
                        </b>.
							</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Charge name for which tax is being charged. For example, Price Ticket.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference1" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE1" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Additional reference information. This field can be customized by the Enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference2" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE2" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Additional reference information. This field can be customized by the Enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference3" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE3" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Additional reference information. This field can be customized by the Enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Tax" type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="TAX" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
                        <div>
								The tax amount. <code>Tax</code> cannot be less than zero.  During the default repricing logic, this is recalculated using the <code>TaxPercentage</code> and the price of the related <code>ChargeCategory</code>.  This can be overridden with the YFSRecalulateLineTaxUE.
		                            </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxName" type="yfctype:Text-40" use="required">
                <xsd:annotation yfc:columnName="TAX_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>The tax name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxPercentage" type="yfctype:TaxType" use="optional">
                <xsd:annotation yfc:columnName="TAX_PERCENTAGE" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Percentage amount for tax calculation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>This flag indicates if a charge can be taxed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="KitLines">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:KitLine"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="KitLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_KIT_LINE">
            <xsd:documentation>This table stores information about kitted component items. A kit is a collection of items that form a unit. For example, a home theatre system is a unit consisting of component items such as a DVD player, TV, CD player, etc.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:KitLineTranQuantity"/>
            </xsd:all>
            <xsd:attribute name="CustomerItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ITEM" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Cross reference to the customer item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerItemDesc"
                type="yfctype:Text-500" use="optional">
                <xsd:annotation
                    yfc:columnName="CUSTOMER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Customer item description.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DepartmentCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT_CODE" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Department that deals with this item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemDesc" type="yfctype:Text-500" use="optional">
                <xsd:annotation yfc:columnName="ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Description for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>The unique identifer for the component item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemShortDesc" type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="ITEM_SHORT_DESCRIPTION" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>short description for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Weight of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT_UOM" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Unit of measure of the ITEM_WEIGHT attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitQty" type="yfctype:Quantity" use="required">
                <xsd:annotation yfc:columnName="KIT_QTY" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>The number of components required to one kit.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductClass"
                type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Component Product Class.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_ITEM" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Cross reference to the supplier ietm.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItemDesc"
                type="yfctype:ItemDesc" use="optional">
                <xsd:annotation
                    yfc:columnName="SUPPLIER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Supplier item description.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UPCCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="UPC_CODE" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>UPC code for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitCost" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_COST" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Unit cost of the component item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_KIT_LINE">
                    <xsd:documentation>Component Unit Of Measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="KitLineTranQuantity">
        <xsd:complexType>
            <xsd:attribute name="KitQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The number of components required to one kit.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation>
                    <xsd:documentation>UOM on which all the transactional quantities are specified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Schedules">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
            <xsd:documentation>
                <div>
				Associate multiple lot numbers with an <code>OrderLine</code>.
				<br/>Schedule information can be attached to order quantity at the time of adding that quantity. It cannot be specified or changed later using <code>changeOrder</code>.
				<br/>Schedule information can be passed only if there is a change in <code>OrderedQty</code>. If <code>OrderedQty</code> is getting changed by more than the total scheduled quantity passed, then remaining quantity is created with no schedule information, or is canceled from non schedule quantity.  Refer to the <code>changeOrderSchedule</code> API for schedule modifications.
			</div>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
             </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Schedule"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Schedule">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
            <xsd:documentation>
                <div>
					Contains Order Line Schedule information for Order Lines. An order line can be broken up into multiple schedules. Each Schedule contains information about ShipNode, ReceivingNode, ExpectedShipmentDate, ExpectedDeliveryDate and info about any tags associated with the order line.Note that ExpectedShipmentDate and ExpectedDeliveryDate will be honoured  for purchase orders while adding/removing quantity when 'Do not recompute the expected dates when requested dates on the order are changed' flag is unchecked.
					<Validation>
                        <Condition>If the item on the line is not tag controlled and <code>BatchNo</code>, <code>LotNumber</code>, or <code>RevisionNo</code> is passed.
						 </Condition>
                        <Error Description="Item is not tag controlled." ErrorCode="OMP11032"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ScheduleTranQuantity"/>
            </xsd:all>
            <xsd:attribute name="BatchNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO" yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
                    <xsd:documentation>The batch number that has been specified on the scheduled order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChangeInQuantity"
                type="yfctype:NegativeQuantity" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
						How quantity should be changed. If quantity is being added to the line, supply a non-zero positive number.
						If quantity is being cancelled from the line, supply a non-zero negative number. <code>ChangeInQuantity</code> can be specified only for the quantity that is being added or  canceled.
						<Validation>
                                <Condition>Total scheduled quantity is greater than the change in <code>OrderedQty</code>.
						</Condition>
                                <Error
                                    Description="Invalid Schedule Quantity" ErrorCode="YFS10377"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>ChangeInQuantity</code> is not passed.
						</Condition>
                                <Error
                                    Description="Invalid Schedule Quantity" ErrorCode="YFS10377"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedDeliveryDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_DELIVERY_DATE" yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
                    <xsd:documentation>The date the scheduled order line is expected to be delivered by.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedShipmentDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_SHIPMENT_DATE" yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
                    <xsd:documentation>The date the scheduled order line is expected to ship by.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotNumber" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_NUMBER" yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
                    <xsd:documentation>The lot number that has been specified on the scheduled order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RevisionNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="REVISION_NO" yfc:tableName="YFS_ORDER_LINE_SCHEDULE">
                    <xsd:documentation>The revision number that has been specified on the scheduled order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ScheduleTranQuantity">
        <xsd:complexType>
            <xsd:attribute name="ChangeInQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation>
                    <xsd:documentation>UOM on which all the transactional quantities are specified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Dependency">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
				 Pass this element if any of the following needs to be done:
				<br/> A line that depends on another line needs to be added. Refer to <code>createOrder</code> for validations.
				<br/>
                    <br/>Dependency information needs to be changed for an existing line.The line needs to depend on some other line. Dependency attributes are copied onto this line from the parent line depending on dependency shipping rule.
				<br/>
                    <br/>The dependency needs to be removed. Pass <code>RemoveDependency</code> as <b>
                        <code>Y</code>
                    </b> for this operation. The line is no longer be dependent on another line. <code>DependentOnLineKey</code> is reset on this line and <code>ParentOfDependentGroup</code> is reset to <b>
                        <code>N</code>
                    </b> on the parent line if no other line depends on this parent line.   Dependency attributes defaulted to the line are retained.
				<br/>
                    <br/>Note: Modification type <code>OTHERS</code> is used for any change in dependency information for existing lines.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DependentOnPrimeLineNo"
                type="yfctype:LineNo" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Both <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> must be passed for dependent <code>OrderLines</code>.
					<Validation>
                                <Condition>The <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> combination passed does not match any other <code>OrderLine</code>.
						</Condition>
                                <Error
                                    Description="Invalid dependent on line information passed" ErrorCode="YFS10444"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DependentOnSubLineNo"
                type="yfctype:LineNo" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Both <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> must be passed for dependent <code>OrderLine</code>s.
					<Validation>
                                <Condition>The <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> combination passed does not match any other <code>OrderLine</code>.
						</Condition>
                                <Error
                                    Description="Invalid dependent on line information passed" ErrorCode="YFS10444"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DependentOnTransactionalLineId"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					This is a virtual field meant to identify <code>OrderLine</code> when <code>DependentOnPrimeLineNo</code> and <code>DependentOnSubLineNo</code> is not passed. This field is is not persistent in the database.
					<Validation>
                                <Condition> The <code>DependentOnTransactionalLineId</code> passed does not match any <code>OrderLine</code>.
						</Condition>
                                <Error
                                    Description="Invalid dependent on line information passed" ErrorCode="YFS10444"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RemoveDependency" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Determines if this dependency is removed. Valid Values: Y or N</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineInvAttRequest">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
            <xsd:documentation>
                <div>
                    <Validation>
                        <Condition>If the item on the line is not tag controlled, and the <code>OrderLineInvAttRequest</code> element is passed.
				 </Condition>
                        <Error Description="Item is not tag controlled." ErrorCode="OMP11032"/>
                    </Validation>
                    <br/>
                    <code>pending changes</code>
                </div>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
		             </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							Action to perform on this <code>OrderLineInvAttRequest</code> element.
							<br/>Default: <code>Null</code> - if not passed, <code>OrderLineInvAttRequest</code> element will be added if it doesn't exist or modified if it does exist.
							<br/>Valid Values: 
								<br/>
                            <b>
                                <code>REMOVE</code>
                            </b> - Removes the <code>OrderLineInvAttRequest</code> element from the order line.
							<Validation>
                                <Condition>If the item on the line is always tag controlled, the <code>OrderLineInvAttRequest</code> element cannot be removed.</Condition>
                                <Error
                                    Description="INV: Missing Inventory Identifier Attribute." ErrorCode="INV10029"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BatchNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Batch Number that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute1" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE1" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>Lot Attribute 1 used to specify the details of the lot.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute2" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE2" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>Lot Attribute 2 used to specify the details of the lot.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute3" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE3" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>Lot Attribute 3 used to specify the details of the lot.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotKeyReference"
                type="yfctype:Text-4000" use="optional">
                <xsd:annotation yfc:columnName="LOT_KEY_REFERENCE" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Lot key reference that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotNumber" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_NUMBER" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Lot Number that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturingDate" use="optional">
                <xsd:annotation yfc:columnName="LOT_MANUFACTURE_DATE" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Lot manufacturing date that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Date"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="RevisionNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="REVISION_NO" yfc:tableName="YFS_ORDER_LINE_REQ_TAG">
                    <xsd:documentation>The Revision Number that has been specifed in the Order Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductItems">
        <xsd:annotation>
            <xsd:documentation>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
             </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ProductItem"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductItem">
        <xsd:annotation yfc:tableName="YFS_ORDER_SER_PROD_ITEM">
            <xsd:documentation>
                <div>
                    <code>ProductItem</code> contains the definition of product items involved in a stand-alone service line. This definition is used to calculate the service line quantity and to identify the skills needed to provide the service. These product items will not be fulfilled as part of order fulfillment process.
			</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Action" type="yfctype:Action" use="required">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Action to perform
					<br/>Valid Values:
					<br/>
                            <b>
                                <code>CREATE</code>
                            </b> - Adds a new product item definition for the Service Line. Service line quantity will be recalculated.
					<br/>
                            <b>
                                <code>REMOVE</code>
                            </b> - Removes a new product item definition for the Service Line. Service line <code>Quantity</code> will be recalculated. If there are no other definitions existing for the Service line, its <code>Quantity</code> becomes zero.
					<br/> The Modification Types used for adding a new definition are :
					<br/>
                            <br/>
                            <code>Add Product Item Definition for Stand Alone Service Line</code> - Validates status of Service Line for which a product item definition being added. The maximum allowed status for this modification type is less than schduled.
					  <br/>
                            <br/> The Modification Types used for removing a definition are:
					  <br/>
                            <br/>
                            <code>Remove Product Item Definition for Stand Alone Service Line</code> - Validates status of Service Line for which a product item definition being added. The maximum allowed status for this modification type is less than schduled.
					  <br/>
                            <br/>For validations performed while modifying a <code>ProductItem</code>, refer to <code>createOrder</code> api documentation.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_SER_PROD_ITEM">
                    <xsd:documentation>The unique identifier for the Product Item. The item ID, UOM and OrganizationCode form the unique item identifier for Product Item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="QUANTITY" yfc:tableName="YFS_ORDER_SER_PROD_ITEM">
                    <xsd:documentation>Product item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_SER_PROD_ITEM">
                    <xsd:documentation>Unit of measure of the product item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="BundleParentLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
					Identifies the bundle parent order line for this line.
					<Validation>
                    <Condition>
                        <code>OrderLineKey</code>
                        <i>or</i>
                        <code>PrimeLineNo</code> and <code>SubLineNo</code>
                        <i>or</i>
                        <code>TransactionalLineId</code> must be passed, and must identify a valid order line.
						</Condition>
                    <Error Description="Invalid order line" ErrorCode="OMP10009"/>
                </Validation>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Action to perform
					<br/>Valid Values:
					<br/>
                            <b>
                                <code>CREATE</code>
                            </b> - To add a component to already existing bundle. Modification ADD_COMPONENT is executed on bundle parent line.
										<Validation>
                                <Condition>A non-bundle parent line cannot be specified as bundle parent line.
										</Condition>
                                <Error
                                    Description="Line is not a bundle parent line" ErrorCode="OMP11080"/>
                            </Validation>
                            <br/>
                            <b>
                                <code>REMOVE</code>
                            </b> - To remove a component from a bundle. Modification REMOVE_COMPONENT is executed on bundle parent line.
						<Validation>
                                <Condition>A bundle parent line must have at least one component.
						</Condition>
                                <Error
                                    Description="Bundle must have at least one component" ErrorCode="OMP11079"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					Primary key generated by the system. May be passed to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i>
                            <br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code> can be passed.

					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					The order line number. Must be passed with the <code>SubLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
					Must be passed with the <code>PrimeLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                            <i>or</i> the <code>OrderLineKey</code> can be passed.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in Line Associations. This value is not stored in the database. While creating a new line and associating it with another line, the same attribute must be passed in the <code>OrderLine</code> element and in this element to uniquely identify a line if <code>OrderLineKey</code> or <code>PrimeLineNo</code> and <code>SubLineNo</code> are not present.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineReservations">
        <xsd:annotation>
            <xsd:documentation>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
             </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderLineReservation"/>
            </xsd:sequence>
            <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The <code>Reset</code> flag can be passed as <code>Y</code> to reset all previous reservations and replace them with the new list.  When reservations are removed, any inventory allocations will be cleared.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineReservation">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_RESERVATION">
            <xsd:documentation>
                <div>Identifies a reservation to make for the order line.  Existing reservations cannot be modified directly; instead, if a new reservation is created with the same reservation attributes as an existing reservation, the new reservation will be consolidated into the existing reservation, and demand will be created for the new quantity.<br/>If new quantity is being added to the order line in the same call that a reservation is being added, and the reservation matches the order line's attributes, then the reservation will be rolled up in to the order line, and the matching quantity will move to "Reserved" status.</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BatchNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The batch number that has been specified on the reservation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DemandType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DEMAND_TYPE" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>
                        <div>
                        The inventory demand type used by the reservation.  For all user-created reservations, this will be defaulted from the document params.
                        When promising server is present and existing inventory reservation is used in order line reservation, pass same DemandType as inventory reservation</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The reserved item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotNumber" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_NUMBER" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The lot number that has been specified on the reservation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Node" type="yfctype:Key" use="required">
                <xsd:annotation yfc:columnName="NODE_KEY" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The ship node at which the item has been reserved.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductClass"
                type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The reserved item's product class.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" type="yfctype:Quantity" use="required">
                <xsd:annotation yfc:columnName="QUANTITY" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>
                        <div>The quantity to reserve.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequestedReservationDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="REQ_RESERVATION_DATE" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The date when the reserved inventory is needed, such as the shipping date.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReservationID" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="RESERVATION_ID" yfc:tableName="YFS_INVENTORY_RESERVATION">
                    <xsd:documentation>
                        <div>The reservation ID from an existing inventory reservation.  If the reservation exists, that quantity will be consumed first.  Any quantity that cannot be obtained from the reservation will be reserved on the fly.  If <code>ReservationID</code> is not passed, all quantity for this reservation will be reserved on the fly.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RevisionNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="REVISION_NO" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The revision number that has been specified on the reservation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_LINE_RESERVATION">
                    <xsd:documentation>The reserved item's unit of measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderOverride">
        <xsd:annotation yfc:tableName="YFS_ORDER_OVERRIDE">
            <xsd:documentation>This table stores a set of flags indicating whether an order attribute has been overridden.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="QuantityLimitOverridden"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="QUANTITY_LIMIT_OVERRIDDEN" yfc:tableName="YFS_ORDER_OVERRIDE">
                    <xsd:documentation>This flag indicates whether the order line's min/max quantity limits have been overridden. Valid values: 'Y' or 'N'.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ValidateItem" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="VALIDATE_ITEM" yfc:tableName="YFS_ORDER_OVERRIDE">
                    <xsd:documentation>This flag indicates whether item validation is enabled. Valid Values: Y or N. Default: Y</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalLinePrices">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:AdditionalLinePrice"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalLinePrice">
        <xsd:annotation yfc:tableName="YFS_ADDNL_LINE_PRICE">
            <xsd:documentation>Maintains details of various Price Types applicable to an order line.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="Promotions">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="Promotion">
                                <xsd:annotation yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>
                                    <div>
											Promotions that are associated to an order or any of its lines. PromotionId must be passed to identify the promotion.
										</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Action"
                                    type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
										Action to perform on this promotion.
										<br/>Default: <code>Null</code> - if not passed, promotion will be added if it doesn't exist or modified if it does exist.
										<br/>Valid Values: 
											<br/>
                                    <b>
                                    <code>CREATE</code>
                                    </b> - Add the specified promotion to the order.
											<br/>
                                    <b>
                                    <code>REMOVE</code>
                                    </b> - Removes the specified promotion from the order.
										<Validation>
                                    <Condition>If an award record is associated with this promotion, it may not be removed.</Condition>
                                    <Error
                                    Description="You are attempting to delete a Promotion associated with an award on the order." ErrorCode="OMP77_10004"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>If a promotion already exists, can't add the same promotion twice.</Condition>
                                    <Error
                                    Description="Record already exists in the database." ErrorCode="YFC0001"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DenialReason"
                                    type="yfctype:Text-200" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DENIAL_REASON" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>If the promotion is denied, the reason for the denial.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ExclusivityId"
                                    type="yfctype:VARCHAR2-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXCLUSIVITY_ID" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>ExclusivityID is used to group set of Promotions for mutual exclusivity. Exclusivity Type settings are applicable among Pricing Rules having same ExclusvityID.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ExclusivityType"
                                    type="yfctype:VARCHAR2-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="EXCLUSIVITY_TYPE" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>Exclusivity Type of the Promotion. Valid Values are: ALL, RULE_TYPE_ITEM, RULE_TYPE_ORDER, ONE_RULE_WITHIN_ID and GLOBAL_RULE</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="IsExternal"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_EXTERNAL" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>This flag specifies whether the promotion is external.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OverrideAdjustmentValue" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OVERRIDE_ADJUSTMENT_VALUE" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>For promotions, the overridden Adjustment value used in place of the configured Adjustment of the corresponding Pricing Rule Action.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Price"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OverrideMaxAdjustment" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OVERRIDE_MAX_ADJUSTMENT" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>
                                    <div> For Coupon promotions, the overridden maximum adjustment value for the coupon pricing rule. 
										</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Money"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="Priority" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRIORITY" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>This attribute is used to prioritize Promotion among a list of applicable Promotions with same Exclusivity settings. Higher the value of this attribute, higher the priority of Promotion, to be applied to an order/item.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:NUMBER-10"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PromotionApplied"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_APPLIED" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>A flag indicating if the promotion is applied. When ValidateReferences=Y the value can be 'P' meaning the promotion was partially applied.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PromotionCategory"
                                    type="yfctype:VarcharID-24" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_CATEGORY" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>Specifies category of external promotion. Valid values are ORDER,ITEM and SHIPPING</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionId"
                                    type="yfctype:PromoCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_ID" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>This identifies the promotion that is added to this order. This will be used as the CouponID when determining the price of the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionType"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_TYPE" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>The type of promotion. Like DISCOUNT, BOGO.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ValidateReferences"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>For Coupon promotions, if <code>'Y"</code> the references will be validated and only valid references will be used</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="Reset" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>Determines whether all Promotions, except those listed below, 
									should be removed.</div>
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Flag">
                                    <xsd:enumeration value="N"/>
                                    <xsd:enumeration value="Y"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="Awards">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="Award">
                                <xsd:annotation yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>
                                    <div>
											Awards that are created out of the promotions. A single promotion can result in to multiple awards. AwardId must be passed to identify the award.
											<Validation>
                                    <Condition>PromotionId must be passed and it must be a valid one to add the award</Condition>
                                    <Error
                                    Description="All awards must be associated to a promotion on the order" ErrorCode="OMP77_10003"/>
                                    <Condition>Manual Line Charge generated by this Award must not conflict with non-manual Line Charge already on OrderLine</Condition>
                                    <Error
                                    Description="Manual Award charge and pricing charge are conflicting." ErrorCode="OMP85_0062"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Action"
                                    type="yfctype:Action" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												Action to perform on this <code>Award</code>.
												<br/>Default: <code>Null</code> - if not passed awards will be added if not exist or modified.
												<br/>Valid Values: 
												<br/>
                                    <b>
                                    <code>REMOVE</code>
                                    </b> - Removes the award.
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardAmount"
                                    type="yfctype:ExternalAmount" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_AMOUNT" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The amount of discount or charge that was applied as a part of this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardApplied"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_APPLIED" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>A flag indicating if the award is applied. When ValidateReferences=Y the value can be 'P' meaning the award was partially applied</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardId"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_ID" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>A unique identifier for the award. If pricing is maintained within the system, this will be the PricingRuleName of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardQuantity" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_QUANTITY" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>Indicates the remaining free gift quantity available for the promotion.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Quantity"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardType"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_TYPE" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The type of award. If pricing is maintained within the system, this will be the RuleCategory of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeCategory"
                                    type="yfctype:ChargeCategory" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The charge category of the LineCharge or HeaderCharge for which this award was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeName"
                                    type="yfctype:ChargeName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The charge name of the LineCharge or HeaderCharge for which this award was created.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="DenialReason"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DENIAL_REASON" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>If the award is denied, the reason for the denial.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Description"
                                    type="yfctype:Text-2000" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DESCRIPTION" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>A description for the award. If pricing is maintained within the system, this will be the Description of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PosReasonCode"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="POS_REASON_CODE" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>Reason this promotion was applied.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PromotionId"
                                    type="yfctype:PromoCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_ID" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>This identifies the promotion that is added to this order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="IsPriceLocked" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PRICE_LOCKED" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                    <xsd:documentation>
				            		Flag to indicate if the AdditionalLinePrice is Price-Locked. If passed with a value of <code>Y</code>, internal logic will use the passed Price
				            		from input for calculation instead of the Price generated by the Pricing Engine.
				            	</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPriceTypeForInformationOnly"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PRICE_TYPE_INFO_ONLY" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                    <xsd:documentation>Flag to indicate that his Price Type is for informational use only.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div> Operation to perform
										<br/>Valid Values: 
										<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
										<br/>
                            <b>
                                <code>Create</code>
                            </b> - Creates a new record. 
										<br/>
                            <b>
                                <code>Modify</code>
                            </b> - Change values in an existing record.
										<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
										<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
									</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Price" use="optional">
                <xsd:annotation yfc:columnName="PRICE" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                    <xsd:documentation>Price value of the corresponding Price Type.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Price"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="PriceTypeName"
                type="yfctype:PriceTypeName" use="optional">
                <xsd:annotation yfc:columnName="PRICE_TYPE_NAME" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                    <xsd:documentation>Price Type associated with this record.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLinePriceMatch">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_PRICE_MATCH">
            <xsd:documentation>This Table is a hang off table for the yfs_order_line table and stores the information about the price match and the relationship between Order Line and Price Match</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CompetingCharges" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="COMPETING_CHARGES" yfc:tableName="YFS_ORDER_LINE_PRICE_MATCH">
                    <xsd:documentation>Competitors Charges applied to the order line</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DTDOccurrence" type="xsd:string" use="optional"/>
            <xsd:attribute name="ExistingCharges" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="EXISTING_CHARGES" yfc:tableName="YFS_ORDER_LINE_PRICE_MATCH">
                    <xsd:documentation>Existing Charges for the item, used to derive the price match difference</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExistingDiscounts" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="EXISTING_DISCOUNTS" yfc:tableName="YFS_ORDER_LINE_PRICE_MATCH">
                    <xsd:documentation>Existing Discounts for the item, used to derive the price match difference</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExistingUnitPrice"
                type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="EXISTING_UNIT_PRICE" yfc:tableName="YFS_ORDER_LINE_PRICE_MATCH">
                    <xsd:documentation>Existing price for a unit quantity of item, used to derive the price match difference</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE_PRICE_MATCH">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLinePriceMatchKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="ORDER_LINE_PRICE_MATCH_KEY" yfc:tableName="YFS_ORDER_LINE_PRICE_MATCH">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE_PRICE_MATCH table</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PriceMatchKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PRICE_MATCH_KEY" yfc:tableName="YFS_ORDER_LINE_PRICE_MATCH">
                    <xsd:documentation>The primary key for the YFS_PRICE_MATCH table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductServiceAssocs">
        <xsd:annotation>
            <xsd:documentation>
                <div>
                    <br/> This element cannot be passed for orders with <code>pending changes</code>.
             </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ProductServiceAssoc"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductServiceAssoc">
        <xsd:annotation yfc:tableName="YFS_ORDER_PROD_SER_ASSOC">
            <xsd:documentation>
                <div>
                    <code>ProductServiceAssoc</code> contains the <code>ProductLine</code> and <code>ServiceLine</code> elements which identify the Product and Service line. Both <code>ProductLine</code> and <code>ServiceLine</code> elements must be passed to uniquely identify the lines. If multiple associations need to be made, multiple <code>ProductServiceAssoc</code> nodes should be passed. Refer to <code>ProductLine</code> and <code>ServiceLine</code> elements for further details.
			</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ProductLine"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ServiceLine"/>
            </xsd:all>
            <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Action to perform
					<br/>Valid Values:
					<br/>
                            <b>
                                <code>CREATE</code>
                            </b> - Adds a new association between a Product and Service  line.
					<br/>
                            <b>
                                <code>REMOVE</code>
                            </b> - Removes the association between the Product and the Service line. The <code>Quantity</code> for the Service or Delivery line is recalculated. If there are no other associations existing for the Service or Delivery line, its <code>Quantity</code> becomes zero.  The options attached to the Delivery or Service Line are also removed if quantity has become zero.
					<br/> The Modification Types used for adding a new Assocation are :
					<br/>
                            <br/>
                            <code>Associate Delivery Line with Product Line </code> - Validates status of Delivery line being associated with a Product line. The maximum allowed status for this modification type is less than shipped.
					  <br/>
                            <code>Associate Product Line with Delivery Line </code> - Validates status of Product line being associated with a Delivery line. The maximum allowed status for this modificaiton type is less than scheduled. A scheduled Product line cannot be associated with a delivery line.
					  <br/>
                            <code>Associate Service Line with Product Line </code> - Validates status of Service lines being associated with a Product line. The maximum allowed status for this modification type is less than shipped.
					  <br/>
                            <code>Associate Product Line with Service Line </code> - Validates status of Product lines being associated with a Service line. The maximum allowed status for this modification type is less than shipped.
					  <br/>
                            <br/> When a  Product and Delivery lines association is created, the first two modification types specified above are used to validate the statuses of Product and Delivery lines respectively. For association between Product and Service line, the next two modification types are used.
					  <br/>
                            <br/> The Modification Types used for removing an association are:
					  <br/>
                            <br/>
                            <code>Remove Delivery Line From Product Line Association </code> - Validates the status of a delivery line before removing the association. The maximum allowed status for a delivery line for association removal is less than Scheduled.
					  <br/>
                            <code>Remove Service Line From Product Line Association </code> - Validates the status of a Service  line before removing the association. The association can be removed even after Service line has been completed, that is, the association can be removed even if the Service line has been shipped.
					  <br/>
                            <br/>For validations performed while modifying a <code>ProductServiceAssociation</code>, refer to <code>createOrder</code> api documentation.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_QUANTITY" yfc:tableName="YFS_ORDER_PROD_SER_ASSOC">
                    <xsd:documentation>Product quantity for which service quantity is stored in ServiceQuantity attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_QUANTITY" yfc:tableName="YFS_ORDER_PROD_SER_ASSOC">
                    <xsd:documentation>Service Capacity to be used for delivering (or servicing )product quantity mentioned.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProductLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The sub line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>This is a virtual field meant to identify order lines when prime line and sub line numbers are not passed. This field is used in line associations, and is not persistent in the database.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ServiceLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The order line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The sub line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>This is a virtual field meant to identify order lines when prime line and sub line numbers are not passed. This field is used in line associations, and is not persistent in the database.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineRelationships">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderLineRelationship"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLineRelationship">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE_RELATIONSHIP">
            <xsd:documentation>
                <div>
                    <code>OrderLineRelationship</code> contains the <code>ParentLine</code> and <code>ChildLine</code> elements which identify the Parent and Child line. Both <code>ParentLine</code> and <code>ChildLine</code> elements must be passed to uniquely identify the lines. If multiple OrderLineRelationships need to be made, multiple <code>OrderLineRelationship</code> nodes should be passed. Refer to <code>ParentLine</code> and <code>ChildLine</code> elements for further details.
			<Validation>
                        <Condition> If circular dependency is detected for order lines of the same <code>RelationshipType</code>. The following error is displayed.
					</Condition>
                        <Error Description="Circular dependency exists." ErrorCode="YFS10446"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ParentLine"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ChildLine"/>
            </xsd:all>
            <xsd:attribute name="Operation" type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Operation to perform
					<br/>Valid Values:
					<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Removes the relationship between the Parent and the Child line. If <code>Operation</code> is not passed the relationship will be created if it does not exist already.
					<br/> The Modification Type used for adding a new relationships are :
					<br/>
                            <br/>
                            <code>Add Line Relationship</code> - Validates status of both the Parent and the Child line for addition of the relationship. The maximum allowed status for this modification type is less than Cancelled.
					  <br/> The Modification Type used for removing an relationships are:
					  <br/>
                            <br/>
                            <code>Delete Line Relationship</code> - Validates status of both the Parent and the Child line for deletion of the relationship. The maximum allowed status for this modification type is less than Cancelled.
					  <br/>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RelationshipType"
                type="yfctype:Text-40" use="required">
                <xsd:annotation yfc:columnName="RELATIONSHIP_TYPE" yfc:tableName="YFS_ORDER_LINE_RELATIONSHIP">
                    <xsd:documentation>
                        <div>
					The type of relationship between the parent and the child line. There is no validation for the <code>RelationshipType</code> attribute.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ParentLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
