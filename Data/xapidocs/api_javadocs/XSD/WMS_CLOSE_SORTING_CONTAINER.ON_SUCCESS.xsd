                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemLocaleKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ITEM_LOCALE_KEY" yfc:tableName="YFS_ITEM_LOCALE">
                    <xsd:documentation>The primary key of the YFS_ITEM_LOCALE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Language" type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="LANGUAGE" yfc:tableName="YFS_ITEM_LOCALE">
                    <xsd:documentation>The language associated with the locale.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Variant" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="VARIANT" yfc:tableName="YFS_ITEM_LOCALE">
