<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/YDM/changeShipment/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/YDM/changeShipment/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="YDM_changeShipment_input.xsd" yfc:rootElement="Shipment">
        <xsd:appinfo source="yfc:structure">
            <Shipment hasAttributes="Y">
                <ScacAndService hasAttributes="Y"/>
                <ToAddress hasAttributes="Y"/>
                <BillToAddress hasAttributes="Y"/>
                <FromAddress hasAttributes="Y"/>
                <MarkForAddress hasAttributes="Y"/>
                <Containers hasAttributes="Y">
                    <Container hasAttributes="Y">
                        <ContainerDetails hasAttributes="Y">
                            <ContainerDetail hasAttributes="Y">
                                <ShipmentTagSerials hasAttributes="Y">
                                    <ShipmentTagSerial hasAttributes="Y"/>
                                </ShipmentTagSerials>
                                <ShipmentLine hasAttributes="Y">
                                    <ShipmentLineInvAttRequest hasAttributes="Y"/>
                                </ShipmentLine>
                            </ContainerDetail>
                        </ContainerDetails>
                        <Instructions hasAttributes="Y">
                            <Instruction hasAttributes="Y"/>
                        </Instructions>
                    </Container>
                </Containers>
                <Instructions hasAttributes="Y">
                    <Instruction hasAttributes="Y"/>
                </Instructions>
                <Notes>
                    <Note hasAttributes="Y">
                        <User hasAttributes="Y"/>
                    </Note>
                </Notes>
                <OrderReleases>
                    <OrderRelease hasAttributes="Y">
                        <Order hasAttributes="Y"/>
                    </OrderRelease>
                </OrderReleases>
                <ShipmentLines hasAttributes="Y">
                    <ShipmentLine hasAttributes="Y">
                        <BundleParentLine hasAttributes="Y"/>
                        <ShipmentLineInvAttRequest hasAttributes="Y"/>
                        <ShipmentTagSerials hasAttributes="Y">
                            <ShipmentTagSerial hasAttributes="Y"/>
                        </ShipmentTagSerials>
                        <Instructions hasAttributes="Y">
                            <Instruction hasAttributes="Y"/>
                        </Instructions>
                    </ShipmentLine>
                </ShipmentLines>
                <ShipmentCharges hasAttributes="Y">
                    <ShipmentCharge hasAttributes="Y"/>
                </ShipmentCharges>
                <AdditionalDates hasAttributes="Y">
                    <AdditionalDate hasAttributes="Y"/>
                </AdditionalDates>
                <ShipmentStatusAudit hasAttributes="Y"/>
                <SpecialServices>
                    <SpecialService hasAttributes="Y"/>
                </SpecialServices>
                <OrderInvoiceList>
                    <OrderInvoice hasAttributes="Y">
                        <TaxBreakupList>
                            <TaxBreakup hasAttributes="Y"/>
                        </TaxBreakupList>
                        <LineDetails>
                            <LineDetail hasAttributes="Y">
                                <LineDetailTranQuantity hasAttributes="Y"/>
                                <LineChargeList>
                                    <LineCharge hasAttributes="Y"/>
                                </LineChargeList>
                            </LineDetail>
                        </LineDetails>
                        <HeaderChargeList>
                            <HeaderCharge hasAttributes="Y"/>
                        </HeaderChargeList>
                    </OrderInvoice>
                </OrderInvoiceList>
                <ShipmentHoldTypes>
                    <ShipmentHoldType hasAttributes="Y"/>
                </ShipmentHoldTypes>
            </Shipment>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="Shipment" type="yfc:ShipmentXSDType">
        <xsd:annotation yfc:tableName="YFS_SHIPMENT">
            <xsd:documentation>
                <div>
	   The shipment to be changed. If <code>Action</code> is <code>Modify</code> or <code>Delete</code>, the shipment must be identified. Pass  <code>ShipmentKey</code>
                    <i>or</i> pass <code>ShipmentNo</code>, <code>ShipNode</code> and <code>SellerOrganizationCode</code>  to identify the shipment.
	   <br>In the scenario when one or more shipment lines with different information passed to the API, the order release information from the first available <code>ShipmentLine</code> will be used to derive the <code>Shipment</code> header attributes.</br>
                </div>
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="ShipmentXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ScacAndService"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ToAddress"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:BillToAddress"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:FromAddress"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:MarkForAddress"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Containers"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="Instructions">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="Instruction">
                            <xsd:annotation yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                <xsd:documentation>
                                    <div>
			The <code>InstructionDetailKey</code> attribute or <code>InstructionType</code> and <code>SequenceNo</code> attributes must be passed.
			Related shipment instructions are stored in this table. To identify the <code>Instruction</code> pass <code>InstructionDetailKey</code>, or  <code>InstructionType</code> and <code>SequenceNo</code>.
			</div>
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:attribute name="Action"
                                    type="yfctype:ID-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
		      Indicates the action that can be perfomed on the container instruction detail.
		      <br/>Default: <code>Create-Modify</code>
                                    <br/>Valid values: <code>Create</code>, <code>Delete</code>, <code>Create-Modify</code>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute
                                    name="InstructionDetailKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_DETAIL_KEY" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Primary key attribute of YFS_INSTRUCTION_DETAIL table that identifies an instruction.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="InstructionText"
                                    type="yfctype:InstructionText" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_TEXT" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Free-form text indicating any additional information related to the instruction type or otherwise.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="InstructionType"
                                    type="yfctype:InstructionType" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_TYPE" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Categorization of instructions so that they can be grouped together for reference. For example, you may have instructions of type Pick, Pack, and Ship for an order.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="InstructionURL"
                                    type="yfctype:URL" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_URL" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>The URL relating to and/or containing the instructions.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="InstructionUsage"
                                    type="yfctype:InstructionUsage" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_USAGE" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Another way of categorization of instructions, over and above the instruction type.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="ReferenceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_KEY" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Link to the entity key for which the instruction is stored. For example, if this record stores order header level instructions, then this attribute stores the value of order header key.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="SequenceNo"
                                    type="yfctype:Count" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SEQUENCE_NO" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>You could have an instruction exceeding the limits of a single record. Sequence number is used to provide a way to store a link between such instructions.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="TableName"
                                    type="yfctype:TableName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TABLE_NAME" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Identifies the entity for which this instruction is stored. For example, if this record stores order header instructions, then you will see this field reading YFS_ORDER_HEADER.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="Replace" type="yfctype:Flag" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>
                                <div>
		       Indicates if information in the database must be replaced by the Instructions information passed. 
		       <br/>Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: 
			<br/>
                                    <b>
                                    <code>Y</code>
                                    </b> - Replaces all existing <code>Instructions</code> 
			with the values passed in the input XML. Any new <code>Instructions</code> passed in the XML are created. Existing <code>Instructions</code> are deleted if they are not passed in the input XML, or  modified with the attributes passed in the XML. 
			<br/>
                                    <b>
                                    <code>N</code>
                                    </b> - Any new <code>Instructions</code> passed in the XML are created. Existing <code>Instructions</code> are  modified with the attributes passed in the XML.  Existing <code>Instructions</code> that are in the database and not passed in the input XML are not changed.
		       </div>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Notes"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderReleases"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipmentLines"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipmentCharges"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AdditionalDates"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipmentStatusAudit"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:SpecialServices"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderInvoiceList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipmentHoldTypes"/>
        </xsd:all>
        <xsd:attribute name="Action" type="yfctype:ID-40" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
	    Indicates the action to be perfomed on the shipment. If <code>Action</code> is <code>Modify</code>, <code>Cancel</code> or <code>Delete</code>, pass <code>ShipmentKey</code>
                        <i>or</i>
                        <code>ShipmentNo</code>, <code>ShipNode</code> and <code>SellerOrganizationCode</code> to identify the shipment. 
	    <br/>Default: <code>Create-Modify</code>
                        <br/>Valid Values: <code>Create</code>, <code>Modify</code>, <code>Create-Modify</code>, <code>Delete</code>, <code>Cancel</code>.
	    <br/>
		If <code>Action</code> is <code>Cancel</code> then except for <code>Shipment</code> and <code>ShipmentLines</code> all related entities are deleted. Shipment status moves to <code>Cancelled (9000)</code> and all the ShipmentLines' quantity becomes 0. 
		<Validation>
                            <Condition>
                                <code>Action</code> is <code>Create</code> and record already exists
		</Condition>
                            <Error Description="" ErrorCode="YCP0051"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>Action</code> is <code>Modify</code> and record does not exist 
			</Condition>
                            <Error Description="" ErrorCode="YCP0045"/>
                        </Validation>
                        <br/>Only <code>Shipments</code> without <code>ShipmentLines</code> can be deleted.
	    	<Validation>
                            <Condition>Once a shipment is invoiced, it cannot be cancelled.
				</Condition>
                            <Error
                                Description="Certain modifications are not allowed on invoiced shipment." ErrorCode="YDM77_00002"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ActualDeliveryDate"
            type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="ACTUAL_DELIVERY_DATE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The date on which the shipment was actually delivered at the buyer organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ActualShipmentDate"
            type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="ACTUAL_SHIPMENT_DATE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The actual shipment date. This is updated by the confirmShipment API.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AllowOverage" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="ALLOW_OVERAGE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>This flag indicates whether an overage is allowed for the shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AppointmentNo"
            type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="APPOINTMENT_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>A number which helps the ship node and the carrier identify the pick up details and any relevant details about the shipment delivery.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AssignedToUserId" type="yfctype:LoginId" use="optional">
            <xsd:annotation yfc:columnName="ASSIGNED_TO_USER_ID" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The backroom pick user who is currently working on this shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BackOrderRemovedQuantity"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
	    	Pass as <code>Y</code> if  removed quantity should be backordered. 
		<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or  <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BolNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="BOL_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The bill of lading number.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BuyerMarkForNodeId" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="BUYER_MARK_FOR_NODE_ID" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>Mark For Node as identified by the buyer for eg: it could be a person,store,DC.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BuyerReceivingNodeId"
            type="yfctype:ShipNode" use="optional">
            <xsd:annotation yfc:columnName="BUYER_RECEIVING_NODE_ID" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>Receiving node as identified by the buyer for eg:Store,DC Number.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CODPayMethod" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="COD_PAY_METHOD" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The pay method for the COD amount.Valid values are CASH, CASHIERS_CHECK, BUSINESS_CHECK, MONEY_ORDER, PERSONAL_CHECK, ANY. This will be a common code, with code type COD_PAY_METHOD.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CancelRemovedQuantity" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>Specify if removed quantity needs to be cancelled.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                        <br/>If the <code>BackOrderRemovedQuantity</code> flag is passed as <b>
                            <code>Y</code>
                        </b>, this flag is ignored.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CancelShipmentOnZeroTotalQuantity"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>Specify if shipment needs to be cancelled when all shipment lines have zero quantity.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CarrierAccountNo" type="yfctype:AccountNo" use="optional">
            <xsd:annotation yfc:columnName="CUSTCARRIER_ACCOUNT_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The account number of the customer or buying organization with the carrier. This is used for arriving at the billing account number. See YFS_SHIPMENT_CONTAINER.carrier_bill_account.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CarrierServiceCode" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The carrier service availed for transporting this shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CarrierSortLocationId"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="CARRIER_SORT_LOCATION_ID" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>
                    <div>
                        <Validation>
                            <Condition>If <code>CarrierSortLocationId</code> is not void and not a valid location in the <code>Shipnode</code>.
					</Condition>
                            <Error
                                Description="Location does not exist." ErrorCode="WMS00005"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CommercialValue" type="yfctype:Money" use="optional">
            <xsd:annotation yfc:columnName="COMMERCIAL_VALUE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The commercial or declared value of the shipment for insurance purposes.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Currency" type="yfctype:Currency" use="optional">
            <xsd:annotation yfc:columnName="CURRENCY" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The currency corresponding to the shipment charges.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerPoNo" type="yfctype:Text-100" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The customer PO # for the order during order creation time. While creating shipments, this value is consolidated across all orders belonging to this shipment and stored in the shipment header.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DestinationZone" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="DESTINATION_ZONE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>This field is not currently used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DoNotMixEnterprisesInLoad"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
			When DoNotMixEnterprisesInLoad is set to <code>Y</code> and rule <code>ALLOW_ADDITION_OF_SHIPMENT_NA_YANTRA_TO_LOAD</code> is enabled, system will NOT mix shipments belonging to different enterprises in a load.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DoNotVerifyCaseContent" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="DO_NOT_VERIFY_CASE_CONTENT" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>Thif flag indicates whether the case content is to be verified.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DoNotVerifyPalletContent"
            type="yfctype:Flag" use="optional">
            <xsd:annotation
                yfc:columnName="DO_NOT_VERIFY_PALLET_CONTENT" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>This flag indicates whether the pallet content is to be verified.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DownLoadCount" type="yfctype:Count" use="optional">
            <xsd:annotation yfc:columnName="DOWNLOAD_COUNT" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The number of times that the shipment information has been downloaded from the user interface.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="EmailReturnLabel" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="EMAIL_RETURN_LABEL" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>This field is not currently used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExpectedDeliveryDate"
            type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="EXPECTED_DELIVERY_DATE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The date on which the shipment is expected to be delivered at the buyer organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExpectedShipmentDate"
            type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="EXPECTED_SHIPMENT_DATE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The shipment date as planned or arrived at in the system.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExportTaxpayerID" type="yfctype:ID-40" use="optional">
            <xsd:annotation yfc:columnName="EXPORT_TAXPAYER_ID" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The tax payer ID of the shipping organization if the shipment is meant for export.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FreightTerms" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="FREIGHT_TERMS" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The entity that pays the freight. Freight Terms determines whether a shipment is Pre Paid, 3rd Party Billing or Collect Billing.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FromAppointment" type="yfctype:Date" use="optional">
            <xsd:annotation yfc:columnName="FROM_APPOINTMENT" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The time after which an appointment can be taken by the ship node with the carrier.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="GenerateBolNo" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
		    Pass as <b>
                            <code>Y</code>
                        </b> to generate a VICS standard <code>BolNo</code>. It is generated if <code>BolNo</code> is void using the Shipnode's <code>UCCPrefix</code> defined in YFS_Organization table, a one up sequence number and checkdigit computed using modulo 10 algorithm.
		    <br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="HeldForConfirmation" type="yfctype:String" use="optional"/>
        <xsd:attribute name="HoldLocation" type="yfctype:LocationId" use="optional">
            <xsd:annotation yfc:columnName="HOLD_LOCATION" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>Deposit location of picked items of shipment which are waiting for customer pick up in a store. If there are multiple locations, then those should be updated in a comma separated string. Primarily used in store application for backroom picking.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ITDate" type="yfctype:Date" use="optional">
            <xsd:annotation yfc:columnName="IT_DATE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The date on which the IT number was stamped.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ITNNo" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="ITN_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The field is used for storing the Internal Transaction Number(ITN), FTSR Exemption Number or XTN returned by Automated Export System(AES) after filing the Shipper's Export Declaration(SED).</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ITNo" type="yfctype:NText-30" use="optional">
            <xsd:annotation yfc:columnName="IT_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The license number given by government or customs for shipments going to bonded warehouses.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ManifestNo" type="yfctype:DocumentNo" use="optional">
            <xsd:annotation yfc:columnName="MANIFEST_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>An attribute used to store manifest number assigned by carrier server. ManifestNo on Shipment is just a reference attribute pointing to manifest data maintained in an external system. It is not related to the manifesting process in the system.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NumOfCartons" type="yfctype:ContainerCount" use="optional">
            <xsd:annotation yfc:columnName="NUM_OF_CARTONS" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The number of cartons in the shipment. This field is only an approximation and not accurate depiction of the number of containers in a shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NumOfPallets" type="yfctype:ContainerCount" use="optional">
            <xsd:annotation yfc:columnName="NUM_OF_PALLETS" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The number of pallets in the shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OriginZone" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="ORIGIN_ZONE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>This field is not currently used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OverrideModificationRules"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
		    Pass as <b>
                            <code>Y</code>
                        </b> to  overrides the statuses of shipment that can be considered for various modifications.
		    <br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OverrideReceivingNode" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
                       If OverrideReceivingNode is set to <code>Y</code> then system allows user to pass receiving nodes different from receiving nodes defined on the release line during <code>changeShipment</code> call. The node passed must be a valid node.
					   <br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or  <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PackAndHold" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="PACK_AND_HOLD" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>This flag indicates whether the order needs to be packed and put away for retrieval at a later date. It needs to be released manually in order to ship the order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PackListType" type="yfctype:Text-20" use="optional">
            <xsd:annotation yfc:columnName="PACKLIST_TYPE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The packlist type. One of the attributes used in shipment consolidation.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PickListNo" type="yfctype:DocumentNo" use="optional">
            <xsd:annotation yfc:columnName="PICK_LIST_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>A number that indicates a group of shipments which will be picked together. A picklist is a document used for picking. It is generated for a ship node across one or more shipments and contains information on the shipment lines and the quantity for each line that is to be picked. Picklist is created by the printPickList API.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PickticketNo" type="yfctype:VarcharID-24" use="optional">
            <xsd:annotation yfc:columnName="PICKTICKET_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>A number assigned to group related shipments under one head. This is input by the user.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PodNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="POD_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The proof of delivery number.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ProNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="PRO_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The progressive number assigned by a carrier to track a shipment (usually for LTL shipments).</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ProfileID" type="yfctype:ShipmentProfileID" use="optional">
            <xsd:annotation yfc:columnName="PROFILE_ID" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>
                    <div>
                 Each <code>Shipment</code> has a ProfileID which is derived based on its attributes.
                </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReceivingNode" type="yfctype:ShipNode" use="optional">
            <xsd:annotation yfc:columnName="RECEIVING_NODE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The node that receives the shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RequestedCarrierServiceCode"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation
                yfc:columnName="REQUESTED_CARRIER_SERVICE_CODE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The requested carrier service code.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RequestedDeliveryDate"
            type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="REQUESTED_DELIVERY_DATE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The date on which the shipment must be delivered at the buyer organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RequestedShipmentDate"
            type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="REQUESTED_SHIPMENT_DATE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The shipment date as requested by the buyer organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReturnBillingAccount"
            type="yfctype:AccountNo" use="optional">
            <xsd:annotation yfc:columnName="RETURN_BILLING_ACCOUNT" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The account number to be used for billing the return shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReturnCarrierService"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="RETURN_CARRIER_SERVICE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The carrier service to be used for the return shipment associated with the outbound shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReturnFreightTerms" type="yfctype:Text-3" use="optional">
            <xsd:annotation yfc:columnName="RETURN_FREIGHT_TERMS" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The entity that pays the freight for the return shipment. Freight Terms determines whether a shipment is Pre Paid, 3rd Party Billing or Collect Billing. The valid value for Pre Paid is PRE, for 3rd Party Billing is TPB and COL for collect billing.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SCAC" type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>
                    <div>
		    If changed so that <code>CarrierType</code> changes from <code>PARCEL</code> to <code>LTL</code>or<code>TL</code> then a Load is created for the shipment.
		    </div>
                    <Validation>
                        <Condition>If changed and shipment is included in a load taking it to its destination. 
		      </Condition>
                        <Error
                            Description="Cannot modify Shipment's carrier information after it has been included in a Load." ErrorCode="YDM00208"/>
                    </Validation>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SealNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="SEAL_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The seal number on the load.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SellerOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="SELLER_ORGANIZATION_CODE" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The seller organization to which the shipment belongs.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipNode" type="yfctype:ShipNode" use="optional">
            <xsd:annotation yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The ship node where the shipment was created.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipVia" type="yfctype:Text-10" use="optional">
            <xsd:annotation yfc:columnName="SHIP_VIA" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The means of shipment. This is usually Land, Air, Sea, or Ocean.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentConsolidationGroupId"
            type="yfctype:VarcharShipNode" use="optional">
            <xsd:annotation yfc:columnName="SHIPMENT_CONSOL_GROUP_ID" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>It is a consolidation group id  that is used as a standard do not mix constraint.Releases belonging to same shipment consolidation group may consolidate to same shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="SHIPMENT_KEY" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The primary key for the YFS_SHIPMENT table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentNo" type="yfctype:DocumentNo" use="optional">
            <xsd:annotation yfc:columnName="SHIPMENT_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The shipment number. This is a part of the unique key or business key.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentPackComplete" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>If equals <b>
                            <code>Y</code>
                        </b> then for Shipment with Containers without contents <code>IsPackProcessComplete</code> on the shipment is computed
			<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                        <Validation>
                            <Condition>
                                <code>ShipmentPackComplete</code> equals <b>
                                    <code>Y</code>
                                </b> then Shipment must require container without contents.
			</Condition>
                            <Error
                                Description="Cannot use the api for marking pack process complete on shipment when shipment requires containers with contents." ErrorCode="YDM00216"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentSortLocationId"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="SHIPMENT_SORT_LOCATION_ID" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>
                    <div>
                        <Validation>
                            <Condition>If <code>ShipmentSortLocationId</code> is not void and not a valid location in the <code>Shipnode</code>.
					</Condition>
                            <Error
                                Description="Location does not exist." ErrorCode="WMS00005"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ToAddressKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="TO_ADDRESS_KEY" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The to address key that is associated with this shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ToAppointment" type="yfctype:Date" use="optional">
            <xsd:annotation yfc:columnName="TO_APPOINTMENT" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The time before which an appointment could be taken by the ship node with the carrier.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TotalVolume" type="yfctype:Dimension" use="optional">
            <xsd:annotation yfc:columnName="TOTAL_VOLUME" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>
                    <div>
			If passed then it is not computed by the API else it is computed using the unit volume of <code>Items</code> corresponding to the shipment lines.
			</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TotalVolumeUOM" type="yfctype:UOM" use="optional">
            <xsd:annotation yfc:columnName="TOTAL_VOLUME_UOM" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The unit of measure for the units of volume specified in TotalVolume attribute of this shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TotalWeight" type="yfctype:Dimension" use="optional">
            <xsd:annotation yfc:columnName="TOTAL_WEIGHT" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>
                    <div>
			If passed then it is not computed by the API else it is computed using the unit weights of <code>Orderlines</code> corresponding to the shipment lines when <code>Order</code> exists on system and using the unit weights of the <code>Items</code> when <code>Order</code> does not exist on system.
			</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TotalWeightUOM" type="yfctype:UOM" use="optional">
            <xsd:annotation yfc:columnName="TOTAL_WEIGHT_UOM" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The unit of measure for the units of weight specified in Total_Weight attribute of this shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TrackingNo"
            type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="TRACKING_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>This field is not currently used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TrailerNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="TRAILER_NO" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The trailer number by which the shipment leaves the ship node.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="UpdateSIMPickReq" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
                       For sales and transfer orders, if SIM is enabled and there is a shipment line quantity change, pick requests are also updated. Pass this flag as N if pick request updates are not required.
					   <br/>Default: <b>
                            <code>Y</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or  <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="WorkOrderApptKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="WORK_ORDER_APPT_KEY" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The work order appt key that is associated with this shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="WorkOrderKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="WORK_ORDER_KEY" yfc:tableName="YFS_SHIPMENT">
                <xsd:documentation>The work order key that is associated with this shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="ScacAndService">
        <xsd:annotation yfc:tableName="YFS_SCAC_AND_SERVICE">
            <xsd:documentation>
                <div>
	      This element is not needed if <code>Shipment/CarrierServiceCode</code> and <code>Shipment/SCAC</code> are passed in the Shipment header or those elements are passed in  any of the <code>Order</code>, <code>OrderRelease</code> or<code>OrderLine</code> associated with the Shipment. If the element <code>ScacAndService</code> is passed, it should point to a SCAC and Service on the system. 
	      <code>ScacAndService</code>  is used to set default  values for <code>/Shipment/SCAC</code> and <code>/Shipment/CarrierServiceCode</code>.
	      </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CarrierServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_SERVICE_KEY" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An identifier to a master service from which this service is derived.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>The organization code of organization that defines this service offered by the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndService" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An identifier for a service offered by a carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE_KEY" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Primary key attribute of YFS_SCAC_AND_SERVICE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SCAC_KEY" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Servicing carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ToAddress">
        <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
            <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AlternateEmailID"
                type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="City" type="yfctype:City" use="optional">
                <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The city in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The country in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ErrorTxt" type="yfctype:ErrorText" use="optional">
                <xsd:annotation yfc:columnName="ERROR_TXT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
