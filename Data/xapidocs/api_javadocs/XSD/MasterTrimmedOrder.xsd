    <xsd:element name="OrderHoldTypes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderHoldType"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderHoldType">
        <xsd:annotation yfc:tableName="YFS_ORDER_HOLD_TYPE">
            <xsd:documentation>This table stores contains all the hold types defined for an order.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderHoldTypeLogs"/>
            </xsd:all>
            <xsd:attribute name="HoldType" type="yfctype:CodeType" use="optional">
                <xsd:annotation yfc:columnName="HOLD_TYPE" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                    <xsd:documentation>Hold type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastHoldTypeDate"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="LAST_HOLD_TYPE_DATE" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                    <xsd:documentation>Timestamp when this hold type was added to the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderAuditKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_AUDIT_KEY" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                    <xsd:documentation>Primary key for YFS_ORDER_AUDIT table. This can be used to view the modification types that would have added this hold type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                    <xsd:documentation>Primary Key of YFS_ORDER_HEADER.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                    <xsd:documentation>Primary Key of YFS_ORDER_Line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReasonText" type="yfctype:ReasonText" use="optional">
                <xsd:annotation yfc:columnName="REASON_TEXT" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                    <xsd:documentation>The reason text associates to the order hold type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ResolverUserId" type="yfctype:LoginId" use="optional">
                <xsd:annotation yfc:columnName="RESOLVER_USER_ID" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                    <xsd:documentation>This is the user who can resolve or reject this hold. This field maps to User_Id field of YFS_Customer_Contact.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Status" type="yfctype:ShortStatus" use="optional">
                <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                    <xsd:documentation>Status of the order hold type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StatusDescription"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Description of hold type status.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionId"
                type="yfctype:TransactionID" use="optional">
                <xsd:annotation yfc:columnName="TRANSACTION_ID" yfc:tableName="YFS_ORDER_HOLD_TYPE">
                    <xsd:documentation>The unique identifier of the transaction which applied the hold type on order .</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionName"
                type="yfctype:TransactionName-50" use="optional">
                <xsd:annotation yfc:columnName="TRANNAME" yfc:tableName="YFS_TRANSACTION">
                    <xsd:documentation>Transaction name which created the hold type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderHoldTypeLogs">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderHoldTypeLog"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderHoldTypeLog">
        <xsd:annotation yfc:tableName="YFS_ORDER_HOLD_TYPE_LOG">
            <xsd:documentation>This table stores the log notes when status of a hold type changes.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderAuditKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_AUDIT_KEY" yfc:tableName="YFS_ORDER_HOLD_TYPE_LOG">
                    <xsd:documentation>Primary key for YFS_ORDER_AUDIT table. This can be used to view the modification types that would have added this hold type log.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderHoldTypeLogKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_HOLD_TYPE_LOG_KEY" yfc:tableName="YFS_ORDER_HOLD_TYPE_LOG">
                    <xsd:documentation>Primary key for YFS_ORDER_HOLD_TYPE_LOG table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReasonText" type="yfctype:ReasonText" use="optional">
                <xsd:annotation yfc:columnName="REASON_TEXT" yfc:tableName="YFS_ORDER_HOLD_TYPE_LOG">
                    <xsd:documentation>The reason text associates to this order hold type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ResolverUserId" type="yfctype:LoginId" use="optional">
                <xsd:annotation yfc:columnName="RESOLVER_USER_ID" yfc:tableName="YFS_ORDER_HOLD_TYPE_LOG">
                    <xsd:documentation>This is the user who can approve or reject this hold. This field maps to User_Id field of YFS_Customer_Contact.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Status" type="yfctype:ShortStatus" use="optional">
                <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_ORDER_HOLD_TYPE_LOG">
                    <xsd:documentation>Status of the order hold type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StatusDescription"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Description of hold type status.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionId"
                type="yfctype:TransactionID" use="optional">
                <xsd:annotation yfc:columnName="TRANSACTION_ID" yfc:tableName="YFS_ORDER_HOLD_TYPE_LOG">
                    <xsd:documentation>The unique identifier for the transaction which applied the hold type on order .</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionName"
                type="yfctype:TransactionName-50" use="optional">
                <xsd:annotation yfc:columnName="TRANNAME" yfc:tableName="YFS_TRANSACTION">
                    <xsd:documentation>Transaction name which created the hold type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UserId" type="yfctype:Modifyuserid" use="optional">
                <xsd:annotation yfc:columnName="USER_ID" yfc:tableName="YFS_ORDER_HOLD_TYPE_LOG">
                    <xsd:documentation>User who modified the record.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OverallTotals">
        <xsd:annotation>
            <xsd:documentation>
                <div>
		  Contains information about the price and total amount details for the order line.
		  </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OverallChargeTotals"/>
            </xsd:all>
            <xsd:attribute name="AdditionalLinePriceTotal"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Total Price for all Non-Informational and Non-Recurring Additional
					Line Prices for the Order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandCharges" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The total amount of charges applied on the order and lines.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandDiscount" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The total amount of discounts applied on the order and lines.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandShippingBaseCharge"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The amount that will be charged for shipping without discounts or charges for the entire order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandShippingCharges"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The total of charges associated with shipping for the entire order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandShippingDiscount"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The total of discounts associated with shipping for the entire order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandShippingTotal"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The total amount for shipping the line including shipping discounts and charges for the entire order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandTax" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The total amount of taxes applied on the order and lines.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandTotal" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="TOTAL_AMOUNT" yfc:tableName="YFS_ORDER_HEADER">
                    <xsd:documentation>The grand total for the order. Includes all line charges and taxes.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrCharges" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The total amount of charges applied directly to the order header.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrDiscount" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The total amount of discounts applied directly to the order header.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrShippingBaseCharge"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The amount that will be charged for shipping without discounts or charges for the order header only.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrShippingCharges"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The total of charges associated with shipping for the order header only.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrShippingDiscount"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The total of discounts associated with shipping for the order header only.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrShippingTotal" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The total amount for shipping the line including shipping discounts and charges for the order header only.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrTax" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The total amount of taxes applied directly to the order header.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrTotal" type="yfctype:ComputedMoney" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The total amount of the order excluding line totals.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LineSubTotal" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The line sub total for this order. This is the sum of each line's <code>ExtendedPrice</code>.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManualDiscountPercentage"
                type="yfctype:Percentage" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The percentage of the total for which manual discounts were applied.
			  <code>Sum of Manual Charges on Order and Lines / (Order Total - Sum of Manual Charges on Order and Lines)</code>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PercentProfitMargin"
                type="yfctype:Percentage" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The percentage of the amount being made on the product being sold.<code>
			  	( (Extended Price of all items on the order
			  	+ charges that are considered for profit margin calculation) - (Sum of Item Cost across all lines))/(Extended Price of all items on the order
			  	+ charges that are considered for profit margin calculation)</code>
                            <br/>If <code>ExtendedPrice+Charges</code> is zero, 
			  	<code>PercentProfitMargin</code> is not returned.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RecurringAdditionalLinePriceTotal"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Total Price for all Non-Informational and Recurring Additional
					Line Prices for the Order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OverallChargeTotals">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OverallChargeTotal"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OverallChargeTotal">
        <xsd:annotation>
            <xsd:documentation>Represents the total charges and discounts on the order for the
						specified <code>ChargeCategory</code> and <code>ChargeName</code>.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeCategory"
                type="yfctype:ChargeCategory" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_CHARGE_CATEGORY">
                    <xsd:documentation>The charge category for the <code>OverallChargeTotal</code>.
							</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_CHARGE_NAME">
                    <xsd:documentation>The charge name for the <code>OverallChargeTotal</code>.
							</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandCharges" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The total charges on the order for the
							<code>ChargeCategory</code> - <code>ChargeName</code> combination.
							</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandDiscount" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>The total discounts on the order for the
							<code>ChargeCategory</code> - <code>ChargeName</code> combination.
							</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsBillable" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_BILLABLE" yfc:tableName="YFS_CHARGE_CATEGORY">
                    <xsd:documentation>Indicates if a charge is billable to the customer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsDiscount" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_DISCOUNT" yfc:tableName="YFS_CHARGE_CATEGORY">
                    <xsd:documentation>Indicates if a charge category is a discount category.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsManual"
                type="yfctype:EnumIsManualCharge" use="optional">
                <xsd:annotation yfc:columnName="IS_MANUAL" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>Determines whether the charge was manually created or created
							automatically through pricing engine.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsRefundable" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_REFUNDABLE" yfc:tableName="YFS_CHARGE_CATEGORY">
                    <xsd:documentation>Indicates if a charge is refundable during returns.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsShippingCharge" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Determines whether the charge is a shipping charge. This attribute value is system defined and cannot be overridden. The value for this attribute is set as <code>Y</code> only for Charge Name configured as <code>CHARGE_NAME_FOR_SHIPPING</code> in financial rule. For all other Charges this attribute is set to <code>N</code>.
							</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value=""/>
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Reference" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>Reference information about this charge.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="InvoicedTotals">
        <xsd:complexType>
            <xsd:attribute name="GrandCharges" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Grand Charges</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandDiscount" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Grand Discount</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandTax" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Grand Tax</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandTotal" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Grand Total</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrCharges" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Header Charges</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrDiscount" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Header Discount</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrTax" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Header Tax</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrTotal" type="yfctype:ComputedMoney" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Header Total</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LineSubTotal" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Line Sub Total</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="RemainingTotals">
        <xsd:annotation>
            <xsd:documentation>
                <div>
						Contains information about the price and total amount details
						that are not
						invoiced for the order.
					</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="GrandCharges" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Grand Charges</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandDiscount" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Grand Discount</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandTax" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Grand Tax</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GrandTotal" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Grand Total</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrCharges" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Header Charges</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrDiscount" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Header Discount</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrTax" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Header Tax</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HdrTotal" type="yfctype:ComputedMoney" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Header Total</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LineSubTotal" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Line Sub Total</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderCharges">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:HeaderCharge"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderCharge">
        <xsd:annotation yfc:tableName="YFS_HEADER_CHARGES">
            <xsd:documentation>This table stores charges that are applied at the header level.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="ChargeNameDetails">
                    <xsd:annotation yfc:tableName="YFS_CHARGE_NAME">
                        <xsd:documentation>This table stores next level of categorization of charges. For a given charge category, there could be  multiple charge names associated with it. For example, "Shipping" charge category may have  charges like "Mileage", "Documentation Fees" etc.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="Description"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_CHARGE_NAME">
                                <xsd:documentation>Description of a charge name.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ChargeCategoryDetails">
                    <xsd:annotation yfc:tableName="YFS_CHARGE_CATEGORY">
                        <xsd:documentation>This table stores categorization of charges in the system. Categorization is stored at a rule set level.  Examples would be "Shipping", "Handling", "Personalization", etc.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="ChargeCategoryKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="CHARGE_CATEGORY_KEY" yfc:tableName="YFS_CHARGE_CATEGORY">
                                <xsd:documentation>Primary key of charge category.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ConsiderForProfitMargin"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="CONSIDER_FOR_PROFIT_MARGIN" yfc:tableName="YFS_CHARGE_CATEGORY">
                                <xsd:documentation>Indicates if a charge is included in the profit margin calculation.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Description"
                            type="yfctype:Text-100" use="optional">
                            <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_CHARGE_CATEGORY">
                                <xsd:documentation>Description of charge. Used for display purposes during configuration.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IsBillable"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation yfc:columnName="IS_BILLABLE" yfc:tableName="YFS_CHARGE_CATEGORY">
                                <xsd:documentation>Indicates if a charge is billable to the customer.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IsDiscount"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation yfc:columnName="IS_DISCOUNT" yfc:tableName="YFS_CHARGE_CATEGORY">
                                <xsd:documentation>Indicates if a charge category is a discount category.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IsRefundable"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="IS_REFUNDABLE" yfc:tableName="YFS_CHARGE_CATEGORY">
                                <xsd:documentation>Indicates if a charge is refundable during returns.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ChargeBreakups">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ChargeBreakup">
                                <xsd:annotation yfc:tableName="YFS_CHARGE_BREAKUP">
                                    <xsd:documentation>This table stores breakup of the charges copied to return order from linked sales order(s).</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="AwardId"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_ID" yfc:tableName="YFS_CHARGE_BREAKUP">
                                    <xsd:documentation>A unique identifier for the award. If pricing is maintained within the system, this will be the PricingRuleName of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AwardType"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_TYPE" yfc:tableName="YFS_CHARGE_BREAKUP">
                                    <xsd:documentation>The type of award. If pricing is maintained within the system, this will be the RuleCategory of the pricing rule that created this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeAmount"
                                    type="yfctype:PlusMinusMoney" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_AMOUNT" yfc:tableName="YFS_CHARGE_BREAKUP">
                                    <xsd:documentation>The amount of the charge.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ChargeBreakupKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CHARGE_BREAKUP_KEY" yfc:tableName="YFS_CHARGE_BREAKUP">
                                    <xsd:documentation>Primary key of charge breakup.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DerivedFromOrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DERIVED_FROM_ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_BREAKUP">
                                    <xsd:documentation>The key of the order header from which this charge is derived.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DerivedFromOrderLineKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DERIVED_FROM_ORDER_LINE_KEY" yfc:tableName="YFS_CHARGE_BREAKUP">
                                    <xsd:documentation>The key of the order line from which this charge is derived.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OrderHeaderKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_CHARGE_BREAKUP">
                                    <xsd:documentation>Key of the order header to which this charge breakup belongs.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="ChargeAmount"
                type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="CHARGE" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>The amount of the charge.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeCategory"
                type="yfctype:ChargeCategory" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>The charge category. For example, Labeling or Handling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>The name of the charge. For example, Price Ticket.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeNameKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME_KEY" yfc:tableName="YFS_CHARGE_NAME">
                    <xsd:documentation>Primary key for a charge name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InvoicedChargeAmount"
                type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="INVOICED_CHARGE" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>Applicable only to charges for an Order. This is the amount which has already been invoiced.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsBillable" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
							Describes if the charge is billable. Default:
							<code>N</code>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsDiscount" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
							Describes if the charge is discountable. Default:
							<code>N</code>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsManual" use="optional">
                <xsd:annotation yfc:columnName="IS_MANUAL" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>
							Determines whether the charge was manually created or created
							automatically through pricing engine. Valid Values:
							<code>Y</code>
							,
							<code>N</code>
							, or
							<code>M</code>
							Default:
							<code>Y</code>
							.
						</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:EnumIsManualCharge">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                        <xsd:enumeration value="M"/>
                        <xsd:enumeration value=""/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsRefundable" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
							Describes if the charge is refundable. Default:
							<code>Y</code>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsShippingCharge" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
							Determines whether the charge is a shipping charge. This
							attribute value is system defined and cannot be overridden. The
							value for this attribute is set as
							<code>Y</code>
							only for Charge Name configured as
							<code>CHARGE_NAME_FOR_SHIPPING</code>
							in financial rule. For all other Charges this attribute is set to
							<code>N</code>
							.
						</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value=""/>
                        <xsd:enumeration value="N"/>
                        <xsd:enumeration value="Y"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Reference" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE" yfc:tableName="YFS_HEADER_CHARGES">
                    <xsd:documentation>Reference information about this charge.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RemainingChargeAmount"
                type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Remaining Charge Amount</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderTaxes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:HeaderTax"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="TaxSummary">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="TaxSummaryDetail">
                                <xsd:complexType>
                                    <xsd:attribute name="InvoicedTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount of this tax which had been invoiced.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OverallTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total amount of this tax.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="RemainingTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount of this tax that has not been invoiced.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TaxName"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Tax name.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderTax">
        <xsd:annotation yfc:tableName="YFS_TAX_BREAKUP">
            <xsd:documentation>
                <div>
							Contains tax related information for various entities in the
							system. If
							<code>ChargeCategory</code>
							and
							<code>ChargeName</code>
							are not provided, tax will be considered as a billable and
							positive
							charge. If not implementing
							<code>YFSRecalculateLineTaxUE</code>
							,
							<code>ChargeCategory</code>
							,
							<code>Tax</code>
							, and
							<code>TaxPercentage</code>
							are required for the default repricing logic.
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeCategory"
                type="yfctype:ChargeCategory" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>The charge category for which tax is being charged. For example, Labeling or Handling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Charge name for which tax is being charged. For example, Price Ticket.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeNameKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME_KEY" yfc:tableName="YFS_CHARGE_NAME">
                    <xsd:documentation>Primary key for a charge name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InvoicedTax" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="INVOICED_TAX" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Tax amount invoiced.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference1" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE1" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Reference 1</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference2" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE2" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Reference 2</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference3" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE3" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Reference 3</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RemainingTax" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Remaining Tax</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Tax" type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="TAX" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
                        <div>
								The tax amount.
								<code>Tax</code>
								cannot be less than zero. During the default repricing logic,
								this is recalculated using the
								<code>TaxPercentage</code>
								and the price of the related
								<code>ChargeCategory</code>
								. This can be overridden with the YFSRecalulateHeaderTaxUE.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxName" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TAX_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>The tax name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxPercentage" type="yfctype:TaxType" use="optional">
                <xsd:annotation yfc:columnName="TAX_PERCENTAGE" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Percentage amount for tax calculation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>This flag indicates if a charge can be taxed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Opportunity">
        <xsd:annotation yfc:tableName="YFS_OPPORTUNITY">
            <xsd:documentation>
                <div>
			This tables stores an opportunity. An opportunity is a sales opportunity that a sales representative foresees and is used to keep track of multiple quotes that will be presented to a specific buyer. Any of these quotes can be negotiated upon but only one of these quotes can be accepted by the buyer, thereby permanently rejecting all other quotes. The opportunity will be closed once an order is created from the buyer selected quote.
			<Validation>
                        <Condition>Input document is invalid.
				</Condition>
                        <Error
                            Description="Input document is empty or contains invalid data." ErrorCode="YFS10363"/>
                    </Validation>
                    <Validation>
                        <Condition>Mandated parameters missing.
				</Condition>
                        <Error
                            Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                    </Validation>
                    <Validation>
                        <Condition>Opportunity record already existed.
				</Condition>
                        <Error Description="Record Already Exists" ErrorCode="YCP0051"/>
                    </Validation>
                    <Validation>
                        <Condition>Event template not found.
				</Condition>
                        <Error
                            Description="Template xml for raising event not found" ErrorCode="YFS10429"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OwnerUser"/>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:CoOwnerUser"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CustomerContact"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:BuyerContactAddress"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PrimaryQuote"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Notes"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="CustomAttributes">
                    <xsd:annotation yfc:tableName="YFS_OPPORTUNITY_EXTENSION">
                        <xsd:documentation>This table stores dynamic extension attributes of opportunity entity.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType/>
                </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="BillToID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="BILL_TO_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The identifier of the bill to customer. If a customer definition representing the buyer organization exists within the system, the BillToID can represent the CustomerID of the customer. Otherwise, the BillToID can represent the PersonID of the bill to address or the BuyerOrganizationCode of the buyer organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BuyerContactAddressKey"
                type="yfctype:Text-24" use="optional">
                <xsd:annotation
                    yfc:columnName="BUYER_CONTACT_ADDRESS_KEY" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The buyer's contact address. For new records, the <code>BuyerContactAddressKey</code> is the bill to address key 
			defaulted from the customer contact address. If customer contact is not available, the customer account address is
			used, otherwise obtaining from the buyer organization default address.
			</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BuyerOrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="BUYER_ORGANIZATION_CODE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The buyer organization that is associated with this opportunity. This must be a valid organization defined in the system.
			<Validation>
                                <Condition>
                                    <code>BuyerOrganizationCode</code> must be valid in the configuration
				</Condition>
                                <Error
                                    Description="Invalid Buyer Organization" ErrorCode="YFS10368"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CoOwnerUserID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="CO_OWNER_USER_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The co-owner user is a user who jointly owns this opportunity with the orginal owner user.
			<Validation>
                                <Condition>Invalid User identifiers passed.
				</Condition>
                                <Error
                                    Description="Invalid User identifiers passed." ErrorCode="YCP0084"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CurrencyValue" use="optional">
                <xsd:annotation yfc:columnName="CURRENCY_VALUE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The currency value or budget of the opportunity.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Amount"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="CustomerContactID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_CONTACT_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			This is the customer contact ID for the opportunity. This field maps to contact ID field of YFS_Customer_Contact.
			<Validation>
                                <Condition>Invalid Contact Identifier.
				</Condition>
                                <Error
                                    Description="YFS: Invalid Customer Id" ErrorCode="YFS10319"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Description" type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The detailed description of the opportunity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The document type that is associated with this opportunity. For example, Opportunity document (0020). OpportunityID, DocumentType, EnterpriseCode are required attributes to uniquely identify an opportunity.
			<Validation>
                                <Condition>
                                    <code>DocumentType</code> must be defined in the configuration
				</Condition>
                                <Error
                                    Description="Invalid Document Type" ErrorCode="YFS10367"/>
                            </Validation>
                            <Validation>
                                <Condition>Opportunity record does not exist.
				</Condition>
                                <Error
                                    Description="YCP:No Record Found." ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="required">
                <xsd:annotation yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The enterprise code. This field is a part of an opportunity's unique key. The EnterpriseCode, DocumentType and OpportunityID together uniquely identify an opportunity. <br/>Default value: <code>DEFAULT</code>
                            <Validation>
                                <Condition>
                                    <code>EnterpriseCode</code> must be valid in the configuration
				</Condition>
                                <Error
                                    Description="YFS:Invalid Enterprise Code" ErrorCode="YFS10113"/>
                            </Validation>
                            <Validation>
                                <Condition>Opportunity record does not exist.
				</Condition>
                                <Error
                                    Description="YCP:No Record Found." ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedCloseDate" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_CLOSE_DATE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The anticipated close date for the opportunity.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LeadOrigin" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LEAD_ORIGIN" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The original lead to creation of this opportunity. E.g. Trade Shows, Call Center, Existing Customer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LostReasonCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOST_REASON_CODE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>If the opportunity is lost, this specifies the reason for losing this opportunity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OpportunityDate" use="optional">
                <xsd:annotation yfc:columnName="OPPORTUNITY_DATE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The date that the opportunity was created.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OpportunityID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="OPPORTUNITY_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The identification number of the opportunity, which is required during opportunity creation. If not passed the <code>OpportunityID</code> generates based on a system rule setting or from the user exit implementation. Whenever modification is required, <code>OpportunityID</code> is mandatory. OpportunityID, DocumentType, EnterpriseCode are required attributes to uniquely identify an opportunity.
			<Validation>
                                <Condition>Mandatory parameter missing.
				</Condition>
                                <Error
                                    Description="YCP:No Record Found." ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OpportunityKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="OPPORTUNITY_KEY" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The primary key of the YFS_OPPORTUNITY table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OpportunityName"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="OPPORTUNITY_NAME" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>A custom name given to this opportunity to help identify the record uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OwnerUserID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="OWNER_USER_ID" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>
                        <div>
			The user in which this opportunity is assigned to.
			<Validation>
                                <Condition>Invalid User identifiers passed.
				</Condition>
                                <Error
                                    Description="Invalid User identifiers passed." ErrorCode="YCP0084"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PipelineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PIPELINE_KEY" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>Pipeline this opportunity belongs to. The pipeline key is determined based on the input attribute passed and the pipeline conditions in the system configuration.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimaryQuoteKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PRIMARY_QUOTE_KEY" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The primary quote for this opportunity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProbableSuccessRate" use="optional">
                <xsd:annotation yfc:columnName="PROBABLE_SUCCESS_RATE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>The probability of success of the opportunity.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Percentage"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Status" type="yfctype:BaseStatus" use="optional">
                <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>

                        <div>
			This field indicates the status of this opportunity. The status is derived based on the opportunity pipeline system configurator.
			<br/>Below is a list of default statuses provided out of the box installation.
			<table
                                border="1" cellpadding="1"
                                cellspacing="0" class="table">
                                <thead>
                                    <tr>
                                    <td class="tablecolumnheader">Status Name</td>
                                    <td class="tablecolumnheader">Base Status</td>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                    <td class="tablecolumn">Inquiry</td>
                                    <td class="tablecolumn">1000</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">Negotiation</td>
                                    <td class="tablecolumn">1000.010</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">Lost</td>
                                    <td class="tablecolumn">1000.030</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">Won</td>
                                    <td class="tablecolumn">1000.020</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StatusName" type="yfctype:Text-50" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			Status full name for the current opportunity status.
			</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TeamCode"
                type="yfctype:CodeLongDescription" use="optional">
                <xsd:annotation yfc:columnName="TEAM_CODE" yfc:tableName="YFS_OPPORTUNITY">
                    <xsd:documentation>Indicates a team that can manage this opportunity.  Default is Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OwnerUser">
        <xsd:annotation yfc:tableName="YFS_USER">
            <xsd:documentation>This table stores information on users in the system.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="ContactPersonInfo">
                    <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
                        <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="AddressLine1"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 1 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine2"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 2 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine3"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 3 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine4"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 4 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine5"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 5 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AddressLine6"
                            type="yfctype:Address" use="optional">
                            <xsd:annotation
                                yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The address line 6 field.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="AlternateEmailID"
                            type="yfctype:EMailID" use="optional">
                            <xsd:annotation
                                yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Beeper"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="City" type="yfctype:City" use="optional">
                            <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The city in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Company"
                            type="yfctype:Company" use="optional">
                            <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The company this person is associated with.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Country"
                            type="yfctype:Country" use="optional">
                            <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The country in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DayFaxNo"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DayPhone"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Department"
                            type="yfctype:Department" use="optional">
                            <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EMailID"
                            type="yfctype:EMailID" use="optional">
                            <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ErrorTxt"
                            type="yfctype:ErrorText" use="optional">
                            <xsd:annotation yfc:columnName="ERROR_TXT" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>This field is currently not used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EveningFaxNo"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="EveningPhone"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation
