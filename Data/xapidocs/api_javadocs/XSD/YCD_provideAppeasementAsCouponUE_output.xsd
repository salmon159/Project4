                        <xsd:attribute name="TargetAttributeMaxValue" use="optional">
                            <xsd:annotation
                                yfc:columnName="ATTRIBUTE_MAX_VALUE" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
