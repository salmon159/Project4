                                    <xsd:restriction base="yfctype:QuantityPositiveAndNegative"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="TargetAttributeValue"
                            type="yfctype:Text-200" use="optional">
                            <xsd:annotation
                                yfc:columnName="ATTRIBUTE_VALUE" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                                <xsd:documentation>The TEXT value of the item attribute.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRuleAttributeValueList">
