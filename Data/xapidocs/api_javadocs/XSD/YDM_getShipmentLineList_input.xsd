            <xsd:attribute name="ToShipmentContainerizedFlag"
                type="yfctype:EnumShipmentContainerizedFlag" use="optional"/>
            <xsd:attribute name="ToShipmentDeliverUpdatesDone"
                type="yfctype:Flag" use="optional"/>
