            <xsd:attribute name="ToITDate" type="yfctype:Date" use="optional"/>
            <xsd:attribute name="ToITNo" type="yfctype:NText-30" use="optional"/>
            <xsd:attribute name="ToManifestKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToManifestNo"
