            <xsd:attribute name="ToOrderReleaseKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToOriginZone" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToPackListType" type="yfctype:Text-20" use="optional"/>
            <xsd:attribute name="ToParentShipmentKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToPickListNo" type="yfctype:DocumentNo" use="optional"/>
            <xsd:attribute name="ToPickticketNo"
