<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/YDM/getShipmentLineList/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/YDM/getShipmentLineList/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="YDM_getShipmentLineList_input.xsd" yfc:rootElement="ShipmentLine">
        <xsd:appinfo source="yfc:structure">
            <ShipmentLine hasAttributes="Y">
                <ContainerDetails>
                    <ContainerDetail>
                        <Container hasAttributes="Y"/>
                    </ContainerDetail>
                </ContainerDetails>
                <BundleParentLine hasAttributes="Y"/>
                <ShipmentLineInvAttRequest hasAttributes="Y"/>
                <ShipmentTagSerials>
                    <ShipmentTagSerial hasAttributes="Y"/>
                </ShipmentTagSerials>
                <Shipment hasAttributes="Y"/>
                <Order hasAttributes="Y"/>
                <OrderLine hasAttributes="Y"/>
                <OrderRelease hasAttributes="Y"/>
                <Instructions>
                    <Instruction hasAttributes="Y"/>
                </Instructions>
                <OrderBy>
                    <Attribute hasAttributes="Y"/>
                </OrderBy>
            </ShipmentLine>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="ShipmentLine" type="yfc:ShipmentLineXSDType">
        <xsd:annotation yfc:tableName="YFS_SHIPMENT_LINE">
            <xsd:documentation>Stores information about shipment lines. Shipment lines are created using createShipment or changeShipment API.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="ShipmentLineXSDType">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ContainerDetails"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:BundleParentLine"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipmentLineInvAttRequest"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ShipmentTagSerials"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Shipment"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Order"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderLine"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderRelease"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Instructions"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderBy"/>
        </xsd:sequence>
        <xsd:attribute name="ActualQuantity" type="yfctype:Quantity" use="optional">
            <xsd:annotation yfc:qryTypeSupported="Y">
                <xsd:documentation>Actual quantity of the shipment line shipped.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ActualQuantityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="BackroomPickComplete" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="BACKROOM_PICK_COMPLETE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field indicates whether the backroom pick of shipment line is complete.The filed will be Y when the Backroom pick is complete</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BackroomPickCompleteQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="BackroomPickedQty" use="optional">
            <xsd:annotation yfc:columnName="BACKROOM_PICKED_QTY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field contains the quantity picked during backroom pick.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:Quantity"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="BackroomPickedQtyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="BackroomPickedQuantity" type="xsd:string" use="optional">
            <xsd:annotation yfc:qryTypeSupported="Y"/>
        </xsd:attribute>
        <xsd:attribute name="BackroomPickedQuantityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="BatchPickPriority" use="optional">
            <xsd:annotation yfc:columnName="BATCH_PICK_PRIORITY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field contains the priority in which the shipment line is picked when the shipment line is included in a batch.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:LineNo"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="BatchPickPriorityQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="BuyerMarkForNodeId" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="BUYER_MARK_FOR_NODE_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>Mark For Node as identified by the buyer for eg: it could be a person,store,DC.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BuyerMarkForNodeIdQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CancelReason" type="yfctype:CodeValue" use="optional">
            <xsd:annotation yfc:columnName="CANCEL_REASON"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field contains the cancel reason for shipment line</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CancelReasonQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ChainedFromOrderHeaderKey"
            type="yfctype:Key" use="optional">
            <xsd:annotation
                yfc:columnName="CHAINED_FROM_ORDER_HEADER_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORDER_LINE">
                <xsd:documentation>The order header that is associated with this chained order line. If this line is chained from another order line, this key will point to the parent order line's header record.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ChainedFromOrderHeaderKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ChainedFromOrderLineKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CHAINED_FROM_ORDER_LINE_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORDER_LINE">
                <xsd:documentation>The order line that is associated with this chained order line. If this line is chained from another order line, this key will point to the parent order line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ChainedFromOrderLineKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CountryOfOrigin" type="yfctype:Country" use="optional">
            <xsd:annotation yfc:columnName="COUNTRY_OF_ORIGIN"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The country of origin.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CountryOfOriginQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CustomerPickedQuantity" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_PICKED_QTY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field contains the quantity picked by the customer</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:Quantity"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="CustomerPickedQuantityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CustomerPoLineNo" type="yfctype:Text-100" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_PO_LINE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The customer PO # for the order during createorder time. While creating shipments, this values is consolidated acroos all orders belonging to this shipment and stored in the shipment header.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerPoLineNoQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CustomerPoNo" type="yfctype:Text-100" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_PO_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The customer PO # for the order during createorder time. While creating shipments, this values is consolidated acroos all orders belonging to this shipment and stored in the shipment header.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerPoNoQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="DocumentType" type="yfctype:ID-40" use="optional">
            <xsd:annotation yfc:qryTypeSupported="Y">
                <xsd:documentation>The document type of the orders comprising the shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DocumentTypeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ExternalReleaseIdentifier"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="EXTERNAL_RELEASE_IDENTIFIER"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The identifier of the external release. This attribute along with OrderNo and ReleaseNo will uniquely identify the order release external to the system.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExternalReleaseIdentifierQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="FifoNo" type="yfctype:Integer" use="optional">
            <xsd:annotation yfc:columnName="FIFO_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>A date based inventory attribute that helps understand the order in which stock arrived at the node. This is used to send out items that arrived earlier than the ones that arrived later.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FifoNoQryType" type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="FromActualQuantity" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="FromBackroomPickComplete"
            type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromBackroomPickedQty"
            type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="FromBackroomPickedQuantity"
            type="xsd:string" use="optional"/>
        <xsd:attribute name="FromBatchPickPriority"
            type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="FromBuyerMarkForNodeId" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromCancelReason" type="yfctype:CodeValue" use="optional"/>
        <xsd:attribute name="FromChainedFromOrderHeaderKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromChainedFromOrderLineKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromCountryOfOrigin" type="yfctype:Country" use="optional"/>
        <xsd:attribute name="FromCustomerPickedQuantity"
            type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="FromCustomerPoLineNo"
            type="yfctype:Text-100" use="optional"/>
        <xsd:attribute name="FromCustomerPoNo" type="yfctype:Text-100" use="optional"/>
        <xsd:attribute name="FromDocumentType" type="yfctype:ID-40" use="optional"/>
        <xsd:attribute name="FromExternalReleaseIdentifier"
            type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="FromFifoNo" type="yfctype:Integer" use="optional"/>
        <xsd:attribute name="FromGroupSequenceNumber"
            type="yfctype:ConversionRate" use="optional"/>
        <xsd:attribute name="FromIsCustomerPickComplete"
            type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromIsHazmat" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromIsPackComplete" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromIsPickable" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromItemDesc" type="yfctype:Text-500" use="optional"/>
        <xsd:attribute name="FromItemID" type="yfctype:ItemID" use="optional"/>
        <xsd:attribute name="FromKitCode" type="yfctype:Text-20" use="optional"/>
        <xsd:attribute name="FromLevelOfService"
            type="yfctype:LevelOfService" use="optional"/>
        <xsd:attribute name="FromNetWeight" type="yfctype:Dimension" use="optional"/>
        <xsd:attribute name="FromNetWeightUom" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="FromOrderAvailableOnSystem"
            type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromOrderHeaderKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromOrderLineKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromOrderNo"
            type="yfctype:VarcharDocumentNo" use="optional"/>
        <xsd:attribute name="FromOrderReleaseKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromOverShipQuantity"
            type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="FromPickLocation" type="yfctype:LocationId" use="optional"/>
        <xsd:attribute name="FromPickLocationSeq" type="yfctype:Integer" use="optional"/>
        <xsd:attribute name="FromPlacedQuantity" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="FromPrimeLineNo" type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="FromProductClass"
            type="yfctype:ProductClass" use="optional"/>
        <xsd:attribute name="FromQuantity" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="FromReceivedQuantity"
            type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="FromReleaseNo" type="yfctype:Count" use="optional"/>
        <xsd:attribute name="FromRequestedSerialNo"
            type="yfctype:SerialNo" use="optional"/>
        <xsd:attribute name="FromReturnShippingLabelLevel"
            type="yfctype:Text-1" use="optional"/>
        <xsd:attribute name="FromSegment" type="yfctype:Segment" use="optional"/>
        <xsd:attribute name="FromSegmentType" type="yfctype:SegmentType" use="optional"/>
        <xsd:attribute name="FromShipToCustomerId" type="yfctype:ID-40" use="optional"/>
        <xsd:attribute name="FromShipmentConsolidationGroupId"
            type="yfctype:VarcharShipNode" use="optional"/>
        <xsd:attribute name="FromShipmentKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromShipmentLineKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromShipmentLineNo" type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="FromShipmentSubLineNo"
            type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="FromShortageQty" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="FromShortageResolutionReason"
            type="yfctype:CodeValue" use="optional"/>
        <xsd:attribute name="FromStoreBatchKey" type="yfctype:Text-24" use="optional"/>
        <xsd:attribute name="FromSubLineNo" type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="FromUnitOfMeasure" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="FromWaveNo" type="yfctype:SequenceNo" use="optional"/>
        <xsd:attribute name="GroupSequenceNumber" use="optional">
            <xsd:annotation yfc:columnName="GROUP_SEQUENCE_NUM"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field is used to assign a decimal value to each line for the purpose of sorting them in a specific sequence.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:ConversionRate"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="GroupSequenceNumberQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="IsCustomerPickComplete" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_CUSTOMER_PICK_COMPLETE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field indicates whether the customer pick for a shipment line is complete.The filed will be Y when the customer picking is complete</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsCustomerPickCompleteQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IsHazmat" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_HAZMAT"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>Indicates whether the shipment line is for hazardous item or not.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsHazmatQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IsPackComplete" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_PACK_COMPLETE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field indicates whether the packing of shipment line is complete.The filed will be Y when the container packing is complete</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsPackCompleteQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IsPickable" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_PICKABLE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This flag indicates whether this shipment line can be picked or not.This flag is set to 'N' only for Logical Kit Parent line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsPickableQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ItemDesc" type="yfctype:Text-500" use="optional">
            <xsd:annotation yfc:columnName="ITEM_DESCRIPTION"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The description of item.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemDescQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
            <xsd:annotation yfc:columnName="ITEM_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The identifier of the item.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemIDQryType" type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="KitCode" use="optional">
            <xsd:annotation yfc:columnName="KIT_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The kit code of the item, if it is a Kit item.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="yfctype:Text-20">
                    <xsd:enumeration value="LK"/>
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value=" "/>
                    <xsd:enumeration value="Blanks"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="KitCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="LevelOfService"
            type="yfctype:LevelOfService" use="optional">
            <xsd:annotation yfc:columnName="LEVEL_OF_SERVICE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>Indicates the level of service required for this shipment line.  Default from corresponding order release.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="LevelOfServiceQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="MaximumRecords" type="yfctype:Count" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifies maximum number of records that must be fetched.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NetWeight" type="yfctype:Dimension" use="optional">
            <xsd:annotation yfc:columnName="NET_WEIGHT"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The weight of the items in the shipment. The unit of weight is as specified by the NetWeightUom attribute.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NetWeightQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="NetWeightUom" type="yfctype:UOM" use="optional">
            <xsd:annotation yfc:columnName="NET_WEIGHT_UOM"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The unit of net weight.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NetWeightUomQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OrderAvailableOnSystem" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:qryTypeSupported="Y">
                <xsd:documentation>Indicates whether the order is available on the system or not.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderAvailableOnSystemQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ORDER_HEADER_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The order header key that is associated with this shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderHeaderKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ORDER_LINE_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The order line key that is associated with this shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderLineKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OrderNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="ORDER_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The order number corresponding to the order for the shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderNoQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OrderReleaseKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ORDER_RELEASE_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The order release key that is associated with this shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderReleaseKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OverShipQuantity" type="yfctype:Quantity" use="optional">
            <xsd:annotation yfc:columnName="OVER_SHIP_QUANTITY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The quantity over picked for the shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OverShipQuantityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="PickLocation" type="yfctype:LocationId" use="optional">
            <xsd:annotation yfc:columnName="PICK_LOCATION"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>Pick Location of the line item in a store sales floor or backroom. Primarily used in store application for backroom picking.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PickLocationQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="PickLocationSeq" use="optional">
            <xsd:annotation yfc:columnName="PICK_LOCATION_SEQ"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>Pick Location Sequence of the pick location for the line. Used in store application for suggesting picks to user in a sequential manner.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:Integer"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="PickLocationSeqQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="PlacedQuantity" type="yfctype:Quantity" use="optional">
            <xsd:annotation yfc:qryTypeSupported="Y">
                <xsd:documentation>The sum of all placed quantity in the container details for the shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PlacedQuantityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
            <xsd:annotation yfc:columnName="PRIME_LINE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The prime line number of the corresponding order line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrimeLineNoQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="ProductClass" type="yfctype:ProductClass" use="optional">
            <xsd:annotation yfc:columnName="PRODUCT_CLASS"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The product class.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ProductClassQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Quantity" type="yfctype:Quantity" use="optional">
            <xsd:annotation yfc:columnName="QUANTITY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The shipment line quantity. Entering a quantity is mandatory when creating a shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="QuantityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ReadFromHistory" type="yfctype:Text-1" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
                    Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
                    <br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_SHIPMENT_LINE table.
                    <br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_SHIPMENT_LINE_H table (the history table).
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReceivedQuantity" type="yfctype:Quantity" use="optional">
            <xsd:annotation yfc:qryTypeSupported="Y">
                <xsd:documentation>Quantity received for the shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReceivedQuantityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ReleaseNo" type="yfctype:Count" use="optional">
            <xsd:annotation yfc:columnName="RELEASE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The release number of the order release for the shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReleaseNoQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="RequestedSerialNo" type="yfctype:SerialNo" use="optional">
            <xsd:annotation yfc:columnName="REQUESTED_SERIAL_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The requested serial number of the item.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RequestedSerialNoQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ReturnShippingLabelLevel"
            type="yfctype:Text-1" use="optional">
            <xsd:annotation yfc:columnName="RETURN_SHIPPING_LABEL_LEVEL"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field indicates the level at which the Return Shipping Label should be generated for the item present in the shipment line. Valid values are N,S and P. N indicates that the item does not require return label, S indicates that the item requires return label at SKU level and P indicates that the item requires retun label at package level. If the value for the field is not passed, it is defaulted to the corresponding value of the item.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReturnShippingLabelLevelQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Segment" type="yfctype:Segment" use="optional">
            <xsd:annotation yfc:columnName="SEGMENT"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The segment of inventory customized for an order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SegmentQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="SegmentType" type="yfctype:SegmentType" use="optional">
            <xsd:annotation yfc:columnName="SEGMENT_TYPE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The segment type of inventory customized for an order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SegmentTypeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ShipToCustomerId" type="yfctype:ID-40" use="optional">
            <xsd:annotation yfc:columnName="SHIP_TO_CUSTOMER_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The ship to identifier.  If a customer definition representing the buyer organization exists within the system, the ShipToID can represent the CustomerID of the customer.  Otherwise, the ShipToID can represent the PersonID of the ship to address or the receiving node of the order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipToCustomerIdQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ShipmentConsolidationGroupId"
            type="yfctype:VarcharShipNode" use="optional">
            <xsd:annotation yfc:columnName="SHIPMENT_CONSOL_GROUP_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>It is a consolidation group id  that is used as a standard do not mix constraint. Releases belonging to same shipment consolidation group may consolidate to same shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentConsolidationGroupIdQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ShipmentKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="SHIPMENT_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The shipment key that is associated with this shipment line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ShipmentLineKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="SHIPMENT_LINE_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The primary key for the YFS_SHIPMENT_LINE table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentLineKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ShipmentLineNo" type="yfctype:LineNo" use="optional">
            <xsd:annotation yfc:columnName="SHIPMENT_LINE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>A part of the primary key . The shipment line number is unique for a shipment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentLineNoQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="ShipmentSubLineNo" type="yfctype:LineNo" use="optional">
            <xsd:annotation yfc:columnName="SHIPMENT_SUB_LINE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This is a part of the unique key. ShipmentSubLineNo along with the ShipmentLineNo is used to identify the logical kit component shipment line. The value of this field is zero - for kit parent shipment lines and shipment lines with items that are not kit components, and greater than zero if the the shipment line corresponds to a logical kit component item.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShipmentSubLineNoQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="ShortageQty" type="yfctype:Quantity" use="optional">
            <xsd:annotation yfc:columnName="SHORTAGE_QTY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The quantity that is not available at the node.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShortageQtyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ShortageResolutionReason"
            type="yfctype:CodeValue" use="optional">
            <xsd:annotation yfc:columnName="SHORTAGE_RESOLUTION_REASON"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field contains the shortage resolution reason while picking the line quantity</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ShortageResolutionReasonQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="StoreBatchKey" type="yfctype:Text-24" use="optional">
            <xsd:annotation yfc:columnName="STORE_BATCH_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>This field contains the StoreBatchKey of the batch to which this shipment line belongs. This is for batch picking.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="StoreBatchKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
            <xsd:annotation yfc:columnName="SUB_LINE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The sub line number of the corresponding order line.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SubLineNoQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="ToActualQuantity" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="ToBackroomPickComplete" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToBackroomPickedQty"
            type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="ToBackroomPickedQuantity" type="xsd:string" use="optional"/>
        <xsd:attribute name="ToBatchPickPriority" type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="ToBuyerMarkForNodeId" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToCancelReason" type="yfctype:CodeValue" use="optional"/>
        <xsd:attribute name="ToChainedFromOrderHeaderKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToChainedFromOrderLineKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToCountryOfOrigin" type="yfctype:Country" use="optional"/>
        <xsd:attribute name="ToCustomerPickedQuantity"
            type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="ToCustomerPoLineNo" type="yfctype:Text-100" use="optional"/>
        <xsd:attribute name="ToCustomerPoNo" type="yfctype:Text-100" use="optional"/>
        <xsd:attribute name="ToDocumentType" type="yfctype:ID-40" use="optional"/>
        <xsd:attribute name="ToExternalReleaseIdentifier"
            type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="ToFifoNo" type="yfctype:Integer" use="optional"/>
        <xsd:attribute name="ToGroupSequenceNumber"
            type="yfctype:ConversionRate" use="optional"/>
        <xsd:attribute name="ToIsCustomerPickComplete"
            type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsHazmat" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsPackComplete" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsPickable" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToItemDesc" type="yfctype:Text-500" use="optional"/>
        <xsd:attribute name="ToItemID" type="yfctype:ItemID" use="optional"/>
        <xsd:attribute name="ToKitCode" type="yfctype:Text-20" use="optional"/>
        <xsd:attribute name="ToLevelOfService"
            type="yfctype:LevelOfService" use="optional"/>
        <xsd:attribute name="ToNetWeight" type="yfctype:Dimension" use="optional"/>
        <xsd:attribute name="ToNetWeightUom" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="ToOrderAvailableOnSystem"
            type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToOrderHeaderKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToOrderLineKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToOrderNo" type="yfctype:VarcharDocumentNo" use="optional"/>
        <xsd:attribute name="ToOrderReleaseKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToOverShipQuantity" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="ToPickLocation" type="yfctype:LocationId" use="optional"/>
        <xsd:attribute name="ToPickLocationSeq" type="yfctype:Integer" use="optional"/>
        <xsd:attribute name="ToPlacedQuantity" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="ToPrimeLineNo" type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="ToProductClass" type="yfctype:ProductClass" use="optional"/>
        <xsd:attribute name="ToQuantity" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="ToReceivedQuantity" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="ToReleaseNo" type="yfctype:Count" use="optional"/>
        <xsd:attribute name="ToRequestedSerialNo"
            type="yfctype:SerialNo" use="optional"/>
        <xsd:attribute name="ToReturnShippingLabelLevel"
            type="yfctype:Text-1" use="optional"/>
        <xsd:attribute name="ToSegment" type="yfctype:Segment" use="optional"/>
        <xsd:attribute name="ToSegmentType" type="yfctype:SegmentType" use="optional"/>
        <xsd:attribute name="ToShipToCustomerId" type="yfctype:ID-40" use="optional"/>
        <xsd:attribute name="ToShipmentConsolidationGroupId"
            type="yfctype:VarcharShipNode" use="optional"/>
        <xsd:attribute name="ToShipmentKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToShipmentLineKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToShipmentLineNo" type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="ToShipmentSubLineNo" type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="ToShortageQty" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="ToShortageResolutionReason"
            type="yfctype:CodeValue" use="optional"/>
        <xsd:attribute name="ToStoreBatchKey" type="yfctype:Text-24" use="optional"/>
        <xsd:attribute name="ToSubLineNo" type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="ToUnitOfMeasure" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="ToWaveNo" type="yfctype:SequenceNo" use="optional"/>
        <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
            <xsd:annotation yfc:columnName="UOM"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The unit of measure.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="UnitOfMeasureQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="WaveNo" type="yfctype:SequenceNo" use="optional">
            <xsd:annotation yfc:columnName="WAVE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                <xsd:documentation>The wave number to which the line belongs.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="WaveNoQryType" type="yfctype:StringQryType" use="optional"/>
    </xsd:complexType>
    <xsd:element name="ContainerDetails">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ContainerDetail"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ContainerDetail">
        <xsd:annotation yfc:tableName="YFS_CONTAINER_DETAILS">
            <xsd:documentation>Stores information about containerized shipment lines.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Container"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Container">
        <xsd:annotation yfc:tableName="YFS_SHIPMENT_CONTAINER">
            <xsd:documentation>Stores container header information. Containers are created using packShipment, createShipment or changeShipment API.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ContainerNo" type="yfctype:ContainerNo" use="optional">
                <xsd:annotation yfc:columnName="CONTAINER_NO" yfc:tableName="YFS_SHIPMENT_CONTAINER">
                    <xsd:documentation>The container number. This is a unique key or business key, which by itself identifies a delivery plan. If not passed as input, the system generates this number using the database sequence SEQ_YFS_CONTAINER_NO.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LoadKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="LOAD_KEY" yfc:tableName="YFS_SHIPMENT_CONTAINER">
                    <xsd:documentation>The load key that is associated with this shipment container.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentContainerKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_CONTAINER_KEY" yfc:tableName="YFS_SHIPMENT_CONTAINER">
                    <xsd:documentation>The primary key for the YFS_SHIPMENT_CONTAINER table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_KEY" yfc:tableName="YFS_SHIPMENT_CONTAINER">
                    <xsd:documentation>The shipment key that is associated with this shipment container.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="BundleParentLine">
        <xsd:annotation yfc:tableName="YFS_SHIPMENT_LINE">
            <xsd:documentation>Stores information about shipment lines. Shipment lines are created using createShipment or changeShipment API.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ShipmentLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_LINE_KEY" yfc:tableName="YFS_SHIPMENT_LINE">
                    <xsd:documentation>
                        <div>Shipment Line key of the bundle parent line.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_LINE_NO" yfc:tableName="YFS_SHIPMENT_LINE">
                    <xsd:documentation>
                        <div>Shipment Line number of the bundle parent line.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ShipmentLineInvAttRequest">
        <xsd:annotation yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
            <xsd:documentation>This table stores requested inventory attributes of an item on a shipment line .</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BatchNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO" yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
                    <xsd:documentation>Batch Number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute1" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE1" yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
                    <xsd:documentation>Any other lot attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute2" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE2" yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
                    <xsd:documentation>Any other lot attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute3" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE3" yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
                    <xsd:documentation>Any other lot attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotKeyReference"
                type="yfctype:Text-4000" use="optional">
                <xsd:annotation yfc:columnName="LOT_KEY_REFERENCE" yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
                    <xsd:documentation>Stores reference information of the lot number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotNumber" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="LOT_NUMBER" yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
                    <xsd:documentation>Lot Number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturingDate" use="optional">
                <xsd:annotation yfc:columnName="LOT_MANUFACTURE_DATE" yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
                    <xsd:documentation>Lot Manufacturing Date.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Date"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="RevisionNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="REVISION_NO" yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
                    <xsd:documentation>Revision Number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ShipmentTagSerials">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ShipmentTagSerial"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ShipmentTagSerial">
        <xsd:annotation yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
            <xsd:documentation>
                <div>
				Stores additional information related to tag number, serial number, ship by date for a shipment line or container detail. Tag information is stored only if the <code>Node</code> tracks tag information in inventory or the <code>Buyer</code> mandates tag information.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BatchNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                    <xsd:documentation>Batch Number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BatchNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ContainerDetailKey" type="yfctype:Key" use="optional">
