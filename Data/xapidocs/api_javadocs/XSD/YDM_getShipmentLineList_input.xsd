                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FromAddressKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromAppointment" type="yfctype:Date" use="optional">
                <xsd:annotation yfc:columnName="FROM_APPOINTMENT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>The time after which an appointment can be taken by the ship node with the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FromAppointmentNo"
                type="yfctype:VarcharDocumentNo" use="optional"/>
            <xsd:attribute name="FromAppointmentQryType"
                type="yfctype:DateQryType" use="optional"/>
            <xsd:attribute name="FromAssignedToUserId"
                type="yfctype:LoginId" use="optional"/>
            <xsd:attribute name="FromBolNo"
                type="yfctype:VarcharDocumentNo" use="optional"/>
            <xsd:attribute name="FromBuyerMarkForNodeId"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromBuyerOrganizationCode"
                type="yfctype:OrgCode" use="optional"/>
            <xsd:attribute name="FromBuyerReceivingNodeId"
                type="yfctype:ShipNode" use="optional"/>
            <xsd:attribute name="FromCarrierAccountNo"
                type="yfctype:AccountNo" use="optional"/>
            <xsd:attribute name="FromCarrierServiceCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromCarrierType" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromCommercialValue"
                type="yfctype:Money" use="optional"/>
            <xsd:attribute name="FromCurrency" type="yfctype:Currency" use="optional"/>
            <xsd:attribute name="FromDeliveryMethod"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromDeliveryPlanKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromDestinationZone"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromDocumentType" type="yfctype:ID-40" use="optional"/>
            <xsd:attribute name="FromDownLoadCount" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="FromEnterpriseCode"
                type="yfctype:OrgCode" use="optional"/>
            <xsd:attribute name="FromExpectedDeliveryDate"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="FromExpectedShipmentDate"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="FromExportTaxpayerID"
