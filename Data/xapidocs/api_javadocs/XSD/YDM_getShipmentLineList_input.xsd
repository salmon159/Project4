            <xsd:attribute name="ToStatusDate" type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToToAddressKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToToAppointment" type="yfctype:Date" use="optional"/>
            <xsd:attribute name="ToTotalActualCharge"
                type="yfctype:Money" use="optional"/>
            <xsd:attribute name="ToTotalEstimatedCharge"
