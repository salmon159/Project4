            <xsd:attribute name="ToDocumentType" type="yfctype:ID-40" use="optional"/>
            <xsd:attribute name="ToDownLoadCount" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="ToEnterpriseCode"
