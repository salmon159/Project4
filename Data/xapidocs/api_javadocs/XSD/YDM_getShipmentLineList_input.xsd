    <xsd:element name="Instruction">
        <xsd:annotation yfc:tableName="YFS_INSTRUCTION_DETAIL">
            <xsd:documentation>Generic table used to store instructions associated with various entities in the system.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="FromInstructionDetailKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromInstructionText"
                type="yfctype:InstructionText" use="optional"/>
            <xsd:attribute name="FromInstructionType"
                type="yfctype:InstructionType" use="optional"/>
            <xsd:attribute name="FromInstructionURL" type="yfctype:URL" use="optional"/>
            <xsd:attribute name="FromInstructionUsage"
                type="yfctype:InstructionUsage" use="optional"/>
            <xsd:attribute name="FromReferenceKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromSequenceNo" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="FromTableName" type="yfctype:TableName" use="optional"/>
            <xsd:attribute name="InstructionDetailKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="INSTRUCTION_DETAIL_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                    <xsd:documentation>Primary key attribute of YFS_INSTRUCTION_DETAIL table that identifies an instruction.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InstructionDetailKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="InstructionText"
                type="yfctype:InstructionText" use="optional">
                <xsd:annotation yfc:columnName="INSTRUCTION_TEXT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                    <xsd:documentation>Free-form text indicating any additional information related to the instruction type or otherwise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InstructionTextQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="InstructionType"
                type="yfctype:InstructionType" use="optional">
                <xsd:annotation yfc:columnName="INSTRUCTION_TYPE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                    <xsd:documentation>Categorization of instructions so that they can be grouped together for reference. For example, you may have instructions of type Pick, Pack, and Ship for an order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InstructionTypeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="InstructionURL" type="yfctype:URL" use="optional">
                <xsd:annotation yfc:columnName="INSTRUCTION_URL"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                    <xsd:documentation>The URL relating to and/or containing the instructions.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InstructionURLQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="InstructionUsage"
                type="yfctype:InstructionUsage" use="optional">
                <xsd:annotation yfc:columnName="INSTRUCTION_USAGE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                    <xsd:documentation>Another way of categorization of instructions, over and above the instruction type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InstructionUsageQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReferenceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                    <xsd:documentation>Link to the entity key for which the instruction is stored. For example, if this record stores order header level instructions, then this attribute stores the value of order header key.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReferenceKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="SequenceNo" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="SEQUENCE_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                    <xsd:documentation>You could have an instruction exceeding the limits of a single record. Sequence number is used to provide a way to store a link between such instructions.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SequenceNoQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="TableName" type="yfctype:TableName" use="optional">
                <xsd:annotation yfc:columnName="TABLE_NAME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                    <xsd:documentation>Identifies the entity for which this instruction is stored. For example, if this record stores order header instructions, then you will see this field reading YFS_ORDER_HEADER.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TableNameQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToInstructionDetailKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToInstructionText"
                type="yfctype:InstructionText" use="optional"/>
            <xsd:attribute name="ToInstructionType"
                type="yfctype:InstructionType" use="optional"/>
            <xsd:attribute name="ToInstructionURL" type="yfctype:URL" use="optional"/>
            <xsd:attribute name="ToInstructionUsage"
                type="yfctype:InstructionUsage" use="optional"/>
            <xsd:attribute name="ToReferenceKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToSequenceNo" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="ToTableName" type="yfctype:TableName" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderBy">
        <xsd:annotation>
            <xsd:documentation>
                <div>
				If OrderBy element is passed, <code>ShipmentLine</code>s returned by the API are sorted in the same order as Attribute elements passed. Having many attributes in OrderBy element may have performance implications when large number of  <code>ShipmentLine</code>s are returned by the API.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:Attribute"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Attribute">
        <xsd:annotation yfc:tableName="YFS_ATTRIBUTE">
            <xsd:documentation>This table stores attribute definitions. Attributes represent characteristics of entities and can be assigned to an attribute group.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Desc" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Sort in decending order.
						Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
						Default value: <b>
                            <code>N</code>
                        </b>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Name" type="yfctype:Text-32" use="required">
                <xsd:annotation>
                    <xsd:documentation>Name of the attribute to use for sorting the results.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
