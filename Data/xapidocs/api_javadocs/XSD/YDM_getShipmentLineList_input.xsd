            <xsd:attribute name="FromFreightTerms"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromFromAddressKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromFromAppointment"
