                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Date"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute name="RevisionNo"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REVISION_NO" yfc:tableName="YFS_SHIPMENT_LINE_REQ_TAG">
                                    <xsd:documentation>Revision Number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="ShipmentTagSerials">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							   The attributes for this entity are treated as information only. Minimum validations are provided for this entity. <br/>
                                    <Validation>
                                    <Condition>If <code>Quantity</code> is less than or equal to 0.
									</Condition>
                                    <Error
                                    Description="Invalid Quantity." ErrorCode="YDM00110"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>If <code>SerialNo</code> is provided and <code>Quantity</code> is not equal to one.
									</Condition>
                                    <Error
                                    Description="Quantity must be equal to one because SerialNo is provided." ErrorCode="YDM00257"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="ShipmentTagSerial">
                                    <xsd:annotation yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>This table stores additional information related to tag number, serial number, ship by date for a shipment line or container detail. Tag information is stored only if the Node tracks tag information in inventory or the Buyer mandates tag information.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="BatchNo"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="BATCH_NO" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Batch Number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ContainerDetailKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CONTAINER_DETAIL_KEY" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Link to YFS_CONTAINER_DETAILS table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="LotAttribute1"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LOT_ATTRIBUTE1" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Any other lot attribute.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="LotAttribute2"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LOT_ATTRIBUTE2" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Any other lot attribute.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="LotAttribute3"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LOT_ATTRIBUTE3" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Any other lot attribute.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="LotKeyReference"
                                    type="yfctype:Text-4000" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LOT_KEY_REFERENCE" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Stores reference information of the lot number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="LotNumber"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LOT_NUMBER" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Lot Number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ManufacturingDate" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="LOT_MANUFACTURE_DATE" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Lot Manufacturing Date.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Date"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Quantity"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="QUANTITY" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Quantity.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RevisionNo"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REVISION_NO" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Revision Number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SerialNo"
                                    type="yfctype:SerialNo" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SERIAL_NO" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Serial number.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ShipByDate"
                                    type="yfctype:Date" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIP_BY_DATE" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>For time sensitive items, date by which it should be shipped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ShipmentLineKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIPMENT_LINE_KEY" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Link to YFS_SHIPMENT_LINE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ShipmentTagSerialKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SHIPMENT_TAG_SERIAL_KEY" yfc:tableName="YFS_SHIPMENT_TAG_SERIAL">
                                    <xsd:documentation>Primary key attribute.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="Instructions">
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="Instruction">
                                    <xsd:annotation yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Generic table used to store instructions associated with various entities in the system.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="InstructionDetailKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_DETAIL_KEY" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Primary key attribute of YFS_INSTRUCTION_DETAIL table that identifies an instruction.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InstructionText"
                                    type="yfctype:InstructionText" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_TEXT" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Free-form text indicating any additional information related to the instruction type or otherwise.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InstructionType"
                                    type="yfctype:InstructionType" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_TYPE" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Categorization of instructions so that they can be grouped together for reference. For example, you may have instructions of type Pick, Pack, and Ship for an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InstructionURL"
                                    type="yfctype:URL" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_URL" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>The URL relating to and/or containing the instructions.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="InstructionUsage"
                                    type="yfctype:InstructionUsage" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="INSTRUCTION_USAGE" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Another way of categorization of instructions, over and above the instruction type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReferenceKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_KEY" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Link to the entity key for which the instruction is stored. For example, if this record stores order header level instructions, then this attribute stores the value of order header key.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SequenceNo"
                                    type="yfctype:Count" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SEQUENCE_NO" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>You could have an instruction exceeding the limits of a single record. Sequence number is used to provide a way to store a link between such instructions.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="TableName"
                                    type="yfctype:TableName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="TABLE_NAME" yfc:tableName="YFS_INSTRUCTION_DETAIL">
                                    <xsd:documentation>Identifies the entity for which this instruction is stored. For example, if this record stores order header instructions, then you will see this field reading YFS_ORDER_HEADER.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                        <xsd:attribute name="CountryOfOrigin"
                            type="yfctype:Country" use="optional">
                            <xsd:annotation
                                yfc:columnName="COUNTRY_OF_ORIGIN" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The country of origin.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="DocumentType"
                            type="yfctype:String" use="optional">
                            <xsd:annotation yfc:isConditional="Y"/>
                        </xsd:attribute>
                        <xsd:attribute name="ExternalReleaseIdentifier"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXTERNAL_RELEASE_IDENTIFIER" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The identifier of the external release. This attribute along with OrderNo and ReleaseNo will uniquely identify the order release external to the system.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="FifoNo"
                            type="yfctype:Integer" use="optional">
                            <xsd:annotation yfc:columnName="FIFO_NO" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>A date based inventory attribute that helps understand the order in which stock arrived at the node. This is used to send out items that arrived earlier than the ones that arrived later.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="History"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>Pass Y if shipment line is to be imported into history tables</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ItemDesc"
                            type="yfctype:Text-500" use="optional">
                            <xsd:annotation
                                yfc:columnName="ITEM_DESCRIPTION" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The description of item.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ItemID"
                            type="yfctype:ItemID" use="optional">
                            <xsd:annotation yfc:columnName="ITEM_ID"
                                yfc:isConditional="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>
                                    <div>
						ItemId.
							<Validation>
                                    <Condition>
                                    <code>ItemID</code> should be non-void.
								</Condition>
                                    <Error
                                    Description="Void Item ID." ErrorCode="YFS10050"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="KitCode" use="optional">
                            <xsd:annotation yfc:columnName="KIT_CODE" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The kit code of the item, if it is a Kit item.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:restriction base="yfctype:Text-20">
                                    <xsd:enumeration value="LK"/>
                                    <xsd:enumeration value=""/>
                                    <xsd:enumeration value=" "/>
                                    <xsd:enumeration value="Blanks"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="LevelOfService"
                            type="yfctype:LevelOfService" use="optional">
                            <xsd:annotation
                                yfc:columnName="LEVEL_OF_SERVICE" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>Indicates the level of service required for this shipment line.  Default from corresponding order release.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NetWeight"
                            type="yfctype:Dimension" use="optional">
                            <xsd:annotation yfc:columnName="NET_WEIGHT" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The weight of the items in the shipment. The unit of weight is as specified by the NetWeightUom attribute.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="NetWeightUom"
                            type="yfctype:UOM" use="optional">
                            <xsd:annotation
                                yfc:columnName="NET_WEIGHT_UOM" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The unit of net weight.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderHeaderKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The order header key that is associated with this shipment line.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderLineKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The order line key that is associated with this shipment line.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderNo"
                            type="yfctype:VarcharDocumentNo" use="optional">
                            <xsd:annotation yfc:columnName="ORDER_NO"
                                yfc:isConditional="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>
                                    <div>
						Order number corresponding to the Order for the ShipmentLine. If OrderAvailableOnSystem on Shipment is <code>
                                    <b>Y</b>
                                    </code> then, the order should already exist in the system. If the shipment is being imported into history tables, the order should be a history order. If the shipment is being imported into normal tables, the order should also be a normal order.
							<Validation>
                                    <Condition>
                                    <code>OrderNo</code> should be valid.
								</Condition>
                                    <Error
                                    Description="Invalid Order." ErrorCode="YFS10003"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrderReleaseKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORDER_RELEASE_KEY" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The order release key that is associated with this shipment line.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PrimeLineNo"
                            type="yfctype:LineNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="PRIME_LINE_NO"
                                yfc:isConditional="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>
                                    <div>
                                    <code>PrimeLineNo</code> of the corresponding OrderLine.
							<Validation>
                                    <Condition>If OrderAvailableOnSystem on Shipment is <code>
                                    <b>Y</b>
                                    </code> then, the order line should belong to the order.
								</Condition>
                                    <Error
                                    Description="Invalid Line." ErrorCode="YFS10009"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ProductClass"
                            type="yfctype:ProductClass" use="optional">
                            <xsd:annotation
                                yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The product class.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Quantity"
                            type="yfctype:Quantity" use="optional">
                            <xsd:annotation yfc:columnName="QUANTITY" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The shipment line quantity. Entering a quantity is mandatory when creating a shipment line.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ReleaseNo"
                            type="yfctype:Count" use="optional">
                            <xsd:annotation yfc:columnName="RELEASE_NO"
                                yfc:isConditional="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>
                                    <div>
						Release number. Generated as a one up number against that release. Required if Order was released before included in Shipment.
							<Validation>
                                    <Condition>If OrderAvailableOnSystem on Shipment is <code>
                                    <b>Y</b>
                                    </code> then, the order release should belong to the order.
								</Condition>
                                    <Error
                                    Description="No Record Found." ErrorCode="YFS10001"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="RequestedSerialNo"
                            type="yfctype:SerialNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="REQUESTED_SERIAL_NO" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The requested serial number of the item.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Segment"
                            type="yfctype:Segment" use="optional">
                            <xsd:annotation yfc:columnName="SEGMENT" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The segment of inventory customized for an order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SegmentType"
                            type="yfctype:SegmentType" use="optional">
                            <xsd:annotation
                                yfc:columnName="SEGMENT_TYPE" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The segment type of inventory customized for an order.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentLineKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_LINE_KEY"
                                yfc:isConditional="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>
                                    <div>
						Primary key attribute.
							<Validation>
                                    <Condition>
                                    <code>ShipmentLineKey</code> should be unique for the table.
								</Condition>
                                    <Error
                                    Description="Unique constraint violation." ErrorCode=""/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentLineNo"
                            type="yfctype:LineNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_LINE_NO"
                                yfc:isConditional="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>
                                    <div>
						Part of Primary Key . Unique for a Shipment.
							<Validation>
                                    <Condition>
                                    <code>ShipmentLineNo</code> should be unique for the Shipment.
								</Condition>
                                    <Error
                                    Description="Missing Attribute." ErrorCode="YFS10460"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShipmentSubLineNo"
                            type="yfctype:LineNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="SHIPMENT_SUB_LINE_NO"
                                yfc:isConditional="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>This is a part of the unique key. ShipmentSubLineNo along with the ShipmentLineNo is used to identify the logical kit component shipment line. The value of this field is zero - for kit parent shipment lines and shipment lines with items that are not kit components, and greater than zero if the the shipment line corresponds to a logical kit component item.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="SubLineNo"
                            type="yfctype:LineNo" use="optional">
                            <xsd:annotation yfc:columnName="SUB_LINE_NO"
                                yfc:isConditional="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>
                                    <div>
                                    <code>SubLineNo</code> of the corresponding OrderLine.
							<Validation>
                                    <Condition>If OrderAvailableOnSystem on Shipment is <code>
                                    <b>Y</b>
                                    </code> then, the order line should belong to the order.
								</Condition>
                                    <Error
                                    Description="Invalid Line." ErrorCode="YFS10009"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="UnitOfMeasure"
                            type="yfctype:UOM" use="optional">
                            <xsd:annotation yfc:columnName="UOM"
                                yfc:isConditional="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                                <xsd:documentation>The unit of measure.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="isHistory"
                            type="yfctype:String" use="optional"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalDates">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:AdditionalDate"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalDate">
        <xsd:annotation yfc:tableName="YFS_ADDITIONAL_DATE">
            <xsd:documentation>This table stores additional date attributes that may be stored for an entity.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ActualDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="ACTUAL_DATE" yfc:tableName="YFS_ADDITIONAL_DATE">
                    <xsd:documentation>Actual Date.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AdditionalDateKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ADDITIONAL_DATE_KEY" yfc:tableName="YFS_ADDITIONAL_DATE">
                    <xsd:documentation>Primary Key attribute of YFS_ADDITIONAL_DATE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DateTypeId" type="yfctype:ID-40" use="required">
                <xsd:annotation yfc:columnName="DATE_TYPE_ID" yfc:tableName="YFS_ADDITIONAL_DATE">
                    <xsd:documentation>
                        <div>
							Identifies a date type for which additional dates are stored.
								<Validation>
                                <Condition>
                                    <code>DateTypeId</code> should be non-void. Should also be unique for the Shipment.
									</Condition>
                                <Error
                                    Description="Missing attribute in input." ErrorCode="YFS10460"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExpectedDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="EXPECTED_DATE" yfc:tableName="YFS_ADDITIONAL_DATE">
                    <xsd:documentation>Expected Date.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReferenceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE_KEY" yfc:tableName="YFS_ADDITIONAL_DATE">
                    <xsd:documentation>A key pointing to the entity for which an additional date is maintained.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReferenceType" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE_TYPE" yfc:tableName="YFS_ADDITIONAL_DATE">
                    <xsd:documentation>A reference type that identifies the entity for which these attributes are stored.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequestedDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="REQUESTED_DATE" yfc:tableName="YFS_ADDITIONAL_DATE">
                    <xsd:documentation>Requested Date.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
