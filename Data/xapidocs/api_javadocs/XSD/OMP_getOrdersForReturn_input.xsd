<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/OMP/getOrdersForReturn/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/OMP/getOrdersForReturn/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="OMP_getOrdersForReturn_input.xsd" yfc:rootElement="Order">
        <xsd:appinfo source="yfc:structure">
            <Order hasAttributes="Y">
                <OrderLines>
                    <OrderLine hasAttributes="Y">
                        <Item hasAttributes="Y"/>
                    </OrderLine>
                </OrderLines>
                <PaymentMethods>
                    <PaymentMethod hasAttributes="Y">
                        <PersonInfoBillTo hasAttributes="Y"/>
                    </PaymentMethod>
                </PaymentMethods>
                <OrderSearchConditions hasAttributes="Y">
                    <OrderSearchCondition hasAttributes="Y"/>
                </OrderSearchConditions>
            </Order>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="Order" type="yfc:OrderXSDType">
        <xsd:annotation yfc:UsedForIndexing="Order" yfc:tableName="YFS_ORDER_HEADER">
            <xsd:documentation>
                <div>
			The <code>OrderHeaderKey</code>, or  the <code>EnterpriseCode</code>, <code>DocumentType</code> and the <code>OrderNo</code> must be passed to identify the <code>Order</code>.
			</div>
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="OrderXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderLines"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PaymentMethods"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderSearchConditions"/>
        </xsd:all>
        <xsd:attribute name="BillToID" type="yfctype:ID-40" use="optional">
            <xsd:annotation yfc:columnName="BILL_TO_ID" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The identifier of the bill to customer.  If a customer definition representing the buyer organization exists within the system, the BillToID can represent the CustomerID of the customer.  Otherwise, the BillToID can represent the PersonID of the bill to address or the BuyerOrganizationCode of the buyer organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BillToKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="BILL_TO_KEY" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The bill to address that is associated with this order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerContactID" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_CONTACT_ID" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>This is the customer contact ID for the order. This field maps to contact ID field of YFS_Customer_Contact.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerRewardsNo" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_REWARDS_NO" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The identifier of the customer's rewards number</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The document type that is associated with this order. For example, Sales Order document (0001), Planned Order document (0002), Return Order document (0003) , and Purchase Order document (0005). This field determines the order's fulfillment cycle and controls the inventory and payment updates performed on the order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ENTERPRISE_KEY" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The enterprise that is associated with this order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The primary key for the YFS_ORDER_HEADER table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="ORDER_NO" yfc:tableName="YFS_ORDER_HEADER">
                <xsd:documentation>The number of the order, which can be passed during order creation or generated based on a system rule setting.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="OrderLines">
        <xsd:annotation>
            <xsd:documentation>If the order doesn't exist in the system, then order line information must be provided
    			to the API.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="1" ref="yfc:OrderLine"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
					Optional. Stores order line information, including item details and shipping information. Multiple <code>OrderLine</code>s can be created for a single order. At least one <code>OrderLine</code> must exist for an order if order is getting created with <code>DraftOrderFlag</code> as 'N'. For draft order, line must be added before confirming the order.
                        </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Item"/>
            </xsd:all>
            <xsd:attribute name="AllocationDate" type="yfctype:String" use="optional"/>
            <xsd:attribute name="BOMXML" type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The BOM XML to be used to create the order line and its components.
                    	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierAccountNo"
                type="yfctype:AccountNo" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_ACCOUNT_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The customers carrier account number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierServiceCode"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The carrier service code for this order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CommittedQuantity" use="optional">
                <xsd:annotation yfc:columnName="COMMITTED_QUANTITY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>It is a data attribute to store the quantity that is committed by supplier.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Number"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ConditionVariable1"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CONDITION_VARIABLE_1" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>A user defined variable that can be used for condition building in process modeling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConditionVariable2"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CONDITION_VARIABLE_2" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>A user defined variable that can be used for condition building in process modeling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConfigurationKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BOM_CONFIG_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is used by Sterling Multi-Channel Selling Solution so that Visual Modeler can be opened with existing options.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerLinePONo"
                type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_PO_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The buyer PO line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerPONo" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The buyer's PO number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeliveryCode"
                type="yfctype:DeliveryCode" use="optional">
                <xsd:annotation yfc:columnName="DELIVERY_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates who will pay the freight. Application understands values such as SUPPLIER, ENTERPRISE and MARKETPLACE to mean that each of these organizations will pay for the freight. You could however pass any other value which is a valid shipment charge type as understood by the carrier by which the shipment is transported. See YFS_SHIPMENT_CONTAINER.carrier_payment_type for additional information.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeliveryMethod" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DELIVERY_METHOD" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Describes how product is delivered. <code>DeliveryMethod</code> is only valid for product lines.
						<br/>Valid Values: <b>
                                <code>DEL</code>
                            </b> - Delivery, <b>
                                <code>SHP</code>
                            </b>-Ship, <b>
                                <code>CARRY</code>
                            </b>- Cash And Carry or <b>
                                <code>PICK</code>
                            </b>- Pickup by the user.
  					    <br/>If a <code>DeliveryMethod</code> value is not passed, the value is set as follows:
					    <br/>If the <code>ProductLine</code> has Delivery associations, set to <b>
                                <code>DEL</code>
                            </b>.
					    <br/>If the <code>Item</code>'s <code>DefaultDeliveryMethod</code> has a value of <b>
                                <code>Y</code>

                            </b> in the catalog, then the <code>Item</code> is checked for shipping methods, as defined by the following fields in the Catalog:
						 <table
                                border="0" cellpadding="0"
                                cellspacing="0" class="table">
                                <tbody>
                                    <tr>
                                    <td class="tablecolumn">
                                    <b>Condition</b>
                                    </td>
                                    <td class="tablecolumn">DeliveryMethod value</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">
                                    <code>IS_SHIPPING_ALLLOWED is <b>
                                    <code>Y</code>
                                    </b>
                                    </code>
                                    </td>
                                    <td class="tablecolumn">
                                    <code>SHP</code>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">
                                    <code>IS_DELIVERY_ALLLOWED is <b>
                                    <code>Y</code>
                                    </b>
                                    </code>
                                    </td>
                                    <td class="tablecolumn">
                                    <code>DEL</code>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">
                                    <code>IS_PICKUP_ALLLOWED is <b>
                                    <code>Y</code>
                                    </b>
                                    </code>
                                    </td>
                                    <td class="tablecolumn">
                                    <code>PICK</code>
                                    </td>
                                    </tr>
                                </tbody>
                            </table>
                            <Validation>
                                <Condition>If a value is passed, it must be a valid value.
						   </Condition>
                                <Error
                                    Description="Invalid Delivery Method Passed" ErrorCode="OMP10020"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DepartmentCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						This field can be used for line classification and reporting purposes. If department code is passed at order header but not passed at order line then value at order header is pushed to order line.

						 </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DependencyShippingRule"
                type="yfctype:Text-2" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							Indicates how the <code>OrderLines</code> are dependent on the parent line. The <code>DependencyShippingRule</code> can only be specified for the parent line.
							<br/> If <code>DependencyShippingRule</code> is specified on the parent line, it is defaulted to the same value on all the dependent lines. If <code>DependencyShippingRule</code> is passed on any of the dependent lines, the value passed is ignored.
							<br/>Default: " "  (Null)
							<br/>Valid values: <b>
                                <code>01</code>
                            </b> - Ship together,<b>
                                <code>02</code>
                            </b> - Deliver together
							<br/>
                            <br/>If <code>DependencyShippingRule</code> value is <b>
                                <code>02</code>
                            </b>, a <code>MergeNode</code> must be specified. Order release created for these lines have a <code>MergeNode</code> value, so shipments are  first made to the <code>MergeNode</code>, where they are consolidated for final shipment.
							<br/>
                            <br/>The <code>DependencyShippingRule</code> can only be specified for a product line (<code>ItemGroupCode</code> value is <b>
                                <code>PROD</code>
                            </b>). A product line associated with a delivery line cannot be a part of any dependency shipping rule.
							 <Validation>
                                <Condition>If <code>DependencyShippingRule</code> is passed for delivery or provided service line or an associated product line.
								 </Condition>
                                <Error
                                    Description="A Product Line with Dependency Shipping Rule cannot be associated with Delivery Service Line." ErrorCode="OMP10041"/>
                                <Error
                                    Description="Service Line cannot have Dependency Shipping Rule" ErrorCode="OMP10058"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>MergeNode</code> is not specified for parent line, and  <code>DependencyShippingRule</code> value is <b>
                                    <code>02</code>
                                    </b>.
								 </Condition>
                                <Error
                                    Description="Merge node is required for shipping rule deliver together" ErrorCode="YFS10443"/>
                            </Validation>
                            <Validation>
                                <Condition>Invalid value is passed.
								 </Condition>
                                <Error
                                    Description="Invalid dependency shipping rule" ErrorCode="YFS10450"/>
                            </Validation>
                            <br/>

                            <br/>Based on the <code>DependencyShippingRule</code>, some of attributes on dependent lines are defaulted from the parent line.  For these attributes have any value other than what parent line has, the value is ignored. In addition to following attributes <code>EarliestShipDate</code> and <code>EarliestDeliveryDate</code> are also copied from parent line to dependent line.
							 <div
                                class="innerpanel" width="30%">
                                <table border="0" cellpadding="0"
                                    cellspacing="0" class="table">
                                    <tbody>
                                    <tr>
                                    <td class="tablecolumn">
                                    <b>Value</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ShippingRule</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ShipNode</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ReceivingNode</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ShipTo</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>MergeNode</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ReqShipDate</b>
                                    </td>
                                    <td class="tablecolumn">
                                    <b>ReqDeliveryDate</b>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">01</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">- </td>
                                    <td class="tablecolumn">- </td>
                                    <td class="tablecolumn">-</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">02</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">-</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn"> defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    <td class="tablecolumn">defaults</td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DistributionRuleId"
                type="yfctype:DistributionRuleId" use="optional">
                <xsd:annotation yfc:columnName="DISTRIBUTION_RULE_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						If passed then supplies and demands are read applying the distribution rule filter for inventory availability.
						<Validation>
                                <Condition>Must be a valid <code>DistributionRuleID</code> for the seller organization passed.
							</Condition>
                                <Error
                                    Description="Distribution Rule not set up for seller" ErrorCode="YFS10495"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EarliestScheduleDate" use="optional">
                <xsd:annotation yfc:columnName="EARLIEST_SCHEDULE_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The date before which the order line will not be picked up by scheduling. This is not supported on a return order.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FillQuantity" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="FILL_QUANTITY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>The minimum quantity of the <code>OrderedQty</code> that must be fulfilled in a single release.
							<br/>Default: <b>
                                <code>0</code>
                            </b>
                            <Validation>
                                <Condition>Any positive number not greater than line ordered quantity.
								</Condition>
                                <Error
                                    Description="Invalid Fill Quantity." ErrorCode="YFS10498"/>
                            </Validation>
                            <Validation>
                                <Condition>If invalid number is passed
								</Condition>
                                <Error
                                    Description="java.lang.NumberFormatException" ErrorCode=""/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstIterationSeqNo" use="optional">
                <xsd:annotation yfc:columnName="FIRST_ITER_SEQ_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The first iteration sequence for which this line is applicable. If Null, it is assumed that the line applies to all sequences before LastIterationSeqNo. Default is Null.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:LongSequence"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FreightTerms" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="FOB" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>Indicates Freight Terms for the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FulfillmentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="FULFILLMENT_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The method of fulfilling the order line during scheduling. This field can be matched with the corresponding field in the YFS_SOURCING_RULE_HDR table and the appropriate sourcing rule can be selected.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GiftFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="GIFT_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether the order line is a gift item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GiftWrap" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_GIFT_WRAP" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates if the order line has to be gift wrapped.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HoldFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="HOLD_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag determines whether a hold has been placed on the order when certain hold type criteria are met. If order is on hold, default configuration will stop the shceduling and release process for the order. The flag is reset to 'N' when all the hold types have been resolved on the order and all of its order lines. Based on hold type configuration, certain transactions may not be allowed to process. Note that there is no validation made on the hold applied but this flag can be reset by the application based on hold types existed on the order. Default: N Valid Values: Y or N</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HoldReasonCode"
                type="yfctype:HoldReasonCode" use="optional">
                <xsd:annotation yfc:columnName="HOLD_REASON_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ImportLicenseExpDate"
                type="yfctype:Date" use="optional">
                <xsd:annotation yfc:columnName="IMPORT_LICENSE_EXP_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The expiration date of the import license number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ImportLicenseNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="IMPORT_LICENSE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The import license number to be used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IntentionalBackorder"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="INTENTIONAL_BACKORDER" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						This flag indicates whether the order line should be backordered upon creation. If passed as 'Y', the line will be backordered even if inventory is available. If a reservation ID is passed on the order line and reservation exists for a current, past or future date, reservation will be consumed to block inventory for the backordered demand. In this scenario, demand created by the reservation ID is removed, and a backordered demand will be generated. No attempt is made to look for inventory if reservation is not available for the passed reservation ID. For draft orders, the reservation is consumed and the order line is created in created status. During confirmDraftOrder, this line goes in backordered status. This flag is ignored if reservation is mandatory. This flag should not be passed for bundle parent lines. If it is passed as 'Y' for bundle parent order line, the line is created in backorder status. It is not propagated to bundle components.
						 </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsFirmPredefinedNode"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FIRM_PREDEFINED_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                    Indicates if the pre-defined node on the line is firm or not.  If IsFirmPredefinedNode is marked as <b>N</b>, then the node on the line is ignored during scheduling. If not passed, the default value for this Flag is <b>Y</b>. If <code>ProcureFromNode</code> is passed, this flag will always be set to <b>Y</b>. For a <b>DS</b> or a <code>PS</code> line so, it is defaulted to <b>Y</b>. If the ship node is not present on the line but the value passed is <b>Y</b>, then it is treated as <b>N</b> during scheduling.
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsForwardingAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FORWARDING_ALLOWED" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether forwarding is allowed for the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsProcurementAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PROCUREMENT_ALLOWED" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This flag indicates whether procurement is allowed for the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemGroupCode"
                type="yfctype:ItemGroupCode" use="required">
                <xsd:annotation yfc:columnName="ITEM_GROUP_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Specifies whether the <code>OrderLine</code> is a
						product,  delivery, or provided service Line.
						<br/>Default: <b>
                                <code>PROD</code>
                            </b> - <i>Product</i>
                            <br/>Valid Values: <b>
                                <code>PROD</code>
                            </b>, <b>
                                <code>DS</code>
                            </b> or <b>
                                <code>PS</code>
                            </b>.
						<br/>If the <code>ItemGroupCode</code> is not passed, the <code>Item</code> is looked up in the catalog or is assigned during the user exit <code>YCMGetItemDetailsUE</code>. If the <code>Item</code> is not found, the <code>ItemGroupCode</code> is defaulted to <b>
                                <code>PROD</code>
                            </b>.
						<Validation>
                                <Condition>If the value is passed, it must be a valid value.
							</Condition>
                                <Error
                                    Description="Invalid Item Group Code Passed" ErrorCode="OMP10019"/>
                            </Validation>
                            <Validation>
                                <Condition>When service item which exists in the catalog is passed for bundle parent line.
						</Condition>
                                <Error
                                    Description="Bundle parent item cannot be a service item." ErrorCode="OMP80_0005"/>
                            </Validation>
                            <Validation>
                                <Condition>When DS item is passed as component for a bundle.
						</Condition>
                                <Error
                                    Description="A bundle cannot have DS component" ErrorCode="OMP80_0015"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="KIT_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                            <br/>Specifies a collection of items that form a unit. For example, a home theater system is a unit, or kit, consisting of components such as a VCR, a TV, a stereo, and other components.
						 <br/>Valid values:  <b>
                                <code>LK</code>
                            </b> - <i>Logical kit</i>, <b>
                                <code>PK</code>
                            </b> - <i>Physical kit</i>, <b>
                                <code>DPK</code>
                            </b> - <i>Dynamic Physical kit</i>, <b>
                                <code>BUNDLE</code>
                            </b> - <i>Bundle</i>
                            <br/> If no KitCode is passed, the <code>createOrder</code> API searches the item in the Catalog. The following are the possible scenarios.  See Validation Conditions for possible error scenarios.
						 <br/>
                            <table border="0" cellpadding="0"
                                cellspacing="0" class="table">
                                <tbody>
                                    <tr>
                                    <td class="tablecolumn" width="20%">
                                    <b>Kit definition passed</b>
                                    </td>
                                    <td class="tablecolumn" width="20%">
                                    <b>Item in catalog</b>
                                    </td>
                                    <td class="tablecolumn" width="20%">
                                    <b>KitItem</b>
                                    </td>
                                    <td class="tablecolumn" width="40%">
                                    <b>Orderline based on</b>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" width="20%">Y</td>
                                    <td class="tablecolumn" width="20%">Y </td>
                                    <td class="tablecolumn" width="20%">Y</td>
                                    <td class="tablecolumn" width="40%">Passed KitDefinition</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" width="20%">N</td>
                                    <td class="tablecolumn" width="20%">Y </td>
                                    <td class="tablecolumn" width="20%">Y</td>
                                    <td class="tablecolumn" width="40%">Catalog definition</td>
                                    </tr>
                                </tbody>
                            </table>
                            <Validation>
                                <Condition>Kit definition not passed, item exists in the catalog, it is a kit item, but there is no kit definition.
							</Condition>
                                <Error
                                    Description="YFS: No Kit Lines Found" ErrorCode="YFS10273"/>
                            </Validation>
                            <Validation>
                                <Condition>Kit definition not passed, item does not exist in the catalog.
							</Condition>
                                <Error
                                    Description="YFS: No Kit Lines Found" ErrorCode="YFS10273"/>
                            </Validation>
                            <Validation>
                                <Condition>Kit definition passed as the KitLines element, and the Item is not in catalog.
							</Condition>
                                <Error Description="Invalid kit code" ErrorCode="YFS10442"/>
                            </Validation>
                            <Validation>

                                <Condition>Kit definition passed as the KitLines element, and the Item is not a KitItem.
							<Error
                                    Description="Invalid kit code" ErrorCode="YFS10442"/>
                                </Condition>
                            </Validation>
                            <Validation>
                                <Condition>Bundle parent line can not have a logical kit component.
							 </Condition>
                                <Error
                                    Description="Logical kit component not supported for bundle" ErrorCode="OMP80_0001"/>
                            </Validation>
                            <Validation>
                                <Condition>Bundle parent line can not have a dynamic physical kit component.
							 </Condition>
                                <Error
                                    Description="Dynamic physical kit component not supported for bundle" ErrorCode="OMP79_0023"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitQty" use="optional">
                <xsd:annotation yfc:columnName="KIT_QTY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The quantity that is used to compute ordered quantity for bundle component line. It can be specified only for bundle components. If specified ordered quantity will be set to KitQty*OrderedQty of the bundle parent.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Quantity"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LastIterationSeqNo" use="optional">
                <xsd:annotation yfc:columnName="LAST_ITER_SEQ_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The last iteration sequence for which this line is applicable. If Null, it is assumed to go on indefinitely. Default is Null.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:LongSequence"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LevelOfService"
                type="yfctype:LevelOfService" use="optional">
                <xsd:annotation yfc:columnName="LEVEL_OF_SERVICE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>Indicates the level of service required for this order line.  Default is Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LineType" type="yfctype:LineType" use="optional">
                <xsd:annotation yfc:columnName="LINE_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The line type. This field can be used in process modeling for pipeline determination, or for doing conditional processing for this line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MergeNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="MERGE_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                            <br/>If two or more <code>OrderLines</code> are to be delivered together, this is the node where these lines are merged before final delivery. This field is required when the <code>DependencyShippingRule</code> is set to <b>
                                <code>02</code>
                            </b>.
						<Validation>
                                <Condition>Must exist in table <code>YFS_SHIP_NODE</code>.
							 </Condition>
                                <Error Description="Invalid ship node" ErrorCode="YFS10048"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MinShipByDate" use="optional">
                <xsd:annotation yfc:columnName="MIN_SHIP_BY_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                        The preferred minimum ship by date of the available supply that order line can allocate against.  Available supply with ship by date earlier than the minimum ship by date will not be considered.  This applies to the order line with a valid time-sensitive item.  The <code>MinShipByDate</code> of the order line will also be used by all kit lines with valid time-sensitive item.
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OrderedQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="ORDERED_QTY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The quantity of the line item that is being ordered. This quantity is in Inventory UnitOfMeasure. If Quantity needs to be passed in a different UnitOfMeasure (only allowed for a Product line), then it should be passed in OrderLineTranQuantity node.
						<br/> For a service line item which is used as a standalone service, <code>OrderedQty</code> is treated in the same manner as product lines and must be a non-zero positive number.
						<br/>The <code>OrderedQty</code> value passed on the service line is ignored, if ordered quantity computaion is not based on entered quantity.
						 <Validation>
                                <Condition>Any non-zero positive number for a product Line or standalone service line.
							 </Condition>
                                <Error
                                    Description="java.lang.NumberFormatException" ErrorCode=""/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>SerialNo</code> is provided and <code>OrderedQty</code> is not equal to one.
							  </Condition>
                                <Error
                                    Description="OrderedQty must be equal to one because SerialNo is provided." ErrorCode="OMP11024"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PackListType" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PACKLIST_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The code indicating the type of packlist to be used for the line. Lines requiring different pack lists are not mixed in a single order release.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonalizeCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PERSONALIZE_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The code for personalization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PickableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PICKABLE_FLAG" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is used internally.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PipelineId" type="yfctype:Identifier" use="optional">
                <xsd:annotation yfc:columnName="PIPELINE_ID" yfc:tableName="YFS_PIPELINE">
                    <xsd:documentation>
                        <div>
					Pipeline id on order line. If not passed, system will determine the pipeline based on attributes on the line.
					<Validation>
                                <Condition>
						It should be a valid pipeline for the process type order line belongs.
						</Condition>
                                <Error Description="Invalid pipeline id" ErrorCode="OMP11071"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The order line number. Part of an <code>OrderLine</code>'s unique key.
						If <code>PrimeLineNo</code> is not passed, it is generated. A starting number can be specified  in the configurator at each document type and enterprise level. If a starting number is specified the <code>PrimeLineNo</code> will be generated starting from that number.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProcureFromNode"
                type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="PROCURE_FROM_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                    The node from which the order line should be procured if cannot source from the shipnode on the order line.
                    <Validation>
                                <Condition>
                        ProcureFromNode can be specified only if shipnode is passed.
                        </Condition>
                                <Error
                                    Description="Cannot pass ProcureFromNode without Shipnode" ErrorCode="OMP10086"/>
                            </Validation>
                            <Validation>
                                <Condition>
                        If Reservation is attempted from ProcureFromNode, then shipnode must have <code>Procure To Ship Allowed</code> Flag set to <b>Y</b>
                                </Condition>
                                <Error
                                    Description="Cannot pass ProcureFromNode when ShipNode cannot procure." ErrorCode="OMP11042_55SP1"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PromisedApptEndDate" use="optional">
                <xsd:annotation yfc:columnName="PROMISED_APPT_END_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The date and time when the appointment ends for an <code>OrderLine</code>.This date only applies to Provided (ItemGroupCode=PS) and Delivered (ItemGroupCode=DS) lines.
						<Validation>
                                <Condition>
                                    <code>PromisedApptEndDate</code> must be greater than or equal to <code>PromisedApptStartDate</code>.
							</Condition>
                                <Error
                                    Description="PromisedApptStartDate cannot be greater than PromisedApptEndDate" ErrorCode="OMP10056"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>PromisedApptEndDate</code> is passed, but  <code>PromisedApptStartDate</code> is not passed.
							</Condition>
                                <Error
                                    Description="Both PromisedApptStartDate and PromisedApptEndDate must be passed, if either one is passed." ErrorCode="OMP11047_55SP2"/>
                            </Validation>
                            <Validation>
                                <Condition>If the date corresponds to a slot which cannot be used for appointment planning.
							</Condition>
                                <Error
                                    Description="Slot based Capacity Consumption cannot be done on slots that cannot be used for taking appointments." ErrorCode="INV80_002"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="PromisedApptStartDate" use="optional">
                <xsd:annotation
                    yfc:columnName="PROMISED_APPT_START_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>	The date and time when the appointment starts for an <code>OrderLine</code>. This date only applies to Provided (ItemGroupCode=PS) and Delivered (ItemGroupCode=DS) lines.
						<Validation>
                                <Condition>
                                    <code>PromisedApptStartDate</code> cannot be after <code>PromisedApptEndDate</code>.
							</Condition>
                                <Error
                                    Description="PromisedApptStartDate cannot be greater than PromisedApptEndDate." ErrorCode="OMP10056"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>PromisedApptStartDate</code> is passed, but  <code>PromisedApptEndDate</code> is not passed.
                            </Condition>
                                <Error
                                    Description="Both PromisedApptStartDate and PromisedApptEndDate must be passed, if either one is passed." ErrorCode="OMP11047_55SP2"/>
                            </Validation>
                            <Validation>
                                <Condition>If the date corresponds to a slot which cannot be used for appointment planning.
							</Condition>
                                <Error
                                    Description="Slot based Capacity Consumption cannot be done on slots that cannot be used for taking appointments." ErrorCode="INV80_002"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Purpose" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="PURPOSE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is passed out in the Tax calculation user exits.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceivingNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="RECEIVING_NODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						The node to which an <code>OrderLine</code> is to be shipped.<br>
						Note: When calling this API for a return order, this ReceivingNode attribute should contain the receiving_node ID associated with the "Ship From" node (the node from which the returned item is shipped).</br>
                            <Validation>
                                <Condition>Must exist in table <code>YFS_SHIP_NODE</code>.
							</Condition>
                                <Error Description="Invalid Ship node" ErrorCode="YFS10048"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReqCancelDate" use="optional">
                <xsd:annotation yfc:columnName="REQ_CANCEL_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The requested cancel date. If not provided, this field is defaulted from the order header during order creation. Refer to the YFS_ORDER_HEADER table for more details.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReqDeliveryDate" use="optional">
                <xsd:annotation yfc:columnName="REQ_DELIVERY_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The requested delivery date. If not provided, this field is defaulted from the order header during order creation. Refer to the YFS_ORDER_HEADER table for more details.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReqShipDate" use="optional">
                <xsd:annotation yfc:columnName="REQ_SHIP_DATE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The requested ship date. If not provided, this field is defaulted from the order header during order creation. Refer to the YFS_ORDER_HEADER table for more details.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:TimeStamp"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReservationID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="RESERVATION_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The reservation ID. If this item was reserved before order creation, pass an ID to associate the line with the reservation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReservationMandatory" use="optional">
                <xsd:annotation yfc:columnName="RESERVATION_MANDATORY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						This flag indicates whether this line can be created without a reservation. If set to Y, creation of this line or any quantity addition to this line will be allowed only if the quantity can be reserved. If a ProcureFromNode is also present on the line, then the reservation will be carried out on the procure from node. This flag is not supported by bundle parent. It is ignored by bundle parent order lines. It is not propagated to bundle components.
						 </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="Y"/>
                        <xsd:enumeration value="N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReturnReason" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="RETURN_REASON" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The reason for returning the order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SCAC" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The carrier to be used for this line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndService" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>
                        <div>Value of SCAC and Service code.
							<br/>If <code>ScacAndService</code> or <code>ScacAndServiceKey</code> is passed, the values of <code>SCAC</code> and <code>CarrierServiceCode</code> are overriden based on the corresponding values in the <code>YFS_SCAC_AND_SERVICE</code> table.
							<Validation>
                                <Condition>
									Must be an existing ScacAndService code in the table <code>YFS_SCAC_AND_SERVICE</code>.
								</Condition>
                                <Error
                                    Description="YFS:Invalid SCAC/Service code combination" ErrorCode="YFS10240"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE_KEY" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Primary key attribute of YFS_SCAC_AND_SERVICE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Segment" type="yfctype:CommonCode" use="optional">
                <xsd:annotation yfc:columnName="SEGMENT" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Indicates Segment for the order line. For a Made To Customer order, the value is defaulted to <code>BuyerOrganizationCode</code>. For a Made To Order Order, the value is defaulted to <code>OrderLineKey</code>.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SegmentType" type="yfctype:ID-10" use="optional">
                <xsd:annotation yfc:columnName="SEGMENT_TYPE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Indicates SegmentType for the order line. For a Made to Customer Order, the value is <b>MTC</b>. For a Dynamic Physical Kit item which is Made To Order, SegmentType is <b>MTO</b>. For an MTC order, Inventory Tag passed at line level is used. Inventory Tags passed at schedule level are not used for creation of work orders.
						<br/>If passed as MTC, the following validations are done:
						<br/>a) Buyer Organization must have <code>Requires_Vas_Compliance</code> flag set to <b>Y</b>
                            <br/>b) Buyer Organization must have have Inbound Vas Compliance setup for the item passed in Order Line.
						<br/>c) The Service item selected must have Involves_Segment_Change flag set to <b>Y</b>.
						<br/>
                            <Validation>
                                <Condition>If <code>SegmentType</code> is <b>MTC</b>, Buyer Organization must have VAS Compliance setup as specified above.
							</Condition>
                                <Error
                                    Description="Cannot Pass SegmentType as MTC for a Buyer that does not require VAS Compliance" ErrorCode="OMP11031"/>
                            </Validation>
                            <br/>If passed as MTO, the following validations are done:
						<br/>a) The KitCode passed must be <b>DPK</b>.
						<br/>b) The kit components must be passed.
						<Validation>
                                <Condition>If <code>SegmentType</code> is <b>MTO</b>, the KitCode passed must be <b>DPK</b>
                                </Condition>
                                <Error Description="Invalid Kit Item" ErrorCode="YFS10399"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>SegmentType</code> is <b>MTO</b>, the kit components must be passed.
							</Condition>
                                <Error
                                    Description="YFS: No Kit Lines Found." ErrorCode="YFS10273"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SerialNo" type="yfctype:SerialNo" use="optional">
                <xsd:annotation yfc:columnName="SERIAL_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                        Requested serial number of the item.
                        <Validation>
                                <Condition>If <code>SerialNo</code> is provided and <code>OrderedQty</code> is not equal to one.
                              </Condition>
                                <Error
                                    Description="OrderedQty must be equal to one because SerialNo is provided." ErrorCode="OMP11024"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>SerialNo</code> is provided but <code>ShipNode</code> is not.
                            </Condition>
                                <Error
                                    Description="Ship node is mandatory because SerialNo is provided." ErrorCode="OMP11025"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
						Ship node from which the order line will be fulfilled.<br>
						Note: When calling this API for a return order, this ShipNode attribute should contain the shipnode_key associated with the "Return To" node (the node to which the returned item is being sent).</br>
                            <Validation>
                                <Condition>Must exist in table <code>YFS_SHIP_NODE</code>
                                </Condition>
                                <Error Description="Invalid ship node" ErrorCode="YFS10048"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>SerialNo</code> is provided, <code>ShipNode is mandatory</code>
                                </Condition>
                                <Error
                                    Description="Ship node is mandatory because SerialNo is provided." ErrorCode="OMP11025"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>ShipNode</code> is provided and <code>ReservationID</code> is provided to represent a capacity reservation for the service line and <code>ShipNode</code> is not the same as the node of the resource pool on the reservation.
							</Condition>
                                <Error
                                    Description="Node does not match with the node of the resource pool of the reservation." ErrorCode="OMP73_10001"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipToID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TO_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The ship to identifier.  If a customer definition representing the buyer organization exists within the system, the ShipToID can represent the CustomerID of the customer.  Otherwise, the ShipToID can represent the PersonID of the ship to address or the receiving node of the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipToKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TO_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The ship to address that is associated with this order line. If not provided, it is defaulted from order header.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipTogetherNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SHIP_TOGETHER_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="SUB_LINE_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The sub line number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Timezone" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TIMEZONE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
                            <br/>Times from the database are converted into the timezone specified by this field by the UI. Used with order lines with an <code>ItemGroupCode</code> of <b>
                                <code>PS</code>
                            </b> - <i>Provided Service</i> or <b>
                                <code>DS</code>
                            </b> - <i>Delivery Service</i>.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionalLineId"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
                            <code>TransactionalLineId</code> used to identify a line when creating associations of this line with other lines. This <code>TransactionalLineId</code> is not stored in the database and cannot be used after an order has been created. See <code>ProductServiceAssoc</code> entity for more information.
						<Validation>
                                <Condition>Must be unique for the <code>createOrder</code> transaction.
							</Condition>
                                <Error
                                    Description="Duplicate TransactionalLineId passed" ErrorCode="OMP10077"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ValidateItem" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						  Indicates if item needs to be validated against catalog. It overrides configuration rule for validating the item if passed non-blank. When set to "N", this order line will not be considered for min/max ordered quantity validation. Must be set to <code>Y</code> to apply entitlement logic.
						  <br/>Default: <b>
                                <code>Null</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Item">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>
                <div>
			 Contains information regarding the item for which order is created. Service items should be defined in the system. If ItemID is not passed, the catalog will be searched for the item which has the same alias as the AlternateItemID. The passed UOM will also be used to search for the item in the catalog. If an item is not found, the AlternateItemID will be used as the ItemID.
				 <Validation>
                        <Condition>If item needs to be validated (based on process type attribute or if ItemGroupCode is PS or DS) then it need to be defined in YFS_ITEM, or be accessible through the getItemDetails user exit.
				</Condition>
                        <Error
                            Description="Item (ItemId and UOM) does not exist" ErrorCode="YFS10437"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AlternateItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_ITEM_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates the alternate ItemID, which can be passed instead of the actual ItemID. This is an alias for the ItemID.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BundleFulfillmentMode" use="optional">
                <xsd:annotation yfc:columnName="BUNDLE_FULFILLMENT_MODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The way in which the bundle will be fulfilled. It can be Ship Independent (00), Ship Together (01), or Deliver Together (02) for Bundle Item. Default value is based on the catalog. If the item is not in the catalog, then Ship Together(01) is defaulted for Bundle Item.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Text-2">
                        <xsd:enumeration value=""/>
                        <xsd:enumeration value="00"/>
                        <xsd:enumeration value="01"/>
                        <xsd:enumeration value="02"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="CostCurrency" type="yfctype:Currency" use="optional">
                <xsd:annotation yfc:columnName="COST_CURRENCY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The currency in which the unit cost is specified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CountryOfOrigin" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY_OF_ORIGIN" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>
                        <div>
					The <code>Item</code>'s  country of origin/manufacture. We recommend usage of 2 byte ISO Country Codes.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ITEM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item ID for the buyer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerItemDesc"
                type="yfctype:Text-500" use="optional">
                <xsd:annotation
                    yfc:columnName="CUSTOMER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item description of the buyer organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ECCNNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ECCN_NO" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The Export Commodity Control Number for a commodity contained in the shipment, as indicated on the Waybill.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HarmonizedCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="HARMONIZED_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The harmonized code, if applicable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ISBN" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ISBN" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The ISBN number, if applicable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemDesc" type="yfctype:Text-500" use="optional">
                <xsd:annotation yfc:columnName="ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The identifier for the item. The ITEM_ID, UOM, and PRODUCT_CLASS form the unique inventory identifier for an order line. The ITEM_ID, UOM and ORGANIZATION_CODE form the unique item identifier for the Product Management module.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemShortDesc" type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="ITEM_SHORT_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The short description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The weight of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemWeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="ITEM_WEIGHT_UOM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit of measure used for weight.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItem"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item identifier for the manufacturer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItemDesc"
                type="yfctype:Text-254" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM_DESC" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item description for the manufacturer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerName"
                type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_NAME" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The name of the manufacturer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCClass" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="NMFC_CLASS" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The National Motor Freight Classification - A way of classifying goods for the purposes of transportation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="NMFC_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The National Motor Freight Classification - A way of classifying goods for the purposes of transportation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCDescription"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="NMFC_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The description of the NMFC. National Motor Freight Classification is a way of classifying goods for the purposes of transportation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductClass"
                type="yfctype:ProductClass" use="required">
                <xsd:annotation yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product class. This field is used as an inventory identifier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductLine" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_LINE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product line for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScheduleBCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCHEDULE_B_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The Schedule B commodity codes - A 10-digit numeric codes used to identify products that are exported to other countries.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItem" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_ITEM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item ID of the supplier (if this is a PO or the line is being drop-shipped and the supplier is known upfront).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierItemDesc"
                type="yfctype:ItemDesc" use="optional">
                <xsd:annotation
                    yfc:columnName="SUPPLIER_ITEM_DESCRIPTION" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The item description of the supplier (if this is a PO or line is being drop-shipped and the supplier is known upfront).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxProductCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TAX_PRODUCT_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product code classification for the purposes of taxation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UPCCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="UPC_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The UPC Code of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitCost" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_COST" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit cost of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="required">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The unit of measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentMethods">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:PaymentMethod"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentMethod">
        <xsd:annotation yfc:tableName="YFS_PAYMENT">
            <xsd:documentation>
                <div>
                    <code>PaymentMethod</code>
							can be added, modified or removed .
							<br/>
							If payment processing is not required for the order, any payment
							method passed in the input xml is still subject to validations.
							<br/>
							Only the
							<code>PaymentMethod</code>
							attributes specified in the input xml can be changed. Attempting
							to change other attributes
							on
							<code>PaymentMethod</code>
							, such as
							<code>TotalAuthorized</code>
							or
							<code>TotalCharged</code>
							results in an error.
							<Validation>
                        <Condition>Pass changed value for attributes not specfied in
									input xml.
				</Condition>
                        <Error
                            Description="Payment attributes cannot be modified." ErrorCode="YFS10458"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PersonInfoBillTo"/>
            </xsd:all>
            <xsd:attribute name="BillToKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BILL_TO_KEY" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The bill to address that is associated with this payment. Can be passed if different from the bill to address on the order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CashBackAmount" use="optional">
                <xsd:annotation yfc:columnName="CASH_BACK_AMOUNT" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The amount that is given back as cash for this payment method. This is not supported on a return order.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Money"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ChargeSequence"
                type="yfctype:ShorterNumber" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_SEQUENCE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>
                        <div>
								Used to identify the order to which charges are applied.
								<br/>
								The Seller Organization can configure the payment types so that
								they are charged and refunded in a particular sequence. The
								charge sequence and refund sequence are independent of each
								other. A
								<code>PaymentType</code>
								with a
								<code>ChargeSequence</code>
								set to
								<b>
                                <code>0</code>
                            </b>
								is charged before another
								<code>PaymentType</code>
								with
								<code>ChargeSequence</code>
								set to
								<b>
                                <code>1</code>
                            </b>
								.
								<br/>
								The Buyer can also specify the
								<code>ChargeSequence</code>
								of the
								<code>PaymentMethod</code>
								in the
								<code>createOrder</code>
								input xml. If a charge sequence is not specified by the Buyer,
								the Seller's
								<code>ChargeSequence</code>
								is followed.
								<br/>
                            <br/>
								If both the Buyer and the Seller supply
								<code>ChargeSequence</code>
								, charges are made in the order
								that the Seller specifies, but
								within a particular charge type, such
								as CreditCard, the Buyer's
								sequence is honored.
								<br/>
                            <br/>
								For example:
								<br/>
								A seller specifies the
								<code>ChargeSequence</code>
								as 0 for CUSTOMER_ACCOUNT, 1 for CREDIT_CARD, 2 for OTHER.
								<br/>
								A buyer specifies the
								<code>ChargeSequence</code>
								as 1 for MyBank-CreditCard, 2 for CUSTOMER_ACCOUNT, and 3 for
								MyOther-CreditCard.
								<br/>
								The payment methods are charged in the following sequence:
								<br/>
								CUSTOMER_ACCOUNT, MyBank-CreditCard, MyOther-CreditCard.
								<br/>
								The Customer account is charged because that is the seller's
								preferred method, and the two credit cards are charged based on
								the order that the buyer specified.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CheckNo" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="CHEQUE_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The cheque number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CheckReference" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="CHEQUE_REFERENCE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The cheque reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreditCardExpDate"
                type="yfctype:CreditCardExpDate" use="optional">
                <xsd:annotation yfc:columnName="CREDIT_CARD_EXP_DATE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The expiration date for the credit card.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreditCardName" type="yfctype:Text-300" use="optional">
                <xsd:annotation yfc:columnName="CREDIT_CARD_NAME" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The credit card name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreditCardNo" type="yfctype:Text-1000" use="optional">
                <xsd:annotation yfc:columnName="CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CREDIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreditCardType" type="yfctype:Text-32" use="optional">
                <xsd:annotation yfc:columnName="CREDIT_CARD_TYPE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The credit card type group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerAccountNo"
                type="yfctype:AccountNo" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ACCOUNT_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup CUSTOMER_ACCOUNT; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerPONo" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_PO_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The customer PO account number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DebitCardNo" type="yfctype:Text-1000" use="optional">
                <xsd:annotation yfc:columnName="DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup DEBIT_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplayCreditCardNo"
                type="yfctype:Text-4" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_CREDIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The four digits of the credit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the credit card number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplayCustomerAccountNo"
                type="yfctype:Text-4" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_CUST_ACCT_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The four digits of the customer account number that should be displayed.  If not passed, it is defaulted to be the last four digits of the customer account number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplayDebitCardNo"
                type="yfctype:Text-4" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_DEBIT_CARD_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The four digits of the debit card number that should be displayed.  If not passed, it is defaulted to be the last four digits of the debit card number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplayPaymentReference1"
                type="yfctype:Text-4" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_PAYMENT_REF1" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The four digits of the payment reference 1 that should be displayed.  If not passed, it is defaulted to be the last four digits of the payment reference 1.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplaySvcNo" type="yfctype:Text-4" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_SVC_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The four digits of the stored value card that should be displayed.  If not passed, it is defaulted to be the last four digits of the stored value card number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EntryType" type="yfctype:Text-100" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The Entry Type used to enter payment information into the
								system.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:Text-300" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The first name of the credit card holder.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastName" type="yfctype:Text-300" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The last name of the credit card holder.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:Text-300" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The middle name of the credit card holder.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_KEY"
                    yfc:isConditional="Y" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The primary key for the YFS_PAYMENT table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference1"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE1" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 1. Additionally, this is the primary account number for payment methods that are defined under the PaymentTypeGroup OTHER; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference2"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE2" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 2.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference3"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE3" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 3.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference4"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE4" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 4. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference5"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE5" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 5. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference6"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE6" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 6. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference7"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE7" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 7. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference8"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE8" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 8. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentReference9"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_REFERENCE9" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The payment reference 9. This does not identify the payment method uniquely.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentType" type="yfctype:Text-20" use="required">
                <xsd:annotation yfc:columnName="PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>
                        <div>
								Type of payment.
								<br/>
								The
								<code>PaymentType</code>
								must be a value that is already configured for the Seller
								Organization.
								<br/>
								If a
								<code>PaymentMethod</code>
								element is passed, the
								<code>PaymentType</code>
								is validated, even if payment processing is not required for the
								order.
								<Validation>
                                <Condition>Must be a value defined for the Seller Organization.
					</Condition>
                                <Error
                                    Description="Payment Type not set up for seller." ErrorCode="YFS10491"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>PaymentType</code>
										must be passed.
									</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PlannedRefundAmount" use="optional">
                <xsd:annotation yfc:columnName="PLANNED_REFUND_AMOUNT" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The amount that will be refunded on this payment method. This field is set for only Return Orders indicating the refund amount.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Money"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ReasonCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="REASON_CODE" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>Reason code for last update.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SuspendExpiredAuthReversal"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="SUSPEND_EXP_AUTH_REVERSAL" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>This flag indicates whether or not the reversal of expired authorization has to be initiated for a payment method that has been suspended. If the value is set to Y, then the authorization reversal are not created even though Authorization Reversal Strategy is configured for the payment type. Please note the flag comes into action only when SuspendAnyMoreCharges is either set to Y or B. Valid Values are N or Y (Null value behaves similar to setting it to N ).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SvcNo" type="yfctype:Text-1000" use="optional">
                <xsd:annotation yfc:columnName="SVC_NO" yfc:tableName="YFS_PAYMENT">
                    <xsd:documentation>The primary account number for payment methods that are defined under the PaymentTypeGroup STORED_VALUE_CARD; payment methods of this group will be considered incomplete if this is not passed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PersonInfoBillTo">
        <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
            <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AddressID" type="yfctype:Text-256" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>A user specified label for an address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AlternateEmailID"
                type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="City" type="yfctype:City" use="optional">
                <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The city in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The country in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The first name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsAddressVerified" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_ADDRESS_VERIFIED" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This flag indicates whether or not the address in this record has been verified by an address verification program. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsCommercialAddress"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="JobTitle" type="yfctype:Title-40" use="optional">
                <xsd:annotation yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The job title of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The last name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Latitude" use="optional">
                <xsd:annotation yfc:columnName="LATITUDE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The latitude of this location.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Longitude" use="optional">
                <xsd:annotation yfc:columnName="LONGITUDE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The longitude of this location.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonInfoKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PERSON_INFO_KEY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary key for the YFS_PERSON_INFO table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="State" type="yfctype:State" use="optional">
                <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The state in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                <xsd:annotation yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderSearchConditions">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrderSearchCondition"/>
            </xsd:sequence>
            <xsd:attribute name="Reset" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Determines whether the <code>OrderSearchCondition</code>s should be reset.
   					</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="yfctype:Flag">
                        <xsd:enumeration value="N"/>
                        <xsd:enumeration value="Y"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderSearchCondition">
        <xsd:annotation yfc:tableName="YFS_ORDER_SEARCH_CONDITION">
            <xsd:documentation>
                <div>
                    <Validation>
                        <Condition>Must pass one of the following combinations:
						<br/>the <code>OrderNo</code>, <code>EnterpriseCode</code> and  the <code>DocumentType</code>
                            <br/>
                            <i>or</i>
                            <code>EntityAttributeName</code>
                            <i>or</i> the <code>EntityAttributeValue</code>.
						</Condition>
                        <Error
                            Description="Order Search Condition parameters missing" ErrorCode="OMP921_0102"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE" yfc:tableName="YFS_ORDER_SEARCH_CONDITION">
                    <xsd:documentation>Document type of the order to search for.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_ORDER_SEARCH_CONDITION">
                    <xsd:documentation>Enterprise code of the order to search for.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EntityAttributeName"
                type="yfctype:Text-400" use="optional">
                <xsd:annotation yfc:columnName="ENTITY_ATTR_NAME" yfc:tableName="YFS_ORDER_SEARCH_CONDITION">
                    <xsd:documentation>The name of the custom attribute or extended field of the order header entity. If none are found on the order entity, this will be ignored.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EntityAttributeValue"
                type="yfctype:Text-400" use="optional">
                <xsd:annotation yfc:columnName="ENTITY_ATTR_VALUE" yfc:tableName="YFS_ORDER_SEARCH_CONDITION">
                    <xsd:documentation>The value of the custom attribute or extended field of the order header entity. If none are found on the order entity, this will be ignored.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderNo"
                type="yfctype:VarcharDocumentNo" use="optional">
                <xsd:annotation yfc:columnName="ORDER_NO" yfc:tableName="YFS_ORDER_SEARCH_CONDITION">
                    <xsd:documentation>The order number to search for.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderSearchConditionKey"
