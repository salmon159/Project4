    <xsd:element name="CustomerSlotPreference">
        <xsd:annotation yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
            <xsd:documentation>
                <div>
					Identify the preferred day of week and time slot that the customer would like to consider for appointment planning.
						<Validation>
                        <Condition>
                            <code>StartTime</code> or <code>EndTime</code> is not passed.
								</Condition>
                        <Error
                            Description="StartTime and EndTime must be passed." ErrorCode="YCP73_00024"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="EndTime" type="yfctype:Time" use="optional">
                <xsd:annotation yfc:columnName="END_TIME" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The end time of the preferred slot.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FridayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="FRIDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Friday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MondayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="MONDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Monday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SaturdayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SATURDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Saturday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StartTime" type="yfctype:Time" use="optional">
                <xsd:annotation yfc:columnName="START_TIME" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The start time of the preferred slot.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SundayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SUNDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Sunday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ThursdayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="THURSDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Thursday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TuesdayPreferred" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TUESDAY_PREFERRED" yfc:tableName="YFS_CUSTOMER_SLOT_PREFERENCE">
                    <xsd:documentation>The flag indicates that the specified time slot is preferred on Tuesday.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
