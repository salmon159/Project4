    <xsd:element name="CustomerSlotPreferenceList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:CustomerSlotPreference"/>
            </xsd:sequence>
        </xsd:complexType>
