<!--getTransactionSummary Input XML-->
<getTransactionSummary RollingCount="" StartDate="" Year="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getTransactionSummary/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="RollingCount">
                <Description>
                    <div>
					Flag to indicate if rolling count is to be done.System will do rolling count only when both StartDate is passed and value of RollingCount is Y.
					Rolling count will not be applicable for YTD calculations. 
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Date" Name="StartDate">
                <Description>
                    <div>
					Date to indicate year and month to start rolling count.For Startdate to get considered RollingCount must be set to Y.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="Year">
                <Description>Valid year in YYYY format.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</getTransactionSummary>
