<?xml version="1.0" encoding="UTF-8"?>
<Wave Node="Conditional" WaveKey="Conditional" WaveNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/WMS/assignLane/input">
    <yfc:doc>
        <Element EntityName="Wave">
            <Description>
                <div>
                    <Validation>
                        <Condition>
                            <code>WaveKey</code> attribute OR the <code>WaveNo</code> and <code>Node</code> attributes must be passed and must be valid. 
					</Condition>
                        <Error
                            Description="Either WaveKey or WaveNo and Node must be passed." ErrorCode="WMS00156"/>
                    </Validation>
                </div>
            </Description>
        </Element>
    </yfc:doc>
</Wave>
