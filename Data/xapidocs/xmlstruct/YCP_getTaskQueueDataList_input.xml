<GetTaskQueueDataInput InstanceNumber="" LastTaskQueueKey=""
    MaxInstances="" MaximumRecords="Required" TransactionId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getTaskQueueDataList/input">
    <yfc:doc>
        <Element EntityName="Transaction">
            <Description>When multiple instances of a transaction are processing tasks from the task queue, these instances can call this API, passing the instance number and the total number of running instances that can work on their data segment.</Description>
        </Element>
        <Attributes>
            <Attribute DataType="Count" Name="InstanceNumber">
                <Description>Requesting instance number</Description>
            </Attribute>
            <Attribute EntityName="Task_Queue" Name="LastTaskQueueKey" NameInEntity="Task_Q_Key">
                <Description>To fetch beyond a particular task</Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaxInstances">
                <Description>Total number of instances running</Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>To return at the most this many records</Description>
            </Attribute>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid"/>
        </Attributes>
    </yfc:doc>
</GetTaskQueueDataInput>
