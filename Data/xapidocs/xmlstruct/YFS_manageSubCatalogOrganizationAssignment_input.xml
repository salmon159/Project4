<SubCatalogOrganizationAssignmentList xmlns="http://www.sterlingcommerce.com/documentation/YFS/manageSubCatalogOrganizationAssignment/input">
    <SubCatalogOrganizationAssignment Operation=""
        OrganizationCode="Required"
        SubCatalogOrganizationCode="Required" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
                        <Validation>
                            <Condition>
						Both <code>OrganizationCode</code> and <code>SubCatalogOrganizationCode</code> must be provided.
						</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                        </Validation>
                        <Validation>
                            <Condition>
						The organization, specified as <code>SubCatalogOrganizationCode</code> does not have its catalog organization set to the organization that is specified in the <code>OrganizationCode</code> field
						</Condition>
                            <Error
                                Description="Invalid catalog organization" ErrorCode="YCM83_013"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="ID-40" Name="Operation">
                    <Description>
                        <div>
					The action that is to be performed during the invocation of this API.
					<br/>Valid values:  <b>
                                <code>Create</code>
                            </b>, <b>
                                <code>Modify</code>
                            </b>, or 	<b>
                                <code>Delete</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </SubCatalogOrganizationAssignment>
</SubCatalogOrganizationAssignmentList>
