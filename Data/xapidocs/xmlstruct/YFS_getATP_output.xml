<!--getATP Output XML-->
<InventoryInformation xmlns="http://www.sterlingcommerce.com/documentation/YFS/getATP/output">
    <Item AdvanceNotificationTime="" AvailableToSell=""
        CalculateProjectedOnhandQty="" ConsiderAllNodes=""
        ConsiderAllSegments="" ConsiderInventoryNodeControl=""
        ConsiderUnassignedDemand="" Description="" DistributionRuleId=""
        EndDate="" ExtendedDescription="" InventoryOrganization=""
        ItemID="" LeadTime="" OrganizationCode="" PeriodicalLength=""
        ProcessingTime="" ProductClass="" Segment="" SegmentType=""
        ShipNode="" ShortDescription="" TagControl="" TimeSensitive=""
        TotalExpiredSupply="" TrackedEverywhere="" UnitOfMeasure=""
        yfc:DTDOccurrence="REQUIRED" yfc:IgnoreOrdering="Y">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="NumberOfDays" Name="AdvanceNotificationTime"/>
                <Attribute DataType="Quantity" Name="AvailableToSell">
                    <Description>
                        <div>
					The quantity available for selling.
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="CalculateProjectedOnhandQty">
                    <Description>
                        <div>
						Result was calculated with <code>CalculateProjectedOnhandQty</code> flag from the input.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="ConsiderAllNodes">
                    <Description>
                        <div>
						Result was calculated with <code>ConsiderAllNodes</code> flag from the input.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="ConsiderAllSegments">
                    <Description>Y/N   Default is 'N'.   If set to 'Y', SegmentType and Segment will be ignored.  </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="ConsiderInventoryNodeControl">
                    <Description>
                        <div>
                    Indicates whether inventory node control data were used in the inventory reporting.
                    </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="ConsiderUnassignedDemand">
                    <Description>
                        <div>
						Result was calculated with <code>ConsiderUnassignedDemand</code> flag from the input.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="DistributionRuleId">
                    <Description>
                        <div>
				If passed, supplies and demands are read applying the distribution rule filter identified by this <code>DistributionRuleID</code>.
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Date" Name="EndDate">
                    <Description>
                        <div>
                            <code>EndDate</code> specifies the latest date for which availability information is required.
				</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Item"
                    Name="InventoryOrganization" NameInEntity="InventoryOrganizationCode"/>
                <Attribute EntityName="Inventory_Item" Name="ItemID"/>
                <Attribute DataType="OrgCode" Name="OrganizationCode"/>
                <Attribute DataType="Text-1" Name="PeriodicalLength">
                    <Description>
                        <div>Indicates how availability for the required quantity should be returned. 
				<br/>Valid values:
				 <br/>
                            <b>
                                <code>0</code>
                            </b> -  daily, whenever there is any supply
				 <br/>
                            <b>
                                <code>1</code>
                            </b> - weekly, supply for the week is consolidated 
				 <br/>
                            <b>
                                <code>2</code>
                            </b> - monthly, supply for the whole month is consolidated
				</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Atp_Rules" Name="ProcessingTime"/>
                <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
                <Attribute DataType="CommonCode" Name="Segment">
                    <Description>
                        <div>
					Segment of inventory. Supplies and Demands with matching <code>Segment</code> will be considered for availability calculations.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="SegmentType" Name="SegmentType">
                    <Description>
                        <div>
					Segment type of inventory.   Supplies and Demands with matching <code>SegmentType</code> will be considered for availability calculations.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="ShipNode" Name="ShipNode">
                    <Description>
                        <div>
				Returns availability information for the <code>ShipNode</code> specified. If no <code>ShipNode</code> is passed,
				returns availability for all <code>ShipNodes</code> in this organization.
				</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Item" Name="TagControl" NameInEntity="Tag_Control_Flag"/>
                <Attribute DataType="Quantity" Name="TotalExpiredSupply">
                    <Description>
                        <div>
				Total expired quantity across all ship nodes where the item searched for is available. Only makes sense if the item is time sensitive.
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="TrackedEverywhere">
                    <Description>
                        <div>
					Indicate if this item's supply is tracked. Values: <code>Y</code> - Item is tracked, <code>N</code> - Item has infinite supply.
				</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
            </Attributes>
        </yfc:doc>
        <InventoryTotals yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
			Shows all existing demands and supplies for the item.
			</div>
                    </Description>
                </Element>
            </yfc:doc>
            <Supplies TotalSupply="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
						Shows all inventory supplies.
					</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="TotalSupply">
                            <Description>
                                <div>
								Total inventory supply quantity.
								</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Supply OrganizationCode="" Quantity="" SupplyType="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
						Contains inventory supply information.
						</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="OrgCode" Name="OrganizationCode">
                                <Description>
                                    <div>
								Inventory supply Organization Code.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="Quantity">
                                <Description>
                                    <div>
								Inventory supply quantity.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Text-40" Name="SupplyType">
                                <Description>
                                    <div>
								Inventory supply type.
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <InventoryItem ItemID="" UnitOfMeasure=""/>
                </Supply>
            </Supplies>
            <Demands TotalDemand="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="TotalDemand"/>
                    </Attributes>
                </yfc:doc>
                <Demand DemandType="" OrganizationCode="" Quantity="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
									Contains inventory demand information.
								</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Text-40" Name="DemandType">
                                <Description>
                                    <div>
									Inventory demand type.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="OrgCode" Name="OrganizationCode">
                                <Description>
                                    <div>
								Inventory Demand Organization Code.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="Quantity">
                                <Description>
                                    <div>
									Inventory demand quantity.
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <InventoryItem ItemID="" UnitOfMeasure=""/>
                </Demand>
            </Demands>
        </InventoryTotals>
        <CurrentInventory yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
			Shows all existing demands and supplies for the item.
			</div>
                    </Description>
                </Element>
            </yfc:doc>
            <Supplies TotalSupply="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="TotalSupply"/>
                    </Attributes>
                </yfc:doc>
                <Supply OrganizationCode="" Quantity="" SupplyType="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="OrgCode" Name="OrganizationCode">
                                <Description>
                                    <div>
									Inventory supply Organization Code.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="Quantity"/>
                            <Attribute DataType="Text-40" Name="SupplyType"/>
                        </Attributes>
                    </yfc:doc>
                    <InventoryItem ItemID=""/>
                </Supply>
            </Supplies>
            <Demands TotalDemand="" TotalOrders="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
						Shows all existing demands.
						</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="TotalDemand">
                            <Description>
                                <div>
								Total demand quantity.
							</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="TotalOrders">
                            <Description>
                                <div>
								Total number of Orders.
							</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Demand DemandType="" OrganizationCode="" Quantity="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Text-40" Name="DemandType"/>
                            <Attribute DataType="OrgCode" Name="OrganizationCode"/>
                            <Attribute DataType="Quantity" Name="Quantity"/>
                        </Attributes>
                    </yfc:doc>
                    <InventoryItem ItemID="" UnitOfMeasure=""/>
                </Demand>
            </Demands>
        </CurrentInventory>
        <AvailableToPromiseInventory AccumulatedShortage="" Available=""
            Demand="" EndDate="" FromDate="" ProjectedOnhandQty=""
            Shortage="" Supply="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
				Shows incoming demands and supplies for the item within the ATP horizon.
				</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Quantity" Name="AccumulatedShortage">
                        <Description>
                            <div>
						AccumulatedShortage represents the accumulated shortages within the period between the FromDate and EndDate. It is calculated as the sum of the shortage demands (with ship date earlier than or on the end date of the period).
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="Available">
                        <Description>
                            <div>
						Available quantity.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="Demand">
                        <Description>
                            <div>
						Inventory demand quantity.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="EndDate">
                        <Description>
                            <div>
						The end date of the available to promise inventory picture timeframe.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="FromDate">
                        <Description>
                            <div>
						The start date of the available to promise inventory picture timeframe.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="QuantityPositiveAndNegative" Name="ProjectedOnhandQty">
                        <Description>
                            <div>
						ProjectedOnhandQty represents the projected on-hand quantity within the period between the FromDate and EndDate. The ProjectedOnhandQty is calculated as the sum of available quantity of all supplies (within the period). Where the available quantity of each supply is calculated as the supply quantity minus the matched demand quantity of demands earlier than or on the end of the period. Available quantity also excludes demands earlier than start date of the period.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="Shortage">
                        <Description>
                            <div>
						Inventory shortage quantity.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="Supply">
                        <Description>
                            <div>
						Inventory supply quantity.
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Supplies yfc:DTDOccurrence="REQUIRED">
                <Supply OrganizationCode="" Quantity="" SupplyType="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="OrgCode" Name="OrganizationCode"/>
                            <Attribute DataType="Quantity" Name="Quantity"/>
                            <Attribute DataType="Text-40" Name="SupplyType"/>
                        </Attributes>
                    </yfc:doc>
                    <InventoryItem ItemID="" UnitOfMeasure=""/>
                </Supply>
            </Supplies>
            <Demands yfc:DTDOccurrence="REQUIRED">
                <Demand DemandType="" OrganizationCode="" Quantity="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Text-40" Name="DemandType"/>
                            <Attribute DataType="OrgCode" Name="OrganizationCode"/>
                            <Attribute DataType="Quantity" Name="Quantity"/>
                        </Attributes>
                    </yfc:doc>
                    <InventoryItem ItemID="" UnitOfMeasure=""/>
                </Demand>
            </Demands>
            <Shortages yfc:DTDOccurrence="REQUIRED">
                <Shortage DemandType="" Quantity="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>Contains inventory shortage information. 
								</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Text-40" Name="DemandType">
                                <Description>
                                    <div>Inventory demand type.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="Quantity">
                                <Description>
                                    <div>Inventory shortage quantity 
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <InventoryItem ItemID="" UnitOfMeasure=""/>
                </Shortage>
            </Shortages>
            <Availability yfc:DTDOccurrence="REQUIRED">
                <Available Quantity="" SupplyType="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>Available inventory supply.
						</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="Quantity">
                                <Description>
                                    <div>Available quantity.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Text-40" Name="SupplyType">
                                <Description>
                                    <div>Available inventory supply type.
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <InventoryItem ItemID="" UnitOfMeasure=""/>
                </Available>
            </Availability>
        </AvailableToPromiseInventory>
        <InventoryCalculator RequiredQty="">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>If <code>RequiredQty</code> is passed as input, indicates when the <code>RequiredQty</code> can be fulfilled.
			</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Quantity" Name="RequiredQty">
                        <Description>
                            <div>
							Required quantity that needs be fulfilled.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Inventory AvailableQuantity="" Date="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
						Available inventory.
						</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="AvailableQuantity">
                            <Description>
                                <div>
							Available quantity.
							</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Date" Name="Date">
                            <Description>
                                <div>
							Available date.
							</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Inventory>
        </InventoryCalculator>
        <LanguageDescriptionList>
            <LanguageDescription Description="" ExtendedDescription=""
                LocaleCode="" ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
        </LanguageDescriptionList>
        <ItemFilter GetUnpublishedItems="N|Y" IgnoreInvalidItems="N|Y">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="YFS_getATP_input.xml" IncludeXMLPath="/GetATP/ItemFilter"/>
            <yfc:doc>
                <Element EntityName="Item">
                    <Description>Determines the filtering attributes when determining availability of a model item.
				This can be used to specify the correct style, size, or color of a model item and only include
				model children inventory whose item definition meet the required input.
				<br/>Note: Only extended attributes of the YFS_ITEM table are supported.</Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Flag" Name="GetUnpublishedItems">
                        <Description>Indicates if unpublished items needs to be considered.</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IgnoreInvalidItems">
                        <Description>
                            <div>
					This flag indicates whether invalid items should be considered.
					Valid values: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>
					Default value: <b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <PrimaryInformation ColorCode="" ItemType=""
                ManufacturerItem="" ManufacturerName="" ProductLine="" SizeCode="">
                <yfc:doc>
                    <Element EntityName="Item"/>
                </yfc:doc>
            </PrimaryInformation>
            <ClassificationCodes CommodityCode="" ECCNNo=""
                HarmonizedCode="" NAICSCode="" NMFCCode=""
                StorageType="" TaxProductCode="" UNSPSC="" VelocityCode="">
                <yfc:doc>
                    <Element EntityName="Item"/>
                </yfc:doc>
            </ClassificationCodes>
        </ItemFilter>
        <ShipNode Description="" IdentifiedByParentAs="" OwnerKey="">
            <yfc:includeInParent IncludeXMLChildNodes="N" IncludeXMLFileName="YFS_Ship_Node_Element_master.xml"/>
            <yfc:doc>
                <Element EntityName="Ship_Node"/>
            </yfc:doc>
        </ShipNode>
    </Item>
</InventoryInformation>
