<?xml version="1.0" encoding="UTF-8"?>
<BarCode BarCodeData="Required" BarCodeType="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/translateBarCode/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="BarCodeData">
                <Description> BarCode data translated</Description>
            </Attribute>
            <Attribute EntityName="BarCode_Translation"
                Name="BarCodeType" NameInEntity="BarCode_Type"/>
        </Attributes>
    </yfc:doc>
    <Translations BarCodeTranslationSource=""
        TotalNumberOfRecords="Required" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="BarCodeTranslationSource">
                    <Description>Indicates the first source that could translate the barcode successfully.</Description>
                </Attribute>
                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                    <Description>Total number of records </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Translation yfc:DTDOccurrence="ZeroOrMany">
            <ItemContextualInfo AlternateUOM="" ContainerQuantity=""
                InventoryUOM="" IsSerialTracked="" ItemID="" KitCode=""
                ProductClass="" Quantity="">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Item_UOM"
                            Name="AlternateUOM" NameInEntity="Uom"/>
                        <Attribute DataType="Quantity" Name="ContainerQuantity">
                            <Description>
                            This attribute represents total quantity of the item present in the container, if the container has only one kind of item.
                        </Description>
                        </Attribute>
                        <Attribute EntityName="Item" Name="InventoryUOM" NameInEntity="Uom"/>
                        <Attribute EntityName="Item"
                            Name="IsSerialTracked" NameInEntity="Is_Serial_Tracked"/>
                        <Attribute EntityName="Item" Name="ItemID" NameInEntity="Item_Id"/>
                        <Attribute EntityName="Item" Name="KitCode" NameInEntity="Kit_Code"/>
                        <Attribute EntityName="Common_Code"
                            Name="ProductClass" NameInEntity="Code_Value"/>
                        <Attribute EntityName="Item_UOM" Name="Quantity" NameInEntity="Quantity"/>
                    </Attributes>
                </yfc:doc>
                <Inventory CountryOfOrigin="" FifoNo=""
                    InventoryStatus="" Segment="" SegmentType="" ShipByDate="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute EntityName="Location_Inventory"
                                Name="CountryOfOrigin" NameInEntity="Country_Of_Origin">
                                <Description>
                                    <div>
									Country in which the item was manufactured. If passed, then, only the inventory with matching <code>CountryOfOrigin</code> is returned.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Location_Inventory"
                                Name="FifoNo" NameInEntity="Fifo_No">
                                <Description>
                                    <div>
									FIFO number is a date based inventory attribute that helps understand the order in which stock arrived at the node. This is used to send out items that arrived first than the ones that arrived later. If passed, then, only inventory satisfying the <code>FifoNo</code> is returned.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Location_Inventory"
                                Name="InventoryStatus" NameInEntity="Inventory_Status">
                                <Description>
                                    <div>
									Unique key of <code>YFS_INVENTORY_STATUS</code> table. Refers to an inventory sub classification based on the results of the inventory control processes within the warehouse. If passed, then, only inventory with matching <code>InventoryStatus</code> is returned.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Location_Inventory"
                                Name="Segment" NameInEntity="Segment">
                                <Description>
                                    <div>
									When inventory is customized for a specific order, it needs to be tracked separately so that it can be allocated to that order. If passed, then, only inventory belonging to the <code>Segment</code> is returned.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Location_Inventory"
                                Name="SegmentType" NameInEntity="Segment_Type">
                                <Description>
                                    <div>
									When inventory is customized, it needs to be tracked separately. This field indicates the buyer or buyer category for who the inventory is customized. If passed, then, only inventory belonging to the <code>SegmentType</code> is returned.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Location_Inventory"
                                Name="ShipByDate" NameInEntity="Ship_By_Date">
                                <Description>
                                    <div>
									Date by which the item must be shipped so that it is usable. This is also referred to as the expiration date. If passed, then, only inventory satisfying the <code>ShipByDate</code> is returned.
									</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <TagDetail BatchNo="" LotAttribute1=""
                        LotAttribute2="" LotAttribute3=""
                        LotKeyReference="" LotNumber=""
                        ManufacturingDate="" RevisionNo="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:includeInParent IncludeXMLChildNodes="N"
                            IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/TagDetail"/>
                        <yfc:doc>
                            <Element EntityName="Inventory_Tag"/>
                        </yfc:doc>
                        <Extn/>
                    </TagDetail>
                    <SerialDetail SecondarySerial1=""
                        SecondarySerial2="" SecondarySerial3=""
                        SecondarySerial4="" SecondarySerial5=""
                        SecondarySerial6="" SecondarySerial7=""
                        SecondarySerial8="" SecondarySerial9=""
                        SerialNo="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:includeInParent IncludeXMLChildNodes="N"
                            IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/SerialList/SerialDetail"/>
                        <yfc:doc>
                            <Element EntityName="Global_Serial_Num"/>
                        </yfc:doc>
                    </SerialDetail>
                    <Receipt ReceiptNo=""/>
                </Inventory>
                <SIMSerialDetail/>
            </ItemContextualInfo>
            <ContextualInfo BuyerOrganizationCode="" EnterpriseCode=""
                Node="" OrganizationCode="Required" SellerOrganizationCode="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="BuyerOrganizationCode">
                            <Description> Identifies the buying organization </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="EnterpriseCode">
                            <Description>Enterprise Code (broker/mediator) </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="Node">
                            <Description> Identifies the Ship Node</Description>
                        </Attribute>
                        <Attribute DataType="String" Name="OrganizationCode">
                            <Description> Contextual OrganizationCode </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="SellerOrganizationCode">
                            <Description> Identifies the selling organization </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ContextualInfo>
            <ShipmentContextualInfo IsProductPlacingComplete=""
                SellerOrganizationCode="" ShipNode="" ShipmentKey=""
                ShipmentLineKey="" ShipmentNo="">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Shipment"
                            Name="IsProductPlacingComplete" NameInEntity="Is_Product_Placing_Complete"/>
                        <Attribute EntityName="Shipment"
                            Name="SellerOrganizationCode" NameInEntity="Seller_Organization_Code"/>
                        <Attribute EntityName="Shipment" Name="ShipNode" NameInEntity="Shipnode_key"/>
                        <Attribute EntityName="Shipment"
                            Name="ShipmentKey" NameInEntity="Shipment_Key"/>
                        <Attribute DataType="String"
                            EntityName="Shipment" Name="ShipmentLineKey" NameInEntity="Shipment_Line_Key"/>
                        <Attribute EntityName="Shipment"
                            Name="ShipmentNo" NameInEntity="Shipment_No"/>
                    </Attributes>
                </yfc:doc>
            </ShipmentContextualInfo>
            <OrderContextualInfo DocumentType="" OrderHeaderKey=""
                OrderLineKey="" OrderNo="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="DocumentType" NameInEntity="Document_Type"/>
                        <Attribute DataType="String"
                            Name="OrderHeaderKey" NameInEntity="Order_Header_Key"/>
                        <Attribute DataType="String" Name="OrderLineKey" NameInEntity="Order_Line_Key"/>
                        <Attribute DataType="String" Name="OrderNo" NameInEntity="Order_No"/>
                    </Attributes>
                </yfc:doc>
            </OrderContextualInfo>
            <WorkOrderContextualInfo EligibleActivityCode="" NodeKey=""
                WorkOrderKey="" WorkOrderNo="">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Work_Order_Activity"
                            Name="EligibleActivityCode" NameInEntity="Activity_Code">
                            <Description>The next eligible activity that can be performed.</Description>
                        </Attribute>
                    </Attributes>
                    <Element EntityName="Work_Order"/>
                </yfc:doc>
            </WorkOrderContextualInfo>
            <BatchContextualInfo BatchKey="" BatchNo="" TaskType="">
                <yfc:doc>
                    <Element EntityName="Batch"/>
                </yfc:doc>
            </BatchContextualInfo>
            <ActivityContextualInfo ActivityCode="" ActivityGroupId="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="ActivityCode">
                            <Description>Activity code to which translation belongs to</Description>
                        </Attribute>
                        <Attribute DataType="String" Name="ActivityGroupId">
                            <Description>Activity group to which translation belongs to</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ActivityContextualInfo>
            <ContainerContextualInfo CaseId="" ContainerNo=""
                IsOutboundContainer="" LoadKey="" PalletId=""
                ShipmentContainerKey="" ShipmentKey="" TrackingNo="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="LPNNo" Name="CaseId">
                            <Description>A CaseId</Description>
                        </Attribute>
                        <Attribute EntityName="Shipment_Container"
                            Name="ContainerNo" NameInEntity="Container_No"/>
                        <Attribute EntityName="LPN_Hdr"
                            Name="IsOutboundContainer" NameInEntity="Is_Outbound_Container"/>
                        <Attribute EntityName="Shipment_Container"
                            Name="LoadKey" NameInEntity="Load_Key"/>
                        <Attribute DataType="LPNNo" Name="PalletId">
                            <Description>A PalletId</Description>
                        </Attribute>
                        <Attribute EntityName="Shipment_Container"
                            Name="ShipmentContainerKey" NameInEntity="Shipment_Container_Key"/>
                        <Attribute EntityName="Shipment_Container"
                            Name="ShipmentKey" NameInEntity="Shipment_Key"/>
                        <Attribute EntityName="Shipment_Container"
                            Name="TrackingNo" NameInEntity="Tracking_No"/>
                    </Attributes>
                </yfc:doc>
            </ContainerContextualInfo>
            <LocationContextualInfo LocationId="" ZoneId="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="LocationId" Name="LocationId">
                            <Description> Location Identifier</Description>
                        </Attribute>
                        <Attribute DataType="ZoneId" Name="ZoneId">
                            <Description> Zone Identifier</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </LocationContextualInfo>
            <ToteContextualInfo IsToteInducted="" Node=""
                ReferenceKey="" ReferenceName="" ReferenceValue="" ToteId="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="IsToteInducted">
                            <Description>This flag specifies whether the tote is already inducted with another reference or not. If all the tasks associated with the tote are completed then the tote can be re-used and the flag will be false</Description>
                        </Attribute>
                        <Attribute DataType="Key" Name="Node">
                            <Description> Node Identifier</Description>
                        </Attribute>
                        <Attribute DataType="Key" Name="ReferenceKey">
                            <Description> Key which identifies the reference associated with the Tote.</Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ReferenceName">
                            <Description> Name of Reference associated with the Tote.</Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ReferenceValue">
                            <Description> Value of Reference associated with the Tote.</Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ToteId">
                            <Description> Tote Identifier</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ToteContextualInfo>
            <EquipmentContextualInfo EquipmentId="" HasOpenTasks=""
                IsMultiLocation="" Node="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Text-40" Name="EquipmentId">
                            <Description> Equipment Identifier</Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="HasOpenTasks">
                            <Description>This flag indicates whether there are any open tasks pending for the equipment.</Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsMultiLocation">
                            <Description> This flag indicates whether the equipment passed is identified by a single physical location. If equipment is modelled as single location or multiple slot location with all logical locations having same physical location then this flag will be false else it will be true.</Description>
                        </Attribute>
                        <Attribute DataType="Key" Name="Node">
                            <Description> Node Identifier</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </EquipmentContextualInfo>
            <ZoneContextualInfo LocationId="" Node="" ZoneId="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="LocationId"/>
                        <Attribute DataType="Key" Name="Node">
                            <Description> Node Identifier</Description>
                        </Attribute>
                        <Attribute DataType="ZoneId" Name="ZoneId">
                            <Description> Zone Identifier</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ZoneContextualInfo>
            <SIMLocationContextualInfo/>
        </Translation>
    </Translations>
</BarCode>
