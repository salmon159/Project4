<?xml version="1.0" encoding="UTF-8"?>
<ActivityGroupInfo ActivityGroup="" ActivityGroupId=""
    CanActivityBePerformed="Required" EnterpriseCode="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getCanActivitybePerformed/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="ActivityGroup"/>
            <Attribute DataType="String" Name="ActivityGroupId">
                <Description>
                    <div>
						The API is called to find out if any activity belonging to this activity group can be performed or not.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="CanActivityBePerformed">
                <Description>
                    <div>
						This attribute states whether the activity passed in the input can be performed or not.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Enterprise" Name="EnterpriseCode">
                <Description>
                    <div>
						Enterprise passed in input.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</ActivityGroupInfo>
