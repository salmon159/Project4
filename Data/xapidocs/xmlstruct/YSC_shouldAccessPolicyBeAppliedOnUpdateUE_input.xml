<!-- input YSC_shouldAccessPolicyBeAppliedOnUpdate -->
<AccessPolicy APIName="" ClaimAsUserID="" UserID="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/shouldAccessPolicyBeAppliedOnUpdateUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="APIName"/>
            <Attribute DataType="UserId" Name="ClaimAsUserID">
                <Description>
                    <div>The user id that was stored in the system cache such as browser cookies. If present, the <code>UserID</code> is ignored.</div>
                </Description>
            </Attribute>
            <Attribute DataType="UserId" Name="UserID">
                <Description>
                    <div>The current user login id from the environment. This could be a guest user, anonymous user or a valid user.</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Entities>
        <yfc:doc>
            <Element>
                <Description>
                    <div>Contains a list of entities for determining whether data filter needs to be applied for read request.</div>
                </Description>
            </Element>
        </yfc:doc>
        <Entity APIName="" TableName="Required" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>The entity in which the user required access from.</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="APIName">
                        <Description>
                            <div>The <code>APIName</code> specifies the API for the current call to the user exit.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="TableName" Name="TableName">
                        <Description>
                            <div>The database table name or entity in which the given user is requesting access to.</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Entity>
    </Entities>
</AccessPolicy>
