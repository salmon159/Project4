<Order DocumentType="" EnterpriseCode="" LastOrderLineKey=""
    MaximumRecords="" OrderHeaderKey="" OrderNo="" SortingType="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getSalesOrderDetails/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Order_Line" Name="LastOrderLineKey" NameInEntity="Order_Line_Key">
                <Description>
					The order line key to begin retreiving records from.  Use in conjunction with <code>MaximumRecords</code>.
				</Description>
            </Attribute>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>
					The number of order lines to be returned.
				</Description>
            </Attribute>
            <Attribute DataType="String" Name="SortingType">
                <Description>
                    <div>
						This attribute is used to sort the Order Lines that are returned from the Api based on the criteria passed.				
						<br/>Valid Value: <b>
                            <code>ITEM</code>
                        </b>.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SpecificOrderLines>
        <OrderLine OrderLineKey="" PrimeLineNo="" SubLineNo="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element>
                    <Description>
						Identifies a specific order line to be included in the output of the API.
					</Description>
                </Element>
            </yfc:doc>
        </OrderLine>
    </SpecificOrderLines>
</Order>
