<Item DemandType="" EnterpriseCode="" FirstShipDate="" IgnorePromised=""
    IgnoreUnpromised="" ItemID="" LastShipDate="" OrganizationCode=""
    ProductClass="" TransactionId="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getAvailabilityCorrectionsUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Inventory_Demand" Name="DemandType" NameInEntity="Demand_Type"/>
            <Attribute EntityName="Item" Name="EnterpriseCode" NameInEntity="Organization_Code"/>
            <Attribute DataType="TimeStamp" Name="FirstShipDate">
                <Description>
                    <div>
		The first ship date to look for availability.
		</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnorePromised">
                <Description>
                    <div>
		This flag indicates whether to ignore promised demands in the availability picture.
		</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreUnpromised">
                <Description>
                    <div>
		This flag indicates whether to ignore uppromised demands in the availability picture.
		</div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="LastShipDate">
                <Description>
                    <div>
		The last ship date to look for availability.
		</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="ProductClass" NameInEntity="Product_Class"/>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid"/>
        </Attributes>
    </yfc:doc>
    <Supplies yfc:DTDOccurrence="REQUIRED">
        <Supply FirstShipDate="" IsUnplannedSupply="" LastShipDate=""
            OrganizationCode="" Quantity="" Segment="" SegmentType=""
            ShipNode="" SoftAssignedQty="" Track="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Inventory_Supply"/>
                <Attributes>
                    <Attribute DataType="TimeStamp" Name="FirstShipDate">
                        <Description>
                            <div>
			The first ship date of the supply.
			</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IsUnplannedSupply">
                        <Description>
                            <div>
			This flag indicates whether this supply is from unplanned inventory or not.
			</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="TimeStamp" Name="LastShipDate">
                        <Description>
                            <div>
			The last ship date of the supply.
			</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Inventory_Supply"
                        Name="OrganizationCode" NameInEntity="Owner_Key"/>
                    <Attribute DataType="Quantity" Name="SoftAssignedQty">
                        <Description>
                            <div>
			The soft assigned demand quantity for this available supply.
			</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Inventory_Supply"
                        Name="Track" NameInEntity="Availability_Type"/>
                </Attributes>
            </yfc:doc>
            <InventoryTag BatchNo="" InventoryItemKey=""
                InventoryTagKey="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotKeyReference="" LotNumber=""
                ManufacturingDate="" RevisionNo="" TagNumber=""
                TotalDemand="" TotalOnhandSupply="" TotalOtherSupply=""/>
        </Supply>
    </Supplies>
    <DeliveryMethodForSupplyList>
        <DeliveryMethodForSupply DeliveryMethod="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="DeliveryMethod">
                        <Description>
                            <div>
						The <code>DeliveryMethod</code> requested for the promise line. Possible values include Delivery DEL, Ship SHP or Pick Up PICK
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </DeliveryMethodForSupply>
    </DeliveryMethodForSupplyList>
</Item>
