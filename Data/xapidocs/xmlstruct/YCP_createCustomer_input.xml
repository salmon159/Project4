<!-- createCustomer Input XML -->
<Customer BuyerOrganizationCode="" CanConsumeSupplementalCapacity=""
    CommitmentTime="" CustomerAssignedVendorID=""
    CustomerClassificationCode="" CustomerID="" CustomerType=""
    ExternalCustomerID="" FuncAckTime="" OrganizationCode="Required"
    SendsAsn="" SendsCommitment="" SendsFuncAck="" SlotPreferenceType="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/createCustomer/input">
    <yfc:doc>
        <Element EntityName="Customer"/>
        <Attributes>
            <Attribute EntityName="Customer"
                Name="BuyerOrganizationCode" NameInEntity="Buyer_Organization_Code">
                <Description>
                    <div>
                        <code>BuyerOrganizationCode</code> attribute must be passed when creating a customer representing an existing organization. If <code>BuyerOrganizationCode</code> is passed, the API will update the organization with information provided in <code>BuyerOrganization</code> element.   If <code>BuyerOrganizationCode</code> is not passed, the API will create a new organization with information provided in <code>BuyerOrganization</code> element.
				<Validation>
                            <Condition>Organization does not exist.
					</Condition>
                            <Error Description="Invalid Organization." ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>BuyerOrganizationCode</code> attribute is not passed when creating a customer representing an existing organization.
					</Condition>
                            <Error
                                Description="Record already exists in the database." ErrorCode="YFC0001"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Customer"
                Name="CustomerAssignedVendorID" NameInEntity="Customer_Assigned_Vendor_ID">
                <Description>
                    <div>
                        <code>CustomerAssignedVendorID</code> is issued by the customer to the vendor. This explains how the customer (buyer) identifies the vendor (seller).
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Customer" Name="CustomerID" NameInEntity="Customer_ID">
                <Description>
                    <div>
				Identifier for a customer.  If <code>CustomerID</code> is not provided and Customer is a business customer, it will be defaulted as <code>BuyerOrganizationCode</code>.  If <code>CustomerID</code> is not provided and Customer is a consumer customer, it will be defaulted as consumer's <code>BillingPersonInfo's PersonID</code>.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Customer" Name="CustomerType" NameInEntity="Customer_Type">
                <Description>
                    <div>
				Type of customer.  Valid values are: 01-Business, 02-Consumer.  Default value is 01.  If <code>CustomerType</code> is <b>
                            <code>01</code>
                        </b>, <code>BuyerOrganization</code> information is required and <code>Consumer</code> element will be ignored.  If <code>CustomerType</code> is <b>
                            <code>02</code>
                        </b>, <code>Consumer</code> element is required and <code>BuyerOrganization</code> information will be ignored.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Customer" Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
				Organization code of organization that defines this customer.
				<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not provided.
					</Condition>
                            <Error Description="Missing attribute." ErrorCode="YFS10460"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Customer" Name="SlotPreferenceType" NameInEntity="Slot_Preference_Type">
                <Description>
                    <div>
				Slot preference type of the customer.  Valid values are: <code>
                            <b>HARD</b>
                        </code> - Only the preferred slots are to be considered.  <code>
                            <b>SOFT</b>
                        </code> - Other slots will also be considered.  <code>
                            <b>NONE</b>
                        </code> - No preferences.  Default value: <code>
                            <b>NONE</b>
                        </code>
                        <Validation>
                            <Condition>
                                <code>SlotPreferenceType</code> is not a valid value.
					</Condition>
                            <Error
                                Description="Invalid SlotPreferenceType." ErrorCode="YCP73_00022"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>SlotPreferenceType</code> is <code>
                                    <b>NONE</b>
                                </code> and <code>CustomerSlotPreferenceList</code> is passed.
					</Condition>
                            <Error
                                Description="Cannot pass CustomerSlotPreferenceList for NONE SlotPreferenceType." ErrorCode="YCP73_00023"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ParentCustomer CustomerID="" OrganizationCode="">
        <yfc:doc>
            <Element EntityName="Customer">
                <Description>
                    <div>
				The parent customer of this customer.  
				<Validation>
                            <Condition>If <code>ParentCustomer</code> is provided, the following condition must be true.
					<ul>
                                    <li>It must be a valid existing customer.</li>
                                    <li>It must be of the same CustomerType.</li>
                                    <li>It cannot be a child of another customer.</li>
                                    <li>It must belong to the same organization as the child.</li>
                                </ul>
                            </Condition>
                            <Error
                                Description="Invalid Parent Customer." ErrorCode="YCP77_00001"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
    </ParentCustomer>
    <BuyerOrganization AccountWithHub="" AuthorityType=""
        BillingAddressKey="" CatalogOrganizationCode=""
        ContactAddressKey="" CorporateAddressKey=""
        CreatorOrganizationKey="" DunsNumber=""
        InventoryOrganizationCode="" InventoryPublished="" IsBuyer=""
        IsHubOrganization="" IsLegalEntity="" IsNode=""
        IsSourcingKept="" IssuingAuthority="" ItemXrefRule=""
        LocaleCode="" OrganizationCode="" OrganizationKey=""
        OrganizationName="" ParentOrganizationCode=""
        PaymentProcessingReqd="" PrimaryEnterpriseKey="Required"
        PrimarySicCode="" PrimaryUrl="" TaxExemptFlag=""
        TaxExemptionCertificate="" TaxJurisdiction="" TaxpayerId=""
        XrefAliasType="" XrefOrganizationCode="">
        <yfc:doc>
            <Element EntityName="Organization">
                <Description>
                    <div>
				Identify the organization or node which customer represents.  <code>BuyerOrganization</code> is required if customer is a business customer.
				<Validation>
                            <Condition>If <code>BuyerOrganization</code> is not provided.
					</Condition>
                            <Error Description="Missing element in XML." ErrorCode="YFS10507"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Organization"
                    Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
					OrganizationCode which identifies an organization.
					<Validation>
                                <Condition>If <code>OrganizationCode</code> is not passed and Customer's Organization doesn't have an OrganizationSuffix.
						</Condition>
                                <Error Description="Missing attribute." ErrorCode="YFS10460"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Node AcceptanceRequired="" ActivateFlag=""
            AdvanceNotificationTime="" AgentCriteriaGroup=""
            CurrentBolNumber="" DcmIntegrationRealTime=""
            DefaultDeclaredValue="" ExportLicenseExpDate=""
            ExportLicenseNo="" GLN="" GenerateBolNumber=""
            IdentifiedByParentAs="" InheritConfigFromShipNode=""
            InterfaceSubType="" InterfaceSubTypeCode="" InterfaceType=""
            InventoryTracked="" Inventorytype=""
            IsItemBasedAllocationAllowed="" Latitude="" Longitude=""
            MaintainInventoryCost="" MaxDaysToScheduleBefore=""
            MinNotificationTime="" OwnerType="" PicklistType=""
            ProcureToShipAllowed="" ReceiptProcessingTime=""
            ReceiptProcessingTimeForForwarding=""
            ReceivingCalendarKey="" ReceivingNode=""
            RequiresChangeRequest="" ReturnCenterFlag="" ReturnsNode=""
            SerialTracking="" ShipNode="" ShipNodeClass=""
            ShipnodeType="" ShippingCalendarKey="" ShippingNode=""
            StoreInvConfig="" SupplierKey="" ThreePlNode="" TimeDiff=""
            UseEndOfShiftTime="" XDocNode="">
            <yfc:doc>
                <Element EntityName="Ship_Node"/>
                <Attributes>
                    <Attribute DataType="NumberOfDays" Name="AdvanceNotificationTime">
                        <Description>
                            <div>
                                <b>Deprecated.</b> See <code>manageNodeNotification</code> API. The number of 
					days before which the ship node needs notification of an impending receipt or shipment.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="InterfaceSubTypeCode"/>
                    <Attribute DataType="NumberOfDays" Name="MinNotificationTime">
                        <Description>
                            <div>
                                <b>Deprecated.</b> See <code>manageNodeNotification</code> API. The number of 
					days before which the communication to ship the order to the shipping node or supplier should be 
					made. This field is used to determine the earliest ship date on the line.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Node>
        <CorporatePersonInfo AddressID="" AddressLine1=""
            AddressLine2="" AddressLine3="" AddressLine4=""
            AddressLine5="" AddressLine6="" AlternateEmailID=""
            Beeper="" City="" Company="" Country="" DayFaxNo=""
            DayPhone="" Department="" EMailID="" ErrorTxt=""
            EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
            IsCommercialAddress="" JobTitle="" LastName="" Latitude=""
            Longitude="" MiddleName="" MobilePhone="" OtherPhone=""
            PersonID="" PreferredShipAddress="" State="" Suffix=""
            TaxGeoCode="" Title="" UseCount="" VerificationStatus="" ZipCode="">
            <yfc:doc>
                <Element EntityName="Person_Info"/>
            </yfc:doc>
        </CorporatePersonInfo>
        <ContactPersonInfo AddressID="" AddressLine1="" AddressLine2=""
            AddressLine3="" AddressLine4="" AddressLine5=""
            AddressLine6="" AlternateEmailID="" Beeper="" City=""
            Company="" Country="" DayFaxNo="" DayPhone="" Department=""
            EMailID="" ErrorTxt="" EveningFaxNo="" EveningPhone=""
            FirstName="" HttpUrl="" IsCommercialAddress="" JobTitle=""
            LastName="" Latitude="" Longitude="" MiddleName=""
            MobilePhone="" OtherPhone="" PersonID=""
            PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
            Title="" UseCount="" VerificationStatus="" ZipCode="">
            <yfc:doc>
                <Element EntityName="Person_Info"/>
            </yfc:doc>
        </ContactPersonInfo>
        <BillingPersonInfo AddressID="" AddressLine1="" AddressLine2=""
            AddressLine3="" AddressLine4="" AddressLine5=""
            AddressLine6="" AlternateEmailID="" Beeper="" City=""
            Company="" Country="" DayFaxNo="" DayPhone="" Department=""
            EMailID="" ErrorTxt="" EveningFaxNo="" EveningPhone=""
            FirstName="" HttpUrl="" IsCommercialAddress="" JobTitle=""
            LastName="" Latitude="" Longitude="" MiddleName=""
            MobilePhone="" OtherPhone="" PersonID=""
            PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
            Title="" UseCount="" VerificationStatus="" ZipCode="">
            <yfc:doc>
                <Element EntityName="Person_Info"/>
            </yfc:doc>
        </BillingPersonInfo>
    </BuyerOrganization>
    <Consumer>
        <yfc:doc>
            <Element EntityName="Consumer">
                <Description>
                    <div>
				Identify the consumer which customer represents.  <code>Consumer</code> is required if customer is a consumer customer.  
				<Validation>
                            <Condition>If <code>Consumer</code> element is not provided.
					</Condition>
                            <Error Description="Missing element in XML." ErrorCode="YFS10507"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
        <BillingPersonInfo AddressID="" AddressLine1="" AddressLine2=""
            AddressLine3="" AddressLine4="" AddressLine5=""
            AddressLine6="" AlternateEmailID="" Beeper="" City=""
            Company="" Country="" DayFaxNo="" DayPhone="" Department=""
            EMailID="" ErrorTxt="" EveningFaxNo="" EveningPhone=""
            FirstName="" HttpUrl="" IsCommercialAddress="" JobTitle=""
            LastName="" Latitude="" Longitude="" MiddleName=""
            MobilePhone="" OtherPhone="" PersonID=""
            PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
            Title="" UseCount="" VerificationStatus="" ZipCode="">
            <yfc:doc>
                <Element EntityName="Person_Info">
                    <Description>
                        <div>
				Billing information of a consumer.  <code>BillingPersonInfo</code> is required for a customer.
				<Validation>
                                <Condition>If <code>BillingPersonInfo</code> is not provided.
					</Condition>
                                <Error
                                    Description="Missing BillingPersonInfo." ErrorCode="YCP0214"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Person_Info" Name="PersonID" NameInEntity="Person_ID">
                        <Description>
                            <div>
				ID identifying a person information
				<Validation>
                                    <Condition>If <code>CustomerID</code> is blank and <code>PersonID</code> is not provided.
					</Condition>
                                    <Error
                                    Description="Missing attribute." ErrorCode="YFS10460"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </BillingPersonInfo>
    </Consumer>
    <CustomerSlotPreferenceList>
        <CustomerSlotPreference EndTime="" FridayPreferred=""
            MondayPreferred="" SaturdayPreferred="" StartTime=""
            SundayPreferred="" ThursdayPreferred="" TuesdayPreferred=""
            WednesdayPreferred="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
					Identify the preferred day of week and time slot that the customer would like to consider for appointment planning.
						<Validation>
                                <Condition>
                                    <code>StartTime</code> or <code>EndTime</code> is not passed.
								</Condition>
                                <Error
                                    Description="StartTime and EndTime must be passed." ErrorCode="YCP73_00024"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
            </yfc:doc>
        </CustomerSlotPreference>
    </CustomerSlotPreferenceList>
    <CustomerSchedulingPreferences CancelOrderOnBackorder="Y|N"
        IsDropShipAllowed="Y|N" IsLineShipComplete="Y|N"
        IsLineShipSingleNode="Y|N" IsShipComplete="Y|N"
        IsShipSingleNode="Y|N" IsSubstitutionAllowed="Y|N" OptimizationType=""/>
