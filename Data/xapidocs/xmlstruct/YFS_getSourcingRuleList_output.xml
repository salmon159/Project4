<SourcingRuleHeaders TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getSourcingRuleList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="TotalNumberOfRecords"/>
        </Attributes>
    </yfc:doc>
    <SourcingRuleHeader FulfillmentType="" ItemClassification=""
        ItemClassification2="" ItemClassification3=""
        ItemClassification4="" ItemClassification5=""
        ItemClassification6="" ItemClassification7="" ItemGroupCode=""
        ItemID="" NodeTypeID="" OrderSourcingClassification=""
        OrganizationCode="" ProductClass="" Purpose="" RegionKey=""
        SellerOrganizationCode="" SourcingRuleHeaderKey="" ToNodeKey="" yfc:DTDOccurrence="ZeroOrMany">
        <SourcingRuleDetails>
            <SourcingRuleDetail DistributionRuleId="" FromNodeKey=""
                FutureInventoryWindow="" ProcureToShipAllowed=""
                SeqNo="" SourcingRuleDetailKey="" SubstitutionAllowed=""
                TemplateType="" WorkOrderCreationAllowed="" yfc:DTDOccurrence="ZeroOrMany">
                <SourcingTemplate>
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>	
            SourcingTemplate must contain xml corresponding to the passed <code>TemplateType</code>.  Complete <code>SourcingTemplate</code> will be stored as a string in <code>YFS_SOURCING_RULE_DTL.TEMPLATE_XML</code> column.
            </div>
                            </Description>
                        </Element>
                    </yfc:doc>
                    <AdditionalData ExpandToMinimizeNumOfShipments=""
                        IdleAsset="" Radius="" RadiusUOM="">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>
							Additional flags and information to use when sourcing.
						</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Flag" Name="ExpandToMinimizeNumOfShipments">
                                    <Description>
						This flag is applicable only when optimization type is cost based, and minimizing the number of shipments. It is not applicable for Procurement Rule.
                               <div>
								If set to <b>Y</b>, then the sourcing rule will be expanded further to minimize number of shipments for lines.
								Sourcing Rule is expanded further, when supply is unavailable for one of the lines. Based on the flag, sourcing rule will be expanded further for lines that have supply available also, in order to minimize the number of shipments. 
								Default value is <b>N</b>.
								</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="IdleAsset">
                                    <Description>
                                    <div>
								If set to <code>
                                    <b>Y</b>
                                    </code>, indicates that only nodes where the item being sourced is an idle asset should be considered.  Default value is <code>
                                    <b>N</b>
                                    </code>.
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Number" Name="Radius">
                                    <Description>
                                    <div>
								Indicates the radius from the ship to address that a node must fall within to be considered for sourcing.  In the case of procurement sourcing rule, the radius from the final ship node is used.  If <code>Radius</code> is passed, <code>RadiusUOM</code> must also be passed.
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute EntityName="Uom"
                                    Name="RadiusUOM" NameInEntity="Uom">
                                    <Description>
                                    <div>
								The unit of measure that the <code>Radius</code> is specified in.  This should be a dimension UOM, and it must be possible to convert between this UOM and MILE.
							</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </AdditionalData>
                    <RelationshipType ArgID="" Direction=""
                        RelationshipTypeValue="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>	
                    Indicates type of the relationship used in sourcing template.
                  </div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Text-24" Name="ArgID">
                                    <Description>
                                    <div>
                    Indicates position of the <code>RelationshipType</code> in sourcing tempate.
                  </div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-1" Name="Direction">
                                    <Description>
                                    <div>
                  Indicates which direction should be used when evaluaing relationships.
                  <br/>Valid Values: 
                  <br/>
                                    <b>T</b> - To direction.  Nodes that are stored in 'YFS_ORG_RELATIONSHIP.TO_ORG_CODE' part of the relationship will be returned.
                  <br/>
                                    <b>F</b> - From direction.  Nodes that are stored in 'YFS_ORG_RELATIONSHIP.FROM_ORG_CODE' part of the relationship will be returned.
                  </div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-24" Name="RelationshipTypeValue">
                                    <Description>
                                    <div>
                    Indicates relationship type ID.
                  </div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </RelationshipType>
                    <NodeTypes ArgID="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>	
                    Indicates node types used in sourcing template.
                  </div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Text-24" Name="ArgID">
                                    <Description>
                                    <div>
                    Indicates position of the <code>NodeTypes</code> in sourcing tempate.
                  </div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <NodeType NodeTypeValue="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Element>
                                    <Description>
                                    <div>	
                    Indicates node type used in sourcing template.
                  </div>
                                    </Description>
                                </Element>
                                <Attributes>
                                    <Attribute DataType="Text-24" Name="NodeTypeValue">
                                    <Description>
                                    <div>
                    Indicates node type ID.
                  </div>
                                    </Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                        </NodeType>
                    </NodeTypes>
                </SourcingTemplate>
            </SourcingRuleDetail>
        </SourcingRuleDetails>
    </SourcingRuleHeader>
</SourcingRuleHeaders>
