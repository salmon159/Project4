<Query CallingOrganizationCode="" OrganizationCode="" QueryDefinition=""
    QueryKey="" QueryName="" QueryType="" TableType="!TableType!" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getQueryList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="CallingOrganizationCode">
                <Description>
                    <div>
					If <code>CallingOrganizationCode</code> is passed, the <code>Query</code> list for the organization that has configured <code>Query</code>s for this <code>CallingOrganizationCode</code> is returned.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" EntityName="Query"
                Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
					The organization that owns this query.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="TableType" Name="TableType">
                <Description>
                    <div>Return data for the given table type. Note this is not a filter.
    				If the resulting schema contains multiple table types, the output may
    				contain data from all of them.
					<br/>Default: <b>
                            <code>MASTER</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>MASTER</code>
                        </b> or <b>
                            <code>CONFIGURATION</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Query>
