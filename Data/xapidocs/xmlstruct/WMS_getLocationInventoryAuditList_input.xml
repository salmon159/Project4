    <yfc:doc>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>
                    <div>
						A CaseId
							<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
								</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="Createts">
                <Description>The timestamp for the creation of this record. 
					If createts is not passed, it will take today as the default date and returns audits written today.
					</Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>Specifies maximum number of records that must be fetched.</Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="OuterMostCaseId">
                <Description>Outer most case in the containment hierarchy</Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="OuterMostPalletId">
                <Description>Outer most Pallet in the containment hierarchy</Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>
                    <div>
						A PalletId
							<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
								</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
