<!-- Either (ContainerNo, Load Key and ShipmentKey) or ShipmentContainerKey is required. -->
<Container ContainerNo="Conditional" LoadKey="Conditional"
    ReadFromHistory="" ShipmentContainerKey="Conditional"
    ShipmentKey="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YDM/getShipmentContainerDetails/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
          To identify a <code>Container</code>, pass either <code>ShipmentContainerKey</code>
                    <i>or</i>
                    <code>ContainerNo</code> and <code>ShipmentKey</code>.  
          </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Shipment_Container"
                Name="ContainerNo" NameInEntity="Container_No"/>
            <Attribute EntityName="Load" Name="LoadKey" NameInEntity="LoadKey"/>
            <Attribute DataType="Flag" Name="ReadFromHistory">
                <Description>
                    <div>
               Indicates if the data should be read from the history tables.
               <br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                        <br/>If the value is <b>
                            <code>N</code>
                        </b>, the application seeks data from the regular table, returning the data if it is found. If the value is <b>
                            <code>Y</code>
                        </b>, the application seeks data from the corresponding history tables.
               </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Shipment_Container"
                Name="ShipmentContainerKey" NameInEntity="Shipment_Container_Key"/>
            <Attribute EntityName="Shipment_Container"
                Name="ShipmentKey" NameInEntity="ShipmentKey"/>
        </Attributes>
    </yfc:doc>
</Container>
