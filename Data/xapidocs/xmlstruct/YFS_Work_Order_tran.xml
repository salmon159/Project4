<WorkOrder AllocatedLevel="" CaseId="" DocumentType="" EnterpriseCode=""
    FinishNoLaterThan="" HoldFlag="" InvUpdateActivityCode="" ItemID=""
    LevelOfService="" NextAlertTs="" NodeKey="" OpenWorkOrderFlag=""
    OrderHeaderKey="" OrderLineKey="" OrderNo="" PalletId=""
    PipeLineKey="" PreCallStatus="" Priority="" ProductClass=""
    PromisedApptEndDate="" PromisedApptStartDate="" Purpose=""
    QuantityAllocated="" QuantityCompleted="" QuantityReleased=""
    QuantityRemoved="" QuantityRequested="" ReasonCode="" ReasonText=""
    ReleasedLevel="" Segment="" SegmentType="" SerialNo=""
    ServiceComplexityLevel="" ServiceItemGroupCode="" ServiceItemID=""
    ServiceSupervisorId="" ServiceUom="" ShipByDate="" ShipmentKey=""
    ShipmentNo="" StartNoEarlierThan="" Status="" StatusDate="" Uom=""
    WorkOrderKey="" WorkOrderNo="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/Work_Order_tran">
    <yfc:doc>
        <Element EntityName="Work_Order"/>
        <Attributes>
            <Attribute DataType="String" Name="AllocatedLevel">
                <Description>
                    <div>Indicates the extent of allocation for the work order. The valid values are FULL, PARTIAL, NONE, and ANY. The respective values tells wether the work order is fully, partially, not allocated or is in any of the the previously mentioned states.
	        		</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>
                    <div>Case for which the work order is processed.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>
                    <div>Pallet for which the work order is processed.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ReleasedLevel">
                <Description>
                    <div>Indicates the extent to which the work order is released. The valid values are FULL, PARTIAL, NONE, and ANY. The respective values tells wether the work order is fully, partially, not relased or is in any of the previously mentioned states.
	            	</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <WorkOrderTag BatchNo="" LotAttribute1="" LotAttribute2=""
        LotAttribute3="" LotKeyReference="" LotNumber=""
        ManufacturingDate="" RevisionNo="" WorkOrderKey="" WorkOrderTagKey=""/>
</WorkOrder>
