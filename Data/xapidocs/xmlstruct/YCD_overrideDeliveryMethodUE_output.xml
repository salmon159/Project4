<!--overrideDeliveryMethod Output XML-->
<OrderLineList ShipNode="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/overrideDeliveryMethodUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Ship_Node" Name="ShipNode" NameInEntity="ShipNode">
                <Description>This is the logged-in storeId. </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderLine OrderLineKey="Required" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element EntityName="Order_Line"/>
        </yfc:doc>
        <ItemDetails ItemID="Required" UnitOfMeasure="Required" yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element EntityName="Item"/>
            </yfc:doc>
            <PrimaryInformation DefaultProductClass="Required"
                IsDeliveryAllowed="Required" IsPickupAllowed="Required"
                IsShippingAllowed="Required" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Element EntityName="Item"/>
                </yfc:doc>
            </PrimaryInformation>
            <Extn/>
        </ItemDetails>
        <Shipnode ShipNode="">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Ship_Node" Name="ShipNode" NameInEntity="ShipNode"/>
                </Attributes>
            </yfc:doc>
        </Shipnode>
        <Extn/>
    </OrderLine>
</OrderLineList>
