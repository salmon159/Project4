<Item Available="" AvailableQty="" ConsiderAllNodes=""
    ConsiderAllSegments="" ConsiderInventoryNodeControl=""
    ConsiderUnassignedDemand="" Description="" DistributionRuleId=""
    ExtendedDescription="" ItemID="Required" ProductClass="" Quantity=""
    Segment="" SegmentType="" ShortDescription="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/checkAvailability/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="Available">
                <Description>
                    <div>
				Indicates whether the requested quantity is available.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="AvailableQty">
                <Description>
                    <div>
					Returned as <b>
                            <code>0</code>
                        </b> if <code>RequiredQty</code> was not passed. Otherwise <code>AvailableQty</code> is set to how much is available for the required quantity.
                </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderAllNodes">
                <Description>
                    <div>
				Indicates whether all nodes are to be considered in the availability checking.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderAllSegments">
                <Description>
                    <div>
				Indicates whether all segments are to be considered in the availability checking.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderInventoryNodeControl">
                <Description>
                    <div>
                Indicates whether inventory node control data were used in the inventory reporting.
                </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderUnassignedDemand">
                <Description>
                    <div>
				Indicates whether all unassigned demands are to be considered in the availability checking.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Item_Ship_Node" Name="DistributionRuleId"/>
            <Attribute EntityName="Inventory_Item" Name="ItemID"/>
            <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
            <Attribute DataType="Quantity" Name="Quantity">
                <Description>
                    <div>
                The available quantity.
                </div>
                </Description>
            </Attribute>
            <Attribute DataType="CommonCode" Name="Segment">
                <Description>
                    <div>
                        <code>Segment</code> of the inventories to be considered.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="SegmentType" Name="SegmentType">
                <Description>
                    <div>
                        <code>SegmentType</code> of the inventories to be considered.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
        </Attributes>
    </yfc:doc>
    <LanguageDescriptionList>
        <LanguageDescription Description="" ExtendedDescription=""
            LocaleCode="" ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
    </LanguageDescriptionList>
</Item>
