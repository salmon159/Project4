<Container AcceptIncorrectContainers="" CaseId="" ContainerNo=""
    ContainerScm="" DockLocationId="" LoadKey="" Node="Required"
    PalletId="" ShipmentContainerKey="" ShipmentKey="" TrailerNo="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/addContainerToTrailer/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="AcceptIncorrectContainers">
                <Description>
                    <div>Specifies whether to accept incorrect trailer loading. 
						<Validation>
                            <br/>Default: <b>
                                <code>N</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                            <Condition> If <code>AcceptIncorrectContainers</code> is <b>
                                    <code>N</code>
                                </b> and the container is loaded to a wrong trailer.
						</Condition>
                            <Error
                                Description="Container is being loaded in to a Trailer that is different from the input." ErrorCode="YDM00191"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>Identifies the container that is processed by the API.
				</Description>
            </Attribute>
            <Attribute DataType="LocationId" Name="DockLocationId">
                <Description>The Dock Location where the container should be moved for loading the trailer.
				</Description>
            </Attribute>
            <Attribute EntityName="Ship_Node" Name="Node" NameInEntity="Ship_Node"/>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>Identifies the container that is processed by the API.
				</Description>
            </Attribute>
            <Attribute EntityName="Load" Name="TrailerNo" NameInEntity="Trailer_No"/>
        </Attributes>
    </yfc:doc>
    <Shipment SellerOrganizationCode="" ShipNode="" ShipmentNo=""/>
    <Load LoadNo=""/>
</Container>
