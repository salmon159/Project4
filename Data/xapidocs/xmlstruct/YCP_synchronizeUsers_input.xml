<Users DisplayUserID="" EnterpriseCode="" Reset="" SystemName="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/synchronizeUsers/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Text-150" EntityName="User"
                Name="DisplayUserID" NameInEntity="Display_User_ID"/>
            <Attribute DataType="Text-24" EntityName="User"
                Name="EnterpriseCode" NameInEntity="Enterprise_Code"/>
            <Attribute DataType="Flag" Name="Reset">
                <Description>
                    <div>
						Indicates if all users need to be deleted and
						rerecorded.
						<br/>
						If passed as "Y", all users belonging to the
						passed system, other than the ones passed in
						input, are deleted. If not passed, or passed
						with a value other than "Y", the input XML is
						treated as a delta-change to the set of users in
						the system.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="SystemName">
                <Description>
                    <div>
						Identifies an external system with which users
						are synchronized, such as an LDAP server, that
						maintains user authentication information.
						<Validation>
                            <Condition>
								If
								<code>SystemName</code>
								is not passed:
							</Condition>
                            <Error Description="Field Mandatory" ErrorCode="YCP0046"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <User DepartmentCode="" Loginid="Required"
        OrganizationCode="Required" ShipNode=""
        TemplateLoginid="Required" Username="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Text-20" Name="DepartmentCode">
                    <Description>
                        <div>The department to which a user belongs.
							<Validation>
                                <Condition>
									The department code is invalid or
									the organization does not contain
									the department
								</Condition>
                                <Error
                                    Description="The Department code does not exist for the organization." ErrorCode="YCP0256"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="User" Name="Loginid" NameInEntity="Loginid">
                    <Description>
                        <div>The login ID for the user.
							<Validation>
                                <Condition>
									If
									<code>Loginid</code>
									is not passed:
								</Condition>
                                <Error Description="Field Mandatory" ErrorCode="YCP0046"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="User" Name="OrganizationCode" NameInEntity="Organization_Key">
                    <Description>
                        <div>The organization to which a user belongs.
							<Validation>
                                <Condition>
									If
									<code>OrganizationCode</code>
									is not passed:
								</Condition>
                                <Error Description="Field Mandatory" ErrorCode="YCP0046"/>
                            </Validation>
                            <Validation>
                                <Condition>
									If
									<code>OrganizationCode</code>
									is not a valid organization code:
								</Condition>
                                <Error Description="No Record Found" ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="User" Name="ShipNode" NameInEntity="Business_Key">
                    <Description>
                        <div>The business associated with this user. Identifies a ship node when filled.
							<Validation>
                                <Condition>
									If
									<code>ShipNode</code>
									is not a valid shipnode of the
									user's organization:
								</Condition>
                                <Error Description="No Record Found" ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="User" Name="TemplateLoginid" NameInEntity="Loginid">
                    <Description>
                        <div>
							A Login Id used as a template for creating
							other users. This user's settings are
							inherited by the passed
							<code>Loginid.</code> Which attributes are inherited is
							determined by the createsynchuser and modifysyncuser
							resource templates.
							<Validation>
                                <Condition>
									If
									<code>TemplateLoginid</code>
									is not passed:
								</Condition>
                                <Error Description="Field Mandatory" ErrorCode="YCP0046"/>
                            </Validation>
                            <Validation>
                                <Condition>
									If
									<code>TemplateLoginid</code>
									is not a valid user in the system:
								</Condition>
                                <Error Description="No Record Found" ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <UserGroupLists Reset="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="Reset">
                        <Description>
                            <div>
								Indicates whether all groups for a user
								need to be deleted and rerecorded.
								<br/>
								If passed as "Y", only user groups
								listed under the user's child nodes are
								members of this user (that is, if the
								user belonged to other groups before
								modification, membership to those groups
								is removed).
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
                <Element>
                    <Description>
                        <div>This is an optional element.</div>
                    </Description>
                </Element>
            </yfc:doc>
            <UserGroupList UsergroupId="Required" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="User_Group" Name="UsergroupId">
                            <Description>
                                <div>
									A unique identifier to the user
									group. Used to manage access control
									and permissions for users.
									<Validation>
                                    <Condition>
											If
											<code>UsergroupId</code>
											is not passed:
										</Condition>
                                    <Error
                                    Description="Field Mandatory" ErrorCode="YCP0046"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
											If
											<code>UsergroupId</code>
											is not a valid user group:
										</Condition>
                                    <Error
                                    Description="No Record Found" ErrorCode="YCP0045"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                    <Element>
                        <Description>
                            <div>
								Indicates groups to which the user
								belongs. A child element under
								<code>UserGroupLists</code>
								.
							</div>
                        </Description>
                    </Element>
                </yfc:doc>
            </UserGroupList>
        </UserGroupLists>
    </User>
</Users>
