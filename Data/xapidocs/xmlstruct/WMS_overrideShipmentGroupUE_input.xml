<?xml version="1.0" encoding="UTF-8"?>
<ShipmentGroup Node="Required" ShipmentGroupId="" ShipmentGroupKey="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/overrideShipmentGroupUE/input">
    <Shipment ActualDeliveryDate="" ActualFreightCharge=""
        ActualShipmentDate="" AllowOverage="" AppointmentNo=""
        AssignedToUserId="" AssociationAction="" BBNMinVolume=""
        BBNMinWeight="" BillToAddressKey="" BillToCustomerId="" BolNo=""
        BreakBulkLoadKey="" BreakBulkNode="" BuyerMarkForNodeId=""
        BuyerOrganizationCode="" BuyerReceivingNodeId="" CODPayMethod=""
        CarrierAccountNo="" CarrierServiceCode="" CarrierType="" Code=""
        CommercialValue="" Currency="" DeliveryCode="" DeliveryMethod=""
        DeliveryPlanKey="" DeliveryTS="" DestinationZone=""
        DoNotVerifyCaseContent="" DoNotVerifyPalletContent=""
        DocumentType="" DownLoadCount="" EmailReturnLabel=""
        EnterpriseCode="" EspCheckRequired="" ExpectedDeliveryDate=""
        ExpectedShipmentDate="" ExportTaxpayerID="" FreightTerms=""
        FromAddressKey="" FromAppointment="" HasOtherShipments=""
        HazardousMaterialFlag="" HoldLocation="" ITDate="" ITNNo=""
        ITNo="" InvoiceComplete="" IsPackProcessComplete=""
        IsPackingRequired="" IsPackingRequiredWithContents=""
        IsProductPlacingComplete="" IsShipmentLevelIntegration=""
        IsSingleOrder="" LevelOfService="" LinesEntered="" Lockid=""
        ManifestKey="" ManifestNo="" ManuallyEntered="" MergeNode=""
        NextAlertTs="" NumOfCartons="" NumOfPallets=""
        OrderAvailableOnSystem="" OrderHeaderKey="" OrderNo=""
        OrderReleaseKey="" OriginZone="" OverrideManualShipmentEntry=""
        PackAndHold="" PackListType="" ParentShipmentKey=""
        PickListNo="" PickticketNo="" PipelineKey="" PodNo="" ProNo=""
        ProfileID="" ReceivingNode="" ReleaseNo=""
        RequestedCarrierServiceCode="" RequestedDeliveryDate=""
        RequestedShipmentDate="" ReturnAuthorizationNumber=""
        ReturnBillingAccount="" ReturnByDate="" ReturnCarrierService=""
        ReturnFreightTerms="" SCAC="" ScacAndService=""
        ScacAndServiceKey="" ScacIntegrationRequired="" SealNo=""
        SellerOrganizationCode="" ShipDate="" ShipMode="" ShipNode=""
        ShipToCustomerId="" ShipVia="" ShipmentClosedFlag=""
        ShipmentConfirmUpdatesDone="" ShipmentConsolidationGroupId=""
        ShipmentContainerizedFlag="" ShipmentDeliverUpdatesDone=""
        ShipmentKey="" ShipmentNo="" ShipmentPlannedFlag=""
        ShipmentSortLocationId="" ShipmentType="" Status=""
        StatusDate="" ToAppointment="" TotalActualCharge=""
        TotalEstimatedCharge="" TotalNumOfPickableSKUs="" TotalVolume=""
        TotalVolumeUOM="" TotalWeight="" TotalWeightUOM="" TrackingNo=""
        TrailerNo="" WorkOrderApptKey="" WorkOrderKey="" yfc:DTDOccurrence="REQUIRED">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Shipment_master.xml" IncludeXMLPath="/Shipment"/>
        <yfc:doc>
            <Element EntityName="Shipment"/>
            <Attributes>
                <Attribute DataType="String" Name="BreakBulkLoadKey"/>
                <Attribute DataType="String" Name="IsPackingRequired"/>
                <Attribute DataType="String" Name="IsPackingRequiredWithContents"/>
                <Attribute DataType="String" Name="IsShipmentLevelIntegration"/>
                <Attribute EntityName="SCAC_And_Service"
                    Name="ScacAndService" NameInEntity="Scac_And_Service"/>
                <Attribute EntityName="SCAC_And_Service"
                    Name="ScacAndServiceKey" NameInEntity="Scac_And_Service_Key"/>
                <Attribute DataType="String" Name="TotalNumOfPickableSKUs">
                    <Description>
                        <div>
					This field indicates the total number of pickable units in a shipment. This attribute will be returned in the output only if passed in the template. Only pickable line quantities are counted for this attribute; parent lines and bundle parent lines are ignored.
					</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <EligibleShipmentLineList>
            <EligibleShipmentLine BuyerMarkForNodeId=""
                CountryOfOrigin="" CustomerPoLineNo="" CustomerPoNo=""
                DepartmentCode="" DocumentType="Conditional"
                ExternalReleaseIdentifier="" FifoNo="" ItemDesc=""
                ItemID="Conditional" KitCode="LK|| || |Blanks" KitQty=""
                LevelOfService="" NetWeight="" NetWeightUom=""
                OrderHeaderKey="" OrderLineKey="" OrderNo="Conditional"
                OrderReleaseKey="" PrimeLineNo="Conditional"
                ProductClass="" Quantity="" ReleaseNo="Conditional"
                RequestedSerialNo="" ReturnShippingLabelLevel=""
                Segment="" SegmentType="" ShipAdviceNo=""
                ShipToCustomerId="" ShipmentConsolidationGroupId=""
                ShipmentLineKey="Conditional"
                ShipmentLineNo="Conditional"
                ShipmentSubLineNo="Conditional" SubLineNo="Conditional"
                UnitOfMeasure="Conditional" yfc:DTDOccurrence="REQUIRED">
                <yfc:includeInParent
                    IncludeXMLFileName="YFS_Shipment_Line_tran.xml" IncludeXMLPath="/ShipmentLine"/>
                <yfc:doc>
                    <Element EntityName="Shipment_Line">
                        <Description>
                            <div>
                Stores information about the <code>ShipmentLine</code>. To modify an existing <code>ShipmentLine</code>, identify the <code>ShipmentLine</code> by passing either the <code>ShipmentLineKey</code> or the <code>ShipmentKey</code>,  <code>ShipmentLineNo</code> and <code>ShipmentSubLineNo</code> or the <code>ShipmentKey</code>,  {<code>OrderNo</code>, <code>ReleaseNo</code>}/{<code>ShipAdviceNo</code>},  <code>PrimeLineNo</code>,  <code>SubLineNo</code>,  <code>ItemID</code>,  <code>UnitOfMeasure</code> and tag attributes if any under <code>ShipmentLineInvAttRequest</code> element. 
                <br/>If given under <code>ContainerDetail</code> element, identify the <code>ShipmentLine</code> by passing either the <code>ShipmentLineKey</code> or the <code>ShipmentKey</code>, <code>ShipmentLineNo</code> and <code>ShipmentSubLineNo</code>.
            </div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="String" Name="DocumentType"/>
                        <Attribute Name="KitQty">
                            <Description>
                                <div>The number of components required for one kit.  The quantity is used only for bundle components only when the order is not available for the shipment line. When order is available for the shipment line, <code>KitQty</code> is defaulted from order information.
					  </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Order_Release"
                            Name="ShipAdviceNo" NameInEntity="Ship_Advice_No">
                            <Description>
                                <div>Ship advice number of the order release.  <code>ShipAdviceNo</code> will only be used when order release identifiers such as <code>OrderReleaseKey</code>
                                    <i>or</i>
                                    <code>ReleaseNo</code> and <code>OrderHeaderKey</code> are not provided.  <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided) will be used to determine the order release in the system.  If more than one order releases are found in the system with the same <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided), the order release with a pickable status will be used. 
				  <Validation>
                                    <Condition>More than one order releases in a pickable status are found with the same  <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided).
					</Condition>
                                    <Error
                                    Description="Multiple order releases found with ShipAdviceNo." ErrorCode="OMP75_10000"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>System cannot find an order release with the provided <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided).
					</Condition>
                                    <Error
                                    Description="Invalid ShipAdviceNo." ErrorCode="YDM75_00001"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </EligibleShipmentLine>
        </EligibleShipmentLineList>
    </Shipment>
</ShipmentGroup>
