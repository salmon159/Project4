<reprintCarrierLabel xmlns="http://www.sterlingcommerce.com/documentation/YDM/reprintCarrierLabel/output">
    <Container ContainerNo="" PierbridgeLabelURL=""
        ShipmentContainerKey="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="ContainerNo" Name="ContainerNo">
                    <Description>ContainerNo for which this API could successfully get a Tracking Number.
				</Description>
                </Attribute>
                <Attribute DataType="String" Name="PierbridgeLabelURL">
                    <Description>Publishes the Pierbridge Label URL if populated in the Pierbrdige Server Response XML.
				</Description>
                </Attribute>
                <Attribute DataType="Key" Name="ShipmentContainerKey">
                    <Description>ShipmentContainerKey for which this API could successfully get a Tracking Number.
				</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ContainerReturnTrackingList>
            <ContainerReturnTracking PierbridgeReturnLabelURL=""
                ReturnPrintBuffer="" ReturnTrackingNumber=""
                ShipmentContainerKey="" ShipmentKey="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent
                    IncludeXMLFileName="YFS_Cntr_Ret_Track_master.xml" IncludeXMLPath="/ContainerReturnTracking"/>
                <yfc:doc>
                    <Element EntityName="Cntr_Ret_Track"/>
                    <Attributes>
                        <Attribute DataType="String" Name="PierbridgeReturnLabelURL">
                            <Description>Publishes the Pierbridge Return Label URL if populated in the Pierbrdige Server Response XML.
						</Description>
                        </Attribute>
                        <Attribute DataType="String" Name="ReturnPrintBuffer">
                            <Description>Label print buffer of the return label that is returned by Yantra Carrier Server if WMS chooses to print the label by itself. WMS can use this buffer only using Sterling Pierbridge Adaptor.
						</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ContainerReturnTracking>
        </ContainerReturnTrackingList>
        <Manifest ManifestKey="" ManifestNo="">
            <yfc:doc>
                <Element EntityName="Manifest"/>
            </yfc:doc>
        </Manifest>
    </Container>
</reprintCarrierLabel>
