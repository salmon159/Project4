<!-- YCP_getAlertStatisticsForUser_Output.xml -->
<getAlertStatisticsForUser UserId="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getAlertStatisticsForUser/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="User" Name="UserId" NameInEntity="Loginid">
                <Description>User for whom alert statistics is requested.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Queue AuditThisQueue="" HoursAssignExceedAction=""
        HoursForAssignRealert="" HoursForAssignment=""
        HoursForExceedRealert="" HoursForResolution=""
        HoursForResolveRealert="" HoursResolveExceedAction=""
        LastQueueExceedAlert="" MaximumActiveSize=""
        MaximumSizeExceedAction="" NumberOfAlerts=""
        NumberOfAlertsAssigned="" NumberOfAlertsGeneratedAfter=""
        NumberOfAlertsGeneratedBefore="" OwnerKey="" OwnerType=""
        Priority="" QKeyForAssignAlert="" QKeyForExceedAlert=""
        QKeyForResolveAlert="" QueueDescription="" QueueGroup=""
        QueueId="" QueueKey="" QueueName="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Count" Name="NumberOfAlerts">
                    <Description>Number of Alerts in the Queue</Description>
                </Attribute>
                <Attribute DataType="Count" Name="NumberOfAlertsAssigned">
                    <Description>Number of assigned alerts in the Queue</Description>
                </Attribute>
                <Attribute DataType="Count" Name="NumberOfAlertsGeneratedAfter">
                    <Description>Number of alerts that in the Queue that were generated after a specific time. The time is calculated as the current time less the number of hours set in the CountAlertsGeneratedAfterInHours input attribute</Description>
                </Attribute>
                <Attribute DataType="Count" Name="NumberOfAlertsGeneratedBefore">
                    <Description>Number of alerts in the Queue that were generated before a specific time. The time is calculated as the current time less the number of hours in the CountAlertsGeneratedBeforeInHours input attribute</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Queue>
    <!-- AlertInfo is a template-driven element but its attributes are not. If AlertInfo is present in the template, then all its attributes will appear in the output.
   -->
    <UserAlertInfoList>
        <AlertInfo ExceptionType="" NumberOfAlerts=""
            NumberOfAlertsAssigned="" NumberOfHighAlerts="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Inbox" Name="ExceptionType" NameInEntity="ExceptionType">
                        <Description>The type of Exception</Description>
                    </Attribute>
                    <Attribute DataType="Count" Name="NumberOfAlerts">
                        <Description>Number of Alerts for the Exception Type</Description>
                    </Attribute>
                    <Attribute DataType="Count" Name="NumberOfAlertsAssigned">
                        <Description>Number of assigned alerts for the Exception Type</Description>
                    </Attribute>
                    <Attribute DataType="Count" Name="NumberOfHighAlerts">
                        <Description>Number of high alerts for the Exception Type.This means for a HighPriorityThreshold of 3,all alerts of priority 1 and 2 are high priority.</Description>
                    </Attribute>
                </Attributes>
                <Element>
                    <Description>
                        <div>
						This element will be template driven, but not its attributes. Meaning, if the template has this element then all its attributes will appear in the output.
						</div>
                    </Description>
                </Element>
            </yfc:doc>
        </AlertInfo>
    </UserAlertInfoList>
    <MonitorRuleList>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					If the template has this element, then the monitor rule statistics will appear in the output. These are the monitor rules associated to the alerts in the queues to which the user is subscribed. 
					</div>
                </Description>
            </Element>
        </yfc:doc>
        <MonitorRule ActivateDate="" Age="" AgeType=""
            ComparisonDateSubType="" ComparisonDateType=""
            ComparisonStatus="" ConditionKey="" DateSubTypeToMonitor=""
            DateTypeToMonitor="" ExceptionType="" HoldTypeKey=""
            HoldTypeStatus="" IsActive="" MonitorConsolidationKey=""
            MonitorRuleKey="" MonitorType="" NumberOfAlerts=""
            OrganizationCode="" PipelineKey="" Priority=""
            ProcessTypeKey="" Purpose="" StatusToMonitor="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Count" Name="NumberOfAlerts">
                        <Description>Number of alerts for the Monitor Rule</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </MonitorRule>
    </MonitorRuleList>
</getAlertStatisticsForUser>
