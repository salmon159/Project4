<!-- getResourcePoolConsumptionDetailsList output XML -->
<ConsumptionDetailList xmlns="http://www.sterlingcommerce.com/documentation/INV/getResourcePoolConsumptionDetailsList/output">
    <ConsumptionDetail AppointmentSequence="" Capacity=""
        DocumentType="" IsSlotBased="" ManuallyCreatedReservation=""
        OrderHeaderKey="" OrderLineKey="" OrderNo="" OrderReleaseKey=""
        OverrideCapacityCheck="" ReservationExpirationDate=""
        ReservationId="" ReservationReason="" ResourcePoolId=""
        ServiceResourceId="" ServiceSlotDesc="" WorkOrderKey="" WorkOrderNo="">
        <yfc:doc>
            <Element EntityName="Res_Pool_Consmptn_Dtls">
                <Description>
                    <div>
			Resource pool capacity consumption details information. 
			</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type"/>
                <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No"/>
                <Attribute EntityName="Res_Pool" Name="ResourcePoolId" NameInEntity="Resource_Pool_Id"/>
                <Attribute EntityName="Service_Resource"
                    Name="ServiceResourceId" NameInEntity="Service_Resource_ID"/>
                <Attribute EntityName="Service_Slot"
                    Name="ServiceSlotDesc" NameInEntity="ServiceSlotDesc"/>
                <Attribute EntityName="Work_Order" Name="WorkOrderNo" NameInEntity="Work_Order_No"/>
            </Attributes>
        </yfc:doc>
        <AdditionalCapacityLimits>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>The dimensions in which the resource pool's capacity is constrained, in addition to the resource pool's primary capacity dimension.</div>
                    </Description>
                </Element>
            </yfc:doc>
            <AdditionalCapacityLimit Capacity=""
                CapacityUnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="Capacity">
                            <Description>
                                <div>The quantity consumed in the secondary capacity unit of measure.</div>
                            </Description>
                        </Attribute>
                        <Attribute Name="CapacityUnitOfMeasure">
                            <Description>
                                <div>The secondary capacity unit of measure in which the resource pool's capacity is limited.</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </AdditionalCapacityLimit>
        </AdditionalCapacityLimits>
        <CapacityConsumption ConsumptionDate="" EndTime="" StartTime="">
            <yfc:doc>
                <Element EntityName="Res_Pool_Capcty_Consmptn"/>
            </yfc:doc>
            <ResourcePool Node="" ProviderOrganizationCode=""
                ResourcePoolId="" ResourcePoolKey="">
                <yfc:doc>
                    <Element EntityName="Res_Pool"/>
                </yfc:doc>
                <ResourcePoolAttributeList>
                    <yfc:includeInParent IncludeXMLChildNodes="Y"
                        IncludeXMLFileName="MasterResourcePool.xml" IncludeXMLPath="/ResourcePool/ResourcePoolAttributeList"/>
                    <ResourcePoolAttribute DeliveryMethod=""
                        ResourcePoolAttributeKey="" ResourcePoolKey="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element EntityName="Res_Pool_Attr">
                                <Description>
                                    <div>
						 	This element defines attributes and their values for the resource pool.
						</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="String" Name="DeliveryMethod">
                                    <Description>
                                    <div>
								The delivery method for which the resource pool is defined. <br/>
								Valid values: <code>SHP</code> - Shipment, <code>PICK</code> - Pickup by user
							</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </ResourcePoolAttribute>
                </ResourcePoolAttributeList>
            </ResourcePool>
            <ServiceResource Node="" ProviderOrganizationCode=""
                ResourcePoolId="" ResourcePoolKey=""
                ServiceResourceId="" ServiceResourceKey="">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Res_Pool" Name="Node" NameInEntity="Node_Key"/>
                        <Attribute EntityName="Res_Pool"
                            Name="ProviderOrganizationCode" NameInEntity="Provider_Organization_Code"/>
                        <Attribute EntityName="Res_Pool"
                            Name="ResourcePoolId" NameInEntity="Resource_Pool_Id"/>
                    </Attributes>
                </yfc:doc>
            </ServiceResource>
        </CapacityConsumption>
    </ConsumptionDetail>
</ConsumptionDetailList>
