<!--manageAssociationrelationship Input XML-->
<EventAuditorList xmlns="http://www.sterlingcommerce.com/documentation/YCD/manageEventAuditor/input">
    <EventAuditor Action="" Active="" ApplicationCode=""
        EventAuditorKey="" EventAuditorType="" HoldType="" NoteText=""
        NoteType="" OrganizationCode="" TransformationFile=""
        WizardID="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Event_Auditor">
                <Description>
                    <div>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not provided.</Condition>
                            <Error
                                Description="Missing OrganizationCode" ErrorCode="YCD00052"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>TransformationFile</code> or <code>NoteText</code> should not be null when ShowInUI is Y.</Condition>
                            <Error
                                Description="Missing TransformationFile or NoteText" ErrorCode="YCD00053"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>NoteType</code> should not be null if Active is passed as Y.</Condition>
                            <Error Description="Missing NoteType " ErrorCode="YCD00054"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>ApplicationCode</code> must be provided if <code>WizardID</code> is provided.</Condition>
                            <Error Description="Missing ApplicationCode" ErrorCode="YCD00055"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>EventAuditorKey</code> must be provided if <code>Action</code> performed is <code>Delete</code>.</Condition>
                            <Error Description="Missing EventAuditorKey" ErrorCode="YCD00062"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-40" Name="Action">
                    <Description>
                        <div>Valid value is Delete.This attribute is needed only for deleting a record.Do not pass this attribute for any other actions.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </EventAuditor>
</EventAuditorList>
