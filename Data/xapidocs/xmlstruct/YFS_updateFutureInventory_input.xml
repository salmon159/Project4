<ShipNode RemoveAllPreviousInformation="" ShipNode="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/updateFutureInventory/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
		The ShipNode to be updated. 
		<Validation>
                        <Condition>
                            <code>ShipNode</code> not passed.
			</Condition>
                        <Error Description="Blank Shipnode." ErrorCode="YFS10151"/>
                    </Validation>
                    <Validation>
                        <Condition>Invalid ShipNode passed.
			</Condition>
                        <Error Description="YFS:Invalid Ship Node" ErrorCode="YFS10048"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Flag" Name="RemoveAllPreviousInformation">
                <Description>
                    <div>
				Indicates if all previous supply information for this <code>ShipNode</code> should be removed. If passed as <b>
                            <code>Y</code>
                        </b>, any information in the <code>Remove</code> element is ignored.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <!-- The same item can be repeated multiple times in this block-->
    <Item ItemID="Required" OrganizationCode="Conditional"
        ProductClass="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					Item for which inventory will be updated.
						<Validation>
                            <Condition>
                            	If removing inventory and inventory item does not exist.
							</Condition>
                            <Error Description="Invalid ItemId." ErrorCode="YFS10345"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Inventory_Item" Name="ItemID"/>
                <Attribute DataType="OrgCode" Name="OrganizationCode">
                    <Description>
                        <div>
				Identifies the organization.
				<Validation>
                                <Condition>
                                    <code>OrganizationCode</code> not passed.
					</Condition>
                                <Error
                                    Description="Invalid organization code." ErrorCode="YFS10395"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>OrganizationCode</code> is invalid.
					</Condition>
                                <Error
                                    Description="Invalid organization code." ErrorCode="YFS10395"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
                <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
            </Attributes>
        </yfc:doc>
        <Remove yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
						Any supply that matches the criteria provided will be removed. Parameters which do not have a value will not be considered.
						</div>
                    </Description>
                </Element>
            </yfc:doc>
            <SupplyDetails ETA="Conditional" FromETA="" LotAttribute1=""
                LotAttribute2="" LotAttribute3="" LotKeyReference=""
                LotNumber="" Segment="" SegmentType="" ShipByDate=""
                SupplyReference="" SupplyReferenceType=""
                SupplyType="Conditional" ToETA="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply"/>
                    <Attributes>
                        <Attribute EntityName="Inventory_Supply"
                            Name="ETA" NameInEntity="ETA">
                            <Description>
                                <div>The <code>ETA</code> attribute is required when the element <code>ShipNode\Item\Augment</code> is passed. When the <code>ETA</code> attribute is passed in the <code>ShipNode\Item\Remove</code> element, it is ignored.
					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="FromETA">
                            <Description>
                                <div>Supply with ETA greater than or equals to <b>
                                    <code>FromETA</code>
                                    </b> will be considered. Applicable for element <code>ShipNode\Item\Remove</code> .
					</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotAttribute1" NameInEntity="Lot_Attribute1">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotAttribute1</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
					</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotAttribute2" NameInEntity="Lot_Attribute2">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotAttribute2</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String"
                            EntityName="Inventory_Tag"
                            Name="LotAttribute3" NameInEntity="Lot_Attribute">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotAttribute3</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
					</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotKeyReference" NameInEntity="Lot_Key_Reference">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotKeyReference</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
					</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotNumber" NameInEntity="Lot_Number">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotNumber</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
								</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Supply"
                            Name="SupplyType" NameInEntity="Supply_Type">
                            <Description>
                                <div>The <code>SupplyType</code> attribute is required when the element <code>ShipNode\Item\Augment</code> is passed. When the <code>SupplyType</code> attribute is passed in the <code>ShipNode\Item\Remove</code> element, it is ignored.
					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="ToETA">
                            <Description>
                                <div>Supply with ETA less than or equals to <b>
                                    <code>ToETA</code>
                                    </b> will be considered. Applicable for element <code>ShipNode\Item\Remove</code>.
					</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                    LotAttribute3="" LotKeyReference="" LotNumber=""
                    ManufacturingDate="" RevisionNo="">
                    <yfc:doc>
                        <Element EntityName="Inventory_Tag"/>
                    </yfc:doc>
                </Tag>
            </SupplyDetails>
        </Remove>
        <Augment yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
			Supplies under <b>
                                <code>Augment</code>
                            </b> will be added as new information into the system. If any supply is found matching the criteria, it will be augmented by the provided quantity.
			</div>
                    </Description>
                </Element>
            </yfc:doc>
            <SupplyDetails Currency="" ETA="Required" FromETA=""
                LotAttribute1="" LotAttribute2="" LotAttribute3=""
                LotKeyReference="" LotNumber="" ManufacturingDate=""
                Quantity="" Segment="" SegmentType="" ShipByDate=""
                SupplyReference="" SupplyReferenceType=""
                SupplyType="Required" ToETA="" UnitCost="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply"/>
                    <Attributes>
                        <Attribute EntityName="Inventory_Pending_Adj"
                            Name="Currency" NameInEntity="Currency">
                            <Description>
                                <div>
                                    <code>Currency</code> is only applicable if the node that the adjustment is made for has <code>MaintainInventoryCost</code> set to <b>Y</b>.  
						</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Supply"
                            Name="ETA" NameInEntity="ETA">
                            <Description>
                                <div>The <code>ETA</code> attribute is required when the element <code>ShipNode\Item\Augment</code> is passed. When the <code>ETA</code> attribute is passed in the <code>ShipNode\Item\Remove</code> element, it is ignored.
					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="FromETA">
                            <Description>
                                <div>Supply with ETA greater than or equals to <b>
                                    <code>FromETA</code>
                                    </b> will be considered. Applicable for element <code>ShipNode\Item\Remove</code> .
					</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotAttribute1" NameInEntity="Lot_Attribute1">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotAttribute1</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
								</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotAttribute2" NameInEntity="Lot_Attribute2">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotAttribute2</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String"
                            EntityName="Inventory_Tag"
                            Name="LotAttribute3" NameInEntity="Lot_Attribute">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotAttribute3</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
								</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotKeyReference" NameInEntity="Lot_Key_Reference">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotKeyReference</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
					</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotNumber" NameInEntity="Lot_Number">
                            <Description>
                                <div>
                                    <b>
                                    <code>LotNumber</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String"
                            EntityName="Inventory_Tag"
                            Name="ManufacturingDate" NameInEntity="Manufacturing_Date">
                            <Description>
                                <div>
                                    <b>
                                    <code>ManufacturingDate</code>
                                    </b> should not be passed here.  It should be passed in the <b>
                                    <code>Tag</code>
                                    </b> element instead.  
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="ToETA">
                            <Description>
                                <div>Supply with ETA less than or equals to <b>
                                    <code>ToETA</code>
                                    </b> will be considered. Applicable for element <code>ShipNode\Item\Remove</code> .
					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String"
                            EntityName="Inventory_Supply"
                            Name="UnitCost" NameInEntity="Unit_Cost">
                            <Description>
                                <div>Not used
					</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                    LotAttribute3="" LotKeyReference="" LotNumber=""
                    ManufacturingDate="" RevisionNo=""/>
            </SupplyDetails>
        </Augment>
    </Item>
</ShipNode>
