<ShipNode MinimumNotificationTime="" OrderReference="" ShipNode="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getNodeMinimumNotificationTimeUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="NumberOfDays" Name="MinimumNotificationTime">
                <Description>
                    <div>
				The number of working hours for regular orders before which the communication to ship the order to the shipping node or supplier should be made. This field is used to determine the earliest ship date on the line. 
                </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="OrderReference">
                <Description>
                    <div>
				The unique identifier for the order. The <code>OrderReference</code> is the Order Header Key.
				</div>
                </Description>
            </Attribute>
            <Attribute Name="ShipNode">
                <Description>
                    <div>
				The unique identifier for the ship node. 
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <NodeNotificationList>
        <NodeNotification LevelOfService="" MinimumNotificationTime="">
            <yfc:doc>
                <Attributes>
                    <Attribute Name="MinimumNotificationTime" NameInEntity="MinNotificationTime"/>
                </Attributes>
            </yfc:doc>
        </NodeNotification>
    </NodeNotificationList>
</ShipNode>
