<?xml version="1.0" encoding="UTF-8"?>
<SortingStation Node="Required" SortStationId="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/closeSortingContainers/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Key" EntityName="Ship_Node" Name="Node" NameInEntity="Ship_Node">
                <Description>
                    <div> Node identifier. 
				<Validation>
                            <Condition> Cannot be blank. </Condition>
                            <Error Description="Node not passed." ErrorCode="WMS00054"/>
                        </Validation>
                        <Validation>
                            <Condition> Must be a valid node. </Condition>
                            <Error
                                Description="Node passed does not exist." ErrorCode="WMS00042"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ID-40" Name="SortStationId">
                <Description>
                    <div>The sort station in which the user wants to close the Sorting Containers. </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Containers>
        <Container LPNNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="LPNNo" Name="LPNNo">
                        <Description>
                            <div>The open container identifier which has to be closed. 
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Container>
    </Containers>
</SortingStation>
