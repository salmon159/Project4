<Login ActivateFlag="" BillingAddress_Key="" Business_Key=""
    ChangePasswordLink="" ContactAddress_Key="" DaysUntilExpiration=""
    DisplayUserID="" EnterpriseCode="" ExpiresInDays="" ImageFile=""
    LocaleCode="" LoginID="" LongDesc="" Note_Key="" OrganizationCode=""
    PWDLastChangedOn="" Password="" Preference_Key=""
    RequirePasswordChange="" UserGroupID="" UserGroup_Description=""
    UserGroup_Key="" UserGroup_Name="" UserName="" UserToken=""
    UserType="" User_key="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/login/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="User" Name="ActivateFlag" NameInEntity="Activateflag"/>
            <Attribute EntityName="User" Name="BillingAddress_Key" NameInEntity="Billingaddress_Key"/>
            <Attribute EntityName="User" Name="Business_Key" NameInEntity="Business_Key"/>
            <Attribute DataType="String" Name="ChangePasswordLink">
                <Description>
                    <div>Indicates the URL to change the user password if an authentication to an external system (like an LDAP system) is implemented.
       </div>
                </Description>
            </Attribute>
            <Attribute EntityName="User" Name="ContactAddress_Key" NameInEntity="Contactaddress_Key"/>
            <Attribute DataType="String" Name="DaysUntilExpiration">
                <Description>
                    <div>Indicates the number of days in which the password will expire based on password policy.
       </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-150" EntityName="User"
                Name="DisplayUserID" NameInEntity="Display_User_ID"/>
            <Attribute DataType="Text-24" EntityName="User"
                Name="EnterpriseCode" NameInEntity="Enterprise_Code"/>
            <Attribute DataType="String" Name="ExpiresInDays">
                <Description>
                    <div>Indicates the number of days in which the password will expire if an authentication to an external system (like an LDAP system) is implemented.
       </div>
                </Description>
            </Attribute>
            <Attribute EntityName="User" Name="ImageFile" NameInEntity="Imagefile"/>
            <Attribute EntityName="User" Name="LocaleCode" NameInEntity="Localecode"/>
            <Attribute EntityName="User" Name="LoginID" NameInEntity="Loginid"/>
            <Attribute EntityName="User" Name="LongDesc" NameInEntity="Longdesc"/>
            <Attribute EntityName="User" Name="Note_Key" NameInEntity="Note_Key"/>
            <Attribute EntityName="User" Name="OrganizationCode" NameInEntity="Organization_Key"/>
            <Attribute EntityName="User" Name="PWDLastChangedOn" NameInEntity="Pwdlastchangedon"/>
            <Attribute EntityName="User" Name="Password"/>
            <Attribute EntityName="User" Name="Preference_Key" NameInEntity="Preference_Key"/>
            <Attribute DataType="String" Name="RequirePasswordChange">
                <Description>
                    <div>Indicates if password is required to be changed based on password policy.
       </div>
                </Description>
            </Attribute>
            <Attribute EntityName="User_Group" Name="UserGroupID" NameInEntity="Usergroup_Id"/>
            <Attribute EntityName="User_Group"
                Name="UserGroup_Description" NameInEntity="Usergroup_Description"/>
            <Attribute EntityName="User_Group" Name="UserGroup_Key" NameInEntity="Usergroup_Key"/>
            <Attribute EntityName="User_Group" Name="UserGroup_Name" NameInEntity="Usergroup_Name"/>
            <Attribute EntityName="User" Name="UserName" NameInEntity="Username"/>
            <Attribute EntityName="User_Activity" Name="UserToken" NameInEntity="Session_Id"/>
            <Attribute EntityName="User" Name="UserType" NameInEntity="Usertype"/>
            <Attribute EntityName="User" Name="User_key" NameInEntity="User_Key"/>
        </Attributes>
    </yfc:doc>
    <!--The PasswordPolicyResult element will contain Password Policy Result attributes. -->
    <PasswordPolicyResult/>
</Login>
