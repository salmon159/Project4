<NodeNotificationPeriod EffectiveFromDate="" EffectiveToDate=""
    NodeNotificationPeriodKey=""
    Operation="Manage|Create|Modify|Delete|" OrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageNodeNotificationPeriod/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="YSCOperation" Name="Operation">
                <Description>The operation to perform. The default behavior is <code>Manage</code>.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <NodeNotificationList Reset="N|Y">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>
                        <Div>Removes all <code>NodeNotification</code>s except those passed in the input.</Div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <NodeNotification AdvanceNotificationTime="" LevelOfService=""
            MaximumNotificationTime="" MinNotificationTime=""
            NodeNotificationKey=""
            Operation="Manage|Create|Modify|Delete|" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>This table stores ship node specific notifications required for a 
		    			given ship node within an effective date range. Either NodeNotificationKey or 
		    			LevelOfService uniquely identifies a NodeNotification.
			    		<Validation>
                                <Condition>
                                    <code>NodeNotification</code> could not be found with <code>Operation</code>
			    			equal to <code>Delete</code> or <code>Modify</code>.</Condition>
                                <Error Description="No record found." ErrorCode="YFS10001"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="YSCOperation" Name="Operation">
                        <Description>The operation to perform. The default behavior is <code>Manage</code>.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <NodeNotificationScheduleList Reset="N|Y">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Reset">
                            <Description>Determines whether to remove all <code>NodeNotificationSchedule</code>s
		    				and replace them with those passed in the input XML. Default: N</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <NodeNotificationSchedule FridayValid="" MondayValid=""
                    NodeNotificationDowKey="" NotificationTime=""
                    Operation="Manage|Create|Modify|Delete|"
                    SaturdayValid="" SundayValid="" ThursdayValid=""
                    TuesdayValid="" WednesdayValid="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="YSCOperation" Name="Operation">
                                <Description>The operation to perform. The default behavior is <code>Manage</code>.</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </NodeNotificationSchedule>
            </NodeNotificationScheduleList>
        </NodeNotification>
    </NodeNotificationList>
</NodeNotificationPeriod>
