<Order DocumentType="" EnterpriseCode="" LastOrderLineKey=""
    LocalShipNode="" MaximumRecords="" OrderHeaderKey="" OrderNo=""
    StatusModificationType="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/getSalesOrderDetailsWithLocalQty/input">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
                <div>
			        The <code>OrderHeaderKey</code> attribute <i>or</i> the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed to identify the Order.
			 		<Validation>
                        <Condition>The order cannot be identified.</Condition>
                        <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Order_Line" Name="LastOrderLineKey" NameInEntity="Order_Line_Key">
                <Description>
                    <div>The order line key to begin retreiving records from. Used in conjunction with <code>MaximumRecords</code>.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-24" Name="LocalShipNode">
                <Description>
                    <div>The Ship Node of the logged in user.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>
                    <div>The number of order lines to be returned.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="StatusModificationType">
                <Description>
                    <div>
                		Status Modification Type.
                		<Validation>
                            <Condition>If Status Modification is not available for the ORDER_LINE Modification Level.</Condition>
                            <Error
                                Description="Setup for order modification permissions missing." ErrorCode="YFS10136"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SpecificOrderLines>
        <OrderLine OrderLineKey="" PrimeLineNo="" SubLineNo="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>Identifies a specific order line to be included in the output of the API.</div>
                    </Description>
                </Element>
            </yfc:doc>
        </OrderLine>
    </SpecificOrderLines>
</Order>
