<?xml version="1.0" encoding="UTF-8"?>
<LPN CaseId="" EnterpriseCode="" FreezeOnVariance="" Height=""
    IsOutboundContainer="" LPNHdrKey="" Length="" MaximumRecords=""
    OuterMostCaseId="" OuterMostPalletId="" PalletId="" ParentCaseId=""
    ParentPalletId="" ReceiptHeaderKey="" Segment="" SegmentType=""
    Weight="" Width="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getLPNList/input">
    <yfc:doc>
        <Element EntityName="LPN_Hdr"/>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>
                    <div>
					Indicates the Id of the Case.
						<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="EnterpriseCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
				Inventory organization code for the Enterprise is obtained from <code>YFS_ORGANIZATION</code>.
					<Validation>
                            <Condition>
                                <code>EnterpriseCode</code> passed but does not exist. 
						</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>
                    <div>
					Specifies maximum number of records to be fetched.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="OuterMostCaseId">
                <Description>
                    <div>
					Indicates the Id of the Outer Most Case.
						<Validation>
                            <Condition>Both <code>OuterMostCaseId</code> and <code>OuterMostPalletId</code> cannot be passed. Error if both <code>OuterMostCaseId</code> and <code>OuterMostPalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="OuterMostPalletId">
                <Description>
                    <div>
					 Indicates the Id of the Outer Most Pallet.
						<Validation>
                            <Condition>Both <code>OuterMostCaseId</code> and <code>OuterMostPalletId</code> cannot be passed. Error if both <code>OuterMostCaseId</code> and <code>OuterMostPalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>
                    <div>
					Indicates the Id of the Pallet.
						<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="ParentCaseId">
                <Description>
                    <div>
					Indicates the Id of the Parent Case.
						<Validation>
                            <Condition>Both <code>ParentCaseId</code> and <code>ParentPalletId</code> cannot be passed. Error if both <code>ParentCaseId</code> and <code>ParentPalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="ParentPalletId">
                <Description>
                    <div>
					Indicates the Id of the Parent Pallet.
						<Validation>
                            <Condition>Both <code>ParentCaseId</code> and <code>ParentPalletId</code> cannot be passed. Error if both <code>ParentCaseId</code> and <code>ParentPalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Receipt ReceiptNo="">
        <yfc:doc>
            <Element EntityName="Receipt_Header"/>
            <Attributes>
                <Attribute EntityName="Receipt_Header" Name="ReceiptNo" NameInEntity="Receipt_No">
                    <Description>
                        <div>
							Indicates the receipt to which <code>LPN</code> belongs.							
							</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Receipt>
    <LPNLocation LocationId="" Node="" ZoneId="">
        <yfc:doc>
            <Element EntityName="LPN_Location"/>
            <Attributes>
                <Attribute EntityName="Location" Name="LocationId" NameInEntity="Location_Id">
                    <Description>
                        <div>
						Location where the <code>LPN</code> is residing.							
						</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="LPN_Location" Name="Node" NameInEntity="Node_Key">
                    <Description>
                        <div>
					The Node in which the <code>LPN</code> exists.						
					</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </LPNLocation>
    <InventoryList>
        <Inventory CountryOfOrigin="" FifoNo="" InventoryStatus=""
            Quantity="" Segment="" SegmentType="" ShipByDate="">
            <yfc:doc>
                <Element EntityName="LPN_Dtl"/>
            </yfc:doc>
        </Inventory>
    </InventoryList>
</LPN>
