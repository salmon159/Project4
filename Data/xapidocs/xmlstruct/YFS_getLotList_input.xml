<Tag AvailableTag="" BatchNo="" InventoryItemKey="" InventoryTagKey=""
    LotAttribute1="" LotAttribute2="" LotAttribute3=""
    LotKeyReference="" LotNumber="" ManufacturingDate=""
    MaximumRecords="" RevisionNo="" TotalDemand="" TotalOnhandSupply=""
    TotalOtherSupply="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getLotList/input">
    <yfc:doc>
        <Element EntityName="Inventory_Tag">
            <Description>
                <div>
				Stores tag information for inventory. This is a mandatory element.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Flag" Name="AvailableTag">
                <Description>
                    <div>
					Valid Value: <b>
                            <code>Y</code>
                        </b>. If <code>AvailableTag</code> is passed as <code>
                            <b>Y</b>
                        </code>, then only Tag records, for which (<code>TotalOnhandSupply</code> - <code>TotalDemand</code>) &gt; 0, are returned in the output. If this attribute is passed with any value other than <b>
                            <code>Y</code>
                        </b>, it is ignored.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>
                    <div>
					Maximum number of records to be returned.  
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <InventoryItem InventoryItemKey="" ItemID=""
        OrganizationCode="Required" ProductClass="" UnitOfMeasure="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Inventory_Item">
                <Description>
                    <div>
				The primary inventory table defining all inventory items. This is a mandatory element.
				</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="OrgCode"
                    EntityName="Inventory_Item" Name="OrganizationCode" NameInEntity="InventoryOrganizationCode"/>
            </Attributes>
        </yfc:doc>
    </InventoryItem>
</Tag>
