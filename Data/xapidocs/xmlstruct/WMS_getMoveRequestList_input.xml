        <Attributes>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>Specifies maximum number of records that must be fetched.</Description>
            </Attribute>
            <Attribute DataType="ShipNode" Name="Node">
                <Description>
                    <div>Primary key attribute of YFS_SHIP_NODE table. This is the Node where move request would be executed. 
								<Validation>
                            <Condition> Node must be passed.
									</Condition>
                            <Error Description="Node not passed." ErrorCode="WMS00054"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Shipment SellerOrganizationCode="" ShipNode="" ShipmentNo="">
