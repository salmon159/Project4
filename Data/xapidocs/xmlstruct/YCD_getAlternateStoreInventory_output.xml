    <OrderLines>
        <OrderLine GiftWrap="" LevelOfService="" OrderLineKey=""
            ProductClass="" RequiredQty="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Quantity" Name="RequiredQty">
                        <Description>
                            <div>
	Required Quantity for the order line.  Is mandatory for <code>Mode="03"</code>, is ignored for <code>Mode="02"</code>, and is used to override the OrderedQty on an OrderLine for <code>Mode="01"</code>.
</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Item ExtendedDisplayDescription="" ItemID="" UnitOfMeasure=""/>
            <AvailabilityList>
                <Availability AvailableDate="" AvailableQty=""
                    IsAvailable="" ShipNode="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Date" Name="AvailableDate">
                                <Description>
                                    <div>
					This will be returned when <code>Mode="01"</code> or <code>Mode="03"</code> when there in sufficient inventory available, and in <code>Mode="02"</code> when there is only future inventory available at the node.
				</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="AvailableQty">
                                <Description>
                                    <div>
					This will be returned when <code>Mode="02"</code> and will identify the on hand inventory if no AvailableDate is return, otherwise this will be the future inventory for that date.
				</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Flag" Name="IsAvailable">
                                <Description>
                                    <div>
					This flag will consistently display if the requested item is available at the provided node or not regardless of the API used to derive the information.
				</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Ship_Node"
                                Name="ShipNode" NameInEntity="Ship_Node"/>
                        </Attributes>
                    </yfc:doc>
                </Availability>
            </AvailabilityList>
        </OrderLine>
