                <Element EntityName="Ship_Node"/>
                <Attributes>
                    <Attribute DataType="Number-22" Name="DistanceFromShipToAddress">
                        <Description>
                            <div>
				It is returned in <code>DistanceToConsiderUOM</code> specified in input xml. If <code>DistanceToConsiderUOM</code> is blank then <code>DistanceFromShipToAddress</code> is in MILE.
			</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="UOM" Name="DistanceUOM">
                        <Description>
                            <div>
					Specifies the UOM for <code>DistanceFromShipToAddress</code>.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasEffectiveNodeLevelInventoryControls">
                        <Description>
                            <div>
								Having value 'Y' indicates that at least one Node level Inventory control is applicable on the Node.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
