<!-- YCP_modifyCache_input.xml -->
<CachedGroups ServerId="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/modifyCache/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Key" Name="ServerId">
                <Description>
				The server identifier. This can be obtained by the <code>getServerList</code> api.
				If this attribute is not provided, then the cache modification actions will be broadcast to
				all servers.
				</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <CachedGroup Action="" Name="Required">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Text-40" Name="Action">
                    <Description>
                        <div>
							The action to perform.
							<br/>Valid values at this level: <b>
                                <code>CLEAR</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-200" Name="Name">
                    <Description>The name of the cached group.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <CachedObject Action="" Class="Required" Enabled="" Limit=""
            SizeLimit="" isSizeBasedAlgo="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="Action">
                        <Description>
                            <div>
							The action to perform.
							<br/>Valid values at this level: <b>
                                    <code>CLEAR</code>
                                </b> or <b>
                                    <code>MODIFY</code>
                                </b>.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-200" Name="Class">
                        <Description>The cached object class name.</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="Enabled">
                        <Description>
                            <div>
							Indicates if cache needs to be enabled for this class.
							<br/>Valid values are: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>.
							<br/>If <code>Action</code> is <b>
                                    <code>MODIFY</code>
                                </b> and <code>Enabled</code> is not <b>
                                    <code>Y</code>
                                </b>, the API defaults <code>Enabled</code> to <b>
                                    <code>N</code>
                                </b>.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="CacheLimit" Name="Limit">
                        <Description>The number of rows to cache.</Description>
                    </Attribute>
                    <Attribute DataType="CacheLimit" Name="SizeLimit">
                        <Description>The size of objects in kbytes to cache.</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="isSizeBasedAlgo">
                        <Description>
                            <div>
						Indicates if size based caching needs to be enabled for this class.
						<br/>Valid values are: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>.
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </CachedObject>
    </CachedGroup>
</CachedGroups>
