<AnswerSets xmlns="http://www.sterlingcommerce.com/documentation/MasterAnswerSetTranInput">
    <AnswerSet PersonInfoKey="" QuestionType="Required" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element EntityName="Answer_Set_Tran"/>
            <Attributes>
                <Attribute EntityName="Person_Info" Name="PersonInfoKey" NameInEntity="Person_Info_Key">
                    <Description>The person info record to store this answer set for.</Description>
                </Attribute>
                <Attribute EntityName="Question" Name="QuestionType" NameInEntity="Question_Type">
                    <Description>
                        <div>The question type associated to the defined answer set. 
						<Validation>
                                <Condition>
                                    <code>QuestionType</code> not passed.
								</Condition>
                                <Error
                                    Description="Mandatory attributes for answer set are missing." ErrorCode="YCP73_00005"/>
                            </Validation>
                            <Validation>
                                <Condition>Invalid <code>QuestionType</code>.
								</Condition>
                                <Error
                                    Description="Invalid Question Attribute." ErrorCode="YCP73_00006"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Answers>
            <Answer AnswerValue="" QuestionID="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Answer_Tran"/>
                    <Attributes>
                        <Attribute Name="AnswerValue">
                            <Description>
                                <div>The answer for the question.
								<Validation>
                                    <Condition>
                                    <code>AnswerValue</code> passed should be a valid answer for the question it belongs to.</Condition>
                                    <Error
                                    Description="Answer is not one of the valid pre-defined answer options." ErrorCode="YCP73_00007"/>
                                    <Error
                                    Description="Answer for this question should be an integer number." ErrorCode="YCP73_00020"/>
                                    <Error
                                    Description="Answer for this question should be a decimal number." ErrorCode="YCP73_00021"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute Name="QuestionID">
                            <Description>
                                <div>Identifier for the question that this answer is for.
								<Validation>
                                    <Condition>
                                    <code>QuestionID</code> should be valid.</Condition>
                                    <Error
                                    Description="Invalid Question Attribute." ErrorCode="YCP73_00006"/>
                                    <Error
                                    Description="Question should only be answered when parent answer option is selected." ErrorCode="YCP73_00009"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Answer>
        </Answers>
    </AnswerSet>
</AnswerSets>
