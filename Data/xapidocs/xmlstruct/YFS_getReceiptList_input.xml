<!--getReceiptList Input XML-->
<Receipt ArrivalDateTime="" DocumentType="" DriverName=""
    EnterpriseCode="" MaximumRecords="" NumOfCartons="" NumOfPallets=""
    OpenReceiptFlag="" ReadFromHistory="" ReceiptDate=""
    ReceiptHeaderKey="" ReceiptNo="" ReceivingDock="" ReceivingNode=""
    ShipmentKey="" Status="" TrailerLPNNo="" isHistory="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getReceiptList/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="MasterReceipt.xml" IncludeXMLPath="/Receipt"/>
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>Specifies the Maximum Number of records to be returned. Default is 500.</Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="ReadFromHistory">
                <Description>
                    <div>
                    Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
                    <br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_RECEIPT_HEADER table.
                    <br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_RECEIPT_HEADER_H table (the history table).
                    <br/>
                        <b>
                            <code>B</code>
                        </b> - Returns records that satisfy the input criteria from both YFS_RECEIPT_HEADER and YFS_RECEIPT_HEADER_H tables. Records returned from the current table are first in the output.
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="isHistory"/>
        </Attributes>
    </yfc:doc>
    <ReceiptLines>
        <ReceiptLine AvailableForTranQuantity="" BatchNo="" CaseId=""
            CountryOfOrigin="" DispositionCode="" DispositionLocation=""
            EnterpriseCode="" FifoNo="" InspectedBy=""
            InspectionComments="" InspectionDate=""
            IsInspectionPending="" ItemID="" LocationId=""
            LotAttribute1="" LotAttribute2="" LotAttribute3=""
            LotKeyReference="" LotNumber="" ManufacturingDate=""
            NetWeight="" NetWeightUom="" OrderHeaderKey=""
            OrderLineKey="" OrderNo="" OrderReleaseKey=""
            OrigReceiptLineKey="" OriginalQuantity="" PalletId=""
            PrimeLineNo="" ProductClass="" Quantity=""
            ReceiptHeaderKey="" ReceiptLineKey="" ReceiptLineNo=""
            ReleaseNo="" RevisionNo="" SerialNo="" ShipByDate=""
            ShipmentContainerKey="" ShipmentLineKey="" ShipmentLineNo=""
            ShipmentSubLineNo="" SubLineNo="" TransactionId=""
            TransactionName="" UnitOfMeasure="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterReceiptForInput.xml" IncludeXMLPath="/Receipt/ReceiptLines/ReceiptLine"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="LPNNo" Name="CaseId">
                        <Description> A CaseId. </Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="PalletId">
                        <Description> A PalletId. </Description>
                    </Attribute>
                    <Attribute EntityName="Shipment_Line"
                        Name="ShipmentSubLineNo" NameInEntity="ShipmentSubLineNo">
                        <Description> Part of Unique Key of ShipmentLine entity. </Description>
                    </Attribute>
                    <Attribute EntityName="Transaction"
                        Name="TransactionId" NameInEntity="Tranid"/>
                </Attributes>
            </yfc:doc>
        </ReceiptLine>
    </ReceiptLines>
    <Shipment ActualDeliveryDate="" ActualFreightCharge=""
        ActualShipmentDate="" AppointmentNo="" AssociationAction=""
        BolNo="" BuyerOrganizationCode="" CarrierAccountNo=""
        CarrierServiceCode="" CarrierType="" Code="" CommercialValue=""
        Currency="" DeliveryPlanKey="" DeliveryTS="" DestinationZone=""
        DocumentType="" DownLoadCount="" EnterpriseCode=""
        ExpectedDeliveryDate="" ExpectedShipmentDate=""
        ExportTaxpayerID="" FreightTerms="" FromAddressKey=""
        FromAppointment="" HasOtherShipments="" HazardousMaterialFlag=""
        HeldForConfirmation="" ITDate="" ITNo="" InvoiceComplete=""
        IsSingleOrder="" LevelOfService="" LinesEntered=""
        ManifestKey="" ManifestNo="" ManuallyEntered="" MergeNode=""
        NumOfCartons="" NumOfPallets="" OrderAvailableOnSystem=""
        OrderHeaderKey="" OrderNo="" OrderReleaseKey="" OriginZone=""
        OverrideManualShipmentEntry="" PackListType=""
        ParentShipmentKey="" PickListNo="" PickticketNo=""
        PipelineKey="" PodNo="" ProNo="" ReceivingNode="" ReleaseNo=""
        RequestedDeliveryDate="" RequestedShipmentDate=""
        RequiresApptConfirmation="" SCAC="" ScacIntegrationRequired=""
        SealNo="" SellerOrganizationCode="" ShipDate="" ShipMode=""
        ShipNode="" ShipVia="" ShipmentClosedFlag=""
        ShipmentConfirmUpdatesDone="" ShipmentContainerizedFlag=""
        ShipmentDeliverUpdatesDone="" ShipmentKey="" ShipmentNo=""
        ShipmentPlannedFlag="" ShipmentType="" Status="" StatusDate=""
        ToAppointment="" TotalActualCharge="" TotalEstimatedCharge=""
        TotalVolume="" TotalVolumeUOM="" TotalWeight=""
        TotalWeightUOM="" TrackingNo="" TrailerNo="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Shipment_tran.xml" IncludeXMLPath="/Shipment"/>
        <yfc:doc>
            <Element EntityName="Shipment"/>
            <Attributes>
                <Attribute DataType="String" Name="HeldForConfirmation"/>
            </Attributes>
        </yfc:doc>
    </Shipment>
    <Order FromOrderNo="" OrderHeaderKey="" OrderNo="" OrderNoQryType=""
        OrderReleaseKey="" ReleaseNo="" ToOrderNo="">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
						To identify the order that receipts belong to, pass either the <code>OrderHeaderKey</code> or  <code>OrderNo</code>.
						<br/>To identify the release that receipts belong to, pass either the <code>OrderReleaseKey</code>, a combination of <code>ReleaseNo</code> and <code>OrderHeaderKey</code>, or a combination of <code>OrderNo</code> and <code>ReleaseNo</code>.
						<Validation>
                            <Condition>If <code>ReleaseNo</code> is passed along with <code>OrderNoQryType</code>
                            </Condition>
                            <Error Description="YFS:Invalid XML." ErrorCode="YFS10013"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>ReleaseNo</code> is passed and either <code>OrderNo</code> or <code>OrderHeaderKey</code> is not passed.
							</Condition>
                            <Error Description="YFS:Invalid XML." ErrorCode="YFS10013"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>OrderNoQryType</code> is passed and <code>OrderNo</code>is not passed.
							</Condition>
                            <Error Description="YFS:Invalid XML." ErrorCode="YFS10013"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Order_Release"
                    Name="OrderReleaseKey" NameInEntity="Order_Release_Key"/>
                <Attribute EntityName="Order_Release" Name="ReleaseNo" NameInEntity="Release_No"/>
            </Attributes>
        </yfc:doc>
    </Order>
</Receipt>
