<!-- Any xml may be used, as long as the root element contains the XrefData child element -->
<RootElement xmlns="http://www.sterlingcommerce.com/documentation/YIF/executeXReferenceTransform/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
            Any XML may be used as input, but the input document root node should contain a node <code>XrefData</code> with <code>XmlName</code> as the attribute.
			</div>
            </Description>
        </Element>
    </yfc:doc>
    <XrefData XmlName="Required" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Text-40" Name="XmlName">
                    <Description>
                        <div>
                Uniquely identifies a combination of From System, From Organization, To System and To Organization.
                </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </XrefData>
</RootElement>
