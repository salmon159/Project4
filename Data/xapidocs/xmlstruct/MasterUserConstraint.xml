<User OrganizationKey="" UserId="" UserKey="" xmlns="http://www.sterlingcommerce.com/documentation/MasterUserConstraint">
    <yfc:doc>
        <Element EntityName="User"/>
        <Attributes>
            <Attribute EntityName="User" Name="UserId" NameInEntity="Loginid"/>
        </Attributes>
    </yfc:doc>
    <UserTaskTypeList>
        <UserTaskType TaskType="" TaskTypePriority="" yfc:DTDOccurrence="ZeroOrMany">
            <TaskType ActivityCode="" ActivityGroupId=""
                BatchRequired="" BatchRuleKey="" ExecTasksThruDropoff=""
                HoldTaskOnCreation="" IsAdHocMove="" MaxUsersAllowed=""
                Node="" OrganizationCode=""
                PickIntoInventoryContainer=""
                PickIntoOutboundContainer="" PrimaryTaskReference=""
                ProductivityType="" ResourceIdOverride=""
                SourceTaskOrderBy="" SourceTaskSuggestion=""
                StandardRate="" StandardRateUom="" TargetTaskOrderBy=""
                TargetTaskSuggestion="" TaskType="" TaskTypeKey=""
                TaskTypeName="" TaskTypePriority="" VoiceWorkFlowId="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="MasterTaskType.xml" IncludeXMLPath="/TaskType"/>
            </TaskType>
        </UserTaskType>
    </UserTaskTypeList>
    <UserZoneList>
        <UserZone Aisle="" Node="" RecordType="" ZoneId="" yfc:DTDOccurrence="ZeroOrMany"/>
    </UserZoneList>
    <UserTranConstraintList>
        <UserTranConstraint ActiveFlag="" AssignedToUserId="" BatchNo=""
            ContainerNo="" CountRequestNo="" EnterpriseKey=""
            MoveRequestNo="" OrganizationCode="" ReceiptNo=""
            ShipmentNo="" TaskType="" UserTranConstraintsKey=""
            WaveNo="" WorkOrderNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterUserTranConstraint.xml" IncludeXMLPath="/UserTranConstraint"/>
        </UserTranConstraint>
    </UserTranConstraintList>
    <MostRecentTask AssignedToUserId="" AssignmentNo=""
        AssignmentStatus="" BatchHold="" DependencyHold=""
        EndTimeStamp="" EnterpriseKey="" EquipmentId=""
        EquipmentLocationId="" ExceptionHold="" FinishNoLaterThan=""
        HoldReasonCode="" HoldReasonText="" IsSummaryTask="" Node=""
        OrganizationCode="" ParentTaskId="" PredecessorTaskId=""
        ProductivityKey="" ReasonCode="" ReasonText="" RootTaskId=""
        SourceAisle="" SourceLocationId="" SourceSortSequence=""
        SourceZoneId="" StartNoEarlierThan="" StartTimeStamp=""
        TargetAisle="" TargetCaseId="" TargetLocationId=""
        TargetPalletId="" TargetSortSequence="" TargetZoneId=""
        TaskId="" TaskKey="" TaskPriority="" TaskStatus="" TaskType="" isHistory="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="MasterTask.xml" IncludeXMLPath="/Task"/>
        <yfc:doc>
            <Element EntityName="Task"/>
            <Attributes>
                <Attribute DataType="LPNNo" Name="TargetCaseId" NameInEntity="Target_LPN_No">
                    <Description>
                        <div>Target Case Id of the task</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="TargetPalletId" NameInEntity="Target_LPN_No">
                    <Description>
                        <div>Target Pallet Id of the task</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="isHistory">
                    <Description>
                        <div>returns <code>'Y'</code>: if task is purged and exists in history table.<br/>
				returns <code>'N'</code>: if task is not purged.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </MostRecentTask>
</User>
