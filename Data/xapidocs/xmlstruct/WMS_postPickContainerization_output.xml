<?xml version="1.0" encoding="UTF-8"?>
<ContainerizeShipmentLines BatchNo="" LocationId="" Node=""
    SellerOrganizationCode="" ShipmentKey="" ShipmentNo="" StationId=""
    TargetLPNNo="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/postPickContainerization/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="ID-40" EntityName="Task" Name="BatchNo" NameInEntity="Batch_No">
                <Description>
                    <div>
				Batch # whose shipment lines needs to be containerized.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="LocationId" EntityName="Location"
                Name="LocationId" NameInEntity="Location_Id">
                <Description>
                    <div>A valid location in the Node</div>
                </Description>
            </Attribute>
            <Attribute DataType="ShipNode" EntityName="Ship_Node"
                Name="Node" NameInEntity="Ship_Node">
                <Description>
                    <div>
				Ship Node corresponding to the specified Batch # or Inventory Pallet Id.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" EntityName="Shipment"
                Name="SellerOrganizationCode" NameInEntity="Seller_Organization_Code">
                <Description>
                    <div>
				Seller Organization Code of the shipment that needs to be containerized.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Shipment"
                Name="ShipmentKey" NameInEntity="Shipment_Key">
                <Description>
                    <div>
				Shipment key of the shipment that needs to be containerized.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" EntityName="Shipment"
                Name="ShipmentNo" NameInEntity="Shipment_No">
                <Description>
                    <div>
				Shipment # of the shipment that needs to be containerized.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="EquipmentId" EntityName="Equipment"
                Name="StationId" NameInEntity="Equipment_Id">
                <Description>
                    <div>Station associated to pack location</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" EntityName="Task"
                Name="TargetLPNNo" NameInEntity="Target_LPN_No">
                <Description>
                    <div>
				Inventory Pallet Id whose shipment lines need to be containerized.
			</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</ContainerizeShipmentLines>
