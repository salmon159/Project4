<!-- getResourcePoolCapacity output XML -->
<Node EndDate="" Node="" StartDate="" TransactInWorkDay="" xmlns="http://www.sterlingcommerce.com/documentation/INV/getResourcePoolCapacity/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Date" Name="EndDate">
                <Description>End Date of the date range.</Description>
            </Attribute>
            <Attribute EntityName="Res_Pool" Name="Node" NameInEntity="Node_Key">
                <Description>
                    <div>
					The node associated with the resource pool.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Date" Name="StartDate">
                <Description>Start Date of the date range.</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="TransactInWorkDay">
                <Description>
                    <div>
					The flag will be set to <code>
                            <b>Y</b>
                        </code> only if the flag is passed as  <code>
                            <b>Y</b>
                        </code> in the input.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ResourcePools>
        <ResourcePool AllRegionsServiced="" CalendarKey=""
            CapacityInfoAvailable="" CapacityMaintainAtResourceLevel=""
            CapacityOrganizationCode="" CapacityUnitOfMeasure=""
            ItemGroupCode="" Node="" ProviderContactAddressKey=""
            ProviderOrganizationCode="" ProvidesAllServices=""
            RegionSchemaKey="" ResourcePoolDescription=""
            ResourcePoolId="" ResourcePoolKey="" ServiceSlotGroupId=""
            ServiceSupervisorId="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute Name="CapacityOrganizationCode">
                        <Description>
                            <div>
							A Resource Pool is always created wihin the context of a Capacity Organization. An Organization can source services from any Resource Pools within the same Capacity Organization.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="Node">
                        <Description>
                            <div>
							The node associated with the resource pool.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ResourcePoolAttributeList>
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="MasterResourcePool.xml" IncludeXMLPath="/ResourcePool/ResourcePoolAttributeList"/>
                <ResourcePoolAttribute DeliveryMethod=""
                    ResourcePoolAttributeKey="" ResourcePoolKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Res_Pool_Attr">
                            <Description>
                                <div>
						 	This element defines attributes and their values for the resource pool.
						</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="String" Name="DeliveryMethod">
                                <Description>
                                    <div>
								The delivery method for which the resource pool is defined. <br/>
								Valid values: <code>SHP</code> - Shipment, <code>PICK</code> - Pickup by user
							</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </ResourcePoolAttribute>
            </ResourcePoolAttributeList>
            <ServiceSlots>
                <ServiceSlot CanUseForAppointment="" EndTime=""
                    ParentSlotKey="" ServiceSlotDesc=""
                    ServiceSlotGroupKey="" ServiceSlotKey=""
                    StartTime="" yfc:DTDOccurrence="ZeroOrMany">
                    <ChildServiceSlotList>
                        <ServiceSlot CanUseForAppointment=""
                            Createprogid="" Createts="" Createuserid=""
                            EndTime="" Lockid="" Modifyprogid=""
                            Modifyts="" Modifyuserid="" ParentSlotKey=""
                            ServiceSlotDesc="" ServiceSlotGroupKey=""
                            ServiceSlotKey="" StartTime="" yfc:DTDOccurrence="ZeroOrMany"/>
                    </ChildServiceSlotList>
                    <Dates>
                        <Date AllocatedConsumption="" Availability=""
                            Capacity="" CapacityUnitOfMeasure=""
                            ComputedCapacityFromResources="" Date=""
                            DayOfWeek="" IsCapacityOverridden=""
                            IsEffectiveDateRangeConfigured=""
                            IsWorkingDay="" OverrideReason=""
                            StandardCapacity=""
                            StandardSupplementalCapacity=""
                            SupplementalCapacity="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Element>
                                    <Description>
                                    <div>This element shows the capacity picture on a particular date and for a particular unit of measure.</div>
                                    </Description>
                                </Element>
                                <Attributes>
                                    <Attribute DataType="Capacity" Name="AllocatedConsumption">
                                    <Description>Allocated Consumption of the resource pool for the date.  For the resource pool which maintains capacity at resource level, this is the sum of allocated consumptions of the resources.</Description>
                                    </Attribute>
                                    <Attribute DataType="Capacity" Name="Availability">
                                    <Description>Availability of the resource pool for the date.  If the <code>ConsiderSupplementalCapacity</code> flag is passed as <b>
                                    <code>Y</code>
                                    </b> in the input, supplemental capacity is considered in the <code>Availability</code>.  For the resource pool which maintains capacity at resource level, this is the sum of the availabilities of the resources.</Description>
                                    </Attribute>
                                    <Attribute DataType="Capacity" Name="Capacity">
                                    <Description>Capacity of the resource pool for the date.  For the resource pool which maintains capacity at resource level, this is the sum of capacities of the resources.</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-24" Name="CapacityUnitOfMeasure">
                                    <Description>The capacity unit of measure in which the capacities are defined.</Description>
                                    </Attribute>
                                    <Attribute DataType="Capacity" Name="ComputedCapacityFromResources">
                                    <Description>
                                    <div>
											Sum of capacity from each service resource associated with the resource pool in the slot for the date.
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Date" Name="Date">
                                    <Description>Date.</Description>
                                    </Attribute>
                                    <Attribute DataType="NumberOfDays" Name="DayOfWeek">
                                    <Description>Day of the week.</Description>
                                    </Attribute>
                                    <Attribute DataType="Flag" Name="IsCapacityOverridden">
                                    <Description>Indicate if the Capacity is the overridden capacity for the date.</Description>
                                    </Attribute>
                                    <Attribute DataType="Flag" Name="IsEffectiveDateRangeConfigured">
                                    <Description>Indicates if the date falls within the <code>EffectiveDateRange</code> configured by the resource pool. If the date is not a working day, then this flag is defaulted to <code>N</code>.</Description>
                                    </Attribute>
                                    <Attribute DataType="Flag" Name="IsWorkingDay">
                                    <Description>Indicate if the date is a working day.  For the resource pool which defines resources, <code>IsWorkingDay</code> will be set to <code>
                                    <b>Y</b>
                                    </code>, if at least one of the resource is working on this day. </Description>
                                    </Attribute>
                                    <Attribute DataType="Text-200" Name="OverrideReason">
                                    <Description>The reason this capacity was overridden.</Description>
                                    </Attribute>
                                    <Attribute DataType="Capacity" Name="StandardCapacity">
                                    <Description>Standard Capacity of the resource pool for the date.  For the resource pool which maintains capacity at resource level, <code>StandardCapacity</code> will be 0. </Description>
                                    </Attribute>
                                    <Attribute DataType="Capacity" Name="StandardSupplementalCapacity">
                                    <Description>The standard supplemental capacity of the resource pool for the date.  For the resource pool which maintains capacity at resource level, <code>StandardSupplementalCapacity</code> will be 0. </Description>
                                    </Attribute>
                                    <Attribute DataType="Capacity" Name="SupplementalCapacity">
                                    <Description>The supplemental capacity of the resource pool for the date.  For the resource pool which maintains capacity at resource level, <code>SupplementalCapacity</code> will be 0. </Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                        </Date>
                    </Dates>
                </ServiceSlot>
            </ServiceSlots>
        </ResourcePool>
    </ResourcePools>
</Node>
