<!--getReturnPolicy Output XML-->
<ReturnPolicy xmlns="http://www.sterlingcommerce.com/documentation/YCD/getReturnPolicy/output">
    <OrderLines>
        <OrderLine CanOverridePolicy="" IsReceiptExpected=""
            IsReturnable="" ItemGroupCode="" OrderLineKey=""
            OrderedQty="" PrimeLineNo="" ReturnPolicy=""
            ReturnPolicyUrl="" ReturnQty="" ShipNode="" ShipToKey=""
            Status="" StatusQuantity="" SubLineNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="CanOverridePolicy">
                        <Description>This attribute determines whether we can override return Policy or not. </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="IsReceiptExpected">
                        <Description>This attribute determines whether a receipt is expected for this item or not.</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="IsReturnable">
                        <Description>Is the line returnable. </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ReturnPolicy">
                        <Description>This attribute determines the Return Policy.</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ReturnPolicyUrl">
                        <Description>This attribute determines the Return policy URL. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ReturnQty">
                        <Description>This is the Return Quantity </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Status">
                        <Description>The status description of the line. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="StatusQuantity">
                        <Description>Quantity on the order line in current the status. </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Item ItemID="" ItemShortDesc="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="ItemShortDesc">
                            <Description>The short description of the item.</Description>
                        </Attribute>
                        <Attribute DataType="String" Name="ProductClass">
                            <Description>The product class of the item.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Item>
            <LinePriceInfo UnitPrice=""/>
        </OrderLine>
    </OrderLines>
</ReturnPolicy>
