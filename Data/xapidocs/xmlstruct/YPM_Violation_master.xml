<TransactionViolation ApprovalRuleID="" Domain="" MessageCode=""
    OrganizationCode="" SequenceNumber="" Status=""
    TransactionInfoID="Required" TransactionViolationKey="Required" xmlns="http://www.sterlingcommerce.com/documentation/YPM/Violation_master">
    <yfc:doc>
        <Element EntityName="Transaction_Violation">
            <Description>
                <div>Represents new violations against this transaction. For use with the manager overrides framework in the context of an order.
				</div>
            </Description>
        </Element>
    </yfc:doc>
    <TransactionApprovalStatusList TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Integer" Name="TotalNumberOfRecords">
                    <Description>
                        <div>
 							The total number of Approval Status records returned.
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <TransactionApprovalStatus ApprovalStatusKey="Required"
            ApproverComments="" ApproverSequence="" ApproverUserID=""
            IsMandatory="" PreviousApproverSequence="" Status="" TransactionViolationKey="">
            <yfc:doc>
                <Element EntityName="Transaction_Approval_Status"/>
            </yfc:doc>
            <TransactionApprover ApproverGroupKey=""
                ApproverUserGroupID="" ApproverUserTeam=""
                DelegateUserID="" OrganizationCode=""
                TransactionApproverKey="Required" TransactionInfoID="" UserID="">
                <yfc:doc>
                    <Element EntityName="Transaction_Approver"/>
                </yfc:doc>
            </TransactionApprover>
        </TransactionApprovalStatus>
    </TransactionApprovalStatusList>
    <TransactionViolationReferenceList TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Integer" Name="TotalNumberOfRecords">
                    <Description>
                        <div>
 							The total number of Transaction Violation Reference records returned.
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <TransactionViolationReference Operation="" ReferenceName=""
            ReferenceValue="" TransactionViolationReferenceKey="Required">
            <yfc:doc>
                <Element EntityName="Transaction_Violation_Reference"/>
                <Attributes>
                    <Attribute DataType="Action" Name="Operation">
                        <Description>
                            <div>
								Action to take.
								<br/>
								Valid Values:
								<br/>
                                <code>Create</code>
								- Creates new record. If the record already exists, it is
								neither created nor modified.
								<br/>
                                <code>Delete</code>
								- Deletes existing record.
								<br/>
								The default behavior is Create.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </TransactionViolationReference>
    </TransactionViolationReferenceList>
    <TransactionInfo OrderHeaderKey="" TransactionInfoID="Required">
        <yfc:doc>
            <Element EntityName="Transaction_Info"/>
            <Attributes>
                <Attribute EntityName="Order_Transaction_Info"
                    Name="OrderHeaderKey" NameInEntity="ORDER_HEADER_KEY"/>
            </Attributes>
        </yfc:doc>
    </TransactionInfo>
</TransactionViolation>
