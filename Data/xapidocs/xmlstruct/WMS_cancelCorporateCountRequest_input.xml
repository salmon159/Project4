<?xml version="1.0" encoding="UTF-8"?>
<CancelCountProgram CancellationReasonCode="Required" CountProgramKey=""
    CountProgramName="" EnterpriseCode="" Node="" OwnerKey=""
    ReasonText="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/cancelCorporateCountRequest/input">
    <yfc:doc>
        <Element EntityName="Count_Program">
            <Description>
                <div>
                    <Validation>
                        <Condition>The count program can be canceled either in 'CREATED' or 'CNT_REQ_GNTD' status.
						</Condition>
                        <Error
                            Description="WMS: Count program cannot be canceled." ErrorCode="WMS00266"/>
                    </Validation>
                    <Validation>
                        <Condition>Either primary key (<code>CountProgramKey</code>) or all 
							attributes of unique key (<code>CountProgramName</code> along with either
							<code>OwnerKey</code> or <code>Node</code>) or <code>EnterpriseCode</code>) must be passed. Error if either of them not passed. 
						</Condition>
                        <Error
                            Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="CommonCode" EntityName="Count_Request"
                Name="CancellationReasonCode" NameInEntity="Cancellation_Reason_Code">
                <Description>
                    <div>
                        <Validation>
                            <Condition>
                                <code>CancellationReasonCode</code> not passed.
								</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                        <Validation>
                            <Condition>If passed, should be a valid cancellation reason code defined in <code>YFS_Common_Code</code> entity for CodeType <code>COUNT_REQ_CANCEL_RSN</code>. 
								</Condition>
                            <Error
                                Description="Invalid Cancellation reason code." ErrorCode="WMS00110"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="CountProgramKey">
                <Description>
                    <div>
                        <Validation>
                            <Condition>
                                <code>CountProgramKey</code> passed but not valid.
								</Condition>
                            <Error Description="No record found." ErrorCode="WMS00102"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="CountProgramName">
                <Description>
                    <div>
                        <Validation>
                            <Condition>If CountProgram key is not passed then  <code>CountProgramName</code> 
								should be passed.
								</Condition>
                            <Error
                                Description="Count program name not passed." ErrorCode="WMS00249"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="EnterpriseCode">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Either <code>OwnerKey</code>, <code>Node</code> or <code>EnterpriseCode</code> should be passed.
								</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="Node">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Either <code>OwnerKey</code>, <code>Node</code> or <code>EnterpriseCode</code> should be passed.
								</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="OwnerKey">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Either <code>OwnerKey</code>, <code>Node</code> or <code>EnterpriseCode</code> should be passed.
								</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ReasonText" EntityName="Count_Request"
                Name="ReasonText" NameInEntity="Reason_Text">
                <Description>Free form reason text for the count request cancellation.
					</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</CancelCountProgram>
