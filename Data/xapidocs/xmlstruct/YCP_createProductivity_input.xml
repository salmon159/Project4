<Productivity DateOfExecution="" EarliestTaskStartTimeStamp=""
    EnterpriseKey="Required" LatestTaskEndTimeStamp=""
    OrganizationCode="Required" ProductivityKey=""
    ProductivityType="Required" UserId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/createProductivity/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="YFS_Productivity_tran.xml"
        IncludeXMLPath="/Productivity" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Productivity"/>
        <Attributes>
            <Attribute EntityName="Productivity" Name="EnterpriseKey" NameInEntity="Enterprise_Key">
                <Description>
                    <div>
					Enterprise for which the task included in this productivity is created.
						<Validation>
                            <Condition>
                                <code>EnterpriseKey</code> passed should be a valid <code>EnterpriseKey</code>.
							</Condition>
                            <Error Description="Invalid Enterprise Key." ErrorCode="YCPPY0003"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>EnterpriseKey</code> passed should not be void. Error if <code>EnterpriseKey</code> passed is void.
							</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>EnterpriseKey</code> and <code>OrganizationCode</code> should be an existing combination.
							</Condition>
                            <Error
                                Description="Enterprise does not participate with Organization." ErrorCode="YCPPY0004"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Productivity" Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
					Organization of the Task.
						<Validation>
                            <Condition>
                                <code>OrganizationCode</code> passed should be a valid <code>OrganizationCode</code>.
							</Condition>
                            <Error
                                Description="Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> passed should not be void. Error if <code>OrganizationCode</code> passed is void.
							</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>UserId</code> and <code>OrganizationCode</code> should be an existing combination.
							</Condition>
                            <Error
                                Description="User does not belong to the node." ErrorCode="YCPPY0002"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>EnterpriseKey</code> and <code>OrganizationCode</code> should be an existing combination.
							</Condition>
                            <Error
                                Description="Enterprise does not participate with Organization." ErrorCode="YCPPY0004"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Productivity" Name="ProductivityType" NameInEntity="Productivity_Type">
                <Description>
                    <div>
					Productivity type for which the metrics has been calculated.
						<Validation>
                            <Condition>
                                <code>ProductivityType</code> passed should be a valid <code>ProductivityType</code>.
							</Condition>
                            <Error
                                Description="Invalid productivity type." ErrorCode="YCPPY0001"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>ProductivityType</code> passed should not be void. Error if <code>ProductivityType</code> passed is void.
							</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Productivity" Name="UserId" NameInEntity="User_Id">
                <Description>
                    <div>
					Enterprise for which the task included in this productivity is created.
						<Validation>
                            <Condition>
                                <code>UserId</code> passed should be a valid <code>UserId</code>.
							</Condition>
                            <Error
                                Description="Invalid User identifiers passed." ErrorCode="YCP0084"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>UserId</code> passed should not be void. Error if <code>UserId</code> passed is void.
							</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>UserId</code> and <code>OrganizationCode</code> should be an existing combination.
							</Condition>
                            <Error
                                Description="User does not belong to the node." ErrorCode="YCPPY0002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ProductivityReferences BatchNo="" BolNo="" ContainerNo=""
        DocumentType="" ItemClassificationCode="" LoadNo=""
        ManifestNo="" OrderHeaderKey="" OrderLineKey="" OrderNo=""
        OrderReleaseKey="" PrimeLineNo="" ReceiptNo="" ReleaseNo=""
        Scac="" ShipmentContainerKey="" ShipmentKey="" ShipmentNo=""
        SubLineNo="" TrailerNo="" WaveNo="" yfc:DTDOccurrence="REQUIRED">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Productivity_tran.xml" IncludeXMLPath="/Productivity/ProductivityReferences"/>
    </ProductivityReferences>
    <ProductivityMetrics NoOfItems="" NoOfPallets=""
        NoOfSourceAislesVisited="" NoOfSourceLocationsVisited=""
        NoOfTargetAislesVisited="" NoOfTargetLocationsVisited=""
        NoOfTasks="" NoOfUnits="" NofOfCases="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Productivity_tran.xml" IncludeXMLPath="/Productivity/ProductivityMetrics"/>
    </ProductivityMetrics>
</Productivity>
