<?xml version="1.0" encoding="UTF-8"?>
<PlannedTasks EntityKey="" EntityName="" Reset="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getOrderPlannedEffort/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Planned_Task" Name="EntityKey" NameInEntity="Entity_Key">
                <Description>
					Identifies a entity key being planned for. This is the primary key of the entity being planned for.
				</Description>
            </Attribute>
            <Attribute EntityName="Planned_Task" Name="EntityName" NameInEntity="Entity_Name">
                <Description>
					Identifies the entity being planned for.					
				</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="Reset">
                <Description>
                    <div>The flag indicates whether to reset all the relevant planned tasks.					
					<br/>Valid values - <code>
                            <b>Y</b>
                        </code>, <code>
                            <b>N</b>
                        </code>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PlannedTask Completed="" EntityKey="" EntityName="" Node=""
        PlannedDate="" PlannedTaskKey="" RequiredCapacity=""
        ResourcePoolKey="" TaskType="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Planned_Task" Name="PlannedDate" NameInEntity="Planned_Date">
                    <Description>
						The date planned for consumption of the capacity. Defaulted to expected shipment date of the order when Entity name is YFS_ORDER_HEADER.
					</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </PlannedTask>
</PlannedTasks>
