<!-- YCP_getServerDetails_output.xml -->
<Server Id="" MaxElapsedTraceEnabledTimeInMins="" MaxTraceLevel=""
    Name="" StartTime="" Status="" SubType="" Type="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getServerDetails/output">
    <yfc:doc>
        <Element>
            <Description>
                <div>
				This table stores the Server Information associated with the Services. ServerKey is a foreign key in YFS_SUB_FLOW table. When the IntegrationAdapter is started with a serverName, all the subFlows (or Runtime Ids) associated with the Server are started. All of these subFlows should belong to the same Transport Protocol (such as DB, or JMS).
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Key" Name="Id">
                <Description>Server Identifier.</Description>
            </Attribute>
            <Attribute DataType="Integer" Name="MaxElapsedTraceEnabledTimeInMins">
                <Description>Time elapsed since the trace component was set or the server started, which ever was later. If there are multiple services with same lowest trace level, the one with highest elapsed time is chosen.</Description>
            </Attribute>
            <Attribute DataType="ComponentLevel" Name="MaxTraceLevel">
                <Description>Level of active trace component, if any. Only the lowest level is shown if multiple services have active traces.</Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="Name">
                <Description>Server Name.</Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="StartTime">
                <Description>Time when the server started</Description>
            </Attribute>
            <Attribute DataType="Status" Name="Status">
                <Description>Server Status.</Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="SubType">
                <Description>AgentServer or IntegrationServer, in case any of them</Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="Type">
                <Description>Server Type.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Service Name="" NoOfThreads="" Status="">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				Services are collections of roles. An organization providing a service implies that it plays all the roles listed under the service. 
				</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-40" Name="Name">
                    <Description>Service Name.</Description>
                </Attribute>
                <Attribute DataType="Count" Name="NoOfThreads">
                    <Description>Number Of threads dedicated for the service.</Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="Status">
                    <Description>Status of a service.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <TransportProperties InitialContextFactory="" MessageSelector=""
            ProviderURL="" QCFLookup="" QName="" TransportType="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="InitialContextFactory"/>
                    <Attribute DataType="String" Name="MessageSelector"/>
                    <Attribute DataType="String" Name="ProviderURL"/>
                    <Attribute DataType="String" Name="QCFLookup"/>
                    <Attribute DataType="String" Name="QName"/>
                    <Attribute DataType="String" Name="TransportType"/>
                </Attributes>
            </yfc:doc>
        </TransportProperties>
        <ExceptionHandling ExceptionQName="" ExceptionType=""
            Reprocess="" RestartWaitTime="" Restartable="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="ExceptionQName"/>
                    <Attribute DataType="String" Name="ExceptionType"/>
                    <Attribute DataType="String" Name="Reprocess"/>
                    <Attribute DataType="String" Name="RestartWaitTime"/>
                    <Attribute DataType="String" Name="Restartable"/>
                </Attributes>
            </yfc:doc>
        </ExceptionHandling>
        <ScheduleInfo AdvancedSchedulingCronExpr=""
            AdvancedSchedulingEnabled="" NextScheduleTriggerTime=""
            ScheduleTrigger="" ScheduleTriggerTimeInterval="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="AdvancedSchedulingCronExpr"/>
                    <Attribute DataType="String" Name="AdvancedSchedulingEnabled"/>
                    <Attribute DataType="String" Name="NextScheduleTriggerTime"/>
                    <Attribute DataType="String" Name="ScheduleTrigger"/>
                    <Attribute DataType="String" Name="ScheduleTriggerTimeInterval"/>
                </Attributes>
            </yfc:doc>
        </ScheduleInfo>
        <TraceInfo ElapsedTraceEnabledTimeInMins="" TraceLevel="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="ElapsedTraceEnabledTimeInMins"/>
                    <Attribute DataType="String" Name="TraceLevel"/>
                </Attributes>
            </yfc:doc>
        </TraceInfo>
        <RunInfo PendingJobs="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="PendingJobs"/>
                </Attributes>
            </yfc:doc>
        </RunInfo>
        <AgentCriteriaInfo AgentCriteriaXML="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="AgentCriteriaXML"/>
                </Attributes>
            </yfc:doc>
        </AgentCriteriaInfo>
    </Service>
</Server>
