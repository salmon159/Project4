<Configuration ConfigId="" EnterpriseCode="" IsHistory=""
    OrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YIC/purgeConfiguration/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					The root node of the XML document contains header information for purge.  
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Key" Name="ConfigId">
                <Description>
                    <div>
						The Id of the configuration to purge.                    
						<Validation>
                            <Condition>If the passed <code>ConfigId</code> does not identify an existing configuration.</Condition>
                            <Error
                                Description="Invalid key was passed to the api." ErrorCode="YIC10005"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="EnterpriseCode"/>
            <Attribute DataType="Flag" Name="IsHistory">
                <Description>
                    <div>
						Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
						<br/>
                        <b>
                            <code>N</code>
                        </b> - Purges the configuration from transaction to the history tables.
						<br/>
                        <b>
                            <code>Y</code>
                        </b> - Deletes configuration from history tables.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>The <code>OrganizationCode</code> to be used for schema determination.	
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Configuration>
