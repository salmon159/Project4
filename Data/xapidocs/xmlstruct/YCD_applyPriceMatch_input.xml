<!--applyPriceMatch Input XML-->
<PriceMatch Competitor="" Currency="" EnterpriseCode=""
    ExpirationDate="" ItemID="" MinimumQuantity="" NewUnitPrice=""
    NoHassleOverridden="" Operation="" PriceMatchKey="" RegionCity=""
    RegionState="" ShippingCharges="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/applyPriceMatch/input">
    <yfc:doc>
        <Element EntityName="Price_Match"/>
        <Attributes>
            <Attribute DataType="String" Name="Competitor"/>
            <Attribute DataType="String" Name="NewUnitPrice"/>
            <Attribute DataType="String" Name="Operation"/>
            <Attribute DataType="String" Name="ShippingCharges"/>
        </Attributes>
    </yfc:doc>
    <Order CallingOrganizationCode="" DocumentType="" EnterpriseCode="" OrderHeaderKey="">
        <yfc:doc>
            <Element EntityName="Order_Header"/>
            <Attributes>
                <Attribute DataType="String" Name="CallingOrganizationCode">
                    <Description>This is the Calling Organization Code </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <OrderLine EnterpriseCode="Required" ExpirationDate=""
            Operation="Required" OrderLineKey="Required"
            PriceMatchCompetitor="Required" PriceMatchCurrency=""
            PriceMatchMinQuantity="" PriceMatchPrice="Required"
            PriceMatchShippingCharge="" RegionCity="" RegionState="" UnitPrice="">
            <yfc:doc>
                <Element EntityName="Order_Line"/>
                <Attributes>
                    <Attribute EntityName="Price_Match"
                        Name="EnterpriseCode" NameInEntity="Enterprise_Code"/>
                    <Attribute EntityName="Price_Match"
                        Name="ExpirationDate" NameInEntity="Expiration_Date">
                        <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Operation">
                        <Description>This is the operation(create, modify, delete) to be performed.</Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match"
                        Name="PriceMatchCompetitor" NameInEntity="Competitor">
                        <Description>This is the Competitor  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="PriceMatchCurrency" NameInEntity="Currency">
                        <Description>This the Currency for which is price is applied for the Item </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="PriceMatchMinQuantity" NameInEntity="Minimum_Quantity">
                        <Description>This is the Minimum Quantity for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match" Name="PriceMatchPrice" NameInEntity="NEW_UNIT_PRICE">
                        <Description>Final price allowed for the price match (incorporates shipping charges) </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match"
                        Name="PriceMatchShippingCharge" NameInEntity="Shipping_Charges">
                        <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="RegionCity" NameInEntity="Region_City">
                        <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="RegionState" NameInEntity="Region_State">
                        <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Notes>
                <Note ContactReference="" ContactTime="" ContactType=""
                    ContactUser="" NoteText="" Priority="" ReasonCode="">
                    <yfc:doc>
                        <Element EntityName="Notes"/>
                    </yfc:doc>
                </Note>
            </Notes>
        </OrderLine>
    </Order>
</PriceMatch>
