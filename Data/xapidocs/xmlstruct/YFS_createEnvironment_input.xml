<YFSEnvironment adapterName="" progId="Required" systemName=""
    tokenId="" userId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/createEnvironment/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="adapterName"/>
            <Attribute DataType="String" Name="progId"/>
            <Attribute DataType="String" Name="systemName"/>
            <Attribute DataType="String" Name="tokenId"/>
            <Attribute DataType="String" Name="userId"/>
        </Attributes>
    </yfc:doc>
    <!-- Zero or One Templates elements -->
    <Templates>
        <!-- One or More Template elements -->
        <Template ApiName="Required" yfc:DTDOccurrence="OneOrMany">PCDATA<yfc:doc>
                <Element>
                    <Description>Template document for the API as string node under this element.</Description>
                </Element>
                <Attributes>
                    <Attribute DataType="CLOB" Name="ApiName">
                        <Description>The name of the API for which template is being passed.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Template>
    </Templates>
    <clientProperties>
        <!-- One or More clientProperty elements -->
        <clientProperty propertyName="" propertyValue="" yfc:DTDOccurrence="OneOrMany">PCDATA<yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="propertyName">
                        <Description>The name of the property being passed.</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="propertyValue">
                        <Description>The value of the property being passed.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </clientProperty>
    </clientProperties>
</YFSEnvironment>
