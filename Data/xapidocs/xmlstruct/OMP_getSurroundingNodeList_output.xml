<GetSurroundingNodeList DistanceToConsider="" DistanceToConsiderUOM=""
    FulfillmentType="" NodeType="" OrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/OMP/getSurroundingNodeList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Number-22" Name="DistanceToConsider">
                <Description>
                    <div>
				Nodes in this radius from <code>ShipToAddress</code> will be returned.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="UOM" Name="DistanceToConsiderUOM">
                <Description>
                    <div>
				Specifies the UOM for <code>DistanceToConsider</code>. Defaulted to <code>MILE</code> if not passed.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="ID-40" Name="FulfillmentType">
                <Description>
                    <div>
				The <code>FulfillmentType</code> used to read sourcing rules for the <code>OrganizationCode</code>'s primary enterprise. 
			</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Ship_Node" Name="NodeType" NameInEntity="Node_Type"/>
            <Attribute EntityName="Organization" Name="OrganizationCode" NameInEntity="OrganizationCode"/>
        </Attributes>
    </yfc:doc>
