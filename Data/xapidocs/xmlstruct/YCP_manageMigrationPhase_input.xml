<MigrationPhase Operation="Manage|Create|Modify|Delete|"
    Phase="Required" TableName="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageMigrationPhase/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>Operation to perform
                    <br/>Valid Values: 
                    <br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
                    <br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
                    <br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
                    <br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
                    <br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="MigrationPhase" Name="Phase">
                <Description>The Phase to which to transition.</Description>
            </Attribute>
            <Attribute DataType="String" Name="TableName"/>
        </Attributes>
    </yfc:doc>
</MigrationPhase>
