                <Attribute EntityName="User" Name="CreateUserName" NameInEntity="Username">
                    <Description>Name of the user who created the audit.</Description>
                </Attribute>
                <Attribute EntityName="User" Name="ModifyUserName" NameInEntity="Username">
                    <Description>Name of the user who modified the audit.</Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="ReasonCode">
                    <Description>
		Reason Code
		</Description>
                </Attribute>
                <Attribute DataType="Text-100" Name="ReasonText">
                    <Description>
		Reason Text
		</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Order DocumentType="" EnterpriseCode="" OrderNo=""/>
        <OrderAuditLevels>
            <OrderAuditLevel ModificationLevel=""
                ModificationLevelScreenName="" OrderLineKey=""
                OrderReleaseKey="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Text-40" Name="ModificationLevel">
                            <Description>
            Level at which modification is done. Order, Order_Line, Order_Release or Order_Release_Line.
            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ModificationLevelScreenName">
                            <Description>
            Screen Name for the Modification level.
            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <OrderLine OrderLineKey="" OrderedQty="" PrimeLineNo="" SubLineNo="">
                    <Item AlternateItemID=""
                        BundleFulfillmentMode="|00|01|02"
                        CostCurrency="" CountryOfOrigin=""
                        CustomerItem="" CustomerItemDesc="" ECCNNo=""
                        HarmonizedCode="" ISBN="" ItemDesc="" ItemID=""
                        ItemShortDesc="" ItemWeight="" ItemWeightUOM=""
                        ManufacturerItem="" ManufacturerItemDesc=""
                        ManufacturerName="" NMFCClass="" NMFCCode=""
                        NMFCDescription="" ProductClass=""
                        ProductLine="" ScheduleBCode=""
                        SubstituteItemID="" SubstituteItemUOM=""
                        SupplierItem="" SupplierItemDesc=""
                        TaxProductCode="" UPCCode="" UnitCost="" UnitOfMeasure="">
                        <yfc:doc>
                            <Element EntityName="Order_Line"/>
                            <Attributes>
                                <Attribute DataType="Text-40" Name="SubstituteItemID">
                                    <Description>Substituted ItemID. This field is only populated if the item on the Order line is substituted. 
					</Description>
                                </Attribute>
                                <Attribute DataType="Text-40" Name="SubstituteItemUOM">
                                    <Description>Substituted Item UOM. This field is only populated if the item on the Order line is substituted.
					</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <LanguageDescriptionList>
                            <LanguageDescription Description="" LocaleCode=""/>
                        </LanguageDescriptionList>
                    </Item>
                </OrderLine>
                <OrderRelease ReleaseNo=""/>
                <OrderAuditDetails>
                    <OrderAuditDetail Action="" AuditType="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Text-10" Name="Action">
                                    <Description>
						Action 
						</Description>
                                </Attribute>
                                <Attribute DataType="Text-30" Name="AuditType">
                                    <Description>
						Audit Type
						</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <IDs>
                            <!-- ID is not a template-driven element. If the template contains OrderAuditDetail element, this element will come based on AuditType. For AuditTypes like Order, OrderLine this element does not come.-->
                            <ID Name="Required" Value="Required" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="Text-30" Name="Name">
                                    <Description>
											Name of the ID
											</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-30" Name="Value">
                                    <Description>
											Value of the ID
											</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                            </ID>
                        </IDs>
                        <Attributes>
                            <!-- Attribute is not a template-driven element. If the template contains OrderAuditDetail element, this element will appear in the output xml alongwith all its attributes.-->
                            <Attribute Extended="N|Y"
                                ModificationType="" Name="Required"
                                NewValue="Required" OldValue="Required" yfc:DTDOccurrence="OneOrMany">
                                <yfc:doc>
                                    <Element EntityName="[NONE]"/>
                                    <Attributes>
                                    <Attribute DataType="Flag" Name="Extended">
                                    <Description>Determines whether the attribute is an extended field. If this attribute
								is not available, then it means the attribute is not extended.</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-40" Name="ModificationType">
                                    <Description>
							Modification Type used for changing the attribute
							</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-30" Name="Name">
                                    <Description>
							Name of the Attribute Changed
							</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-100" Name="NewValue">
                                    <Description>
							New Value of the attribute modified
							</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-100" Name="OldValue">
                                    <Description>
							Old Value of the attribute modified
							</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                            </Attribute>
                        </Attributes>
                        <!-- CustomAuditDetails is not a template-driven element. If custom entity is modified, this element will appear in the output xml . -->
                        <!-- Nested CustomAuditDetails element will be returned if custom table has a hangoff table. -->
                        <CustomAuditDetails>
                            <OrderAuditDetail Action="" AuditType="" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:includeInParent
                                    IncludeXMLChildNodes="N"
                                    IncludeXMLFileName="YFS_getOrderAuditDetails_output.xml" IncludeXMLPath="/OrderAudit/OrderAuditLevels/OrderAuditLevel/OrderAuditDetails/OrderAuditDetail"/>
                                <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="Text-10" Name="Action">
                                    <Description>
						Action 
						</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-30" Name="AuditType">
                                    <Description>
						Audit Type
						</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                                <IDs>
                                    <yfc:includeInParent
                                    IncludeXMLChildNodes="Y"
                                    IncludeXMLFileName="YFS_getOrderAuditDetails_output.xml" IncludeXMLPath="/OrderAudit/OrderAuditLevels/OrderAuditLevel/OrderAuditDetails/OrderAuditDetail/IDs"/>
                                    <!-- ID is not a template-driven element. If the template contains OrderAuditDetail element, this element will come based on AuditType. For AuditTypes like Order, OrderLine this element does not come.-->
                                    <ID Name="Required" Value="Required" yfc:DTDOccurrence="ZeroOrMany">
                                    <yfc:doc>
                                    <Attributes>
                                    <Attribute
                                    DataType="Text-30" Name="Name">
                                    <Description>
											Name of the ID
											</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-30" Name="Value">
                                    <Description>
											Value of the ID
											</Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                    </ID>
                                </IDs>
                                <Attributes>
                                    <yfc:includeInParent
                                    IncludeXMLChildNodes="Y"
                                    IncludeXMLFileName="YFS_getOrderAuditDetails_output.xml" IncludeXMLPath="/OrderAudit/OrderAuditLevels/OrderAuditLevel/OrderAuditDetails/OrderAuditDetail/Attributes"/>
                                    <!-- Attribute is not a template-driven element. If the template contains OrderAuditDetail element, this element will appear in the output xml alongwith all its attributes.-->
                                    <Attribute Extended="N|Y"
                                    ModificationType=""
                                    Name="Required"
                                    NewValue="Required"
                                    OldValue="Required" yfc:DTDOccurrence="OneOrMany">
                                    <yfc:doc>
                                    <Element EntityName="[NONE]"/>
                                    <Attributes>
                                    <Attribute
                                    DataType="Flag" Name="Extended">
                                    <Description>Determines whether the attribute is an extended field. If this attribute
								is not available, then it means the attribute is not extended.</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-40" Name="ModificationType">
                                    <Description>
							Modification Type used for changing the attribute
							</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-30" Name="Name">
                                    <Description>
							Name of the Attribute Changed
							</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-100" Name="NewValue">
                                    <Description>
							New Value of the attribute modified
							</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-100" Name="OldValue">
                                    <Description>
							Old Value of the attribute modified
							</Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                    </Attribute>
                                </Attributes>
                            </OrderAuditDetail>
                        </CustomAuditDetails>
                    </OrderAuditDetail>
                </OrderAuditDetails>
                <ModificationTypes>
                    <ModificationType Name="" ScreenName="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element EntityName="Order_Audit"/>
                            <Attributes>
                                <Attribute EntityName="Order_Audit"
                                    Name="Name" NameInEntity="Modification_Type"/>
                                <Attribute
                                    EntityName="Status_Modification_Type"
                                    Name="ScreenName" NameInEntity="Modification_Type_Screen_Name"/>
                            </Attributes>
                        </yfc:doc>
                    </ModificationType>
                </ModificationTypes>
            </OrderAuditLevel>
        </OrderAuditLevels>
    </OrderAudit>
</OrderStatusChange>
