<?xml version="1.0" encoding="UTF-8"?>
<!-- Either CountRequestKey or (CountRequestNo, DocumentType and Node) must be passed. -->
<CancelCountRequest CancellationReasonCode="Required" CountRequestKey=""
    CountRequestNo="" DocumentType="" Node=""
    OverrideModificationRules="" ReasonText="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/cancelCountRequest/input">
    <yfc:includeInParent IncludeXMLFileName="YFS_Count_Request_keys.xml" IncludeXMLPath="/CountRequest"/>
    <yfc:doc>
        <Element EntityName="Count_Request">
            <Description>
                <div>
                    <Validation>
                        <Condition>If Status modification is not allowed for count request or if count request is already cancelled, then status modification cannot be performed on count request.
						</Condition>
                        <Error
                            Description="WMS: Count request status change is not allowed." ErrorCode="WMS00109"/>
                    </Validation>
                    <Validation>
                        <Condition>Either primary key (<code>CountRequestKey</code>) or all 
							attributes of unique key (<code>CountRequestNo</code>,
							<code>DocumentType</code> and <code>Node</code>) must be passed. Error if either of them not passed. 
						</Condition>
                        <Error
                            Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="CommonCode" EntityName="Count_Request"
                Name="CancellationReasonCode" NameInEntity="Cancellation_Reason_Code">
                <Description>
                    <div>
                        <Validation>
                            <Condition>
                                <code>CancellationReasonCode</code> not passed.
								</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                        <Validation>
                            <Condition>If passed, should be a valid cancellation reason code defined in <code>YFS_Common_Code</code> entity for CodeType <code>COUNT_REQ_CANCEL_RSN</code>. 
								</Condition>
                            <Error
                                Description="Invalid Cancellation reason code." ErrorCode="WMS00110"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Count_Request"
                Name="CountRequestKey" NameInEntity="Count_Request_Key">
                <Description>
                    <div>
						Specifies the count request for which count was created.
							<Validation>
                            <Condition>
                                <code>CountRequestKey</code> passed but not valid.
								</Condition>
                            <Error
                                Description="Invalid Count Request Key." ErrorCode="WMS00108"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" Name="CountRequestNo">
                <Description>
                    <div>
                        <Validation>
                            <Condition>If <code>CountRequestNo</code> is passed then <code>DocumentType</code> and <code>Node</code> becomes mandatory.
								</Condition>
                            <Condition>If <code>CountRequestNo</code> not passed.
								</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-24" EntityName="Count_Request"
                Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>
						Indicates the purpose of the count request.
							<Validation>
                            <Condition>If <code>DocumentType</code> is passed then <code>CountRequestNo</code> and <code>Node</code> becomes mandatory.
								If <code>DocumentType</code> is not passed.
								</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>DocumentType</code> passed but not valid.
								</Condition>
                            <Error
                                Description="No record found for passed inputs" ErrorCode="WMS00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Count_Request"
                Name="Node" NameInEntity="Node_Key">
                <Description>
                    <div>
						Indicates the node or warehouse where the count request was created.
							<Validation>
                            <Condition>If <code>Node</code> is passed then <code>CountRequestNo</code> and <code>DocumentType</code> become mandatory. If <code>Node</code> is not passed.					</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Node</code> passed but does not exist.
								</Condition>
                            <Error
                                Description="Node passed does not exist" ErrorCode="WMS00042"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="OverrideModificationRules">
                <Description>
                    <div> Pass as  <b>
                            <code> Y </code>
                        </b> to  override the status of count request that can be considered for various modifications.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ReasonText" EntityName="Count_Request"
                Name="ReasonText" NameInEntity="Reason_Text">
                <Description>Free form reason text for the count request cancellation.
					</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</CancelCountRequest>
