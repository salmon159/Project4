<InvoiceDetail xmlns="http://www.sterlingcommerce.com/documentation/OMP/getInvoiceNoUE/input">
    <!-- InvoiceHeader is a template-driven element but some of its attributes are not. Attributes that are not template driven: TotalCharges, TotalDiscount, TotalHeaderCharges, HeaderCharges, HeaderDiscount, HeaderTax and LineSubTotal.   -->
    <InvoiceHeader HeaderCharges="" HeaderDiscount="" HeaderTax=""
        InvoiceCreationReason="" InvoiceType="" LineSubTotal=""
        TotalCharges="" TotalDiscount="" TotalHeaderCharges="">
        <yfc:doc>
            <Element EntityName="Order_Invoice"/>
            <Attributes>
                <Attribute DataType="String" Name="HeaderCharges"/>
                <Attribute DataType="String" Name="HeaderDiscount"/>
                <Attribute DataType="String" Name="TotalCharges"/>
                <Attribute DataType="String" Name="TotalDiscount"/>
                <Attribute DataType="String" Name="TotalHeaderCharges"/>
            </Attributes>
        </yfc:doc>
        <Order BuyerOrganizationCode="" DeliveryCode="" DocumentType=""
            EnteredBy="" EnterpriseCode="" EntryType=""
            OrderHeaderKey="" OrderNo="" SellerOrganizationCode=""
            Status="" TermsCode="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_getOrderInvoiceDetails_output.xml" IncludeXMLPath="/InvoiceDetail/InvoiceHeader/Order"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="Status"/>
                </Attributes>
            </yfc:doc>
        </Order>
        <Shipment ManifestNo="" ReceivingNode="" SCAC="" ShipDate=""
            ShipNode="" ShipmentKey="" ShipmentNo="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_getOrderInvoiceDetails_output.xml" IncludeXMLPath="/InvoiceDetail/InvoiceHeader/Shipment"/>
        </Shipment>
    </InvoiceHeader>
</InvoiceDetail>
