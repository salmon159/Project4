<!-- getStoreAvailability Output XML-->
<Promise AllocationRuleID="" CarrierServiceCode=""
    ConsiderNextDayForTransfer="" DemandType=""
    IgnoreMinNotificationTime="Y|N" IgnorePromised="Y|N"
    IgnoreUnpromised="Y|N" LevelOfService="" MaximumRecords=""
    OptimizationType="" OrganizationCode="Required" ReqStartDate=""
    SCAC="" ScacAndServiceKey="" SourcingClassification=""
    Store="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getStoreAvailability/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="ID-10" Name="AllocationRuleID">
                <Description>
                    <div>
				Allocation rule that was used.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="CarrierServiceCode">
                <Description>
                    <div>
				Carrier service code. If transfer schedule is present between two nodes , the tranfer schedule's transit days are used to calculate the transit time, otherwise <code>CarrierServiceCode</code> is used for calculating the transit time.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderNextDayForTransfer">
                <Description>
                    <div>If <code>ConsiderNextDayForTransfer</code> is passed as <b>
                            <code>Y</code>
                        </b>, the available date of goods from the transferring node will be pushed off to the beginning of the next day.  This applies to standard transfer pickup option, expedited transfer pickup option, and special order pickup option.  
				<BR/>Default: <code>N</code>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="DemandType">
                <Description>
                    <div>
				Specifies a <code>DemandType</code> used in the inventory availability calculations.
				<BR/>Default: OPEN_ORDER
				<br/>Only valid <code>SupplyType</code>s that can be consumed against the <code>DemandType</code> value are considered in the inventory availability calculations.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreMinNotificationTime">
                <Description>
                    <div>
				Flag to ignore Minimum Notification time associated with Item and Shipnode.
				<br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnorePromised">
                <Description>
                    <div>If <code>IgnorePromised</code> is passed as <b>
                            <code>Y</code>
                        </b>, promised demands are not considered during inventory availability calculations.
				<BR/>Default: <code>N</code>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreUnpromised">
                <Description>
                    <div>If <code>IgnoreUnpromised</code> is passed as <b>
                            <code>Y</code>
                        </b>, unpromised demands are not considered during inventory availability calculations.
				<BR/>Default: <code>N</code>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ID-40" Name="LevelOfService">
                <Description>
                    <div>
					Determines the level of service to be used for the lines if not passed on the lines.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="MaximumRecords">
                <Description>
                    <div>
				Positive integer used to determine the maximum number of possible solutions to find.
				<BR/>Default: <b>
                            <code>1</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Allocation_Rule" Name="OptimizationType">
                <Description>
                    <div>
				This value determines the optimization rules to be applied to the scheduling process.
				<br/>Default: <b>
                            <code>01</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>
			Inventory is being checked on behalf of the organization specified by <code>OrganizationCode</code>.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="Date" Name="ReqStartDate">
                <Description>
                    <div>
				Start Date from which to look inventory
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="SCAC">
                <Description>
                    <div>
				Carrier to be used
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="ScacAndServiceKey">
                <Description>
                    <div>
				Scac And Service Key
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="SourcingClassification">
                <Description>This attribute will be used to determine node for sourcing.
	    </Description>
            </Attribute>
            <Attribute DataType="ShipNode" Name="Store">
                <Description>
                    <div>
			Current store where the availabilities were optained.
			</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <FulfillmentTypes ExpeditedTransferPickupFulfillmentType=""
        ShipToHomeFulfillmentType="" SpecialOrderFulfillmentType="" StdTransferPickupFulfillmentType="">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				Fulfillment types for each sourcing setup.  
			</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-40" Name="ExpeditedTransferPickupFulfillmentType">
                    <Description>
                        <div>
					Fulfillment type for sourcing rule that handles expedited transfer pickup.  It is expected that sourcing rules for this fulfillment type will be set up with "Receiving node" as the store or alternate node.  Expedited procurement is tried only for items that can be shipped through parcel service.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="ShipToHomeFulfillmentType">
                    <Description>
                        <div>
					Fulfillment type for sourcing rule that handles ship to home options.  This will only be used if ShipToAddress is passed.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="SpecialOrderFulfillmentType">
                    <Description>
                        <div>
					Fulfillment type for sourcing rule that handles special order vendor.  It is expected that sourcing rules for this fulfillment type will be set up with "Receiving node" as the store or alternate node.  Node's attributes, CanShipToStore and CanShipToDC, will be used to control whether the special order vendor can ship to the store/DC.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="StdTransferPickupFulfillmentType">
                    <Description>
                        <div>
					Fulfillment type for sourcing rule that handles standard transfer pickup.  It is expected that sourcing rules for this fulfillment type will be set up with "Receiving node" as the store or alternate node.
					</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </FulfillmentTypes>
    <ShipToAddress City="" Country="" State="" ZipCode="">
        <yfc:doc>
            <Element EntityName="Person_Info"/>
        </yfc:doc>
    </ShipToAddress>
    <PromiseLines>
        <PromiseLine AutoReplacedByPreferredSubstitute=""
            BundleFulfillmentMode="|01|02|03" CarrierServiceCode=""
            ConsiderCurrentStoreInventory="" ConsiderSpecialOrderOnly=""
            FillQuantity="" IsBundleParent="" ItemID="Required"
            LevelOfService="" LineId="" MinShipByDate="" NewItemID=""
            ProductClass="" ReqStartDate="" RequiredQty="Required"
            SCAC="" ScacAndServiceKey="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Order_Line"/>
                <Attributes>
                    <Attribute DataType="Flag" Name="AutoReplacedByPreferredSubstitute">
                        <Description>
                            <div>
				Flag to indicates whether inventory is replaced with the Preferred Substitute. 
				<br/>Valid Values: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="ConsiderCurrentStoreInventory">
                        <Description>
                            <div>
					Indicates whether to consider the availability of the current store inventory or not. 
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="ConsiderSpecialOrderOnly">
                        <Description>
                            <div>
					Indicates whether to only consider the availability from special order vendors.  This will exclude the availabilities from in stock inventory, standard transfer pickup, and expedited transfer pickup.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IsBundleParent">
                        <Description>
                            <div>
				Flag to indicate if the promise line is a bundle parent.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="LineId">
                        <Description>
                            <div>
			Unique identifier for the line. 
			</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="TimeStamp" Name="MinShipByDate">
                        <Description>
                            <div>
            The preferred minimum ship by date of the available supply that promise line can promise against.  Available supply with ship by date earlier than the minimum ship by date will not be considered.
            <br/>Default: None
            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="NewItemID">
                        <Description>
                            <div>
			The ItemId of the substitute that is used in place of the original item.
            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ReqStartDate">
                        <Description>
                            <div>
			Look for inventory from the <code>ReqStartDate</code>.
			<br/>Default: Current date
			</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="RequiredQty">
                        <Description>
                            <div>
			Required Quantity.
			</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="ScacAndServiceKey">
                        <Description>
                            <div>
			Scac And Service Key
			</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotKeyReference="" LotNumber=""
                ManufacturingDate="" RevisionNo="">
                <yfc:doc>
                    <Element EntityName="Order_Line_Req_Tag"/>
                </yfc:doc>
            </Tag>
            <AlternateNodes>
                <AlternateNode Node="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="ShipNode" Name="Node">
                                <Description>
                                    <div>
			Alternate node where the availabilities were optained.
			</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </AlternateNode>
            </AlternateNodes>
            <ShipToAddress City="" Country="" State="" ZipCode="">
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                </yfc:doc>
            </ShipToAddress>
            <CurrentStorePickup BackorderedQty="" LastPickupDate=""
                Node="" NodeType="" UnavailableQty="">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
				Contains pickup options for current store.
				</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="BackorderedQty">
                            <Description>
                                <div>
				Backordered quantity for pickup.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="TimeStamp" IsNullable="Y" Name="LastPickupDate">
                            <Description>
                                <div>
				Last pickup date of all the pickup options.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="ShipNode" Name="Node">
                            <Description>
                                <div>
				Current store where the availabilities for pickup were obtained.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Ship_Node"
                            Name="NodeType" NameInEntity="Node_Type">
                            <Description>
                                <div>
				Type of Node.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="UnavailableQty">
                            <Description>
                                <div>
				Unavailable quantity for pickup.
				</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Pickup Backordered="Y|N" Expedited="Y|N" PickupDate=""
                    ProcureFromNode="" Quantity="" SpecialOrder="Y|N" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
				Details on the availability of the inventory for pickup option.
				</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Flag" Name="Backordered">
                                <Description>
                                    <div>
					Indicates that item is currently backordered but will be available for pickup. 
					</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Flag" Name="Expedited">
                                <Description>
                                    <div>
					Indicates that the item(s) will be expedited transferred from the <code>ProcureFromNode</code>.
					</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="TimeStamp" Name="PickupDate">
                                <Description>
                                    <div>
					Pickup Date.
					</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="ShipNode" Name="ProcureFromNode">
                                <Description>
                                    <div>
					Node where the item can be procure from.
					</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="Quantity">
                                <Description>
                                    <div>
					Quantity that can be pickup.
					</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Flag" Name="SpecialOrder">
                                <Description>
                                    <div>
					Indicates that the item(s) will be special ordered from the <code>ProcureFromNode</code>.
					</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Pickup>
            </CurrentStorePickup>
            <AlternateNodePickups>
                <AlternateNodePickup BackorderedQty="" LastPickupDate=""
                    Node="" NodeType="" UnavailableQty="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
				Contains pickup options for alternate node.
				</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="BackorderedQty">
                                <Description>
                                    <div>
				Backordered quantity for pickup.
				</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="TimeStamp"
                                IsNullable="Y" Name="LastPickupDate">
                                <Description>
                                    <div>
				Last pickup date of all the pickup options.
				</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="ShipNode" Name="Node">
                                <Description>
                                    <div>
				Alternate node where the availabilities for pickup were obtained.
				</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Ship_Node"
                                Name="NodeType" NameInEntity="Node_Type">
                                <Description>
                                    <div>
				Type of Node.
				</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="UnavailableQty">
                                <Description>
                                    <div>
				Unavailable quantity for pickup.
				</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <Pickup Backordered="Y|N" Expedited="Y|N"
                        PickupDate="" ProcureFromNode="" Quantity=""
                        SpecialOrder="Y|N" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>
				Details on the availability of the inventory for pickup option.
				</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Flag" Name="Backordered">
                                    <Description>
                                    <div>
					Indicates that item is currently backordered but will be available for pickup. 
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="Expedited">
                                    <Description>
                                    <div>
					Indicates that the item(s) will be expedited transferred from the <code>ProcureFromNode</code>.
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="TimeStamp" Name="PickupDate">
                                    <Description>
                                    <div>
					Pickup Date.
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="ShipNode" Name="ProcureFromNode">
                                    <Description>
                                    <div>
					Node where the item can be procure from.
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Quantity" Name="Quantity">
                                    <Description>
                                    <div>
					Quantity that can be pickup.
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="SpecialOrder">
                                    <Description>
                                    <div>
					Indicates that the item(s) will be special ordered from the <code>ProcureFromNode</code>.
					</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </Pickup>
                </AlternateNodePickup>
            </AlternateNodePickups>
            <ShipToHome BackorderedQty="" UnavailableQty="">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
				Ship to home option according to the ShipToAddress.
				</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="BackorderedQty">
                            <Description>
                                <div>
				Backordered Quantity for ship to home option.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="UnavailableQty">
                            <Description>
                                <div>
				Unavailable Quantity for ship to home option.
				</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Assignments>
                    <Assignment Backordered="" DeliveryDate="" Node=""
                        Quantity="" ShipDate="" SpecialOrder="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>
					Assignment node gives details on the availability of the inventory.
					</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Flag" Name="Backordered">
                                    <Description>
                                    <div>
					Indicates that the assignment is backordered.
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Date" Name="DeliveryDate">
                                    <Description>
                                    <div>
					Delivery Date of the assignment
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="ShipNode" Name="Node">
                                    <Description>
                                    <div>
					Node where the availability picture was taken from.
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Quantity" Name="Quantity">
                                    <Description>
                                    <div>
					Quantity that can be obtained for this assignemnt.
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Date" Name="ShipDate">
                                    <Description>
                                    <div>
					Shipment Date
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="SpecialOrder">
                                    <Description>
                                    <div>
					Indicates that the assignment is for special order.
					</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </Assignment>
                </Assignments>
            </ShipToHome>
        </PromiseLine>
    </PromiseLines>
    <InventorySupplies>
        <Items>
            <Item ItemID="" OrganizationCode="" ProductClass=""
                UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Item"/>
                    <Attributes>
                        <Attribute EntityName="Inventory_Item"
                            Name="OrganizationCode" NameInEntity="Organization_Code"/>
                    </Attributes>
                </yfc:doc>
                <Nodes>
                    <Node Node="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element EntityName="Ship_Node"/>
                            <Attributes>
                                <Attribute EntityName="Ship_Node"
                                    Name="Node" NameInEntity="Ship_Node"/>
                            </Attributes>
                        </yfc:doc>
                        <Supplies>
                            <Supply AvailableQuantity=""
                                FirstShipDate="" LastShipDate=""
                                Track="" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:doc>
                                    <Element EntityName="Inventory_Supply"/>
                                    <Attributes>
                                    <Attribute DataType="Quantity" Name="AvailableQuantity">
                                    <Description>
                                    <div>
									Available quantity. If the ship node does not track inventory, then the <code>AvailableQuantity</code> is returned as <b>0</b>.
									</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Date" Name="FirstShipDate">
                                    <Description>
                                    <div>
									First date that item(s) can be shipped.
									</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Date" Name="LastShipDate">
                                    <Description>
                                    <div>
									Last date that item(s) can be shipped.
									</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Flag" Name="Track">
                                    <Description>
                                    <div>
									Indicates wether the inventory is tracked or not. If inventory is not tracked, the flag is returned as <b>N</b> and <code>AvailableQuantity</code> is returned as <b>0</b>.
									</div>
                                    </Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                                <Tag BatchNo="" LotAttribute1=""
                                    LotAttribute2="" LotAttribute3=""
                                    LotKeyReference="" LotNumber=""
                                    ManufacturingDate="" RevisionNo="" TagNumber="">
                                    <yfc:doc>
                                    <Element EntityName="Inventory_Tag"/>
                                    </yfc:doc>
                                </Tag>
                            </Supply>
                        </Supplies>
                    </Node>
                </Nodes>
            </Item>
        </Items>
    </InventorySupplies>
</Promise>
