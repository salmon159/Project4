<?xml version="1.0" encoding="UTF-8"?>
<getLocationPriorityScheme EnterpriseCode="Required"
    FromActivityGroup="Required" Node="Required" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getLocationPrioritySchemeUE/input">
    <yfc:doc>
        <Element EntityName="Activity_Constraint"/>
    </yfc:doc>
    <Source CaseId="" LocationId="" PalletId="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="LPN_Location"/>
            <Attributes>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description> A CaseId </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description> A PalletId </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Inventory CountryOfOrigin="" FifoNo="" InventoryItemKey=""
            InventoryStatus="" InventoryTagKey="" ReceiptHeaderKey=""
            Segment="" SegmentType="" ShipByDate="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory"/>
            <yfc:doc>
                <Element EntityName="Location_Inventory"/>
            </yfc:doc>
            <Receipt ReceiptNo="">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/Receipt"/>
                <yfc:doc>
                    <Element EntityName="Receipt_Header"/>
                </yfc:doc>
            </Receipt>
            <InventoryItem ItemID="Required" ProductClass="Required" UnitOfMeasure="Required">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/InventoryItem"/>
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Inventory_Item" Name="ItemID"/>
                        <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
                        <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
                    </Attributes>
                </yfc:doc>
            </InventoryItem>
        </Inventory>
    </Source>
    <LocationPriority PriorityDedicatedLocation="Required"
        PriorityEmptyLocation="Required" PrioritySameReceipt="Required"
        PrioritySameSKU="Required" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Priority" Name="PriorityDedicatedLocation">
                    <Description>Determines the priority of a location that is dedicated to the inventory for which the location
						is being considered for put away. The default priority value for an dedicated location is 2.</Description>
                </Attribute>
                <Attribute DataType="Priority" Name="PriorityEmptyLocation">
                    <Description> Determines the priority of an Empty Location while considering location for put away. The default priority value for an empty location is 1.</Description>
                </Attribute>
                <Attribute DataType="Priority" Name="PrioritySameReceipt">
                    <Description> Determines the priority of a location having the inventory belonging to the same
							Receipt as inventory for which the location is being considered for put away. The default priority value for the same receipt is 1.</Description>
                </Attribute>
                <Attribute DataType="Priority" Name="PrioritySameSKU">
                    <Description> Determines the priority of a location having the same SKU as inventory for which the location
						is being considered for put away. The default priority value for the same SKU is 3.
					</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </LocationPriority>
</getLocationPriorityScheme>
