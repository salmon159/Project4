<?xml version="1.0" encoding="UTF-8"?>
<!-- Either TaskQKey or one of (CountRequestNo, Node and DocumentType) 
	and CountRequestKey is mandatory -->
<!-- BaseDropStatus is optional. When not specified, base drop status is 
	read from the transaction configuration -->
<!-- TransactionId if not passed is assumed to be CHANGE_COUNT_REQ_STATUS -->
<ChangeCountRequestStatus AcceptOutOfSequenceUpdates=""
    BaseDropStatus="" CountRequestKey="" CountRequestNo=""
    DocumentType="" Node="" TaskQKey="" TransactionId="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/changeCountRequestStatus/input">
    <yfc:includeInParent IncludeXMLFileName="YFS_Count_Request_keys.xml" IncludeXMLPath="/CountRequest"/>
    <yfc:doc>
        <Element EntityName="Count_Request"/>
        <Attributes>
            <Attribute DataType="Flag" Name="AcceptOutOfSequenceUpdates">
                <Description> Flag used to determine the drop status for the count request. </Description>
            </Attribute>
            <Attribute EntityName="Pipeline_Definition"
                Name="BaseDropStatus" NameInEntity="Drop_Status"/>
            <Attribute EntityName="Task_Queue" Name="TaskQKey" NameInEntity="Task_Q_Key">
                <Description>
                    <div> Task Q request for count request status change. 
					<Validation>
                            <Condition>If <code>TaskQKey</code> is not passed, <code>CountRequestKey</code> or <code>CountRequestNo, Node and DocumentType</code> must be passed.
						</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid"/>
        </Attributes>
    </yfc:doc>
</ChangeCountRequestStatus>
