<Order DocumentType="" EnterpriseCode="" OrderHeaderKey="" OrderNo="" xmlns="http://www.sterlingcommerce.com/documentation/OMP/checkOrderTransactionComplete/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Order_Header" Name="DocumentType"/>
            <Attribute EntityName="Order_Header" Name="EnterpriseCode"/>
            <Attribute EntityName="Order_Header" Name="OrderHeaderKey"/>
            <Attribute EntityName="Order_Header" Name="OrderNo"/>
        </Attributes>
    </yfc:doc>
    <CompletionImpediments ActiveHoldsExist=""
        AdditionalCompletionImpediments="" OrderTransactionComplete="" PaymentProcessingIncomplete="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="ActiveHoldsExist">
                    <Description>
                        <div>This flag indicates whether active Holds are preventing Order Transaction completion or not.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="AdditionalCompletionImpediments">
                    <Description>
                        <div>This flag indicates that additional impediments exist on the Order. These pending processes are returned by the custom implementation of the extension point.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="OrderTransactionComplete">
                    <Description>
                        <div>This is the summary flag that indicates whether the Order Transaction can be marked as complete or not.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="PaymentProcessingIncomplete">
                    <Description>
                        <div>This flag indicates the completion of Payment Processing. All remaining financial totals should be zero for completion.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <CompletionImpediment ImpedimentType="" ImpedimentValue="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-50" Name="ImpedimentType">
                        <Description>
                            <div>This field indicates the type of Impediment that is not allowing the completion of Order Transaction. Examples - HOLDS, PAYMENTS, VIOLATIONS,etc..</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-50" Name="ImpedimentValue">
                        <Description>
                            <div>This field indicates further description of the Impediment.</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </CompletionImpediment>
    </CompletionImpediments>
</Order>
