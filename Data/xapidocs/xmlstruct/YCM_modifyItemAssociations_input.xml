<AssociationList ItemID="Conditional" ItemKey="Conditional"
    OrganizationCode="Conditional" UnitOfMeasure="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YCM/modifyItemAssociations/input">
    <yfc:doc>
        <Element EntityName="Item">
            <Description>
                <div>
			To identify the item, pass either <code>ItemKey</code>, or <code>ItemID</code>, <code>OrganizationCode</code>, and <code>UnitofMeasure</code>.
			<Validation>
                        <Condition>If <code>ItemKey</code>, or <code>ItemID</code>, <code>OrganizationCode</code>, and <code>UnitofMeasure</code> is not passed. </Condition>
                        <Error
                            Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                    </Validation>
                    <Validation>
                        <Condition>The <code>ItemKey</code>, or <code>ItemID</code>, <code>OrganizationCode</code>, and <code>UnitofMeasure</code> does not identify an <code>Item</code>.
				</Condition>
                        <Error Description="Item Not Found" ErrorCode="YCM0033"/>
                    </Validation>
                    <Validation>
                        <Condition>If <code>Item</code> is a bundle item then it cannot be associated to any other item.
				</Condition>
                        <Error
                            Description="Bundle item cannot be associated to any other item." ErrorCode="YCM79_008"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Item" Name="ItemID" NameInEntity="Item_Id"/>
            <Attribute EntityName="Item" Name="ItemKey" NameInEntity="Item_Key"/>
            <Attribute EntityName="Item" Name="OrganizationCode" NameInEntity="Organization_Code"/>
            <Attribute EntityName="Item" Name="UnitOfMeasure" NameInEntity="Uom"/>
        </Attributes>
    </yfc:doc>
    <!--Valid values of Action are Create and Modify. Set Action = "Create" to create new association. Set Action = "Modify" to modify association.-->
    <Association Action="" AssociatedQuantity="" AssociationGroup=""
        AssociationType="Required" EffectiveFrom="Required"
        EffectiveTo="Required" Priority="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element EntityName="Association">
                <Description>
                    <div>
					To identify the association, pass <code>AssociationType</code>, <code>EffectiveFrom</code>, and <code>EffectiveTo</code>
                        <Validation>
                            <Condition>If <code>AssociationType</code>, <code>EffectiveFrom</code>, and <code>EffectiveTo</code> are not passed. </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Action" Name="Action">
                    <Description>
                        <div>
					   Action to take.Valid values are <b>
                                <code>Modify</code>
                            </b>and <b>
                                <code>Create</code>
                            </b>.  Set <code>Action</code> = "<b>
                                <code>Modify</code>
                            </b>" to modify the Association.  Default value is <b>
                                <code>Create</code>
                            </b>. 
					  </div>
                    </Description>
                </Attribute>
                <Attribute Name="AssociationType">
                    <Description>
                        <div>
					Type of association maintained. 
					<br/>
                            <code>AssociationType</code> value for <code>SS</code>(Special Service) item is <code>Service.Y</code>.
					 <Validation>
                                <Condition>
                                Associating any Item to Special Service Item or 
                                Associating Special Service Item to non PROD item or 
                                Not using AssociationType 'Service.Y' to associate Special Service to PROD item or 
                                Using AssociationType 'Service.Y' for non Special Service Association.
				</Condition>
                                <Error
                                    Description="Invalid Special Service Association." ErrorCode="YCM922_003"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Item ItemID="" ItemKey="" OrganizationCode="" UnitOfMeasure="">
            <yfc:doc>
                <Element EntityName="Item">
                    <Description>
                        <div>
			To identify the item, pass either <code>ItemKey</code>, or <code>ItemID</code>, <code>OrganizationCode</code>, and <code>UnitofMeasure</code>.
			<Validation>
                                <Condition>If <code>ItemKey</code>, or <code>ItemID</code>, <code>OrganizationCode</code>, and <code>UnitofMeasure</code> is not passed. </Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                            </Validation>
                            <Validation>
                                <Condition>The <code>ItemKey</code>, or <code>ItemID</code>, <code>OrganizationCode</code>, and <code>UnitofMeasure</code> does not identify an <code>Item</code>.
				</Condition>
                                <Error Description="Item Not Found" ErrorCode="YCM0033"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>Item</code> is a bundle item then it cannot be associated to any other item.
				</Condition>
                                <Error
                                    Description="Bundle item cannot be associated to any other item." ErrorCode="YCM79_008"/>
                            </Validation>
                            <Validation>
                                <Condition>Try to create circular association for item with an <code>EffectiveStartDate</code> or <code>EffectiveEndDate</code>.
				</Condition>
                                <Error
                                    Description="An Item with an EffectiveStartDate or EffectiveEndDate cannot have a circular association relationship." ErrorCode="YCM82_002"/>
                            </Validation>
                            <Validation>
                                <Condition>Try to create multiple association relationships for item with an <code>EffectiveStartDate</code> or <code>EffectiveEndDate</code>.
				</Condition>
                                <Error
                                    Description="An Item with an EffectiveStartDate or EffectiveEndDate cannot have multiple association relationships." ErrorCode="YCM82_003"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
            </yfc:doc>
        </Item>
        <Category CategoryKey="">
            <yfc:doc>
                <Element EntityName="Category">
                    <Description>
                        <div>
			Pass <code>CategoryKey</code> to create association to whole category.
		
			<Validation>
                                <Condition>The <code>Category</code> does not exist.
				</Condition>
                                <Error Description="Category Not Found" ErrorCode="YCM0034"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Category" Name="CategoryKey" NameInEntity="Category_Key"/>
                </Attributes>
            </yfc:doc>
        </Category>
        <Query QueryKey="">
            <yfc:doc>
                <Element EntityName="Query">
                    <Description>
                        <div/>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Query" Name="QueryKey" NameInEntity="Query_Key"/>
                </Attributes>
            </yfc:doc>
        </Query>
    </Association>
    <!-- All matching Associations based on the input parameteres in Association element will be removed for the given Parent Item. -->
    <Remove>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
			All matching Associations based on the input parameteres in <code>Association</code> element will be removed for the given Parent Item.
			For description about Elements and Attributes of <code>Association</code> to remove, refer to <code>/AssociationList/Association</code>.
			<Validation>
                            <Condition>The input parameters do not identify any <code>Association</code> records.
				</Condition>
                            <Error
                                Description="No Item Associations Found With This Input" ErrorCode="YCM711_001"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
        <Association Action="" AssociatedQuantity="" AssociationGroup=""
            AssociationType="Required" EffectiveFrom="Required"
            EffectiveFromQryType="" EffectiveTo="Required"
            EffectiveToQryType="" FromEffectiveFrom=""
            FromEffectiveTo="" Priority="" ToEffectiveFrom=""
            ToEffectiveTo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Association">
                    <Description>
                        <div>
						To identify the association, pass <code>AssociationType</code>, <code>EffectiveFrom</code>, and <code>EffectiveTo</code>
                            <Validation>
                                <Condition>If <code>AssociationType</code>, <code>EffectiveFrom</code>, and <code>EffectiveTo</code> are not passed. </Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Action" Name="Action">
                        <Description>
                            <div>
		   Action to take.  This <code>Action</code> attribute is only applicable for the /AssociationList/Association element.   It is not applicable for the /AssociationList/Remove/Association element.  Valid values are <b>
                                    <code>Modify</code>
                                </b>and <b>
                                    <code>Create</code>
                                </b>.  Set <code>Action</code> = "<b>
                                    <code>Modify</code>
                                </b>" to modify the Association.  Default value is <b>
                                    <code>Create</code>
                                </b>.
		  </div>
                        </Description>
                    </Attribute>
                    <Attribute Name="AssociationType">
                        <Description>
                            <div>
						Type of association maintained. 
						<br/>
                                <code>AssociationType</code> value for <code>SS</code>(Special Service) item is <code>Service.Y</code>.
						 <Validation>
                                    <Condition>
	                                Associating any Item to Special Service Item or 
	                                Associating Special Service Item to non PROD item or 
	                                Not using AssociationType 'Service.Y' to associate Special Service to PROD item or 
	                                Using AssociationType 'Service.Y' for non Special Service Association.
					</Condition>
                                    <Error
                                    Description="Invalid Special Service Association." ErrorCode="YCM922_003"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Item ItemID="" ItemKey="" OrganizationCode="" UnitOfMeasure=""/>
            <!-- Either ItemKey or ItemID, OrganizationCode, UnitOfMeasure is required. -->
            <Category CategoryKey=""/>
            <Query QueryKey=""/>
        </Association>
    </Remove>
</AssociationList>
