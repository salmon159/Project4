<?xml version="1.0" encoding="UTF-8"?>
<!-- WaveKey or Node is mandatory-->
<Wave Description="" DescriptionQryType="" DocumentType=""
    DocumentTypeQryType="" EstimatedMinutes=""
    EstimatedMinutesQryType="" FromDescription="" FromDocumentType=""
    FromEstimatedMinutes="" FromNode="" FromOverrideConcurrentWave=""
    FromOverrideReleaseDate="" FromPipelineKey="" FromPriority=""
    FromReasonCode="" FromReasonText="" FromReleaseNoEarlierThan=""
    FromReleaseNoLaterThan="" FromRemainingMinutes="" FromSeqNo=""
    FromShipmentGroupId="" FromWaveKey="" FromWaveNo=""
    FromWaveStatus="" MaximumRecords="" Node="" NodeQryType=""
    OverrideConcurrentWave="" OverrideConcurrentWaveQryType=""
    OverrideReleaseDate="" OverrideReleaseDateQryType="" PipelineKey=""
    PipelineKeyQryType="" Priority="" PriorityQryType="" ReasonCode=""
    ReasonCodeQryType="" ReasonText="" ReasonTextQryType=""
    ReleaseNoEarlierThan="" ReleaseNoEarlierThanQryType=""
    ReleaseNoLaterThan="" ReleaseNoLaterThanQryType=""
    RemainingMinutes="" RemainingMinutesQryType="" SeqNo=""
    SeqNoQryType="" ShipmentGroupId="" ShipmentGroupIdQryType=""
    ToDescription="" ToDocumentType="" ToEstimatedMinutes="" ToNode=""
    ToOverrideConcurrentWave="" ToOverrideReleaseDate=""
    ToPipelineKey="" ToPriority="" ToReasonCode="" ToReasonText=""
    ToReleaseNoEarlierThan="" ToReleaseNoLaterThan=""
    ToRemainingMinutes="" ToSeqNo="" ToShipmentGroupId="" ToWaveKey=""
    ToWaveNo="" ToWaveStatus="" WaveKey="" WaveKeyQryType="" WaveNo=""
    WaveNoQryType="" WaveStatus="" WaveStatusQryType="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getWaveList/input">
    <yfc:includeInParent GenerateSearchAttributes="Y" IncludeQryType="Y"
        IncludeXMLFileName="YFS_Wave_master.xml" IncludeXMLPath="/Wave" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Wave"/>
        <Attributes>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>Specifies maximum number of records that must be fetched.</Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Wave" Name="Node" NameInEntity="Node_Key">
                <Description>
                    <div>
                        Indicates the node or warehouse where wave(s) are created.
                        <Validation>
                            <Condition>
                                <code>Node</code> attribute OR the <code>WaveKey</code>     attribute must be passed and must be valid.  
                            </Condition>
                            <Error
                                Description="Mandatory parameter not passed." ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Wave" Name="WaveKey" NameInEntity="Wave_Key">
                <Description>
                    <div>
                            Primary key attribute. Identifies a <code>Wave</code>
                        <Validation>
                            <Condition>
                                <code>Node</code> attribute OR the <code>WaveKey</code>     attribute must be passed and must be valid.  
                                </Condition>
                            <Error
                                Description="Mandatory parameter not passed." ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Wave>
