<!--getReleaseList Output XML-->
<!-- OrderReleaseList is a template driven element. But its attributes are not except the attribute TotalNumberOfRecords. TotalNumberOfRecords is the only template driven attribute for this element.-->
<OrderReleaseList LastOrderReleaseKey="" LastRecordSet=""
    ReadFromHistory="" TotalNumberOfRecords="" TotalOrderReleaseList="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderReleaseList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Key" Name="LastOrderReleaseKey">
                <Description>Last Order Release Key in the output.<br/>
                    <em>
                        <code>LastOrderReleaseKey</code> is not supported when getPage API is used in multiTenancy environment.</em>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="LastRecordSet">
                <Description>
                    <div> Flag to indicate if the records obtained are a complete list. If there are more than 500 records matching the criteria passed in input xml, only 500 records are returned and this flag is set to <b>N</b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="ReadFromHistory">
                <Description>
                    <div>
					Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
					<br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_ORDER_RELEASE table.
					<br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_ORDER_RELEASE_H table (the history table).
					<br/>
                        <b>
                            <code>B</code>
                        </b> - Returns records that satisfy the input criteria from both YFS_ORDER_RELEASE and YFS_ORDER_RELEASE_H tables. Records returned from the current table are first in the output.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>Total number of records that match the input criteria.</Description>
            </Attribute>
            <Attribute DataType="Count" Name="TotalOrderReleaseList">
                <Description>Total number of records returned.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderRelease BillToID="" BuyerMarkForNodeId=""
        BuyerOrganizationCode="" BuyerReceivingNodeId=""
        CarrierAccountNo="" CarrierServiceCode="" Currency=""
        CustomerPoNo="" CustomerRewardsNo="" DeliveryCode="" Division=""
        DocumentType="" EnterpriseCode="" FOB="" HoldFlag=""
        HoldReasonCode="" LevelOfService="" MarkForKey=""
        MaxOrderReleaseStatus="" MaxOrderReleaseStatusDesc=""
        MinOrderReleaseStatus="" MinOrderReleaseStatusDesc=""
        NotificationReference="" NotificationType=""
        NotifyAfterShipmentFlag="" OrderDate="" OrderHeaderKey=""
        OrderName="" OrderReleaseKey="" OrderType="" OtherCharges=""
        PacklistType="" PersonalizeCode="" PickListNo="" PriorityCode=""
        Purpose="" ReceivingNode="" ReleaseNo="" ReleaseSeqNo=""
        ReqCancelDate="" ReqDeliveryDate="" ReqShipDate="" SCAC=""
        SalesOrderNo="" ScacAndService="" ScacAndServiceKey=""
        SellerOrganizationCode="" ShipAdviceNo="" ShipCompleteFlag=""
        ShipLineComplete="" ShipNode="" ShipNodeClass=""
        ShipOrderComplete="" ShipToId="" ShipToKey=""
        ShipmentConsolidationGroupId="" Status="" SupplierCode=""
        SupplierName="" TaxpayerId="" isHistory="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="CustomerRewardsNo"/>
                <Attribute DataType="Status" Name="MaxOrderReleaseStatus">
                    <Description>
                        <div>Maximum status of the order release record
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="MaxOrderReleaseStatusDesc">
                    <Description>
                        <div> Description of the maximum status of the order release record
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Status" Name="MinOrderReleaseStatus">
                    <Description>
                        <div> Description of the minimum status of the order release record
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="MinOrderReleaseStatusDesc">
                    <Description>
                        <div> Description of the minimum status of the order release record
				</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="SCAC_And_Service"
                    Name="ScacAndService" NameInEntity="Scac_And_Service"/>
                <Attribute EntityName="SCAC_And_Service"
                    Name="ScacAndServiceKey" NameInEntity="Scac_And_Service_Key"/>
                <Attribute DataType="Status" Name="Status">
                    <Description>
                        <div>Status of the order release record
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="isHistory">
                    <Description>
                        <div> Flag to indicate if the Release record is in History table.
				</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Order AllocationRuleID="" AuthorizationExpirationDate=""
            BillToID="" BuyerMarkForNodeId="" BuyerOrganizationCode=""
