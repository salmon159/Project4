<!-- MasterOrderRelationshipInput XML-->
<OrderRelationship HierarchyLevel="" IsDependencyResolved=""
    RelatedTableKey="" RelatedTableName="" RelationshipType="Required"
    RootNodeKey="" xmlns="http://www.sterlingcommerce.com/documentation/MasterOrderRelationshipInput">
    <yfc:doc>
        <Attributes>
            <Attribute Name="RelatedTableName">
                <Description>
                    <div>The TableName of the related transactional record.</div>
                    <div>If not passed, it will be defaulted to TableName.</div>
                </Description>
            </Attribute>
            <Attribute Name="RelationshipType">
                <Description>
                    <div>The type of relationship between the two transactional records.
						<Validation>
                            <Condition>
								The RelationshipType must be passed.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
        <Element EntityName="Transactional_Relationship">
            <Description>
                <div>This table stores details about the relationship between two transactional records.</div>
            </Description>
        </Element>
    </yfc:doc>
</OrderRelationship>
