<GetPriceListForOrdering EnterpriseCode="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getPriceListForOrdering/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="EnterpriseCode">
                <Description>The enterprise that is associated with this order.
	</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Items>
        <Item AlternateItemID="" IsNew="" ItemID="" PriceProgramName=""
            PricingDate="" ProductClass="" Quantity="" UnitOfMeasure="">
            <yfc:doc>
                <Element EntityName="Order_Line"/>
                <Attributes>
                    <Attribute DataType="Flag" Name="IsNew">
                        <Description>If it is added by price engine. </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="PriceProgramName">
                        <Description>The price program used for pricing the order
			</Description>
                    </Attribute>
                    <Attribute Name="Quantity" NameInEntity="Ordered_Qty"/>
                </Attributes>
            </yfc:doc>
            <PriceInfo IsPriceLocked="" ListPrice="" PricingUOM=""
                RetailPrice="" UnitPrice="">
                <yfc:doc>
                    <Element EntityName="Order_Line"/>
                    <Attributes>
                        <Attribute DataType="Flag" Name="IsPriceLocked">
                            <Description>This may be the negotiated price with the customer. Price engine should not recompute the price for this item. </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </PriceInfo>
            <Charges>
                <Charge ChargeCategory="Required" ChargeName=""
                    ChargePerLine="" ChargePerUnit="" Reference="">
                    <yfc:doc>
                        <Element EntityName="Line_Charges"/>
                    </yfc:doc>
                </Charge>
            </Charges>
            <Taxes>
                <Tax ChargeCategory="" ChargeName="" Reference1=""
                    Reference2="" Reference3="" Tax=""
                    TaxName="Required" TaxPercentage="" TaxableFlag="">
                    <yfc:doc>
                        <Element EntityName="Tax_Breakup"/>
                    </yfc:doc>
                </Tax>
            </Taxes>
            <Awards>
                <Award AwardAmount="" AwardApplied="" AwardId=""
                    AwardKey="" AwardType="" DenialReason=""
                    Description="" PosReasonCode="" PromotionId="" PromotionKey=""/>
            </Awards>
        </Item>
        <HeaderCharges>
            <HeaderCharge ChargeAmount="" ChargeCategory="Required"
                ChargeName="" IsBillable="" IsDiscount=""
                IsShippingCharge="|N|Y" Reference="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="IsBillable">
                            <Description>Describes if the charge is billable. Default: <code>N</code>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsDiscount">
                            <Description>Describes if the charge is discountable. Default: <code>N</code>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsShippingCharge">
                            <Description>Determines whether the charge is a shipping charge. This attribute value is system defined and cannot be overridden. The value for this attribute is set as <code>Y</code> only for Charge Name configured as <code>CHARGE_NAME_FOR_SHIPPING</code> in financial rule. For all other Charges this attribute is set to <code>N</code>.
						</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </HeaderCharge>
        </HeaderCharges>
        <HeaderTaxes>
            <HeaderTax ChargeCategory="Required" ChargeName=""
                Reference1="" Reference2="" Reference3="" Tax=""
                TaxName="Required" TaxPercentage="" TaxableFlag="">
                <yfc:doc>
                    <Element EntityName="Tax_Breakup"/>
                </yfc:doc>
            </HeaderTax>
        </HeaderTaxes>
        <Promotions>
            <Promotion DenialReason="" Description=""
                PromotionApplied="" PromotionId="" PromotionKey="" PromotionType=""/>
        </Promotions>
        <Awards>
            <Award AwardAmount="" AwardApplied="" AwardId="" AwardKey=""
                AwardType="" DenialReason="" Description=""
                PosReasonCode="" PromotionId="" PromotionKey=""/>
        </Awards>
    </Items>
</GetPriceListForOrdering>
