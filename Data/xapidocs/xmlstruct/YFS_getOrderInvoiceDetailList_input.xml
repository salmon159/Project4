        <Element EntityName="Order_Invoice_Detail"/>
        <Attributes>
            <Attribute DataType="Flag" Name="IsHistory">
                <Description>
                    <div>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
						<br/>
                        <b>
                            <code>N</code>
                        </b>- Returns records that satisfy the input criteria from the YFS_ORDER_INVOICE_DETAIL table.
						<br/>
                        <b>
                            <code>Y</code>
                        </b>- Returns records that satisfy the input criteria from the YFS_ORDER_INVOICE_DETAIL_H table (the history table).
						<br/>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>Specifies the Maximum Number of records to be returned. Default is 500.</Description>
