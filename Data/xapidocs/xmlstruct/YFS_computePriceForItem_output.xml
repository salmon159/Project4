<ComputePriceForItem ListPrice="" RetailPrice="" UnitPrice="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/computePriceForItem/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="ListPrice">
                <Description>List Price for an item.</Description>
            </Attribute>
            <Attribute DataType="String" Name="RetailPrice">
                <Description>Retail Price for an item.</Description>
            </Attribute>
            <Attribute DataType="String" Name="UnitPrice">
                <Description>Unit Price for an item.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PriceSet Description="" OrganizationCode="" PriceSetKey="" PriceSetName="">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					The Price list master table. Stores a list of valid price lists for an organization. This is the Price Set that was accessed to get the unit price.	
					</div>
                </Description>
            </Element>
        </yfc:doc>
    </PriceSet>
    <ItemPriceSet ItemID="" ItemPriceSetKey="" ListPrice=""
        ProductClass="" RetailPrice="" Uom="">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					The Price List for items. This is the Item Price Set record that was accessed to get the unit price.	
					</div>
                </Description>
            </Element>
        </yfc:doc>
    </ItemPriceSet>
    <ItemPriceSetDtl BreakQtyHigh="" BreakQtyLow=""
        ItemPriceSetDtlKey="" RegionKey="" UnitPrice="">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					If an item requires step pricing, this table stores the quantity breaks and price associated. This is the Item Price Set detail record that was accessed to get the unit price.	
					</div>
                </Description>
            </Element>
        </yfc:doc>
    </ItemPriceSetDtl>
</ComputePriceForItem>
