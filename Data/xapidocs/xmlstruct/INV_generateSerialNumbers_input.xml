<!--generateSerialNumbers Input XML-->
<Serials xmlns="http://www.sterlingcommerce.com/documentation/INV/generateSerialNumbers/input">
    <SerialList ItemID="Required"
        OrganizationCode="!OrganizationCode!|Required" UnitOfMeasure="Required">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="ItemID"/>
                <Attribute DataType="String" Name="OrganizationCode"/>
                <Attribute DataType="String" Name="UnitOfMeasure"/>
            </Attributes>
        </yfc:doc>
        <SerialElem FromSerialNo="Required" ToSerialNo="Required">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="FromSerialNo">
                        <Description>
                            <div>
				This is the starting serial number of a range.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ToSerialNo">
                        <Description>
                            <div>
				This is the ending serial number of a range. 
				</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </SerialElem>
    </SerialList>
</Serials>
