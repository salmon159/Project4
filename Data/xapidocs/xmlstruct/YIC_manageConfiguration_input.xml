<!-- manageConfiguration input XML-->
<manageConfiguration OrganizationCode="" configId="" sku="" type="" xmlns="http://www.sterlingcommerce.com/documentation/YIC/manageConfiguration/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>The <code>OrganizationCode</code> to be used for schema determination.	
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="BOM_Header"
                Name="configId" NameInEntity="BOM_Id">
                <Description>
                    <div>
						The ID to save this configuration.  If -1 is specified or the attribute is empty or absent, a new unique configuration key will be obtained and the configuration will be persisted using it.
						If the attribute is provided the api looks for the corresponding configuration and updates it with the information in <code>ConfiguratorBOM</code> element. 						
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" EntityName="BOM_Header"
                Name="sku" NameInEntity="SKU">
                <Description>
                    <div>
						An optional SKU from the root node of the configuration.  If it is not passed it will be defaulted to the root line item's sku.						
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" EntityName="BOM_Header"
                Name="type" NameInEntity="Type">
                <Description>
                    <div>
						A caller supplied identifier string used to group saved configurations.  Current known types are "cart" and "config".  If not specified null will be used.						
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ConfiguratorBOM ModelName="" Version="" containerOnly="" errors="">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="masterConfiguratorBOM_manageConfig.xml" IncludeXMLPath="/"/>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					This element has the header information of configurator BOM.  
				</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-254" EntityName="BOM_Header"
                    Name="ModelName" NameInEntity="ModelName">
                    <Description>
                        <div>
						The full configurator name for the model.						
                    </div>
                    </Description>
                </Attribute>
                <Attribute DataType="PositiveInteger"
                    EntityName="BOM_Header" Name="Version" NameInEntity="Version">
                    <Description>
                        <div>
						The current BOM version.						
                    </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" EntityName="BOM_Header"
                    Name="containerOnly" NameInEntity="Container">
                    <Description>
                        <div>
						This attribute is true if the model root has the container only flag set otherwise false.
						<br/>Valid Values: <b>
                                <code>true</code>
                            </b> or <b>
                                <code>false</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="errors">
                    <Description>
                        <div>
						This attribute is true if there are errors in the BOM, otherwise false.
						<br/>Valid Values: <b>
                                <code>true</code>
                            </b> or <b>
                                <code>false</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <message item="" type="" value="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
							BOMs can contain messages about the status of the configuration and are produced by rules or constraint tables.  
						</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Text-480"
                        EntityName="BOM_Message" Name="item" NameInEntity="Name">
                        <Description>
                            <div>
								Name of the message.							
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-120"
                        EntityName="BOM_Message" Name="type" NameInEntity="MessageType">
                        <Description>
                            <div>
								The type of the message in the configurator BOM.	
								<br/>Valid Values: <b>
                                    <code>error</code>
                                </b> or <b>
                                    <code>warning</code>
                                </b> or <b>
                                    <code>suggest</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-240"
                        EntityName="BOM_Message" Name="value" NameInEntity="Message">
                        <Description>
                            <div>
								The displayable value for the message.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </message>
        <LineItem CommerceItemKey="" Description="" ItemID="Required"
            ItemKey="" Name="" Price="" Quantity="Required"
            RulePicked="Required" SKU="" UOM="" Visible="Required"
            lockprice="" uevValue="">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
							LineItems form a tree structure in that they can contain other line items.  The top most LineItem is the root of the model.  
						</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Key" EntityName="BOM_Line"
                        Name="CommerceItemKey" NameInEntity="CommerceItemKey">
                        <Description>
                            <div>
								An optional key for the item from an order.  These keys are preserved if it is passed in the input. It will be returned in the output.							
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-480" EntityName="BOM_Line"
                        Name="Description" NameInEntity="Description">
                        <Description>
                            <div>
								The description of the <code>LineItem</code>.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Integer" EntityName="BOM_Line"
                        Name="ItemID" NameInEntity="ItemId">
                        <Description>
                            <div>
								The item configurator ID for the item.							
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Integer" Name="ItemKey">
                        <Description>
                            <div>
								The visual modeler key for the item.  This attribute is not considered in the input.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-480" EntityName="BOM_Line"
                        Name="Name" NameInEntity="Name">
                        <Description>
                            <div>
								The unique name for the <code>LineItem</code> in a particular depth of the hierarchy.							
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="UnitPrice"
                        EntityName="BOM_Line" Name="Price" NameInEntity="Price">
                        <Description>
                            <div>
								The price of the <code>LineItem</code>.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Integer" EntityName="BOM_Line"
                        Name="Quantity" NameInEntity="Quantity">
                        <Description>
                            <div>
								The quantity of the item ordered for the <code>LineItem</code>.								
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" EntityName="BOM_Line"
                        Name="RulePicked" NameInEntity="RulePick">
                        <Description>
                            <div>
								This attribute is true if the item was picked by a rule otherwise false.
								<br/>Valid Values: <b>
                                    <code>true</code>
                                </b> or <b>
                                    <code>false</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-120" EntityName="BOM_Line"
                        Name="SKU" NameInEntity="SKU">
                        <Description>
                            <div>
								The sku associated to the <code>LineItem</code>.								
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="UOM" EntityName="BOM_Line"
                        Name="UOM" NameInEntity="UOM">
                        <Description>
                            <div>
								The UOM associated to the <code>LineItem</code>.								
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" EntityName="BOM_Line"
                        Name="Visible" NameInEntity="Visible">
                        <Description>
                            <div>
								This attribute is true if the item should be visible to the user in the order, otherwise false.  Very often models contain information needed for reconfiguration that should not be visible in the order.
								<br/>Valid Values: <b>
                                    <code>true</code>
                                </b> or <b>
                                    <code>false</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" EntityName="BOM_Line"
                        Name="lockprice" NameInEntity="PriceLocked">
                        <Description>
                            <div>
								This attribute is true if the price in the BOM should used in the order, otherwise false. 
								<br/>Valid Values: <b>
                                    <code>true</code>
                                </b> or <b>
                                    <code>false</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-240" EntityName="BOM_Line"
                        Name="uevValue" NameInEntity="ItemValue">
                        <Description>
                            <div>
								The user entered value associated to the <code>LineItem</code>.								
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <property external="" name="" type="" value="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
								The holder of properties associated with the <code>LineItem</code>. It contains:
								<ol>
                                    <li>The Configurator Properties that are attached to the items in the model and 
									flagged as "output" properties.</li>
                                    <li>The external input properties injected into the model to influence its
									runtime behavior.</li>
                                </ol>
                            </div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Flag" Name="external">
                            <Description>
                                <div>
									The flag to differentiate externally injected input properties from output properties.
									The attribute value is <b>
                                    <code>Y</code>
                                    </b>, if the property is an externally injected
									input property. Default value is <b>
                                    <code>N</code>
                                    </b>.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-480"
                            EntityName="BOM_Property" Name="name" NameInEntity="Property_Name">
                            <Description>
                                <div>
									Name of the property.							
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Integer"
                            EntityName="BOM_Property" Name="type" NameInEntity="Property_Type">
                            <Description>
                                <div>
									The type of the property in the configurator BOM.	
									<br/>Valid Values: <b>
                                    <code>bigdecimal</code>
                                    </b> or <b>
                                    <code>int</code>
                                    </b> or <b>
                                    <code>List</code>
                                    </b> or <b>
                                    <code>long</code>
                                    </b> or <b>
                                    <code>double</code>
                                    </b> or <b>
                                    <code>String</code>
                                    </b> or <b>
                                    <code>date</code>
                                    </b> or <b>
                                    <code>unknown</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-480"
                            EntityName="BOM_Property" Name="value" NameInEntity="Property_Value">
                            <Description>
                                <div>
									The value of the property in string form.
								</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </property>
            <LineItem CommerceItemKey="" Description=""
                ItemID="Required" ItemKey="" Name="" Price=""
                Quantity="Required" RulePicked="Required" SKU="" UOM=""
                Visible="Required" lockprice="" uevValue="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
											LineItems form a tree structure in that they can contain other line items.  The top most LineItem is the root of the model.  
										</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Key" EntityName="BOM_Line"
                            Name="CommerceItemKey" NameInEntity="CommerceItemKey">
                            <Description>
                                <div>
												An optional key for the item from an order.  These keys are preserved if it is passed in the input. It will be returned in the output.							
											</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-480"
                            EntityName="BOM_Line" Name="Description" NameInEntity="Description">
                            <Description>
                                <div>
												The description of the <code>LineItem</code>.
											</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Integer"
                            EntityName="BOM_Line" Name="ItemID" NameInEntity="ItemId">
                            <Description>
                                <div>
												The item configurator ID for the item.							
											</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Integer" Name="ItemKey">
                            <Description>
                                <div>
												The visual modeler key for the item. This attribute is not considered in the input.
											</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-480"
                            EntityName="BOM_Line" Name="Name" NameInEntity="Name">
                            <Description>
                                <div>
												The unique name for the <code>LineItem</code> in a particular depth of the hierarchy.							
											</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="UnitPrice"
                            EntityName="BOM_Line" Name="Price" NameInEntity="Price">
                            <Description>
                                <div>
												The price of the <code>LineItem</code>.
											</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Integer"
                            EntityName="BOM_Line" Name="Quantity" NameInEntity="Quantity">
                            <Description>
                                <div>
												The quantity of the item ordered for the <code>LineItem</code>.								
											</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" EntityName="BOM_Line"
                            Name="RulePicked" NameInEntity="RulePick">
                            <Description>
                                <div>
												This attribute is true if the item was picked by a rule otherwise false.
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-120"
                            EntityName="BOM_Line" Name="SKU" NameInEntity="SKU">
                            <Description>
                                <div>
												The sku associated to the <code>LineItem</code>.								
											</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="UOM" EntityName="BOM_Line"
                            Name="UOM" NameInEntity="UOM">
                            <Description>
                                <div>
												The UOM associated to the <code>LineItem</code>.								
											</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" EntityName="BOM_Line"
                            Name="Visible" NameInEntity="Visible">
                            <Description>
                                <div>
												This attribute is true if the item should be visible to the user in the order, otherwise false.  Very often models contain information needed for reconfiguration that should not be visible in the order.
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" EntityName="BOM_Line"
                            Name="lockprice" NameInEntity="PriceLocked">
                            <Description>
                                <div>
												This attribute is true if the price in the BOM should used in the order, otherwise false. 
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-240"
                            EntityName="BOM_Line" Name="uevValue" NameInEntity="ItemValue">
                            <Description>
                                <div>
												The user entered value associated to the <code>LineItem</code>.								
											</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </LineItem>
        </LineItem>
    </ConfiguratorBOM>
</manageConfiguration>
