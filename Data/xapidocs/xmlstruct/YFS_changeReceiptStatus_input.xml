<!-- Either TaskQKey or one of ReceiptNo, ReceivingNode or ReceiptNo, ShipmentKey or ReceiptHeaderKey is mandatory -->
<!-- BaseDropStatus is optional. When not specified, base drop status is read from the transaction configuration -->
<!-- TransactionId if not passed is assumed to be CHANGE_RECEIPT_STATUS -->
<Receipt AcceptOutOfSequenceUpdates="" BaseDropStatus=""
    ReceiptHeaderKey="" ReceiptNo="" ReceivingNode="" ShipmentKey=""
    TaskQKey="" TransactionId="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/changeReceiptStatus/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="AcceptOutOfSequenceUpdates">
                <Description>Indicates whether to do updates which are not in sequence (sequence in pipeline).
			</Description>
            </Attribute>
            <Attribute EntityName="Pipeline_Definition"
                Name="BaseDropStatus" NameInEntity="Drop_Status"/>
            <Attribute EntityName="Task_Queue" Name="TaskQKey" NameInEntity="Task_Q_Key">
                <Description>Task Q request for receipt status change.
			</Description>
            </Attribute>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid"/>
        </Attributes>
    </yfc:doc>
    <Shipment DocumentType="" EnterpriseCode="" OrderHeaderKey=""
        OrderNo="" OrderReleaseKey="" ReleaseNo=""
        SellerOrganizationCode="" ShipNode="" ShipmentNo=""/>
    <ReceiptStatusAudit ReasonCode="" ReasonText=""
        ReceiptStatusAuditKey="" StatusDate="">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Receipt_Header" Name="StatusDate" NameInEntity="StatusDate">
                    <Description>Date and time of the status change.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </ReceiptStatusAudit>
</Receipt>
