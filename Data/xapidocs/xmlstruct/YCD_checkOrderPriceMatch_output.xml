<!--checkOrderPricematch Output XML-->
<Order DocumentType="" EnterpriseCode="" OrderHeaderKey="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/checkOrderPriceMatch/output">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
                <div/>
            </Description>
        </Element>
    </yfc:doc>
    <OrderLines yfc:DTDOccurrence="REQUIRED">
        <OrderLine ItemID="" OrderLineKey="" OrderedQty=""
            PersonalizeFlag="" PriceMatchCompetitor=""
            PriceMatchCurrency="" PriceMatchMinQuantity=""
            PriceMatchPrice="" PriceMatchShippingCharge=""
            PriceMatched="" RegionCity="" RegionState="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Price_Match">
                    <Description>
                        <div/>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute Name="ItemID">
                        <Description>This is the ItemID. </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="OrderLineKey" NameInEntity="Order_Line_Key">
                        <Description>This is the Order Line key Of the OrderLine  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line" Name="OrderedQty" NameInEntity="Ordered_Qty">
                        <Description>This is the Ordered Quantity of the Item in the OrderLine. </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="PersonalizeFlag" NameInEntity="Personalize_Flag">
                        <Description>This is the Personalize Flag. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match"
                        Name="PriceMatchCompetitor" NameInEntity="Competitor">
                        <Description>This is the Competitor  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="PriceMatchCurrency">
                        <Description>This is the Currency of the Price of the Item  for which price Match has been performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="PriceMatchMinQuantity" NameInEntity="Minimum_Quantity">
                        <Description>This is the Minimum Quantity for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match" Name="PriceMatchPrice" NameInEntity="New_Unit_Price">
                        <Description>This is the Competitor's Price of the Item  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match"
                        Name="PriceMatchShippingCharge" NameInEntity="Shipping_Charges">
                        <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="PriceMatched">
                        <Description>This is the the Flag which says whether Price was matched or not. </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <PriceMatchResult PriceMatchStatus="" PriceMatchStatusMessage="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="PriceMatchStatus">
                            <Description>This is the Status of the price Match. </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="PriceMatchStatusMessage">
                            <Description>This is the Status Message of the price Match. </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <AbsoluteMatch Competitor="" ExpirationDate=""
                    MinimumQuantity="" NewUnitPrice="" RegionCity=""
                    RegionState="" ShippingCharges="">
                    <yfc:doc>
                        <Element EntityName="Price_Match">
                            <Description>
                                <div/>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="String"
                                EntityName="Price_Match"
                                Name="Competitor" NameInEntity="Competitor">
                                <Description>This is the Competitor  for which price Match is to be performed. </Description>
                            </Attribute>
                            <Attribute EntityName="Price_Match"
                                Name="ExpirationDate" NameInEntity="Expiration_Date">
                                <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                            </Attribute>
                            <Attribute EntityName="Price_Match"
                                Name="MinimumQuantity" NameInEntity="Minimum_Quantity">
                                <Description>This is the Minimum Quantity for which price Match is to be performed. </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="NewUnitPrice"/>
                            <Attribute EntityName="Price_Match"
                                Name="RegionCity" NameInEntity="Region_City">
                                <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                            </Attribute>
                            <Attribute EntityName="Price_Match"
                                Name="RegionState" NameInEntity="Region_State">
                                <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                            </Attribute>
                            <Attribute DataType="String"
                                EntityName="Price_Match"
                                Name="ShippingCharges" NameInEntity="Shipping_Charges">
                                <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </AbsoluteMatch>
                <PossibleMatches>
                    <PossibleMatch Competitor="" ExpirationDate=""
                        MinimumQuantity="" NewUnitPrice="" RegionCity=""
                        RegionState="" ShippingCharges="">
                        <yfc:doc>
                            <Element EntityName="Price_Match">
                                <Description>
                                    <div/>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="String"
                                    EntityName="Price_Match"
                                    Name="Competitor" NameInEntity="Competitor">
                                    <Description>This is the Competitor  for which price Match is to be performed. </Description>
                                </Attribute>
                                <Attribute EntityName="Price_Match"
                                    Name="ExpirationDate" NameInEntity="Expiration_Date">
                                    <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                                </Attribute>
                                <Attribute EntityName="Price_Match"
                                    Name="MinimumQuantity" NameInEntity="Minimum_Quantity">
                                    <Description>This is the Minimum Quantity for which price Match is to be performed. </Description>
                                </Attribute>
                                <Attribute DataType="String" Name="NewUnitPrice"/>
                                <Attribute EntityName="Price_Match"
                                    Name="RegionCity" NameInEntity="Region_City">
                                    <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                                </Attribute>
                                <Attribute EntityName="Price_Match"
                                    Name="RegionState" NameInEntity="Region_State">
                                    <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                                </Attribute>
                                <Attribute DataType="String"
                                    EntityName="Price_Match"
                                    Name="ShippingCharges" NameInEntity="Shipping_Charges">
                                    <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </PossibleMatch>
                </PossibleMatches>
            </PriceMatchResult>
        </OrderLine>
    </OrderLines>
</Order>
