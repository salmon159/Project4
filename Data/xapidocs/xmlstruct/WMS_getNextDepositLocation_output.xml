<?xml version="1.0" encoding="UTF-8"?>
<DepositLocation DepositAll="" LocationId="" Node="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getNextDepositLocation/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="DepositAll">
                <Description>The specifies if all the tasks got for the user have a single target location or not.</Description>
            </Attribute>
            <Attribute DataType="LocationId" Name="LocationId">
                <Description>The first suggested location for deposit.</Description>
            </Attribute>
            <Attribute DataType="ShipNode" Name="Node">
                <Description>Node.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <TaskList IsConsolidatedTask="" TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="IsConsolidatedTask">
                    <Description>
                        <div>
							  Flag to indicate that the returned task is a consolidated task.
						    </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                    <Description>Specifies then number of tasks fetched.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Task AssignedToUserId="" AssignmentNo="" AssignmentStatus=""
            BatchHold="" DependencyHold="" EndTimeStamp=""
            EnterpriseKey="" EquipmentId="" EquipmentLocationId=""
            ExceptionHold="" FinishNoLaterThan="" HoldReasonCode=""
            HoldReasonText="" IsConsolidatedTask="" IsSummaryTask=""
            Node="" OrganizationCode="" ParentTaskId=""
            PredecessorTaskId="" ProductivityKey="" ReasonCode=""
            ReasonText="" RootTaskId="" SourceAisle=""
            SourceLocationId="" SourceSortSequence="" SourceZoneId=""
            StartNoEarlierThan="" StartTimeStamp="" TargetAisle=""
            TargetCaseId="" TargetLocationId="" TargetPalletId=""
            TargetSortSequence="" TargetZoneId="" TaskId="" TaskKey=""
            TaskPriority="" TaskStatus="" TaskType=""
            TotalNumberOfRecords="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterTask.xml"
                IncludeXMLPath="/Task" RemoveDBHouseKeeping="Y"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="LPNNo" Name="TargetCaseId" NameInEntity="Target_LPN_No">
                        <Description>
                            <div>Target Case Id of the task</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="TargetPalletId" NameInEntity="Target_LPN_No">
                        <Description>
                            <div>Target Pallet Id of the task</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="TotalNumberOfRecords"/>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>
                            <div>returns <code>'Y'</code>: if task is purged and exists in history table.<br/>
				returns <code>'N'</code>: if task is not purged.</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Task>
    </TaskList>
    <DropoffList>
        <Location AisleNumber="" AisleSuffix="" BayNumber=""
            CheckDigit="" DockType="" FreezeMoveIn="" FreezeMoveOut=""
            InStagingLocationId="" IsSchedulable="" LevelNumber=""
            LocationBarCode="" LocationId="" LocationKey=""
            LocationSizeCode="" LocationType="" MoveInSeqNo=""
            MoveOutSeqNo="" Node="" OutStagingLocationId=""
            StorageCode="" VelocityCode="" XCoOrdinate="" YCoOrdinate=""
            ZCoOrdinate="" ZoneId="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Location_master.xml"
                IncludeXMLPath="/Location" RemoveDBHouseKeeping="Y"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="IsSchedulable"/>
                </Attributes>
            </yfc:doc>
        </Location>
    </DropoffList>
</DepositLocation>
