<Items xmlns="http://www.sterlingcommerce.com/documentation/INV/INVENTORY_CHANGE/EXTERNAL_SUPPLY_CHANGE">
    <Item ItemID="" OrganizationCode="" ProductClass="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element EntityName="Inventory_Item"/>
            <Attributes>
                <Attribute EntityName="Inventory_Item"
                    Name="OrganizationCode" NameInEntity="Organization_Code"/>
            </Attributes>
        </yfc:doc>
        <SupplyChanges>
            <Supply AccountNo="" AdjustmentType=""
                BuyerOrganizationCode="" ConfirmShipment=""
                CostCurrency="" CostReference="" DocumentType=""
                EnterpriseCode="" IsInboundOrder="" ItemID="" ItemKey=""
                OrderHeaderKey="" OrderLineKey="" OrderNo="" OwnerKey=""
                PrimeLineNo="" ProductClass="" Quantity="" Segment=""
                SegmentType="" SellerOrganizationCode="" ShipByDate=""
                ShipNode="" SubLineNo="" SupplyReference=""
                SupplyReferenceType="" SupplyType="" UnitCost=""
                UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply">
                        <Description>
                            <div>
						Supply change information.
						</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="String" Name="AccountNo">
                            <Description>
                                <div>
                            The account number affected by the adjustment.
                        </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="AdjustmentType">
                            <Description>
                                <div>
						The type of adjustment.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Order_Header" Name="BuyerOrganizationCode"/>
                        <Attribute DataType="Flag" Name="ConfirmShipment">
                            <Description>
                                <div>
						Indicates that supply change happened from the result of confirming a shipment.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="CostCurrency">
                            <Description>
                                <div>
						 Currency in which inventory cost is maintained.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="CostReference">
                            <Description>
                                <div>
						Indicates the type of order related to cost i.e., SalesOrder, PurchaseOrder and TransferOrder.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Order_Header"
                            Name="DocumentType" NameInEntity="Document_Type"/>
                        <Attribute EntityName="Order_Header"
                            Name="EnterpriseCode" NameInEntity="Enterprise_Key"/>
                        <Attribute DataType="Flag" Name="IsInboundOrder">
                            <Description>
                                <div>
						Indicates if order is an inbound order.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Item" Name="ItemID"/>
                        <Attribute EntityName="Item" Name="ItemKey"/>
                        <Attribute EntityName="Order_Header" Name="OrderHeaderKey"/>
                        <Attribute EntityName="Order_Line" Name="OrderLineKey"/>
                        <Attribute EntityName="Order_Header" Name="OrderNo"/>
                        <Attribute EntityName="Order_Line" Name="PrimeLineNo"/>
                        <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
                        <Attribute EntityName="Order_Header" Name="SellerOrganizationCode"/>
                        <Attribute EntityName="Order_Line" Name="SubLineNo"/>
                        <Attribute DataType="String" Name="UnitCost">
                            <Description>
                                <div>
                            UnitCost for the item. 
						</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
                    </Attributes>
                </yfc:doc>
                <Tag BatchNo="" LotNumber="" RevisionNo="">
                    <yfc:doc>
                        <Element EntityName="Inventory_Tag"/>
                    </yfc:doc>
                </Tag>
            </Supply>
        </SupplyChanges>
    </Item>
</Items>
