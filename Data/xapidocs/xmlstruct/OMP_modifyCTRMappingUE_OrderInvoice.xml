<OrderInvoice ActualFreightCharge="" AmountCollected="Required"
    ChargedActualFreight="" CollectedExternally="" CollectedThroughAR=""
    DateInvoiced="" DerivedFromOrderHeaderKey="" DocumentType=""
    EnterpriseCode="" HeaderCharges="" HeaderDiscount="" HeaderTax=""
    InvoiceCreationReason="" InvoiceNo="" InvoiceType="" LineSubTotal=""
    MasterInvoiceNo="" OrderInvoiceKey="" OrderNo="" Reference1=""
    RemainingAmount="" TotalAmount="" TotalCharges="" TotalDiscount=""
    TotalHeaderCharges="" TotalRequiredMappingAmount="" TotalTax=""
    isHistory="" xmlns="http://www.sterlingcommerce.com/documentation/OMP/modifyCTRMappingUE_OrderInvoice">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="YFS_getOrderInvoiceDetails_output.xml" IncludeXMLPath="/InvoiceDetail/InvoiceHeader"/>
    <yfc:doc>
        <Element EntityName="Order_Invoice"/>
        <Attributes>
            <Attribute DataType="Money" Name="HeaderCharges">
                <Description>Header Charge amount in the invoice.</Description>
            </Attribute>
            <Attribute DataType="Money" Name="HeaderDiscount">
                <Description>Header discount amount in the invoice.</Description>
            </Attribute>
            <Attribute DataType="Money" Name="HeaderTax">
                <Description>Tax amount in the invoice.</Description>
            </Attribute>
            <Attribute DataType="Text-100" Name="Reference1">
                <Description>Additional reference information.</Description>
            </Attribute>
            <Attribute DataType="Money" Name="RemainingAmount">
                <Description>
	            	RemainingAmount is how much is remaining to be needed after the current mapping.
		    	</Description>
            </Attribute>
            <Attribute DataType="ComputedMoney" Name="TotalCharges">
                <Description>Total charge amount in the invoice.</Description>
            </Attribute>
            <Attribute DataType="PlusMinusMoney" Name="TotalDiscount">
                <Description>Total discount amount in the invoice.</Description>
            </Attribute>
            <Attribute DataType="Money" Name="TotalHeaderCharges">
                <Description>Total header charge amount in the invoice.</Description>
            </Attribute>
            <Attribute DataType="Money" Name="TotalRequiredMappingAmount">
                <Description>
	            	The total amount to be mapped.
		    	</Description>
            </Attribute>
            <Attribute DataType="PlusMinusMoney" Name="TotalTax">
                <Description>Total tax amount in the invoice.</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="isHistory">
                <Description>
                    <div>Indicates if data is retrieved from the history table or the regular table. 
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ChargeTransactionList>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="OMP_modifyCTRMappingUE_ChargeTranList.xml" IncludeXMLPath="/ChargeTransactionList"/>
        <ChargeTransaction AsyncRequestIdentifier=""
            AuditTransactionID="" AuthorizationExpirationDate=""
            AuthorizationID="" BookAmount=""
            CallForAuthorizationStatus="" CashBackAmount=""
            ChargeTransactionKey="" ChargeType="" CollectionDate=""
            CreditAmount="" DebitAmount="" DistributedAmount=""
            ExecutionDate="" ForAsyncRequestIdentifier=""
            HoldAgainstBook="" InPerson="Y|N|" MappedAmount=""
            OfflineStatus="Y|N|" OpenAuthorizedAmount=""
            OrderHeaderKey="" OrderInvoiceKey="" PaymentEntryType=""
            PaymentKey="" PostponedAmount="" ReasonCode=""
            RequestAmount="" SettledAmount="" Status=""
            TransactionDate="" TransferFromOhKey="" TransferToOhKey=""
            UnusuableAmount="" UserExitStatus=""
            VoidTransaction="V|R|S|C|" yfc:DTDOccurrence="ZeroOrMany" yfc:GroupName="Order:ChargeTransactionDetail">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/ChargeTransactionDetails/ChargeTransactionDetail"/>
            <yfc:doc>
                <Element EntityName="Charge_Transaction"/>
                <Attributes>
                    <Attribute DataType="Money" Name="MappedAmount">
                        <Description>
		            	The amount that is mapped.
			    	</Description>
                    </Attribute>
                    <Attribute Name="TransactionDate" NameInEntity="Modifyts">
                        <Description>Transaction Date.
						</Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="UnusuableAmount">
                        <Description>
		            	The unusable amount.
			    	</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <PaymentMethod AwaitingAuthInterfaceAmount=""
                AwaitingChargeInterfaceAmount="" BillToKey=""
                CashBackAmount="" ChargeSequence="" CheckNo=""
                CheckReference="" CreditCardExpDate="" CreditCardName=""
                CreditCardNo="" CreditCardType="" CustomerAccountNo=""
                CustomerPONo="" DebitCardNo="" DisplayCreditCardNo=""
                DisplayCustomerAccountNo="" DisplayDebitCardNo=""
                DisplayPaymentReference1="" DisplayPrimaryAccountNo=""
                DisplaySvcNo="" FirstName="" FundsAvailable=""
                GetFundsAvailableUserExitInvoked=""
                IncompletePaymentType="" LastName="" MaxChargeLimit=""
                MiddleName="" PaymentKey="" PaymentReference1=""
                PaymentReference2="" PaymentReference3=""
                PaymentReference4="" PaymentReference5=""
                PaymentReference6="" PaymentReference7=""
                PaymentReference8="" PaymentReference9=""
                PaymentTemplateType="" PaymentType=""
                PaymentTypeGroup="" PlannedRefundAmount=""
                PrimaryAccountNo="" ReasonCode="" RequestedAuthAmount=""
                RequestedChargeAmount="" RequestedRefundAmount=""
                SuspendAnyMoreCharges="" SuspendExpiredAuthReversal=""
                SvcNo="" TotalAltRefundedAmount="" TotalAuthorized=""
                TotalCharged="" TotalRefundedAmount="" UnlimitedCharges="">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="MasterPaymentMethod.xml" IncludeXMLPath="/PaymentRelated/OutputForChargeTransaction/PaymentMethod"/>
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="MasterPaymentMethod.xml" IncludeXMLPath="/PaymentRelated/OutputForList/PaymentMethods/PaymentMethod"/>
                <yfc:doc>
                    <Element EntityName="Payment"/>
                    <Attributes>
                        <Attribute DataType="Money" Name="AwaitingAuthInterfaceAmount">
                            <Description>
								The total authorization amount that has not been
								sent to the payment processing system for processing yet.
							</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="AwaitingChargeInterfaceAmount">
                            <Description>
								The total charge amount that has not been sent to
								the payment
								processing system for processing yet.
						</Description>
                        </Attribute>
                        <Attribute DataType="Text-4" Name="DisplayPrimaryAccountNo">
                            <Description>The appropriate display value of the primary
								identifier of this PaymentType, depending upon the
								PaymentTypeGroup.</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="FundsAvailable">
                            <Description>
                                <div>
									The funds available to be charged on this payment method as
									determined by the
									<code>YFSGetFundsAvailableUE</code>
									user exit, if it was invoked. If the order is created for a
									customer and the payment method is a valid customer payment
									method, it indicates the amount available for the customer
									account. If customer account is not maintained internally or if
									the MaxChargeLimit is not set on the customer payment method,
									this value is returned as 0.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="GetFundsAvailableUserExitInvoked">
                            <Description>
                                <div>
									Indicates if the
									<code>YFSGetFundsAvailableUE</code>
									user exit was invoked for this payment method.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-100"
                            EntityName="Payment_Type" Name="PaymentTemplateType"/>
                        <Attribute DataType="CodeValue"
                            EntityName="Payment_Type" Name="PaymentTypeGroup"/>
                        <Attribute DataType="Text-1000" Name="PrimaryAccountNo">
                            <Description>The appropriate primary identifier of this
								PaymentType, depending upon the PaymentTypeGroup.</Description>
                        </Attribute>
                        <Attribute Name="RequestedAuthAmount">
                            <Description>
								The total amount of authorizations sent to an
								external
								system and still
								awaiting response on this payment
								method. 
						</Description>
                        </Attribute>
                        <Attribute Name="RequestedChargeAmount">
                            <Description>
								The total amount of charges sent to an external
								system and still
								awaiting response on this payment method.
							</Description>
                        </Attribute>
                        <Attribute Name="RequestedRefundAmount">
                            <Description>
								The total amount of refunds sent to an external
								system and still
								awaiting response on this payment method. This
								number will be
								positive. This total will no longer be included in
								the
								RequestedChargeAmount, but if an asynchronous refund is
								submitted
								in excess of this amount, the RequestedChargeAmount
								will
								be
								reduced for backwards compatibility.
						</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </PaymentMethod>
        </ChargeTransaction>
    </ChargeTransactionList>
</OrderInvoice>
