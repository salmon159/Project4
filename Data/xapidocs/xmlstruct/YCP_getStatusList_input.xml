<Status BaseProcessType="" CallingOrganizationCode="" Description=""
    DocumentType="Conditional" OwnerKey="" ProcessTypeKey="" Status=""
    StatusKey="" StatusName="" StatusType="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getStatusList/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Process_Type" Name="BaseProcessType" NameInEntity="Base_Process_Type">
                <Description>
                    <div>
				If <code>BaseProcessType</code> is passed, <code>DocumentType</code> should also be passed. If <code>ProcessTypeKey</code> is passed,
				its value is used to make the matches, and <code>BaseProcessType</code> is ignored. 
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="CallingOrganizationCode"/>
            <Attribute EntityName="Process_Type" Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>
				If <code>BaseProcessType</code> is passed, <code>DocumentType</code> should also be passed.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Process_Type" Name="ProcessTypeKey" NameInEntity="Process_Type_Key">
                <Description>
                    <div>
				Identifies the process type for which a status is defined. If passed, any information passed in <code>BaseProcessType</code> is ignored.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Status>
