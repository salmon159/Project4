<PromotionUsage CustomerContactID="" CustomerID="" OrderReference=""
    OrganizationCode="" SellerOrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YPM/confirmPromotionUsage/output">
    <yfc:doc>
        <Element EntityName="Promotion_Usage">
            <Description>
                <div>
					tracks usage for a promotion
				</div>
            </Description>
        </Element>
    </yfc:doc>
    <Promotion Action="" PromotionID="" PromotionReference="">
        <yfc:doc>
            <Element EntityName="Promotion_Usage">
                <Description>
                    <div>
						promotion information
					</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Action" Name="Action">
                    <Description>
                        <div>
							Action to perform
							<br/>
							Valid Values:
							<br/>
                            <b>
                                <code>CREATE</code>
                            </b>
							- Creates a new promotion usage record. If record already exists,
							the existing record will be updated
							<br/>
                            <b>
                                <code>REMOVE</code>
                            </b>
							- removes the existing record from the database.
							<br/>
                            <br/>
							The default behavior is
							<b>
                                <code>CREATE</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Promotion>
</PromotionUsage>
