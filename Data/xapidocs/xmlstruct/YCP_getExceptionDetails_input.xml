<Inbox InboxKey="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getExceptionDetails/input">
    <yfc:doc>
        <Element EntityName="Inbox">
            <Description>
                <div>
		Pass an <code>InboxKey</code> to identify the Inbox to be examined for exceptions.
		<Validation>
                        <Condition>The <code>InboxKey</code> is not passed, or is not valid.
			</Condition>
                        <Error
                            Description="Record does not exist in the database" ErrorCode="YFC0002"/>
                    </Validation>
                </div>
            </Description>
        </Element>
    </yfc:doc>
</Inbox>
