<!--	unreceiveOrder Input XML	-->
<Receipt CaseId="" DocumentType="" LocationId=""
    OverrideModificationRules="" PalletId="" ReceiptHeaderKey=""
    ReceiptNo="" ReceivingDock="" ReceivingNode="" ShipmentKey="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/UNRECEIVE_RECEIPT/ON_SUCCESS">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description> A CaseId. </Description>
            </Attribute>
            <Attribute EntityName="Document_Params" Name="DocumentType"/>
            <Attribute DataType="String" Name="LocationId"/>
            <Attribute DataType="Flag" Name="OverrideModificationRules">
                <Description> Indicates whether to override the status of receipt that can be considered for modification. Valid values are Y, N and Void. Void implies N.</Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description> A PalletId. </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Shipment BuyerOrganizationCode="" DocumentType="" EnterpriseCode=""
        OrderHeaderKey="" OrderNo="" OrderReleaseKey="" ReceivingNode=""
        ReleaseNo="" SellerOrganizationCode="" ShipNode="" ShipmentNo="">
        <yfc:doc>
            <Element EntityName="Shipment"/>
        </yfc:doc>
    </Shipment>
    <ReceiptLines>
        <!--	UnreceiveQuantity is ignored if UnreceiveEntireLine="Y"	-->
        <ReceiptLine InventoryStatus="Conditional" ItemID="" OrderNo=""
            ProductClass="" ReceiptLineKey="Conditional"
            ReceiptLineNo="Conditional" UnitOfMeasure=""
            UnreceiveEntireLine="Y|N" UnreceiveQuantity="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element EntityName="Receipt_Line"/>
                <Attributes>
                    <Attribute DataType="InventoryStatus" Name="InventoryStatus">
                        <Description>
                            <div>The Inventory Status of the item in the line in inventory. 
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="UnreceiveEntireLine">
                        <Description>
                            <div>if <code>
                                    <b>Y</b>
                                </code> entire line is unreceived.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Count" Name="UnreceiveQuantity">
                        <Description>
                            <div>Quantity to be unreceived from the line (Ignored if UnreceiveEntireLine="Y").
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ReceiptLine>
    </ReceiptLines>
    <Audit ReasonCode="" ReasonText="">
        <yfc:doc>
            <Element EntityName="Location_Inventory_Audit"/>
        </yfc:doc>
    </Audit>
</Receipt>
