<GetEncryptedCreditCardNumber CreditCardNo="Required" EnterpriseCode="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getEncryptedCreditCardNumber/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Text-32" Name="CreditCardNo">
                <Description>
                    <div>
					The credit card number which needs to be encrypted. 
				<br/>Valid Values:
				<br/>
                        <ul>Alphabetic characters A through Z
				<br/>Alphabetic characters a through z
				<br/>Numbers from 0 through 9
				<br/>Symbols ~  !  @  #  $  %   ^  &amp;  *  (
				)  _  +  {  }  |  :  "  &lt;  &gt;  ? `
				 -  =  [  ]  \  ;  '  ,  .  /
				<br/>Keys <code>Tab</code> and <code>Space</code>
                        </ul>
                        <Validation>
                            <Condition>Invalid value is passed.
						</Condition>
                            <Error
                                Description="Credit Card Encryption Failed" ErrorCode="YFS10294"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="EnterpriseCode"/>
        </Attributes>
    </yfc:doc>
</GetEncryptedCreditCardNumber>
