<EntityCustomAttribute AttributeDomainID="" AttributeGroupID=""
    AttributeID="" CallingOrganizationCode="Conditional" ColumnName=""
    DocumentType="" EntityAttributeDomainKey=""
    EntityCustomAttributeKey="" OrganizationCode="Conditional"
    Searchable="" Status="" TableName="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getEntityCustomAttributeList/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
                    <Validation>
                        <Condition>
                            <code>CallingOrganizationCode</code> or <code>OrganizationCode</code> must be passed.
						</Condition>
                        <Error
                            Description="OrganizationCode Or CallingOrganizationCode Is Mandatory For This Operation." ErrorCode="YSC91A_006"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Organization"
                Name="CallingOrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
					If <code>CallingOrganizationCode</code> is passed, the EntityCustomAttribute list for the organization that has configured custom attributes for this <code>CallingOrganizationCode</code> is returned.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Entity_Attribute_Domain"
                Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute Name="OrganizationCode">
                <Description>
                    <div>
					The organization that has defined the custom attributes.
					If <code>CallingOrganizationCode</code> is not passed in the input, then this <code>OrganizationCode</code> passed in the input is used to fetch the list.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Entity_Attribute_Domain"
                Name="TableName" NameInEntity="Table_Name"/>
        </Attributes>
    </yfc:doc>
</EntityCustomAttribute>
