<?xml version="1.0" encoding="UTF-8"?>
<Locations TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getLocationList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>Indicates total number of records fetched.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Location AisleNumber="" AisleSuffix="" BayNumber="" CheckDigit=""
        Createprogid="" Createts="" Createuserid="" DockType=""
        FreezeMoveIn="" FreezeMoveOut="" InStagingLocationId=""
        IsSchedulable="" LevelNumber="" LocationBarCode="" LocationId=""
        LocationKey="" LocationSizeCode="" LocationType="" Lockid=""
        Modifyprogid="" Modifyts="" Modifyuserid="" MoveInSeqNo=""
        MoveOutSeqNo="" Node="" OutStagingLocationId="" StorageCode=""
        VelocityCode="" XCoOrdinate="" YCoOrdinate="" ZCoOrdinate=""
        ZoneId="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Location_master.xml" IncludeXMLPath="/Location"/>
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="IsSchedulable"/>
            </Attributes>
        </yfc:doc>
        <LocationSize Createprogid="" Createts="" Createuserid=""
            Description="" DimensionUom="" Height=""
            IsInfiniteCapacity="" Length="" LocationSizeCode=""
            LocationSizeKey="" Lockid="" MaxWeight="" Modifyprogid=""
            Modifyts="" Modifyuserid="" Node="" Volume="" VolumeUom=""
            WeightUom="" Width="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Location_master.xml" IncludeXMLPath="/Location/LocationSize"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="UOM" Name="DimensionUom">
                        <Description>Unit of measure for Dimension. This unit of measure applies to Height, Length and Width attributes.</Description>
                    </Attribute>
                    <Attribute DataType="Volume" Name="Volume">
                        <Description>Volume that a location can hold. This field is computed using the Height, Length and Width attributes as defined above.</Description>
                    </Attribute>
                    <Attribute DataType="UOM" Name="VolumeUom">
                        <Description>Unit of measure for Volume.</Description>
                    </Attribute>
                    <Attribute DataType="UOM" Name="WeightUom">
                        <Description>Unit of measure for Weight.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <LocationCapacities TotalNumberOfRecords="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Count" Name="TotalNumberOfRecords">
                            <Description>Indicates total number of records fetched.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <LocationCapacity Createprogid="" Createts=""
                    Createuserid="" EnterpriseCode=""
                    ItemClassificationCode="" LocationCapacityKey=""
                    LocationSizeCode="" Lockid="" Modifyprogid=""
                    Modifyts="" Modifyuserid="" NoOfPackages="" Node=""
                    PackagingType="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="YFS_Location_master.xml" IncludeXMLPath="/Location/LocationSize/LocationCapacities/LocationCapacity"/>
                </LocationCapacity>
            </LocationCapacities>
        </LocationSize>
        <SKUDedications TotalNumberOfRecords="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Count" Name="TotalNumberOfRecords">
                        <Description>Indicates total number of records fetched.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <SKUDedication Createprogid="" Createts="" Createuserid=""
                EnterpriseCode="Required" ItemId="Required"
                LocationId="" Lockid="" MaxQty="" MaxQuantity=""
                MinQty="" MinQuantity="" Modifyprogid="" Modifyts=""
                Modifyuserid="" Node="" ProductClass="Required"
                SKUDedicationKey="" Segment="" SegmentType=""
                UnitOfMeasure="Required" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="YFS_Location_master.xml" IncludeXMLPath="/Location/SKUDedications/SKUDedication"/>
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="MaxQuantity">
                            <Description> Maximum quantity of SKU that should be stored in the location. This attribute is used by put away location selection algorithm to ensure that no more than the maximum quantity for that item at that location is put away, even if location capacity constraints allow for more quantity than the maximum quantity. It is computed as minimum of LocationSize and LocationCapacity.</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="MinQuantity">
                            <Description>Minimum quantity of SKU for replenishing. It is computed as   <Code> MaxQuantity * (TriggerPercent/100)</Code>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </SKUDedication>
        </SKUDedications>
        <TransactionalLocationAttributes AvailableVolume=""
            AvailableWeight="" Createprogid="" Createts=""
            Createuserid="" FillCode="" FreezeOnVariance=""
            LocationKey="" Lockid="" Modifyprogid="" Modifyts=""
            Modifyuserid="" PendInVolume="" PendInWeight=""
            TransactionalLocationAttributesKey="" VolumeUom="" WeightUom="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Location_master.xml" IncludeXMLPath="/Location/TransactionalLocationAttributes"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="UOM" Name="VolumeUom">
                        <Description>Unit of measure for Volume.</Description>
                    </Attribute>
                    <Attribute DataType="UOM" Name="WeightUom">
                        <Description>Unit of measure for Weight.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </TransactionalLocationAttributes>
    </Location>
</Locations>
