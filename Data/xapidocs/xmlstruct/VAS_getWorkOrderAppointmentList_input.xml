<WorkOrderAppointment AdditionalEquipmentUsed="" AlertSales=""
    AlertScheduler="" AllocatedQuantity="" ApptSeq="" ApptStatus=""
    AttentionRequired="" ConsiderCurrentSlot="" ExecutionBy=""
    ExecutionComments="" ExecutionDuration="" ExecutionEndTime=""
    ExecutionStartTime="" ExpectedApptEndDate=""
    ExpectedApptStartDate="" IsRoutePlanningComplete="" ItemsNotUsed=""
    MaximumRecords="" NonInventoryItemsReturned=""
    PromisedApptEndDate="" PromisedApptStartDate="" RequestedQuantity=""
    RequestedVolume="" RequestedWeight="" ResourcePoolId=""
    ServiceResourceId="" StopNumber="" VehicleNumber=""
    VehicleOdometerReading="" VehicleServiceRequest=""
    WorkOrderApptKey=""
    xmlns="http://www.sterlingcommerce.com/documentation/VAS/getWorkOrderAppointmentList/input" yfc:QryTypeSupported="Y">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="YFS_Work_Order_Appointment_master.xml" IncludeXMLPath="/WorkOrderAppointment"/>
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="ConsiderCurrentSlot">
                <Description>
                    <div>
					The <code>ConsiderCurrentSlot</code> flag indicates that system should consider the available capacity of the current slots, which the current timestamp falls under.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>Specifies maximum number of records that must be fetched.
				</Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="RequestedVolume">
                <Description>
                    <div>The quantity requested in the work order's volume unit of measure, which is specified in the <code>VolumeUOM</code> field of the work order</div>
                </Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="RequestedWeight">
                <Description>
                    <div>The quantity requested in the work order's weight unit of measure, which is specified in the <code>WeightUOM</code> field of the work order</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <WorkOrder AllocatedLevel="" CaseId="" DocumentType=""
        EnterpriseCode="" FinishNoLaterThan="" HoldFlag=""
        InvUpdateActivityCode="" ItemID="" LevelOfService=""
        NextAlertTs="" NodeKey="" OpenWorkOrderFlag="" OrderHeaderKey=""
        OrderLineKey="" OrderNo="" PalletId="" PipeLineKey=""
        PreCallStatus="" Priority="" ProductClass=""
        PromisedApptEndDate="" PromisedApptStartDate="" Purpose=""
        QuantityAllocated="" QuantityCompleted="" QuantityReleased=""
        QuantityRemoved="" QuantityRequested="" ReasonCode=""
        ReasonText="" ReleasedLevel="" Segment="" SegmentType=""
        SerialNo="" ServiceComplexityLevel="" ServiceItemGroupCode=""
        ServiceItemID="" ServiceSupervisorId="" ServiceUom=""
        ShipByDate="" ShipmentKey="" ShipmentNo="" StartNoEarlierThan=""
        Status="" StatusDate="" Uom="" WorkOrderKey="" WorkOrderNo="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Work_Order_tran.xml" IncludeXMLPath="/WorkOrder"/>
        <yfc:doc>
            <Element EntityName="Work_Order"/>
            <Attributes>
                <Attribute DataType="String" Name="AllocatedLevel">
                    <Description>
                        <div>Indicates the extent of allocation for the work order. The valid values are FULL, PARTIAL, NONE, and ANY. The respective values tells wether the work order is fully, partially, not allocated or is in any of the the previously mentioned states.
	        		</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description>
                        <div>Case for which the work order is processed.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description>
                        <div>Pallet for which the work order is processed.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ReleasedLevel">
                    <Description>
                        <div>Indicates the extent to which the work order is released. The valid values are FULL, PARTIAL, NONE, and ANY. The respective values tells wether the work order is fully, partially, not relased or is in any of the previously mentioned states.
	            	</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Order BillToID="">
            <yfc:doc>
                <Element EntityName="Order_Header"/>
            </yfc:doc>
        </Order>
    </WorkOrder>
</WorkOrderAppointment>
