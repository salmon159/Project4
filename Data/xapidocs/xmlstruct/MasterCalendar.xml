<!--MasterCalendar XML-->
<Calendar AllowShiftOverride="" CalendarDescription=""
    CalendarId="Required" CalendarKey="" OrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/MasterCalendar">
    <InheritFromCalendar CalendarId="" InheritCalendarKey="" OrganizationCode="">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Calendar" Name="CalendarId" NameInEntity="Calendar_Id">
                    <Description>
                        <div>
					The <code>CalendarId</code> of the parent calendar.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Calendar"
                    Name="InheritCalendarKey" NameInEntity="Calendar_Key">
                    <Description>
                        <div>
					The <code>CalendarKey</code> of the parent calendar.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Calendar" Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
					The <code>OrganizationCode</code> of the parent calendar.
					</div>
                    </Description>
                </Attribute>
            </Attributes>
            <Element>
                <Description>
                    <div>
				The parent calendar for which this calendar inherites its definition from.
				</div>
                </Description>
            </Element>
        </yfc:doc>
    </InheritFromCalendar>
    <EffectivePeriods>
        <EffectivePeriod EffectiveFromDate="" EffectiveToDate=""
            ShiftEffectivePeriodKey="" yfc:DTDOccurrence="ZeroOrMany">
            <Shifts>
                <Shift FridayValid="" MondayValid="" SaturdayValid=""
                    ShiftEndTime="" ShiftKey="" ShiftName=""
                    ShiftStartTime="" ShiftType="" SundayValid=""
                    ThursdayValid="" TuesdayValid="" WednesdayValid="" yfc:DTDOccurrence="ZeroOrMany"/>
            </Shifts>
        </EffectivePeriod>
    </EffectivePeriods>
    <CalendarDayExceptions>
        <CalendarDayException CalendarDayExceptionKey="" Date=""
            ExceptionType="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-1" Name="ExceptionType">
                        <Description>ExceptionType  = 1 for Working day, 0 for Off day.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ExceptionShifts>
                <ExceptionShift ExceptionShiftKey="" ShiftKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <Shift ShiftEndTime="" ShiftKey="" ShiftName=""
                        ShiftStartTime="" ShiftType=""/>
                </ExceptionShift>
            </ExceptionShifts>
        </CalendarDayException>
    </CalendarDayExceptions>
</Calendar>
