<InventoryAlerts AlertLevel="" AlertType="" DistributionRuleId=""
    EstimatedCost="" InventoryOrganizationCode="Conditional" Latitude=""
    Longitude="" MaximumRecords="" Node=""
    OrganizationCode="Conditional" UseDefaultDistributionRuleId="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getInventoryAlertsList/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Inventory_Alerts" Name="AlertLevel" NameInEntity="Alert_Level"/>
            <Attribute EntityName="Inventory_Alerts" Name="AlertType" NameInEntity="Alert_Type"/>
            <Attribute EntityName="Inventory_Alerts"
                Name="DistributionRuleId" NameInEntity="Distribution_Rule_Id">
                <Description>
                    <div>
				If Node is specified in input, value of DistributionRuleId is ignored.
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="InventoryOrganizationCode">
                <Description>
                    <div>
				Identifies the inventory organization. Either OrganizationCode or InventoryOrganizationCode must be passed in the input XML.
				<Validation>
                            <Condition>Must be a existing <code>OrganizationCode</code>.
					</Condition>
                            <Error
                                Description="Invalid Organization Code" ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>One of these must be passed: <code>OrganizationCode</code> or <code>InventoryOrganizationCode</code>.
					</Condition>
                            <Error
                                Description="OrganizationCode is mandatory" ErrorCode="YFS10435"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>Specifies the Maximum Number of records to be returned. Default is 500.
			</Description>
            </Attribute>
            <Attribute EntityName="Inventory_Alerts" Name="Node" NameInEntity="Ship_Node"/>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>
				Identifies the organization. Either OrganizationCode or InventoryOrganizationCode must be passed in the input XML.
				<Validation>
                            <Condition>Must be a existing <code>OrganizationCode</code>.
					</Condition>
                            <Error
                                Description="Invalid Organization Code" ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>One of these must be passed: <code>OrganizationCode</code> or <code>InventoryOrganizationCode</code>.
					</Condition>
                            <Error
                                Description="OrganizationCode is mandatory" ErrorCode="YFS10435"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="UseDefaultDistributionRuleId">
                <Description>
                    <div>
				DefaultDistributionRuleId belonging to organization is used when Node and DistributionRuleId is blank in input.
				<br/>Default value: N. Valid values: Y or N.                        
               	</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <InventoryItems>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				Pass this element only if list of inventory item is required in input. 
			</div>
                </Description>
            </Element>
        </yfc:doc>
        <InventoryItem InventoryItemKey="Conditional"
            ItemID="Conditional" ProductClass="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
				Either InventoryItemKey or ItemID must be passed for InventoryItem.
			</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Key" Name="InventoryItemKey">
                        <Description>
                            <div>
						Identifies the inventory item. Either InventoryItemKey or ItemID must be passed in the input XML.
						The primary key for the YFS_INVENTORY_ITEM table.
						This attribute corresponds to the PrimaryKey Fact.
				    <Validation>
                                    <Condition>One of these must be passed: <code>InventoryItemKey</code> or <code>ItemID</code>.
							</Condition>
                                    <Error
                                    Description="Missing attribute in input" ErrorCode="YFS10460"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="ItemID" Name="ItemID">
                        <Description>
                            <div>
						Identifies the inventory item. Either InventoryItemKey or ItemID must be passed in the input XML.
				    <Validation>
                                    <Condition>One of these must be passed: <code>InventoryItemKey</code> or <code>ItemID</code>.
							</Condition>
                                    <Error
                                    Description="Missing attribute in input" ErrorCode="YFS10460"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Inventory_Item"
                        Name="ProductClass" NameInEntity="Product_Class"/>
                    <Attribute EntityName="Inventory_Item"
                        Name="UnitOfMeasure" NameInEntity="Uom"/>
                </Attributes>
            </yfc:doc>
        </InventoryItem>
    </InventoryItems>
</InventoryAlerts>
