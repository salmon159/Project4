<?xml version="1.0" encoding="UTF-8"?>
<MovePlannedTaskDate xmlns="http://www.sterlingcommerce.com/documentation/WMS/movePlannedTaskDate/input">
    <Criteria NumberOfHours="" NumberOfShipments="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Count" Name="NumberOfHours">
                    <Description>The number of hours of work to be moved to the given date.</Description>
                </Attribute>
                <Attribute DataType="Count" Name="NumberOfShipments">
                    <Description>The number of shipments to be moved to the given date.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <PlannedTask EntityKey="" EntityName="Required" Node="Required"
            PlannedDate="" PlannedDateType="" ResourcePoolKey=""
            TaskType="" yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element EntityName="Planned_Task"/>
                <Attributes>
                    <Attribute EntityName="Planned_Task" Name="EntityName">
                        <Description>
                            <div>
								Identifies the entity being planned for moving.
								<Validation>
                                    <Condition>
                                    <code>EntityName</code> must be passed.</Condition>
                                    <Error
                                    Description="Type of entity/request to be considered for movement must be passed." ErrorCode="WMSRP80_004"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Planned_Task" Name="Node">
                        <Description>
                            <div>
								Identifies the node for which planning is done.
								<Validation>
                                    <Condition>
                                    <code>Node</code> must be passed.</Condition>
                                    <Error
                                    Description="Shipnode must be passed." ErrorCode="WMS00054"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Shipment RequestedShipmentDate=""/>
            <ResourcePool CapacityOrganizationCode="" ResourcePoolId=""/>
        </PlannedTask>
    </Criteria>
    <Action PlannedDate="Required" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Planned_Task"/>
            <Attributes>
                <Attribute EntityName="Planned_Task" Name="PlannedDate">
                    <Description>
                        <div>
							The date to which planned tasks for the request to be moved.
							<Validation>
                                <Condition>
                                    <code>PlannedDate</code> must be passed.</Condition>
                                <Error
                                    Description="Planning Date is Mandatory." ErrorCode="WMSRP80_002"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>PlannedDate</code> cannot be before today.</Condition>
                                <Error
                                    Description="Planning Date cannot be before today." ErrorCode="WMSRP80_003"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Action>
</MovePlannedTaskDate>
