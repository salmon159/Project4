<Model Country="" Depth="" Id="" Language="" Name="" Path="" Type="" xmlns="http://www.sterlingcommerce.com/documentation/YIC/getConfigurationModel/output">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					A model is a terminal node and represents a configurable product or a reusable building block.  
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Text-120" Name="Country">
                <Description>
                    <div>
						The country identifier for the model.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="Depth">
                <Description>
                    <div>
						The depth of items within the model that is retrieved.  
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="Id">
                <Description>
                    <div>
						Configurator item id of the node.  For any given model the item ids are unique within that model.  The root node of a model always has an ID 0.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Language">
                <Description>
                    <div>
						The language identifier for the model.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Name">
                <Description>
                    <div>
						The human-readable model name. 
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-480" Name="Path">
                <Description>
                    <div>
						The configurator path to a model. This path is mangled such that special characters are replaced by their Unicode code preceded by an underscore char.						
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Type">
                <Description>
                    <div>
						The type of model. This attribute is returned as tree for this api.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Tab Errors="" GuidingText="" Name="" Suggestions="" Warnings="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
						This element holds information about tabs.  
					</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Flag" Name="Errors">
                    <Description>
                        <div>
							Flag indicating if the tab returned has errors or not.							
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-480" Name="GuidingText">
                    <Description>
                        <div>
							The guiding text for the tab.							
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" Name="Name">
                    <Description>
                        <div>
							The name of the tab.									
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="Suggestions">
                    <Description>
                        <div>
							Flag indicating if the tab returned has suggestions or not.							
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="Warnings">
                    <Description>
                        <div>
							Flag indicating if the tab returned has warnings or not.							
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Member Class="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
							This element holds information about the member option classes.  
						</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Text-120" Name="Class">
                        <Description>
                            <div>
								The name of a member option class.									
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Member>
    </Tab>
    <Property Name="" Path="" Type="" Value="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="masterPropertyTitleCase.xml" IncludeXMLPath="/"/>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					This element holds information about properties associated to item.  
				</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-480" Name="Name">
                    <Description>
                        <div>
						The name of the property.							
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" Name="Path">
                    <Description>
                        <div>
						The path to the item that contains the property.									
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" Name="Type">
                    <Description>
                        <div>
						The datatype of the property.
						<br/>Valid Values: <b>
                                <code>bigdecimal</code>
                            </b> or <b>
                                <code>int</code>
                            </b> or <b>
                                <code>List</code>
                            </b> or <b>
                                <code>long</code>
                            </b> or <b>
                                <code>double</code>
                            </b> or <b>
                                <code>String</code>
                            </b> or <b>
                                <code>date</code>
                            </b> or <b>
                                <code>unknown</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-480" Name="Value">
                    <Description>
                        <div>
						The value in string form for the property. List values are returned with each value separated by a carriage return.		
					</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Property>
