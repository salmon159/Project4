                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
					This element holds information about properties associated to item.  
				</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Text-480" Name="Name">
                            <Description>
                                <div>
						The name of the property.							
					</div>
                            </Description>
                        </Attribute>
