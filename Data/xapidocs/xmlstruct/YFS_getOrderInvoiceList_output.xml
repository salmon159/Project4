<!--getOrderInvoiceList Output XML-->
<!-- OrderInvoiceList is a template driven element. But its attributes are not except the attribute TotalNumberOfRecords. TotalNumberOfRecords is the only template driven attribute for this element.-->
<OrderInvoiceList LastOrderInvoiceKey="" LastRecordSet=""
    ReadFromHistory="" TotalNumberOfRecords="" TotalOrderInvoiceList="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderInvoiceList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Key" Name="LastOrderInvoiceKey">
                <Description>
                    <div> Last Order Invoice Key in the output.<br/>
                        <em>
                            <code>LastOrderInvoiceKey</code> is not supported when getPage API is used in multiTenancy environment.</em>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="LastRecordSet">
                <Description>
                    <div>Flag to indicate if the records obtained are a complete list. If there are more than 500 records matching the criteria passed in input xml, only 500 records are returned and this flag is set to <b>N</b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="ReadFromHistory">
                <Description>
                    <div>
                    Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
                    <br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_ORDER_INVOICE table.
                    <br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_ORDER_INVOICE_H table (the history table).
                    <br/>
                        <b>
                            <code>B</code>
                        </b> - Returns records that satisfy the input criteria from both YFS_ORDER_INVOICE and YFS_ORDER_INVOICE_H tables. Records returned from the current table are first in the output.
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>
                    <div>Total number of records that match the input criteria.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="TotalOrderInvoiceList">
                <Description>
                    <div>Total Order Invoice List
			</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <!-- OrderInvoice is a template driven element. -->
    <OrderInvoice ActualFreightCharge="" AmountCollected=""
        AmountPassedToAr="" ChargeTransactionKey=""
        ChargedActualFreight="" CollectedExternally="" Currency=""
        DocumentType="" EnterpriseCode="" InvoiceCreationReason=""
        InvoiceNo="" InvoiceType="" LineSubtotal="" MasterInvoiceNo=""
        OrderHeaderKey="" OrderInvoiceKey="" OrderNo="" OtherCharges=""
        ReturnReleaseKey="" SellerOrganizationCode="" ShipNode=""
        ShipmentKey="" ShipmentNo="" Status="" Tax="" TotalAmount=""
        TotalTax="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Money" EntityName="Order_Invoice"
                    Name="AmountPassedToAr" NameInEntity="Amount_Passed_To_Ar"/>
                <Attribute DataType="Money" EntityName="Order_Invoice"
                    Name="LineSubtotal" NameInEntity="Line_Subtotal"/>
                <Attribute DataType="Money" EntityName="Order_Invoice"
                    Name="Tax" NameInEntity="Tax"/>
                <Attribute DataType="Flag" Name="isHistory">
                    <Description>
                        <div>
                    Valid Values:
                    <br/>
                            <b>
                                <code>N</code>
                            </b> - Record is from the current table, YFS_ORDER_INVOICE.
                    <br/>
                            <b>
                                <code>Y</code>
                            </b> - Record is from the history table, YFS_ORDER_INVOICE_H.
                    </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </OrderInvoice>
</OrderInvoiceList>
