<Configuration ConfigId="Required" OrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YIC/getConfigurationDetail/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					The root node of the XML document contains header information for querying configuration detail. This is a required attribute.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Key" EntityName="BOM_Header"
                Name="ConfigId" NameInEntity="BOM_Id">
                <Description>
                    <div>
						The ID of the configuration to retrieve.
						<Validation>
                            <Condition>
                                <code>ConfigId</code> is not passed or invalid. </Condition>
                            <Error
                                Description="Invalid key was passed to the api." ErrorCode="YIC10005"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>The <code>OrganizationCode</code> to be used for schema determination.	
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Configuration>
