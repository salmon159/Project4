<Batch BatchKey="" BatchNo="" BatchStatusDesc="" CountRequestNo=""
    EquipmentType="" ManuallyCreated="" MoveRequestNo="" Node=""
    OrganizationCode="" RemainingMinutes="" ShipmentNo="" Status=""
    TaskType="" WaveNo="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/registerBatchCompletion/output">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="MasterBatch.xml" IncludeXMLPath="/Batch"/>
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="BatchStatusDesc">
                <Description>
                    <div>Batch Status Description</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Ship_Node" Name="Node" NameInEntity="Ship_Node"/>
        </Attributes>
    </yfc:doc>
    <BatchLocations>
        <BatchLocation BatchKey="" BatchLocationKey="" CartLocationId=""
            MoveRequestKey="" ShipmentContainerKey="" ShipmentKey=""
            SlotNumber="" ToteId="" isHistory="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="isHistory"/>
                </Attributes>
            </yfc:doc>
        </BatchLocation>
    </BatchLocations>
    <Tasks>
        <Task AssignedToUserId="" AssignmentNo="" AssignmentStatus=""
            BatchHold="" DependencyHold="" EndTimeStamp=""
            EnterpriseKey="" EquipmentId="" EquipmentLocationId=""
            ExceptionHold="" FinishNoLaterThan="" HoldReasonCode=""
            HoldReasonText="" IsSummaryTask="" Node=""
            OrganizationCode="" ParentTaskId="" PredecessorTaskId=""
            ProductivityKey="" ReasonCode="" ReasonText="" RootTaskId=""
            SourceAisle="" SourceLocationId="" SourceSortSequence=""
            SourceZoneId="" StartNoEarlierThan="" StartTimeStamp=""
            TargetAisle="" TargetCaseId="" TargetLocationId=""
            TargetPalletId="" TargetSortSequence="" TargetZoneId=""
            TaskId="" TaskKey="" TaskPriority="" TaskStatus=""
            TaskType="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterTask.xml" IncludeXMLPath="/Task"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="LPNNo" Name="TargetCaseId" NameInEntity="Target_LPN_No">
                        <Description>
                            <div>Target Case Id of the task</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="TargetPalletId" NameInEntity="Target_LPN_No">
                        <Description>
                            <div>Target Pallet Id of the task</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>
                            <div>returns <code>'Y'</code>: if task is purged and exists in history table.<br/>
				returns <code>'N'</code>: if task is not purged.</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Inventory CountryOfOrigin="" FifoNo="" InventoryStatus=""
                ItemId="" ProductClass="" Quantity=""
                ReceiptHeaderKey="" Segment="" SegmentType=""
                SerialNo="" ShipByDate="" SourceCaseId=""
                SourcePalletId="" TagNumber="" UnitOfMeasure="">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="MasterTask.xml" IncludeXMLPath="/Task/Inventory"/>
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="LPNNo" Name="SourceCaseId" NameInEntity="Source_LPN_No">
                            <Description>
                                <div>Source Case Id of the task</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="LPNNo"
                            Name="SourcePalletId" NameInEntity="Source_LPN_No">
                            <Description>
                                <div>Source Pallet Id of the task</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Inventory>
            <TaskReferences ContainerNo="" CountRequestKey=""
                CountRequestNo="" DocumentType="" OrderHeaderKey=""
                OrderLineKey="" OrderNo="" OrderReleaseKey=""
                PrimeLineNo="" ReleaseNo="" ShipmentContainerKey=""
                ShipmentKey="" ShipmentNo="" SubLineNo="" WaveNo="">
                <yfc:doc>
                    <Element EntityName="Task"/>
                </yfc:doc>
            </TaskReferences>
            <TaskType ActivityCode="" ActivityGroupId=""
                BatchRequired="" BatchRuleKey="" ExecTasksThruDropoff=""
                HoldTaskOnCreation="" IsAdHocMove="" MaxUsersAllowed=""
                Node="" OrganizationCode=""
                PickIntoInventoryContainer=""
                PickIntoOutboundContainer="" PrimaryTaskReference=""
                ProductivityType="" ResourceIdOverride=""
                SourceTaskOrderBy="" SourceTaskSuggestion=""
                StandardRate="" StandardRateUom="" TargetTaskOrderBy=""
                TargetTaskSuggestion="" TaskType="" TaskTypeKey=""
                TaskTypeName="" TaskTypePriority="" VoiceWorkFlowId="">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="MasterTask.xml" IncludeXMLPath="/Task/TaskType"/>
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="MasterTaskType.xml" IncludeXMLPath="/TaskType"/>
                <BatchRule BatchRuleKey="" EquipmentType="" MaxTasks=""
                    MaxTransactions="" MaxVolume="" OrganizationCode=""
                    PrimaryBatchReference="" SeparateMultiAreaBatches=""/>
            </TaskType>
            <TaskStatusAudits>
                <TaskStatusAudit NewStatus="" OldStatus="" ReasonCode=""
                    ReasonText="" TaskKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Task_Status_Audit"/>
                    </yfc:doc>
                </TaskStatusAudit>
            </TaskStatusAudits>
        </Task>
    </Tasks>
</Batch>
