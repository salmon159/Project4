<!-- ignoreError Input XML -->
<IntegrationError ErrorTxnId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YIF/ignoreIntegrationError/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
				The <code>ignoreIntegrationError</code> API ignores an error.
				The Error Id is passed as an input parameter.
				Only errors that are in an "Initial" state are ignored.
				</div>
            </Description>
        </Element>
    </yfc:doc>
</IntegrationError>
