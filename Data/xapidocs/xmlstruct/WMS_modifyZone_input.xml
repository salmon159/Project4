<?xml version="1.0" encoding="UTF-8"?>
<Zone AttemptSKUConsolidation="" BuildingNumber="" CheckDigit=""
    ConfirmLocnEmptyAfterPick="" Description="" ForceLocnScanOnVisit=""
    IsActiveZone="" LocationsPerItem="" MixCountryOfOrigin=""
    MixEnterprise="" MixFifo="" MixInventoryStatus="" MixInventoryTag=""
    MixProductClass="" MixReceiptNo="" MixSKU="" MixSegment=""
    MixShipByDate="" Node="" PrimaryBulkZoneId="" PutOnVarianceHold=""
    TrackCaseLPN="" TrackInventory="" TrackPalletLPN="" ZoneId=""
    ZoneKey="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/modifyZone/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="YFS_Zone_master.xml" IncludeXMLPath="/Zone" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Zone"/>
        <Attributes>
            <Attribute EntityName="Zone" Name="IsActiveZone" NameInEntity="Is_Active_Zone">
                <Description>
                    <div>
						If passed, it must be a valid bulk zone in the node. Bulk zones are identified by <code>IsActiveZone</code>=<code>
                            <b>N</b>
                        </code>.
							<Validation>
                            <Condition>A primary bulk zone (<code>IsActiveZone</code>=<code>
                                    <b>N</b>
                                </code>) cannot be modified to an active zone if it has active zones. 
								</Condition>
                            <Error
                                Description="The Primary Bulk Zone has active zones." ErrorCode="WMS00079"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Zone" Name="Node" NameInEntity="Node_Key">
                <Description>
                    <div>
                        <code>Node</code> Identifier. Primary Key of the <code>YFS_SHIP_NODE</code> table. Forms unique key of this table in conjunction with the <code>ZONE_ID</code>.
							<Validation>
                            <Condition>should be a valid <code>Node</code> and <code>ZoneId</code>.
								</Condition>
                            <Error
                                Description="Invalid Node and ZoneId combination." ErrorCode="WMS00035"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Zone" Name="PrimaryBulkZoneId" NameInEntity="Primary_Bulk_Zone_Id">
                <Description>
                    <div>
						If passed, it must be a valid bulk zone in the node. Bulk zones are identified by <code>IsActiveZone</code>=<code>
                            <b>N</b>
                        </code>.
							<Validation>
                            <Condition>If passed, <code>PrimaryBulkZoneId</code> must be a valid bulk zone in the node. Bulk zones are identified by <code>IsActiveZone</code>=<code>
                                    <b>N</b>
                                </code>. 
								</Condition>
                            <Error
                                Description="Invalid Node and ZoneId combination" ErrorCode="WMS00035"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Zone" Name="ZoneId" NameInEntity="Zone_Id">
                <Description>
                    <div>
                        <code>Zone</code> Identifier. Forms unique key of the <code>YFS_ZONE</code> table in conjunction with the <code>NODE_KEY</code>.
							<Validation>
                            <Condition>should be a valid <code>Node</code> and <code>ZoneId</code>.
								</Condition>
                            <Error
                                Description="Invalid Node and ZoneId combination." ErrorCode="WMS00035"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ZoneEnterprises Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>
                        If Reset Flag is passed:
                            Y =&gt; All existing Zone enterprise records will be deleted and the currently passed ones will be created.
                            N =&gt; The passed zone enterprise elements will be treated as a delta change.
                        </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ZoneEnterprise EnterpriseCode="" ZoneEnterpriseKey=""
            ZoneKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Zone_master.xml"
                IncludeXMLPath="/Zone/ZoneEnterprises/ZoneEnterprise" RemoveDBHouseKeeping="Y"/>
            <StorageConstraints Reset="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Reset">
                            <Description>
                                If Reset Flag is passed:
                                    Y =&gt; All existing Storage Constraint records will be deleted and the currently passed ones will be created.
                                    N =&gt; The passed StorageConstraint elements will be treated as a delta change.
                                </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <StorageConstraint ItemClassification1=""
                    ItemClassification2="" ItemClassification3=""
                    StorageConstraintKey="" ZoneEnterpriseKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="YFS_Zone_master.xml"
                        IncludeXMLPath="/Zone/ZoneEnterprises/ZoneEnterprise/StorageConstraints/StorageConstraint" RemoveDBHouseKeeping="Y"/>
                </StorageConstraint>
            </StorageConstraints>
        </ZoneEnterprise>
    </ZoneEnterprises>
    <InventoryStatusTransitions Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>
                        If Reset Flag is passed:
                            Y =&gt; All existing quality status transitions will be deleted and the currently passed ones will be created.
                            N =&gt; The passed InventoryStatusTransition elements will be treated as a delta change.
                        </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <InventoryStatusTransition InventoryStatus=""
            InventoryStatusTransitionKey="" NewInventoryStatus=""
            ZoneId="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Zone_master.xml"
                IncludeXMLPath="/Zone/InventoryStatusTransitions/InventoryStatusTransition" RemoveDBHouseKeeping="Y"/>
        </InventoryStatusTransition>
    </InventoryStatusTransitions>
    <ZoneLocations Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>
                        If Reset Flag is passed:
                            Y =&gt; All existing locations in the zone will be deleted and the currently passed ones will be created.
                            N =&gt; The passed ZoneLocation elements will be treated as a delta change.
                        </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ZoneLocation AisleNumber="" AisleSuffix="" BayNumber=""
            CheckDigit="" DockType="" FreezeMoveIn="" FreezeMoveOut=""
            InStagingLocationId="" IsSchedulable="" LevelNumber=""
            LocationBarCode="" LocationId="" LocationKey=""
            LocationSizeCode="" LocationType="" MoveInSeqNo=""
            MoveOutSeqNo="" Node="" OutStagingLocationId=""
            StorageCode="" VelocityCode="" XCoOrdinate="" YCoOrdinate=""
            ZCoOrdinate="" ZoneId="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Location_master.xml"
                IncludeXMLPath="/Location" RemoveDBHouseKeeping="Y"/>
            <yfc:doc>
                <Element EntityName="Location"/>
                <Attributes>
                    <Attribute DataType="String" Name="IsSchedulable"/>
                    <Attribute EntityName="Location" Name="StorageCode" NameInEntity="Storage_Code">
                        <Description>
                            <div>
                                It is an attribute of the location that allows the warehouse to store items that have the same storage profile as that of the location. For example, hazardous inflammable items need locations close to fire extinguishers. In this case the locations will be marked as having a storage code, which is suitable for storing Inflammable items. This ensures that only inflammable items get to these locations.
									<Validation>
                                    <Condition>Should be a valid location storage code for the node.
								</Condition>
                                    <Error
                                    Description="The StorageCode is invalid." ErrorCode="WMS00100"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <SKUDedications Reset="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Reset">
                            <Description>
                                If Reset Flag is passed:
                                    Y =&gt; All existing sku dedications will be deleted and the currently passed ones will be created.
                                    N =&gt; The passed SKUDedication elements will be treated as a delta change.
                                </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <SKUDedication EnterpriseCode="Required"
                    ItemId="Required" LocationId="" MaxQty="" MinQty=""
                    Node="" ProductClass="Required" SKUDedicationKey=""
                    Segment="" SegmentType="" UnitOfMeasure="Required" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="YFS_Location_master.xml"
                        IncludeXMLPath="/Location/SKUDedications/SKUDedication" RemoveDBHouseKeeping="Y"/>
                    <yfc:doc>
                        <Element EntityName="SKU_Dedication"/>
                        <Attributes>
                            <Attribute EntityName="SKU_Dedication"
                                Name="EnterpriseCode" NameInEntity="Enterprise_Code">
                                <Description>
                                    <div>
									Unit of Measure of SKU.
										<Validation>
                                    <Condition>EnterpriseCode should not be void.
											</Condition>
                                    <Error
                                    Description="Mandatory Parameter(s) not passed." ErrorCode="WMS00063"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>Should be a valid EnterpriseCode participating in this Node/Organization.
											</Condition>
                                    <Error
                                    Description="Invalid organization code." ErrorCode="YFS10395"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="SKU_Dedication"
                                Name="ItemId" NameInEntity="Item_Id">
                                <Description>
                                    <div>
									Item Identifier of SKU.
										<Validation>
                                    <Condition>Item Identifier should not be void.
											</Condition>
                                    <Error
                                    Description="Mandatory Parameter(s) not passed." ErrorCode="WMS00063"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="SKU_Dedication"
                                Name="ProductClass" NameInEntity="Product_Class">
                                <Description>
                                    <div>
									Product Class of SKU.
										<Validation>
                                    <Condition>Product Class should not be void.
											</Condition>
                                    <Error
                                    Description="Mandatory Parameter(s) not passed." ErrorCode="WMS00063"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="SKU_Dedication"
                                Name="UnitOfMeasure" NameInEntity="Uom">
                                <Description>
                                    <div>
									Unit of Measure of SKU.
										<Validation>
                                    <Condition>Unit of Measure should not be void.
											</Condition>
                                    <Error
                                    Description="Mandatory Parameter(s) not passed." ErrorCode="WMS00063"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </SKUDedication>
            </SKUDedications>
        </ZoneLocation>
    </ZoneLocations>
</Zone>
