<!-- multiApi input XML -->
<MultiApi xmlns="http://www.sterlingcommerce.com/documentation/YCP/multiApi/output">
    <!-- There can be one or more api elements -->
    <API Name="Required" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Text-100" Name="Name">
                    <Description>Name of the API to call. </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <!-- The output element will contain a child element that corresponds to
 the document element of the api output . The Output node will be non-empty
 if and only if the api returns a non-null document -->
        <Output yfc:AllowAnyElement="skip"/>
    </API>
</MultiApi>
