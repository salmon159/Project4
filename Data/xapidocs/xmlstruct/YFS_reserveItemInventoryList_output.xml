<ReserveItemInventoryList ApplyFutureSafetyFactor=""
    ApplyOnhandSafetyFactor="" CheckInventory="" OrganizationCode=""
    ReserveIfPartial="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/reserveItemInventoryList/output">
    <yfc:doc>
        <Element EntityName="Inventory_Reservation"/>
        <Attributes>
            <Attribute DataType="Flag" Name="ApplyFutureSafetyFactor">
                <Description>
                    <div>
			This flag indicates that future safety factor should be applied in future availability calculation of the item at the node.
			<br/>Default: <b>
                            <code>Y</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ApplyOnhandSafetyFactor">
                <Description>
                    <div>
			This flag indicates that on-hand safety factor should be applied in current availability calculation of the item at the node.
			<br/>Default: <b>
                            <code>Y</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="CheckInventory">
                <Description>
                    <div>
			Indicates whether the requested quantity passed in QtyToBeReserved is reserved without looking for inventory availability.
			</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item"
                Name="OrganizationCode" NameInEntity="Organization_Code"/>
            <Attribute DataType="Flag" Name="ReserveIfPartial">
                <Description>
                    <div>
			Indicates if the reservation should be made if only part of the quantity requested can be reserved. 
			</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ReserveItemInventory CancelledQty="" DeliveryMethod=""
        ExpirationDate="" IsShipNodeRequired="" ItemID=""
        ProductClass="" QtyToBeCancelled="" QtyToBeReserved=""
        ReservationID="" ReservationStatus="" ReservedQty="" ShipNode=""
        UnitOfMeasure="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Element EntityName="Inventory_Reservation"/>
            <Attributes>
                <Attribute EntityName="Inventory_Reservation"
                    Name="CancelledQty" NameInEntity="Quantity">
                    <Description>
                        <div>
			Cancelled Quantity.
			</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="DeliveryMethod">
                    <Description>
                        <div>
		DeliveryMethod used for obtaining the promise line. Posible values include Delivery <b>DEL</b>, Ship <b>SHP</b> or Pick Up <b>PICK</b>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="IsShipNodeRequired">
                    <Description>
                        <div>
			Indicates if <code>ShipNode</code> is required to reserve the inventory.
			<br/>Default: <b>
                                <code>Y</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Item" Name="ItemID"/>
                <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
                <Attribute EntityName="Inventory_Reservation"
                    Name="QtyToBeCancelled" NameInEntity="Quantity">
                    <Description>
                        <div>
			The requested quantity to be cancelled. 
			</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Reservation"
                    Name="QtyToBeReserved" NameInEntity="Quantity">
                    <Description>
                        <div>
			The requested quantity to be reserved. 
			</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Reservation"
                    Name="ReservationID" NameInEntity="Reservation_Id"/>
                <Attribute DataType="Status" Name="ReservationStatus">
                    <Description>
                        <div>
			Status of the reservation.
			</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Reservation"
                    Name="ReservedQty" NameInEntity="Quantity"/>
                <Attribute EntityName="Inventory_Reservation"
                    Name="ShipNode" NameInEntity="Shipnode_Key"/>
                <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
            </Attributes>
        </yfc:doc>
        <Tag BatchNo="" LotNumber="" RevisionNo="">
            <yfc:doc>
                <Element EntityName="Inventory_Tag"/>
            </yfc:doc>
        </Tag>
    </ReserveItemInventory>
</ReserveItemInventoryList>
