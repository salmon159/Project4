<?xml version="1.0" encoding="UTF-8"?>
<DoesItemMatchCountTask ItemID="Required" OrganizationCode="Required"
    TaskId="Required" UnitOfMeasure="Required" xmlns="http://www.sterlingcommerce.com/documentation/WMS/doesItemMatchCountTask/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Count" Name="ItemID">
                <Description>
                    <div>
						Item that has been counted against the task.
						<Validation>
                            <Condition>The <code>ItemID</code> is mandatory.
							</Condition>
                            <Error
                                Description="Item Details not passed." ErrorCode="WMS00020"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count" Name="OrganizationCode">
                <Description>
                    <div>
						Indicates the enterprise of the item counted.
						<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is mandatory.
							</Condition>
                            <Error
                                Description="Item Details not passed." ErrorCode="WMS00020"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count" Name="TaskId">
                <Description>
                    <div>
						The task identifier for which count is being performed.
						<Validation>
                            <Condition>The <code>TaskId</code> is mandatory.
							</Condition>
                            <Error Description="Task is mandatory." ErrorCode="WMS00117"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count" Name="UnitOfMeasure">
                <Description>
                    <div>
						Indicates the unit of measure of the item counted.
						<Validation>
                            <Condition>
                                <code>UnitOfMeasure</code> is mandatory.
							</Condition>
                            <Error
                                Description="Item Details not passed." ErrorCode="WMS00020"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</DoesItemMatchCountTask>
