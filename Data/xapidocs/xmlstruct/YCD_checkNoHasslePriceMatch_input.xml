<Order DocumentType="" EnterpriseCode="" OrderHeaderKey="" OrderNo="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/checkNoHasslePriceMatch/input">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
                <div>
				Contains information about the order for which price match is performed. Either OrderHeaderKey or a combination of OrderNo, EnterpriseCode and DocumentType should be passed.
				</div>
            </Description>
        </Element>
    </yfc:doc>
    <OrderLines yfc:DTDOccurrence="REQUIRED">
        <OrderLine CompetitorDiscounts="" CompetitorShippingcharges=""
            CompetitorTotalPrice="Required" CompetitorUnitPrice=""
            ExistingDiscount="" ExistingShippingCharges=""
            ExistingTotalPrice="Required" ExistingUnitPrice=""
            FinalPriceMatch="" MinimumQuantity="Required"
            OrderLineKey="" PrimeLineNo="" SubLineNo="" yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element EntityName="Order_Line">
                    <Description>
                        <div>
				Contains information about the order line for which price match is performed. Either OrderLineKey or a combination of PrimeLineNo and SubLineNo should be passed.
				</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="CompetitorDiscounts">
                        <Description>This is the Competitor discounts. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="CompetitorShippingcharges">
                        <Description>This is the Competitor shipping charge. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="CompetitorTotalPrice">
                        <Description>This is the Competitor total price. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="CompetitorUnitPrice">
                        <Description>This is the Competitor unit price. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ExistingDiscount">
                        <Description>This is existing discount. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ExistingShippingCharges">
                        <Description>This is existing shipping charges. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ExistingTotalPrice">
                        <Description>This is existing total price. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ExistingUnitPrice">
                        <Description>This is existing unit price. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="FinalPriceMatch">
                        <Description>This is final price match price. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="MinimumQuantity">
                        <Description>This is Quantity for which the price match has to be performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="OrderLineKey" NameInEntity="Order_Line_Key">
                        <Description>This is the Order Line key Of the OrderLine  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="PrimeLineNo" NameInEntity="Prime_Line_No">
                        <Description>This is the Prime LineNo of the OrderLine  for which price match is to be performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line" Name="SubLineNo" NameInEntity="Sub_Line_No">
                        <Description>This is the Sub Line No of the OrderLine  for which price match is to be performed. </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </OrderLine>
    </OrderLines>
</Order>
