<ItemNodeVelocities xmlns="http://www.sterlingcommerce.com/documentation/INV/manageItemNodeVelocity/input">
    <ItemNodeVelocity InventoryItemKey="" Operation="Manage|Delete"
        QuantityInMaxWindow="" QuantityInMinWindow="" ShipNode="" VelocityDate="">
        <yfc:doc>
            <Attributes>
                <Attribute Name="InventoryItemKey">
                    <Description>
                        <div>The primary key of the YFS_INVENTORY_ITEM record for which this is a velocity. 
                        <Validation>
                                <Condition>The inventory item could not be found.
						</Condition>
                                <Error Description="Invalid Item." ErrorCode="INV80_035"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="YSCManageOrDeleteOperation" Name="Operation">
                    <Description>The operation to perform on the specified velocity. Default: Manage.</Description>
                </Attribute>
                <Attribute Name="ShipNode">
                    <Description>
                        <div>The primary key of the YFS_SHIPNODE record for which this is a velocity.
                        <Validation>
                                <Condition>The <code>ShipNode</code> is either not passed or is not a valid ship node.
						</Condition>
                                <Error Description="YFS:Invalid Node" ErrorCode="YFS10048"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <InventoryItem InventoryOrganizationCode="" ItemID=""
            ProductClass="" UnitOfMeasure="">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>The inventory item information related to the velocity. 
                        <Validation>
                                <Condition>The inventory item could not be found.
						</Condition>
                                <Error Description="Invalid Item." ErrorCode="INV80_035"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
            </yfc:doc>
        </InventoryItem>
    </ItemNodeVelocity>
</ItemNodeVelocities>
