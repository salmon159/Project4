<InventoryAuditSummaries SummarizeAcrossShipByDate=""
    SummarizeAcrossTagAndSegment="" TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/INV/getInventoryAuditSummary/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="SummarizeAcrossShipByDate">
                <Description>
                    <div>
						Indicates whether the API output is grouped across <code>ShipByDate</code> of inventory audit or not. 
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="SummarizeAcrossTagAndSegment">
                <Description>
                    <div>
						Indicates whether the API output is grouped across segment and tag attributes of inventory audit or not.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>
                    <div>Total number of records that match the input criteria. </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <InventoryAuditSummary BatchNo="" InventoryItemKey=""
        InventoryOrganizationCode="" ItemID="" LotNumber=""
        ProductClass="" Quantity="" RevisionNo="" Segment=""
        SegmentType="" ShipByDate="" ShipNode="" SupplyType=""
        TagNumber="" TransactionType="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element EntityName="Inventory_Audit"/>
        </yfc:doc>
        <Item Description="" ExtendedDescription="" ItemID=""
            OrganizationCode="" ProductClass="" ShortDescription=""
            UnitOfMeasure="" yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element EntityName="Inventory_Item"/>
                <Attributes>
                    <Attribute EntityName="Item" Name="Description" NameInEntity="Description"/>
                    <Attribute EntityName="Item"
                        Name="ExtendedDescription" NameInEntity="Extended_Description"/>
                    <Attribute EntityName="Inventory_Item"
                        Name="OrganizationCode" NameInEntity="Organization_Code"/>
                    <Attribute EntityName="Item" Name="ShortDescription" NameInEntity="Short_Description"/>
                </Attributes>
            </yfc:doc>
            <LanguageDescriptionList>
                <LanguageDescription Description=""
                    ExtendedDescription="" LocaleCode=""
                    ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
            </LanguageDescriptionList>
        </Item>
    </InventoryAuditSummary>
</InventoryAuditSummaries>
