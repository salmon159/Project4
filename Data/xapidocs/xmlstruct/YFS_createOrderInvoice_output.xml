<OrderInvoiceList xmlns="http://www.sterlingcommerce.com/documentation/YFS/createOrderInvoice/output">
    <OrderInvoice ActualFreightCharge="" AmountCollected=""
        ChargeTransactionKey="" ChargedActualFreight=""
        CollectedExternally="" CollectedThroughAR="" Currency=""
        DateInvoiced="" DerivedFromOrderHeaderKey="" DocumentType=""
        EnterpriseCode="" HeaderTax="" InvoiceNo="" InvoiceType=""
        LineSubTotal="" MasterInvoiceNo="" OrderHeaderKey=""
        OrderInvoiceKey="" OrderNo="" OtherCharges="" Reference1=""
        ReturnReleaseKey="" SellerOrganizationCode="" ShipNode=""
        ShipmentKey="" ShipmentNo="" Status="" TotalAmount=""
        TotalTax="" yfc:DTDOccurrence="ZeroOrMany">
        <TaxBreakupList TotalNumberOfRecords="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Integer" Name="TotalNumberOfRecords">
                        <Description>Total number of records.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <TaxBreakup ChargeCategory="" ChargeName="" HeaderKey=""
                InvoicedTax="" LineKey="" RecordType="" Reference1=""
                Reference2="" Reference3="" Tax="" TaxBreakupKey=""
                TaxName="" TaxPercentage="" TaxableFlag="" yfc:DTDOccurrence="ZeroOrMany"/>
        </TaxBreakupList>
        <LineDetails TotalNumberOfRecords="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Integer" Name="TotalNumberOfRecords">
                        <Description>Total number of records.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <LineDetail Charges="" ExtendedPrice="" ItemID=""
                LineTotal="" OrderInvoiceDetailKey="" OrderInvoiceKey=""
                OrderLineKey="" PrimeLineNo="" ProductClass=""
                ShippedQty="" SubLineNo="" Tax="" UnitOfMeasure=""
                UnitPrice="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Order_Invoice_Detail"/>
                </yfc:doc>
                <LineDetailTranQuantity ShippedQty="" TransactionalUOM="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="ShippedQty"/>
                            <Attribute DataType="UOM" Name="TransactionalUOM">
                                <Description>UOM on which all the transactional quantities are specified.
				  </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </LineDetailTranQuantity>
                <LineChargeList TotalNumberOfRecords="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Integer" Name="TotalNumberOfRecords">
                                <Description>Total number of records.</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <LineCharge ChargeAmount="" ChargeCategory=""
                        ChargeName="" ChargePerLine="" ChargePerUnit=""
                        HeaderKey="" InvoicedChargePerLine=""
                        InvoicedExtendedCharge="" IsBillable=""
                        IsBillalbe="" IsDiscount=""
                        IsShippingCharge="|N|Y" LineChargesKey=""
                        LineKey="" RecordType="" Reference="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Flag" Name="IsBillable">
                                    <Description>Describes if the charge is billable. Default: <code>N</code>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="String" Name="IsBillalbe"/>
                                <Attribute DataType="Flag" Name="IsDiscount">
                                    <Description>Describes if the charge is discountable. Default: <code>N</code>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="IsShippingCharge">
                                    <Description>Determines whether the charge is a shipping charge. This attribute value is system defined and cannot be overridden. The value for this attribute is set as <code>Y</code> only for Charge Name configured as <code>CHARGE_NAME_FOR_SHIPPING</code> in financial rule. For all other Charges this attribute is set to <code>N</code>.
						</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </LineCharge>
                </LineChargeList>
            </LineDetail>
        </LineDetails>
        <HeaderChargeList TotalNumberOfRecords="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Integer" Name="TotalNumberOfRecords">
                        <Description>Total number of records.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <HeaderCharge ChargeAmount="" ChargeCategory=""
                ChargeName="" HeaderChargesKey="" HeaderKey=""
                InvoicedChargeAmount="" IsBillable="" IsDiscount=""
                IsShippingCharge="|N|Y" RecordType="" Reference="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="IsBillable">
                            <Description>Describes if the charge is billable. Default: <code>N</code>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsDiscount">
                            <Description>Describes if the charge is discountable. Default: <code>N</code>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsShippingCharge">
                            <Description>Determines whether the charge is a shipping charge. This attribute value is system defined and cannot be overridden. The value for this attribute is set as <code>Y</code> only for Charge Name configured as <code>CHARGE_NAME_FOR_SHIPPING</code> in financial rule. For all other Charges this attribute is set to <code>N</code>.
						</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </HeaderCharge>
        </HeaderChargeList>
    </OrderInvoice>
</OrderInvoiceList>
