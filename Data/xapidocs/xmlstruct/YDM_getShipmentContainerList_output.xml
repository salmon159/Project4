<Containers TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/getShipmentContainerList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>Indicates total number of records fetched.
			</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Container ActualFreightCharge="" ActualNoOfReturnLabels=""
        ActualWeight="" ActualWeightUOM="" AppliedWeight=""
        AppliedWeightUOM="" AstraCode="" BarcodeDiscount=""
        BasicFreightCharge="" CODReturnTrackingNo="" CarriageValue=""
        CaseId="" ContainerGrossWeight="" ContainerGrossWeightUOM=""
        ContainerGroup="" ContainerHeight="" ContainerHeightUOM=""
        ContainerLength="" ContainerLengthUOM="" ContainerNetWeight=""
        ContainerNetWeightUOM="" ContainerNo="" ContainerScm=""
        ContainerSeqNo="" ContainerType="" ContainerWidth=""
        ContainerWidthUOM="" CustomsValue="" DeclaredValue=""
        DimmedFlag="" DiscountAmount="" ExportLicenseExpDate=""
        ExportLicenseNo="" HasOtherContainers="" HasWorkOrders=""
        IncludedInInventoryPallet="" IsHazmat="" IsLoaded=""
        IsManifested="" IsPackProcessComplete="" IsReceived=""
        LoadKey="" OversizedFlag="" PalletId="" ParentContainerGroup=""
        ParentContainerKey="" ParentContainerNo=""
        RequiredNoOfReturnLabels="" RoutingCode="" SCAC=""
        ShipmentContainerKey="" ShipmentKey=""
        SpecialServicesSurcharge="" SystemSuggested="" TrackingNo=""
        Ucc128code="" UnManifestShipmentIfRequired="" Zone=""
        isHistory="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Shipment_Container_master.xml" IncludeXMLPath="/Container"/>
        <yfc:doc>
            <Attributes>
                <Attribute DataType="LPNType" Name="CaseId">
                    <Description>CaseId would be same as the ContainerScm if ContainerType is Case.
				</Description>
                </Attribute>
                <Attribute DataType="Flag" Name="HasWorkOrders">
                    <Description>Indicates if any non-service workorders exist for the container </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="IncludedInInventoryPallet">
                    <Description>
                        <div> Indicates if the shipment container is included in Inventory pallet.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="IsLoaded"/>
                <Attribute DataType="LPNType" Name="PalletId">
                    <Description>PalletId would be same as the ContainerScm if ContainerType is Pallet.
				</Description>
                </Attribute>
                <Attribute DataType="Flag" Name="UnManifestShipmentIfRequired">
                    <Description>This flag indicates whether removing the container from manifest will result in unmanifesting the whole shipment.
				</Description>
                </Attribute>
                <Attribute DataType="Flag" Name="isHistory">
                    <Description>Indicates if data is fetched from the regular table or the history table. Valid values are Y, N and Void. Void implies N.
				</Description>
                </Attribute>
            </Attributes>
            <Element EntityName="Shipment_Container"/>
        </yfc:doc>
        <Status Description="" OwnerKey="" ProcessTypeKey=""
            RequiresCollaboration="" Status="" StatusKey=""
            StatusName="" StatusType="">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Status_master.xml" IncludeXMLPath="/Status"/>
            <yfc:doc>
                <Element EntityName="Status"/>
            </yfc:doc>
        </Status>
        <Shipment ActualDeliveryDate="" ActualFreightCharge=""
            ActualShipmentDate="" AllowOverage="" AppointmentNo=""
            AssignedToUserId="" AssociationAction="" BBNMinVolume=""
            BBNMinWeight="" BillToAddressKey="" BillToCustomerId=""
            BolNo="" BreakBulkLoadKey="" BreakBulkNode=""
            BuyerMarkForNodeId="" BuyerOrganizationCode=""
            BuyerReceivingNodeId="" CODPayMethod="" CarrierAccountNo=""
            CarrierServiceCode="" CarrierType="" Code=""
            CommercialValue="" Currency="" DeliveryCode=""
            DeliveryMethod="" DeliveryPlanKey="" DeliveryTS=""
            DestinationZone="" DoNotVerifyCaseContent=""
            DoNotVerifyPalletContent="" DocumentType="" DownLoadCount=""
            EmailReturnLabel="" EnterpriseCode="" EspCheckRequired=""
            ExpectedDeliveryDate="" ExpectedShipmentDate=""
            ExportTaxpayerID="" FreightTerms="" FromAddressKey=""
            FromAppointment="" HasOtherShipments=""
            HazardousMaterialFlag="" HoldLocation="" ITDate="" ITNNo=""
            ITNo="" InvoiceComplete="" IsPackProcessComplete=""
            IsPackingRequired="" IsPackingRequiredWithContents=""
            IsProductPlacingComplete="" IsShipmentLevelIntegration=""
            IsSingleOrder="" LevelOfService="" LinesEntered="" Lockid=""
            ManifestKey="" ManifestNo="" ManuallyEntered="" MergeNode=""
            NextAlertTs="" NumOfCartons="" NumOfPallets=""
            OrderAvailableOnSystem="" OrderHeaderKey="" OrderNo=""
            OrderReleaseKey="" OriginZone=""
            OverrideManualShipmentEntry="" PackAndHold=""
            PackListType="" ParentShipmentKey="" PickListNo=""
            PickticketNo="" PipelineKey="" PodNo="" ProNo=""
            ProfileID="" ReceivingNode="" ReleaseNo=""
            RequestedCarrierServiceCode="" RequestedDeliveryDate=""
            RequestedShipmentDate="" ReturnAuthorizationNumber=""
            ReturnBillingAccount="" ReturnByDate=""
            ReturnCarrierService="" ReturnFreightTerms="" SCAC=""
            ScacAndService="" ScacAndServiceKey=""
            ScacIntegrationRequired="" SealNo=""
            SellerOrganizationCode="" ShipDate="" ShipMode=""
            ShipNode="" ShipToCustomerId="" ShipVia=""
            ShipmentClosedFlag="" ShipmentConfirmUpdatesDone=""
            ShipmentConsolidationGroupId="" ShipmentContainerizedFlag=""
            ShipmentDeliverUpdatesDone="" ShipmentKey="" ShipmentNo=""
            ShipmentPlannedFlag="" ShipmentSortLocationId=""
            ShipmentType="" Status="" StatusDate="" ToAppointment=""
            TotalActualCharge="" TotalEstimatedCharge=""
            TotalNumOfPickableSKUs="" TotalVolume="" TotalVolumeUOM=""
            TotalWeight="" TotalWeightUOM="" TrackingNo="" TrailerNo=""
            WorkOrderApptKey="" WorkOrderKey="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Shipment_master.xml" IncludeXMLPath="/Shipment"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="BreakBulkLoadKey"/>
                    <Attribute DataType="String" Name="IsPackingRequired"/>
                    <Attribute DataType="String" Name="IsPackingRequiredWithContents"/>
                    <Attribute DataType="String" Name="IsShipmentLevelIntegration"/>
                    <Attribute EntityName="SCAC_And_Service"
                        Name="ScacAndService" NameInEntity="Scac_And_Service"/>
                    <Attribute EntityName="SCAC_And_Service"
                        Name="ScacAndServiceKey" NameInEntity="Scac_And_Service_Key"/>
                    <Attribute DataType="String" Name="TotalNumOfPickableSKUs">
                        <Description>
                            <div>
					This field indicates the total number of pickable units in a shipment. This attribute will be returned in the output only if passed in the template. Only pickable line quantities are counted for this attribute; parent lines and bundle parent lines are ignored.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>Indicates if data is fetched from the regular table or the history table. Valid values are Y, N and Void. Void implies N.
			</Description>
                    </Attribute>
                </Attributes>
                <Element EntityName="Shipment"/>
            </yfc:doc>
        </Shipment>
        <Wave Createts="" Createuserid="" Description="" DocumentType=""
            EstimatedMinutes="" Node="" OverrideConcurrentWave=""
            OverrideReleaseDate="" PipelineKey="" Priority=""
            ReasonCode="" ReasonText="" ReleaseNoEarlierThan=""
            ReleaseNoLaterThan="" RemainingMinutes="" SeqNo=""
            ShipmentGroupId="" WaveKey="" WaveNo="" WaveStatus="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Wave_master.xml" IncludeXMLPath="/Wave"/>
        </Wave>
    </Container>
    <Load ActualArrivalDate="" ActualDepartureDate=""
        ActualFreightCharge="" AppointmentNo="" BolNo=""
        BuyerOrganizationCode="" CarrierServiceCode="" Currency=""
        DeliveryPlanKey="" DestinationAddressKey="" DestinationNode=""
        DocumentType="" DropAtShipmentDestination="" EnterpriseCode=""
        ExpectedArrivalDate="" ExpectedDepartureDate=""
        HazardousMaterial="" ITNNo="" IsBreakBulkLoad=""
        IsExpInclInManifest="" LoadClosedFlag="" LoadKey="" LoadNo=""
        LoadType="" ManifestKey="" NumberOfCases="" NumberOfPallets=""
        OriginAddressKey="" OriginNode="" OwnerOrganizationCode=""
        PickupFromShipmentOrigin="" PipelineKey="" PodNo="" ProNo=""
        Scac="" ScacAndService="" ScacAndServiceKey="" SealNo=""
        ShipMode="" ShipVia="" Status="" StatusDate=""
        TotalActualCharge="" TotalEstimatedCharge="" TotalVolume=""
        TotalVolumeUOM="" TotalWeight="" TotalWeightUOM="" TrailerNo=""
        VesselName="" VoyageNo="" isHistory="">
        <yfc:includeInParent IncludeXMLFileName="YFS_Load_master.xml" IncludeXMLPath="/Load"/>
        <yfc:doc>
            <Attributes>
                <Attribute DataType="PositiveInteger" Name="NumberOfCases">
                    <Description>Indicates total number of outermost cases in the load.
				</Description>
                </Attribute>
                <Attribute DataType="PositiveInteger" Name="NumberOfPallets">
                    <Description>Indicates total number of outermost pallets in the load.
				</Description>
                </Attribute>
                <Attribute EntityName="SCAC_And_Service"
                    Name="ScacAndService" NameInEntity="Scac_And_Service"/>
                <Attribute EntityName="SCAC_And_Service"
                    Name="ScacAndServiceKey" NameInEntity="Scac_And_Service_Key"/>
                <Attribute DataType="Flag" Name="isHistory">
                    <Description>Indicates if data is fetched from the regular table or the history table. Valid values are Y and N.
			     </Description>
                </Attribute>
            </Attributes>
            <Element EntityName="Load"/>
        </yfc:doc>
    </Load>
    <ContainerActivities TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                    <Description>Indicates total number of records fetched.
			</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ContainerActivity ActivityCode="" ActivityTimeStamp=""
            ActivityUserId="" ContainerActivityKey="" Createprogid=""
            Createts="" Createuserid="" IsCommercialAddress=""
            IsException="" Lockid="" Modifyprogid="" Modifyts=""
            Modifyuserid="" Notes="" ShipmentContainerKey=""
            TaxGeoCode="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="yfs_container_activity_master.xml" IncludeXMLPath="/ContainerActivity"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" EntityName="Person_Info"
                        Name="IsCommercialAddress" NameInEntity="Is_Commercial_Address">
                        <Description>
                            <div>
							Indicates whether the address is commercial, residential or none. Values of this field can be Y is Commercial address. N is Residential address. NULL is None. Default value is Null.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Person_Info"
                        Name="TaxGeoCode" NameInEntity="Tax_Geo_Code"/>
                </Attributes>
                <Element EntityName="Container_Activity"/>
            </yfc:doc>
            <ActivityLocation ActivityLocationId="" ActivityNodeKey=""
                AddressLine1="" AddressLine2="" AddressLine3=""
                AddressLine4="" AddressLine5="" AddressLine6="" City=""
                Company="" Country="" FirstName=""
                IsCommercialAddress="" LastName="" MiddleName=""
                State="" TaxGeoCode="" ZipCode="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="IsCommercialAddress"/>
                        <Attribute DataType="String" Name="TaxGeoCode"/>
                    </Attributes>
                </yfc:doc>
            </ActivityLocation>
        </ContainerActivity>
    </ContainerActivities>
    <ContainerReturnTrackingList TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                    <Description>Indicates total number of records fetched.
			</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ContainerReturnTracking ReturnTrackingNumber=""
            ShipmentContainerKey="" ShipmentKey="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="YFS_Cntr_Ret_Track_master.xml" IncludeXMLPath="/ContainerReturnTracking"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>Indicates if data is fetched from the regular table or the history table. Valid values are Y, N and Void. Void implies N.
					  </Description>
                    </Attribute>
                </Attributes>
                <Element EntityName="Cntr_Ret_Track"/>
            </yfc:doc>
        </ContainerReturnTracking>
    </ContainerReturnTrackingList>
</Containers>
