<Inventory ApplyDifferences="" CompleteInventoryFlag=""
    InsertInventoryItem="" PublishedTime="" ReasonCode="" ReasonText=""
    RemoveInventoryNodeControl="" ShipNode="Required"
    YantraMessageGroupID="" xmlns="http://www.sterlingcommerce.com/documentation/INV/syncLoadedInventory/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="ApplyDifferences">
                <Description>
                    <div>
				 Indicates whether to apply the differences to the <code>YFS_INVENTORY_SUPPLY</code> table in the database during the inventory synchronization process.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                        <br/>If <code>ApplyDifferences</code> is passed as <b>
                            <code>Y</code>
                        </b>, the inventory mismatches detected are applied to the <code>YFS_INVENTORY_SUPPLY</code> table in the database.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="CompleteInventoryFlag">
                <Description>
                    <div>
				Indicates whether to check for mismatches for all of the inventory, or only the supplies, which were loaded into the YFS_INVENTORY_SUPPLY_TEMP table by the loadInventoryMismatch API.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                        <br/>If <code>CompleteInventoryFlag</code> is passed as <b>
                            <code>Y</code>
                        </b>, all the inventory supplies in the database for the <code>ShipNode</code> are checked for mismatches.
				<br/>If <code>CompleteInventoryFlag</code> is passed as <b>
                            <code>N</code>
                        </b>, only the supplies in the YFS_INVENTORY_SUPPLY_TEMP table, are checked for mismatches.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="InsertInventoryItem">
                <Description>
                    <div>
				Indicates whether to have the API inserts new inventory items and updates the <code>INVENTORY_ITEM_KEY</code> of the corresponding record in the <code>YFS_INVENTORY_SUPPLY_TEMP table</code>.   This is needed if the loadInventoryMismatch API was not used to load data into the <code>YFS_INVENTORY_SUPPLY_TEMP table</code>.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="PublishedTime">
                <Description>
                    <div>
				 Date and Time when inventory snapshot was taken.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Audit" Name="ReasonCode" NameInEntity="Reason_Code"/>
            <Attribute EntityName="Inventory_Audit" Name="ReasonText" NameInEntity="Reason_Text"/>
            <Attribute DataType="Flag" Name="RemoveInventoryNodeControl">
                <Description>
                    <div>Indicates if inventory node controls of the item at the node should be removed if exists.
				        <br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Supply" Name="ShipNode" NameInEntity="Shipnode_Key">
                <Description>
                    <div>
				Node where inventory is located.
				<Validation>
                            <Condition>
                                <code>ShipNode</code> is not passed.
					</Condition>
                            <Error
                                Description="Missing mandatory parameters." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Supply_Temp"
                Name="YantraMessageGroupID" NameInEntity="Yantra_Message_Group_ID">
                <Description>
                    <div>
				System message group identification.  Inventory supplies loaded into YFS_Inventory_Supply_Temp table with the provided <code>YantraMessageGroupID</code> will be considered during the inventory synchronization process.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <UnprocessedItems>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
		Unprocessed inventory items whose inventory supply updates were unsuccessful.
		</div>
                </Description>
            </Element>
        </yfc:doc>
        <InventoryItem InventoryOrganizationCode="" ItemID=""
            ProductClass="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Inventory_Item"/>
            </yfc:doc>
            <Supplies>
                <Supply ActualQuantity="" AvailabilityType=""
                    ChangedQuantity="" ETA="" ExpectedQuantity=""
                    Quantity="" Segment="" SegmentType="" ShipByDate=""
                    SupplyLineReference="" SupplyReference=""
                    SupplyReferenceType="" SupplyType="" yfc:DTDOccurrence="OneOrMany">
                    <yfc:doc>
                        <Element EntityName="Inventory_Supply">
                            <Description>
                                <div>
						Unprocessed inventory supply because inventory adjustment was unsuccessful.
						</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute EntityName="Inventory_Supply"
                                Name="ActualQuantity" NameInEntity="Quantity">
                                <Description>
                                    <div>
							Actual quantity of the supply stores within the system.
							</div>
                                </Description>
                            </Attribute>
                            <Attribute
                                DataType="QuantityPositiveAndNegative" Name="ChangedQuantity">
                                <Description>
                                    <div>
							Changed quantity between quantity of supply stores within the system and the quantity of supply at the shipnode.
							</div>
                                </Description>
                            </Attribute>
                            <Attribute
                                EntityName="Inventory_Supply_Temp"
                                Name="ExpectedQuantity" NameInEntity="Quantity">
                                <Description>
                                    <div>
							Expected quantity.  Quantity of the supply at the shipnode.
							</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                        LotAttribute3="" LotKeyReference="" LotNumber=""
                        ManufacturingDate="" RevisionNo="" TagNumber=""
                        TotalDemand="" TotalOnhandSupply="" TotalOtherSupply="">
                        <yfc:doc>
                            <Element EntityName="Inventory_Tag"/>
                        </yfc:doc>
                    </Tag>
                </Supply>
            </Supplies>
        </InventoryItem>
    </UnprocessedItems>
    <SupplyTypes>
        <SupplyType SupplyType="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Inventory_Supply"/>
            </yfc:doc>
        </SupplyType>
    </SupplyTypes>
</Inventory>
