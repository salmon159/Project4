<!--getExternalInventory Output XML-->
<ItemList xmlns="http://www.sterlingcommerce.com/documentation/YFS/getExternalInventoryForItemListUE/output">
    <Item ItemID="" OrganizationCode="" OwnerOrganizationCode=""
        ProductClass="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YFS_getExternalInventoryUE_output.xml" IncludeXMLPath="/Item"/>
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Item" Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>The inventory organization code where inventory is being stored.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Ship_Node"
                    Name="OwnerOrganizationCode" NameInEntity="Owner_Key">
                    <Description>
                        <div>The owner organization code of the ship node.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Item"
                    Name="ProductClass" NameInEntity="Product_Class"/>
            </Attributes>
        </yfc:doc>
        <Supplies yfc:DTDOccurrence="REQUIRED">
            <!-- FirstShipDate and LastShipDate fields are mandatory -->
            <Supply AcrossAllTag="" FirstShipDate="Required"
                LastShipDate="Required" Quantity="" Segment=""
                SegmentType="" ShipNode="" SoftAssignedQty="" Track="" yfc:DTDOccurrence="OneOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply"/>
                    <Attributes>
                        <Attribute DataType="Flag" Name="AcrossAllTag">
                            <Description>
                                <div>
		This flag indicates that the supply is available across all tags.
		</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="TimeStamp" Name="FirstShipDate">
                            <Description>
                                <div>
		The first ship date of the available supply.
		</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="TimeStamp" Name="LastShipDate">
                            <Description>
                                <div>
		The last ship date of the available supply.
		</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="SoftAssignedQty">
                            <Description>
                                <div>
		The soft assigned demand quantity for this available supply.
		</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="Track">
                            <Description>

                                <div>
		This field indicates the availability type for this inventory supply. 
		<div
                                    class="innerpanel" width="20%">
                                    <table border="1"
                                    cellpadding="1"
                                    cellspacing="0" class="table">
                                    <thead>
                                    <tr>
                                    <td class="tablecolumnheader">Allowed Value</td>
                                    <td class="tablecolumnheader">Description</td>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                    <td class="tablecolumn">Y</td>
                                    <td class="tablecolumn">Indicates that availability is tracked. </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">N</td>
                                    <td class="tablecolumn">Indicates that availability is infinite. </td>
                                    </tr>
                                    </tbody>
                                    </table>
                                    </div>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                    LotAttribute3="" LotKeyReference="" LotNumber=""
                    ManufacturingDate="" RevisionNo="" TagNumber="">
                    <yfc:doc>
                        <Element EntityName="Inventory_Tag"/>
                    </yfc:doc>
                </Tag>
            </Supply>
        </Supplies>
    </Item>
</ItemList>
