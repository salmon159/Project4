<!-- YCP_createAccessToken_Input.xml -->
<AccessToken ApplicationName="" SessionId="" TokenExpireTS=""
    TokenExpireTime="" UserId="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/createAccessToken/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>Only one of TokenExpireDuration and TokenExpireTS attributes should be provided. If both are provided, TokenExpireTS will be used. If none are provided, TokenExpireDuration will be defaulted based on properties.</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Access_Token" Name="ApplicationName" NameInEntity="Application_Name"/>
            <Attribute EntityName="Access_Token" Name="SessionId" NameInEntity="Session_Id"/>
            <Attribute EntityName="Access_Token" Name="TokenExpireTS" NameInEntity="Token_Expire_TS"/>
            <Attribute DataType="AccessTokenExpireTime" Name="TokenExpireTime">
                <Description>
                    <div>Number of seconds from creation before the token expires.</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Access_Token" Name="UserId" NameInEntity="User_Id"/>
        </Attributes>
    </yfc:doc>
</AccessToken>
