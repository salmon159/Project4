<TaskType ActivityCode="" UserId="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/determineTaskTypeUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute Name="ActivityCode">
                <Description>
                    <div>
				Pass ActivityCode to identify the Activity.
				<Validation>
                            <Condition>
                                <code>ActivityCode</code> passed does not exist in the YFS_ACTIVITY table.
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="UserId">
                <Description>
                    <div>
				Pass UserId to identify the user.
				<Validation>
                            <Condition> UserId  passed does not exist in the YFS_USER table.
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</TaskType>
