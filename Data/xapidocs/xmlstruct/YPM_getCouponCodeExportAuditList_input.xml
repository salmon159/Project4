<CouponCodeExportAudit CouponID="Required" Createprogid="" Createts=""
    Createuserid="" Modifyprogid="" Modifyts="" Modifyuserid=""
    OrganizationCode="Required" UserId="" xmlns="http://www.sterlingcommerce.com/documentation/YPM/getCouponCodeExportAuditList/input">
    <yfc:doc>
        <Element EntityName="Cpn_Code_Exp_Audit">
            <Description>
                <div>
					Returns the audit list showing who has exported codes for a given coupon defintion.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="CouponId" Name="CouponID">
                <Description>
                    <div>Unique identifier of a coupon.
						<Validation>
                            <Condition>
                                <code>CouponID</code> must be passed.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Cpn_Code_Exp_Audit"
                Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>Pricing organization to which to export from.
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> must be passed.
                            </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not valid.
                            </Condition>
                            <Error
                                Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="UserId"/>
        </Attributes>
    </yfc:doc>
</CouponCodeExportAudit>
