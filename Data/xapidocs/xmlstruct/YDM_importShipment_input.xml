<!-- YDM_importShipment_input.xml -->
<!-- Please maintain the sequence of the Elements.-->
<Shipment ActualDeliveryDate="" ActualShipmentDate="" AppointmentNo=""
    BillToCustomerId="" BolNo="" BuyerOrganizationCode=""
    CarrierAccountNo="" CarrierServiceCode="" CommercialValue=""
    Currency="" DeliveryTS="" DestinationZone="" DocumentType="Required"
    DownLoadCount="" EnterpriseCode="Required" ExpectedDeliveryDate=""
    ExpectedShipmentDate="" ExportTaxpayerID="" FreightTerms=""
    FromAddressKey="" FromAppointment="" HazardousMaterialFlag=""
    HeldForConfirmation="" History="" ITDate="" ITNo="" IsSingleOrder=""
    ManifestNo="" NumOfCartons="" NumOfPallets=""
    OrderAvailableOnSystem="" OrderNo="" OriginZone="" PackListType=""
    PickListNo="" PickticketNo="" PipelineKey="Required" PodNo=""
    ProNo="" ReceivingNode="" ReleaseNo="" RequestedDeliveryDate=""
    RequestedShipmentDate="" SCAC="" SealNo=""
    SellerOrganizationCode="Required" ShipDate="" ShipMode=""
    ShipNode="Required" ShipToCustomerId="" ShipVia="" ShipmentKey=""
    ShipmentNo="Required" Status="Required" ToAddressKey=""
    ToAppointment="" TotalVolume="" TotalVolumeUOM="" TotalWeight=""
    TotalWeightUOM="" TrackingNo="" TrailerNo="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/importShipment/input">
    <yfc:doc>
        <Element EntityName="Shipment">
            <Description>
                <div>
				If the History attribute is passed as <code>
                        <b>Y</b>
                    </code>, the shipment will be imported into the history tables.  Otherwise if the user calls the importShipment API with the History attribute passed as <code>
                        <b>N</b>
                    </code>, the shipment will be imported into the normal tables.  Please note that there is no validation done to ensure that the shipment being imported into the history tables does not already exist in the normal tables or vice-versa. So if you were to call importShipment twice with the same input XML, but once you pass History=<code>
                        <b>N</b>
                    </code> and then you pass History=<code>
                        <b>Y</b>
                    </code> or vice-versa, there will be problems in purge and restore at a later stage.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Shipment"
                Name="BuyerOrganizationCode" NameInEntity="Buyer_Organization_Code">
                <Description>
                    <div>
					Buyer organization to which this shipment belongs.
						<Validation>
                            <Condition>
                                <code>BuyerOrganizationCode</code> should be a valid organization.
							</Condition>
                            <Error
                                Description="Invalid Buyer Organization." ErrorCode="YFS10368"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>
					Indicates a classification of data on which a particular set of business processes must be applied. In this case, it happens to be the document type of the order document - basically the document type of the orders comprising the shipment. A document type derived from the base document type 0001 - ORDER.
						<Validation>
                            <Condition>
                                <code>DocumentType</code> should be a valid <code>DocumentType</code>.
							</Condition>
                            <Error Description="Invalid Document Type." ErrorCode="YFS10367"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="EnterpriseCode" NameInEntity="Enterprise_Code">
                <Description>
                    <div>
					Enterprise to which this shipment belongs.
						<Validation>
                            <Condition>
                                <code>EnterpriseCode</code> should be non void and a valid organization.
							</Condition>
                            <Error
                                Description="Invalid Enterprise Code." ErrorCode="YFS10113"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="HeldForConfirmation"/>
            <Attribute DataType="Flag" Name="History">
                <Description>Pass Y if shipment is to be imported into history tables
				</Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="PipelineKey" NameInEntity="Pipeline_Key">
                <Description>
                    <div>
					Pipeline to which this shipment belongs. This is determined by the <code>createShipment</code> API based on the document type and several other key attributes of the shipment.
						<Validation>
                            <Condition>
                                <code>PipelineKey</code> should have non void value.
							</Condition>
                            <Error
                                Description="Missing attribute in input." ErrorCode="YFS10460"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Shipment"
                Name="SellerOrganizationCode" NameInEntity="Seller_Organization_Code">
                <Description>
                    <div>
					Seller organization to which this shipment belongs.
						<Validation>
                            <Condition>
                                <code>SellerOrganizationCode</code> should be non void and a valid organization.
							</Condition>
                            <Error
                                Description="Invalid Seller Organization." ErrorCode="YFS10369"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" EntityName="Shipment"
                Name="ShipNode" NameInEntity="Ship_Node">
                <Description>
                    <div>
					Seller organization to which this shipment belongs.
						<Validation>
                            <Condition>
                                <code>ShipNode</code> should be non void.
							</Condition>
                            <Error
                                Description="Missing attribute in input." ErrorCode="YFS10460"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="ShipmentNo" NameInEntity="Shipment_No">
                <Description>
                    <div>
					Part of Unique key or business key.
						<Validation>
                            <Condition>
                                <code>ShipmentNo</code> should have non void and unique value.
							</Condition>
                            <Error
                                Description="Missing attribute in input." ErrorCode="YFS10460"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="Status" NameInEntity="Status">
                <Description>
                    <div>
					Status of the shipment. As various transactions in the shipment's pipeline act upon the shipment, the shipment status changes. Typical values are <code>
                            <b>1100</b>
                        </code> - <code>Created</code>, <code>
                            <b>1200</b>
                        </code> - Being <code>Picked</code>, <code>
                            <b>1400</b>
                        </code> - <code>Shipped</code>.
						<Validation>
                            <Condition>
                                <code>ShipNode</code> should be non void.
							</Condition>
                            <Error
                                Description="Missing attribute in input." ErrorCode="YFS10460"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ToAddress AddressID="" AddressLine1="" AddressLine2=""
        AddressLine3="" AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" EveningFaxNo=""
        EveningPhone="" FirstName="" IsAddressVerified=""
        IsCommercialAddress="" JobTitle="" LastName="" Latitude=""
        Longitude="" MiddleName="" MobilePhone="" OtherPhone=""
        PersonID="" State="" Suffix="" TaxGeoCode="" Title="" ZipCode="">
        <yfc:includeInParent
            IncludeXMLFileName="MasterPersonInfo_Import.xml" IncludeXMLPath="/PersonInfo"/>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				Generic table used to store relevant person info and addresses. Address are typically an organization address, ship node address, customer address, and so forth. People information is required in cases where person to person contact needs to be established. For example, taking appointments, or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.
					<Validation>
                            <Condition>at least one attribute in this element must have nonvoid value.
						</Condition>
                            <Error Description="Blank Person Info." ErrorCode="YFS10391"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
    </ToAddress>
    <FromAddress AddressID="" AddressLine1="" AddressLine2=""
        AddressLine3="" AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" EveningFaxNo=""
        EveningPhone="" FirstName="" IsAddressVerified=""
        IsCommercialAddress="" JobTitle="" LastName="" Latitude=""
        Longitude="" MiddleName="" MobilePhone="" OtherPhone=""
        PersonID="" State="" Suffix="" TaxGeoCode="" Title="" ZipCode="">
        <yfc:includeInParent
            IncludeXMLFileName="MasterPersonInfo_Import.xml" IncludeXMLPath="/PersonInfo"/>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				Generic table used to store relevant person info and addresses. Address are typically an organization address, ship node address, customer address, and so forth. People information is required in cases where person to person contact needs to be established. For example, taking appointments, or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.
					<Validation>
                            <Condition>at least one attribute in this element must have nonvoid value.
						</Condition>
                            <Error Description="Blank Person Info." ErrorCode="YFS10391"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
    </FromAddress>
    <Containers>
        <Container ActualFreightCharge="" AppliedWeight=""
            AppliedWeightUOM="" AstraCode="" BarcodeDiscount=""
            BasicFreightCharge="" CarriageValue=""
            ContainerGrossWeight="" ContainerGrossWeightUOM=""
            ContainerHeight="" ContainerHeightUOM="" ContainerLength=""
            ContainerLengthUOM="" ContainerNetWeight=""
            ContainerNetWeightUOM="" ContainerNo="" ContainerScm=""
            ContainerSeqNo="" ContainerType="" ContainerWidth=""
            ContainerWidthUOM="" CustomsValue="" DeclaredValue=""
            DimmedFlag="" DiscountAmount="" ExportLicenseExpDate=""
            ExportLicenseNo="" IsReceived="" LoadKey="" OversizedFlag=""
            ParentContainerNo="" RoutingCode="" ShipmentContainerKey=""
            ShipmentKey="" SpecialServicesSurcharge="" TrackingNo=""
            Ucc128code="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Shipment_Container_tran.xml" IncludeXMLPath="/Container"/>
            <yfc:doc>
                <Attributes>
                    <Attribute Name="ContainerNo" NameInEntity="Container_No">
                        <Description>
                            <div>
							Unique key or business key, which by itself identifies a delivery plan. If not passed as input, the system generates this number using the database sequence <code>SEQ_YFS_CONTAINER_NO</code>.
								<Validation>
                                    <Condition>
                                    <code>ContainerNo</code> should have nonvoid value. Should also be unique for the Shipment.
									</Condition>
                                    <Error
                                    Description="Missing attribute in input." ErrorCode="YFS10460"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ContainerDetails>
                <ContainerDetail ContainerDetailsKey="" Quantity=""
                    ShipmentLineKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute Name="ShipmentLineKey" NameInEntity="Shipment_Line_Key">
                                <Description>
                                    <div>
							Link to <code>YFS_SHIPMENT_LINE</code> table. Required if ShipmentLine child element is not given. 
								<Validation>
                                    <Condition>
                                    <code>ShipmentLineKey</code> should point to a valid ShipmentLine of the Shipment.
									</Condition>
                                    <Error
                                    Description="Invalid ShipmentLine." ErrorCode="YDM00073"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <ShipmentTagSerials>
                        <ShipmentTagSerial BatchNo=""
                            ContainerDetailKey="" LotAttribute1=""
                            LotAttribute2="" LotAttribute3=""
                            LotKeyReference="" LotNumber=""
                            ManufacturingDate="" Quantity=""
                            RevisionNo="" SerialNo="" ShipByDate=""
                            ShipmentLineKey="" ShipmentTagSerialKey="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:includeInParent
                                IncludeXMLFileName="YFS_Shipment_Tag_Serial_master.xml" IncludeXMLPath="/ShipmentTagSerial"/>
                            <yfc:doc>
                                <Element EntityName="Shipment_Tag_Serial">
                                    <Description>
                                    <div>
				Stores additional information related to tag number, serial number, ship by date for a shipment line or container detail. Tag information is stored only if the <code>Node</code> tracks tag information in inventory or the <code>Buyer</code> mandates tag information.
				</div>
                                    </Description>
                                </Element>
                            </yfc:doc>
                        </ShipmentTagSerial>
                    </ShipmentTagSerials>
                    <ShipmentLine DocumentType="" ItemID=""
                        OrderHeaderKey="" OrderLineKey="" OrderNo=""
                        OrderReleaseKey="" PrimeLineNo=""
                        ProductClass="" ReleaseNo="" ShipmentLineNo=""
                        ShipmentSubLineNo="" SubLineNo="" UnitOfMeasure="">
                        <yfc:doc>
                            <Element EntityName="Shipment_Line"/>
                            <Attributes>
                                <Attribute DataType="String" Name="DocumentType"/>
                            </Attributes>
                        </yfc:doc>
                        <ShipmentLineInvAttRequest BatchNo=""
                            LotAttribute1="" LotAttribute2=""
                            LotAttribute3="" LotKeyReference=""
                            LotNumber="" ManufacturingDate="" RevisionNo="">
                            <yfc:doc>
                                <Element EntityName="Shipment_Line_Req_Tag"/>
                            </yfc:doc>
                        </ShipmentLineInvAttRequest>
                    </ShipmentLine>
                </ContainerDetail>
            </ContainerDetails>
            <Instructions>
                <Instruction InstructionDetailKey="" InstructionText=""
                    InstructionType="" InstructionURL=""
                    InstructionUsage="" ReferenceKey="" SequenceNo=""
                    TableName="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent
                        IncludeXMLFileName="YFS_Instruction_Detail_tran.xml" IncludeXMLPath="/Instruction"/>
                    <yfc:doc>
                        <Attributes>
                            <Attribute Name="InstructionText" NameInEntity="Instruction_Text">
                                <Description>
                                    <div>
							Free-form text indicating any additional information related to the instruction type or otherwise.
								<Validation>
                                    <Condition>
                                    <code>InstructionText</code> should not be void.
									</Condition>
                                    <Error
                                    Description="Blank Instruction Text." ErrorCode="YFS10386"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Instruction>
            </Instructions>
        </Container>
    </Containers>
    <Instructions>
        <Instruction InstructionDetailKey="" InstructionText=""
            InstructionType="" InstructionURL="" InstructionUsage=""
            ReferenceKey="" SequenceNo="" TableName="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Instruction_Detail_tran.xml" IncludeXMLPath="/Instruction"/>
            <yfc:doc>
                <Element EntityName="Instruction_Detail"/>
            </yfc:doc>
        </Instruction>
    </Instructions>
    <Notes>
        <Note AuditTransactionId="" ContactReference="" ContactTime=""
            ContactType="" ContactUser="" CustomerSatIndicator=""
            NoteText="" Priority="" ReasonCode="" SequenceNo=""
            Tranid="" Tranname="" VisibleToAll="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Notes"/>
                <Attributes>
                    <Attribute DataType="Text-50" Name="Tranname">
                        <Description>
							Specifies the name of the Transaction
						</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <User Username="" Usertype=""/>
        </Note>
    </Notes>
    <ShipmentLines>
        <ShipmentLine CountryOfOrigin="" DocumentType="Conditional"
            ExternalReleaseIdentifier="" FifoNo="" History=""
            ItemDesc="" ItemID="Conditional" KitCode="LK|| |Blanks"
            LevelOfService="" NetWeight="" NetWeightUom=""
            OrderHeaderKey="" OrderLineKey="" OrderNo="Conditional"
            OrderReleaseKey="" PrimeLineNo="Conditional" ProductClass=""
            Quantity="" ReleaseNo="Conditional" RequestedSerialNo=""
            Segment="" SegmentType="" ShipmentLineKey="Conditional"
            ShipmentLineNo="Conditional" ShipmentSubLineNo="Conditional"
            SubLineNo="Conditional" UnitOfMeasure="Conditional"
            isHistory="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="DocumentType"/>
                    <Attribute DataType="Flag" Name="History">
                        <Description>Pass Y if shipment line is to be imported into history tables
                    </Description>
                    </Attribute>
                    <Attribute Name="ItemID" NameInEntity="Item_ID">
                        <Description>
                            <div>
						ItemId.
							<Validation>
                                    <Condition>
                                    <code>ItemID</code> should be non-void.
								</Condition>
                                    <Error Description="Void Item ID." ErrorCode="YFS10050"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute Name="OrderNo" NameInEntity="Order_No">
                        <Description>
                            <div>
						Order number corresponding to the Order for the ShipmentLine. If OrderAvailableOnSystem on Shipment is <code>
                                    <b>Y</b>
                                </code> then, the order should already exist in the system. If the shipment is being imported into history tables, the order should be a history order. If the shipment is being imported into normal tables, the order should also be a normal order.
							<Validation>
                                    <Condition>
                                    <code>OrderNo</code> should be valid.
								</Condition>
                                    <Error Description="Invalid Order." ErrorCode="YFS10003"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute Name="PrimeLineNo" NameInEntity="Prime_Line_No">
                        <Description>
                            <div>
                                <code>PrimeLineNo</code> of the corresponding OrderLine.
							<Validation>
                                    <Condition>If OrderAvailableOnSystem on Shipment is <code>
                                    <b>Y</b>
                                    </code> then, the order line should belong to the order.
								</Condition>
                                    <Error Description="Invalid Line." ErrorCode="YFS10009"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute Name="ReleaseNo" NameInEntity="Release_No">
                        <Description>
                            <div>
						Release number. Generated as a one up number against that release. Required if Order was released before included in Shipment.
							<Validation>
                                    <Condition>If OrderAvailableOnSystem on Shipment is <code>
                                    <b>Y</b>
                                    </code> then, the order release should belong to the order.
								</Condition>
                                    <Error
                                    Description="No Record Found." ErrorCode="YFS10001"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute Name="ShipmentLineKey" NameInEntity="Shipment_Line_Key">
                        <Description>
                            <div>
						Primary key attribute.
							<Validation>
                                    <Condition>
                                    <code>ShipmentLineKey</code> should be unique for the table.
								</Condition>
                                    <Error
                                    Description="Unique constraint violation." ErrorCode=""/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute Name="ShipmentLineNo" NameInEntity="Shipment_Line_No">
                        <Description>
                            <div>
						Part of Primary Key . Unique for a Shipment.
							<Validation>
                                    <Condition>
                                    <code>ShipmentLineNo</code> should be unique for the Shipment.
								</Condition>
                                    <Error
                                    Description="Missing Attribute." ErrorCode="YFS10460"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute Name="SubLineNo" NameInEntity="Sub_Line_No">
                        <Description>
                            <div>
                                <code>SubLineNo</code> of the corresponding OrderLine.
							<Validation>
                                    <Condition>If OrderAvailableOnSystem on Shipment is <code>
                                    <b>Y</b>
                                    </code> then, the order line should belong to the order.
								</Condition>
                                    <Error Description="Invalid Line." ErrorCode="YFS10009"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="isHistory"/>
                </Attributes>
            </yfc:doc>
            <ShipmentLineInvAttRequest BatchNo="" LotAttribute1=""
                LotAttribute2="" LotAttribute3="" LotKeyReference=""
                LotNumber="" ManufacturingDate="" RevisionNo=""/>
            <ShipmentTagSerials>
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
							   The attributes for this entity are treated as information only. Minimum validations are provided for this entity. <br/>
                                <Validation>
                                    <Condition>If <code>Quantity</code> is less than or equal to 0.
									</Condition>
                                    <Error
                                    Description="Invalid Quantity." ErrorCode="YDM00110"/>
                                </Validation>
                                <Validation>
                                    <Condition>If <code>SerialNo</code> is provided and <code>Quantity</code> is not equal to one.
									</Condition>
                                    <Error
                                    Description="Quantity must be equal to one because SerialNo is provided." ErrorCode="YDM00257"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                </yfc:doc>
                <ShipmentTagSerial BatchNo="" ContainerDetailKey=""
                    LotAttribute1="" LotAttribute2="" LotAttribute3=""
                    LotKeyReference="" LotNumber="" ManufacturingDate=""
                    Quantity="" RevisionNo="" SerialNo="" ShipByDate=""
                    ShipmentLineKey="" ShipmentTagSerialKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent
                        IncludeXMLFileName="YFS_Shipment_Tag_Serial_tran.xml" IncludeXMLPath="/ShipmentTagSerial"/>
                    <yfc:doc>
                        <Element EntityName="Shipment_Tag_Serial"/>
                    </yfc:doc>
                </ShipmentTagSerial>
            </ShipmentTagSerials>
            <Instructions>
                <Instruction InstructionDetailKey="" InstructionText=""
                    InstructionType="" InstructionURL=""
                    InstructionUsage="" ReferenceKey="" SequenceNo=""
                    TableName="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent
                        IncludeXMLFileName="YFS_Instruction_Detail_tran.xml" IncludeXMLPath="/Instruction"/>
                    <yfc:doc>
                        <Element EntityName="Instruction_Detail"/>
                    </yfc:doc>
                </Instruction>
            </Instructions>
        </ShipmentLine>
    </ShipmentLines>
    <AdditionalDates>
        <AdditionalDate ActualDate="" AdditionalDateKey=""
            DateTypeId="Required" ExpectedDate="" ReferenceKey=""
            ReferenceType="" RequestedDate="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Additional_Date_tran.xml" IncludeXMLPath="/AdditionalDate"/>
            <yfc:doc>
                <Attributes>
                    <Attribute Name="DateTypeId" NameInEntity="Date_Type_Id">
                        <Description>
                            <div>
							Identifies a date type for which additional dates are stored.
								<Validation>
                                    <Condition>
                                    <code>DateTypeId</code> should be non-void. Should also be unique for the Shipment.
									</Condition>
                                    <Error
                                    Description="Missing attribute in input." ErrorCode="YFS10460"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </AdditionalDate>
    </AdditionalDates>
</Shipment>
