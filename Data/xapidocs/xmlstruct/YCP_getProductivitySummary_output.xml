<ProductivitySummary EndDate="" StartDate="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getProductivitySummary/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="EndDate">
                <Description>
                    <div>
				End date for the time period to be considered to calculate the productivity.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="StartDate">
                <Description>
                    <div>
				Start date for the time period to be considered to calculate the productivity.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ProductivityType AllowedMinsConstant="" AllowedMinsPerCase=""
        AllowedMinsPerItem="" AllowedMinsPerPallet=""
        AllowedMinsPerSourceAisle="" AllowedMinsPerSourceLevel=""
        AllowedMinsPerSourceLocn="" AllowedMinsPerTargetAisle=""
        AllowedMinsPerTargetLevel="" AllowedMinsPerTargetLocn=""
        AllowedMinsPerTask="" AllowedMinsPerUnit=""
        AllowedMinsPerUnitHorizontalDistance=""
        AllowedMinsPerUnitVerticalDistance=""
        AllowedMinsPerUnitWeight="" CaptureTripLevelMetrics=""
        Createprogid="" Createts="" Createuserid="" Description=""
        EquipmentType="" IsAllowedMinsConstantAppl=""
        IsAllowedMinsPerCaseAppl="" IsAllowedMinsPerItemAppl=""
        IsAllowedMinsPerPalletAppl="" IsAllowedMinsPerSourceAisleAppl=""
        IsAllowedMinsPerSourceLevelAppl=""
        IsAllowedMinsPerSourceLocnAppl=""
        IsAllowedMinsPerTargetAisleAppl=""
        IsAllowedMinsPerTargetLevelAppl=""
        IsAllowedMinsPerTargetLocnAppl="" IsAllowedMinsPerTaskAppl=""
        IsAllowedMinsPerUnitAppl=""
        IsAllowedMinsPerUnitHorizontalDistanceAppl=""
        IsAllowedMinsPerUnitVerticalDistanceAppl=""
        IsAllowedMinsPerUnitWeightAppl="" IsPlannedPerBatchAppl=""
        IsPlannedPerCaseAppl="" IsPlannedPerItemAppl=""
        IsPlannedPerPalletAppl="" IsPlannedPerTaskAppl=""
        IsPlannedPerUnitAppl="" IsPlannedPerUnitWtAppl="" Lockid=""
        Modifyprogid="" Modifyts="" Modifyuserid="" OrganizationCode=""
        PlannedMinsPerBatch="" PlannedMinsPerCase=""
        PlannedMinsPerItem="" PlannedMinsPerPallet=""
        PlannedMinsPerTask="" PlannedMinsPerUnit=""
        PlannedMinsPerUnitWt="" ProductivityType=""
        ProductivityTypeKey="" yfc:DTDOccurrence="REQUIRED">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Productivity_Type_master.xml" IncludeXMLPath="/ProductivityType"/>
        <UserList yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
					List of the users who executed tasks associated with this productivity type during the given period.
					</div>
                    </Description>
                </Element>
            </yfc:doc>
            <User CreditedMins="" Efficiency="" EstimatedCreditedMins=""
                EstimatedEfficiency="" HorizontalDistance=""
                NoOfItems="" NoOfPallets="" NoOfSourceAislesVisited=""
                NoOfSourceLocationsVisited="" NoOfTargetAislesVisited=""
                NoOfTargetLocationsVisited="" NoOfTasks="" NoOfUnits=""
                NofOfCases="" NumSourceLevelsVisited=""
                NumTargetLevelsVisited="" TotalTimeTakenInMins=""
                TotalWeight="" UserId="" Utilization=""
                VerticalDistance="" yfc:DTDOccurrence="OneOrMany">
                <yfc:doc>
                    <Element EntityName="Productivity"/>
                    <Attributes>
                        <Attribute DataType="String" Name="Efficiency">
                            <Description>
                                <div>
						Efficiency is calculated using the formula (CreditedMins/TotalTimeTakenInMins)*100
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="EstimatedCreditedMins">
                            <Description>
                                <div>
						The minutes credited to the user calculated using the estimated SAM values passed in the input xml.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="EstimatedEfficiency">
                            <Description>
                                <div>
						EstimatedEfficiency is calculated using the formula (EstimatedCreditedMins/TotalTimeTakenInMins)*100
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="TotalTimeTakenInMins">
                            <Description>
                                <div>
						The total time taken by the user to complete the tasks
						</div>
                            </Description>
                        </Attribute>
                        <Attribute Name="UserId">
                            <Description>
                                <div>
						UserId of the user who executed the tasks.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="Utilization">
                            <Description>
                                <div>
						This gives the ratio between amount of time spent by the user in executing the tasks and the amount of time spent by him/her in the ware house. This is different from efficiency as it gives information on how much the user has been utilized.
						</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </User>
        </UserList>
    </ProductivityType>
</ProductivitySummary>
