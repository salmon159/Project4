<WorkOrder EnterpriseCode="Conditional" NodeKey="Conditional"
    TransactionId="Required" WorkOrderKey="Conditional"
    WorkOrderNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/VAS/checkHoldTypesForWorkOrder/input">
    <yfc:includeInParent IncludeXMLFileName="YFS_Work_Order_keys.xml" IncludeXMLPath="/WorkOrder"/>
    <yfc:doc>
        <Element EntityName="Work_Order">
            <Description>
                <div>The work order to be cancelled. Pass  <code>WorkOrderKey</code>
                    <i>or</i> pass <code>WorkOrderNo</code>, <code>NodeKey</code> and <code>EnterpriseCode</code> to identify the work order.
				<Validation>
                        <Condition>The passed attributes are unable to identify an existing work order.
					</Condition>
                        <Error Description="Invalid Work Order." ErrorCode="VAS00013"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="TransactionID" Name="TransactionId">
                <Description>
                    <div>
                        <Validation>
                            <Condition>If not passed.</Condition>
                            <Error Description="Invalid Transaction ID." ErrorCode="YCP0068"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</WorkOrder>
