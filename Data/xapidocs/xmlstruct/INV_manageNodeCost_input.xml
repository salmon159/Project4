<NodeCost CalculatedCost="" CostCurrency="" EffectiveEndDate=""
    EffectiveStartDate="" Node="Required" Operation=""
    OrganizationCode="Required" xmlns="http://www.sterlingcommerce.com/documentation/INV/manageNodeCost/input">
    <yfc:doc>
        <Attributes>
            <Attribute Name="Node">
                <Description>
                    <div>
                        <Validation>
                            <Condition>
								Must be an existing <code>Node</code>.
							</Condition>
                            <Error
                                Description="Missing mandatory parameters." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>
						Operation to perform
						<br/>
						Valid Values:
						<br/>
                        <b>
                            <code>Manage</code>
                        </b>
						- Creates a new record if it does not exist, or modifies an
						existing record if it does.
						<br/>
                        <b>
                            <code>Create</code>
                        </b>
						- Creates a new record.
						<br/>
                        <b>
                            <code>Modify</code>
                        </b>
						- Change values in an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b>
						- Deletes an existing record.
						<br/>
						The default behavior is
						<b>
                            <code>Manage</code>
                        </b>
						.
					</div>
                </Description>
            </Attribute>
            <Attribute Name="OrganizationCode">
                <Description>
                    <div>
                        <Validation>
                            <Condition>
								Must be an existing <code>OrganizationCode</code>.
							</Condition>
                            <Error
                                Description="Missing mandatory parameters." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</NodeCost>
