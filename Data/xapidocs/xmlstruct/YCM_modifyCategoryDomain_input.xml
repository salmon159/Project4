<!-- modifyCategoryDomain Input XML -->
<CategoryDomainList xmlns="http://www.sterlingcommerce.com/documentation/YCM/modifyCategoryDomain/input">
    <CategoryDomain Action="" AttributeName=""
        AuthorizedSubCatalogOrganizationCode=""
        CategoryDomain="Conditional" CategoryDomainKey="Conditional"
        Description="" IsClassification="Conditional"
        OrganizationCode="Conditional" ShortDescription="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
                    Highest level of classification of an item. Multiple domains can be created by an organization. All categorization is defined under a particular domain.
                        <Validation>
                            <Condition>
                            Either the <code>CategoryDomainKey</code> attribute or the combination of the <code>OrganizationCode</code>, <code>CategoryDomain</code>, and <code>IsClassification</code> attributes must be passed and must be valid.
                            </Condition>
                        </Validation>
                        <Validation>
                            <Condition>
                            If not passed:
                            </Condition>
                            <Error
                                Description="Mandatory parameters for the operation are missing" ErrorCode="YCM0002"/>
                        </Validation>
                        <Validation>
                            <Condition>
                            If not valid:
                            </Condition>
                            <Error
                                Description="Category Domain Not Found" ErrorCode="YCM0034"/>
                        </Validation>
                        <Validation>
                            <Condition>
                            The <code>CategoryDomain</code>, <code>OrganizationCode</code>, and <code>IsClassification</code> attributes cannot be modified. If modification is attempted:
                            </Condition>
                            <Error
                                Description="Primary Key or Unique Keys cannot be modified" ErrorCode="YCM0040"/>
                        </Validation>
                        <Validation>
                            <Condition>
						   If <code>AuthorizedSubCatalogOrganizationCode</code> is not of a valid sub-catalog organization of the catalog organization specified in <code>OrganziationCode</code> field:
						</Condition>
                            <Error
                                Description="Invalid sub-catalog organization" ErrorCode="YCM83_007"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="YCMAction" Name="Action">
                    <Description>Action to take</Description>
                </Attribute>
                <Attribute Name="AttributeName">
                    <Description>
                        <div>
                        A pre-defined list of fields that can be used for item classification.
                            <Validation>
                                <Condition>
                                If IsClassification is set to "Y", AttributeName must be passed. If not passed:
                                </Condition>
                                <Error
                                    Description="Mandatory parameters for the operation are missing" ErrorCode="YCM0002"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                If AttributeName is passed, it must be a valid attribute name of an item that the system allows. The valid values are HarmonizedCode, Schedule_B_Code, NMFCCode, NMFCClass, ECCNNo, NAICSCode, UNSPSC, HazmatClass, ProductLine, VelocityCode, TaxProductCode, ItemType, StorageType, PickingType, CommodityCode, and extended attributes of Item. If not a valid attribute name:
                                </Condition>
                                <Error
                                    Description="Invalid Item Classification Attribute" ErrorCode="YCM0050"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <AssetList Reset="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="Reset">
                        <Description>
                            <div>
								If <code>Reset</code> is passed as <b>
                                    <code>Y</code>
                                </b>, the old Asset records are removed and replaced with the new ones.
								If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                    <code>N</code>
                                </b>, only the deltas are applied. 
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Asset AssetID="" AssetKey="" ContentID=""
                ContentLocation="" ContentSize="" Description=""
                DisplayLocalizedFieldInLocale="" Label="" Operation="" Type="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Locale" Name="DisplayLocalizedFieldInLocale">
                            <Description>
                                <div>
									The locale for which output should be returned in. Locale should be passed in the standard format of "language_country_variant" (e.g., "fr_FR", "en_US_CA", etc.).
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Action" Name="Operation">
                            <Description>
                                <div>
									Valid value is <b>
                                    <code>Delete</code>
                                    </b>. Set <code>Operation</code> = "<b>
                                    <code>Delete</code>
                                    </b>" to delete the entity.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Action" Name="Type">
                            <Description>
                                <div>
									Type of asset.<br/>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <AssetLocaleList Reset="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Flag" Name="Reset">
                                <Description>
                                    <div>
										If <code>Reset</code> is passed as <b>
                                    <code>Y</code>
                                    </b>, the old records are removed and replaced with the new ones.
										If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                    <code>N</code>
                                    </b>, only the deltas are applied. 
									</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <AssetLocale AssetKey="" AssetLocaleKey=""
                        ContentID="" ContentLocation="" Country=""
                        Description="" Label="" Language="" Operation="" Variant="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Action" Name="Operation">
                                    <Description>
                                    <div>
											Valid value is <b>
                                    <code>Delete</code>
                                    </b>. Set <code>Operation</code> = "<b>
                                    <code>Delete</code>
                                    </b>" to delete the entity.
										</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </AssetLocale>
                </AssetLocaleList>
            </Asset>
        </AssetList>
        <CategoryDomainLocaleList Reset="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="Reset">
                        <Description>
                            <div>
								If <code>Reset</code> is passed as <b>
                                    <code>Y</code>
                                </b>, the old records are removed and replaced with the new ones.
								If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                    <code>N</code>
                                </b>, only the deltas are applied. 
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <CategoryDomainLocale CatDomainLocaleKey=""
                CategoryDomainKey="" Country="" Description=""
                Language="" Operation="" ShortDescription="" Variant="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="CatDomainLocaleKey"/>
                        <Attribute DataType="String" Name="Country"/>
                        <Attribute DataType="String" Name="Language"/>
                        <Attribute DataType="Action" Name="Operation">
                            <Description>
                                <div>
									Valid value is <b>
                                    <code>Delete</code>
                                    </b>. Set <code>Operation</code> = "<b>
                                    <code>Delete</code>
                                    </b>" to delete the entity.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="Variant"/>
                    </Attributes>
                </yfc:doc>
            </CategoryDomainLocale>
        </CategoryDomainLocaleList>
    </CategoryDomain>
</CategoryDomainList>
