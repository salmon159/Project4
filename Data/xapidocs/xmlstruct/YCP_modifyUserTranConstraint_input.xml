<UserTranConstraint ActiveFlag="" AssignedToUserId="Required" BatchNo=""
    ContainerNo="" CountRequestNo="" EnterpriseKey="" MoveRequestNo=""
    OrganizationCode="" ReceiptNo="" ShipmentNo="" TaskType=""
    UserTranConstraintsKey="Required" WaveNo="" WorkOrderNo="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/modifyUserTranConstraint/input">
    <yfc:doc>
        <Attributes>
            <Attribute Name="AssignedToUserId">
                <Description>
                    <div>
					Identifies user.
					<Validation>
                            <Condition>
                                <code>AssignedToUserId</code> not passed.
						</Condition>
                            <Error
                                Description="Missing mandatory parameters" ErrorCode="YFS10364"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>AssignedToUserId</code> value does not exist in YFS_USER table.
						</Condition>
                            <Error Description="YFS:Invalid User" ErrorCode="YCP0084"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="UserTranConstraintsKey">
                <Description>
                    <div>
					Identifies the <code>UserTranContraint</code>.
					<Validation>
                            <Condition>
                                <code>UserTranConstraintsKey</code> passed is not found.
						</Condition>
                            <Error
                                Description="Record does not exist in the database" ErrorCode="YFC0002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</UserTranConstraint>
