<PriceSet ActiveFlag="Y|N" Currency="Required" Description=""
    ItemGroupCode="" OrganizationCode="Required" PriceSetName="Required"
    ValidTillDate="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/createPriceSet/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
				The Price List master table. Stores a list of valid price lists for an organization.
					<Validation>
                        <Condition>
                            <code>PriceSet</code> should be the root node. If not valid:
						</Condition>
                        <Error
                            Description="Input document is empty or contains invalid data" ErrorCode="YFS10363"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute Name="Currency">
                <Description>
                    <div>
						The currency.
						<Validation>
                            <Condition>
                                <code>Currency</code> is a mandatory attribute. If not passed:
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing" ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="OrganizationCode">
                <Description>
                    <div>
						Link to YFS_Organization.
						<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is a mandatory attribute. If not passed:
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing" ErrorCode="YFS10364"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> must be valid. If not valid
							</Condition>
                            <Error
                                Description="YFS:Invalid organization code" ErrorCode="YCM0019"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="PriceSetName">
                <Description>
                    <div>
						The name of a price set.
						<Validation>
                            <Condition>
                                <code>PriceSetName</code> is a mandatory attribute. If not passed:
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing" ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ItemPriceSetList>
        <ItemPriceSet ItemID="Required" ListPrice="" ProductClass=""
            RetailPrice="" Uom="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
						The price list for items.
						</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute Name="ItemID">
                        <Description>
                            <div>
								An Item for which a price set is maintained. 
								<Validation>
                                    <Condition>
									If <code>ItemPriceSet</code> is passed, <code>ItemID</code> is mandatory. If not passed:
									</Condition>
                                    <Error
                                    Description="Mandatory Parameters for the Operation are missing" ErrorCode="YFS10364"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ItemPriceSetDtlList>
                <ItemPriceSetDtl BreakQtyHigh="Required"
                    BreakQtyLow="Required" RegionKey="" UnitPrice="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
									If an item requires step pricing, this table stores the quantity breaks and associated price.
									</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute Name="BreakQtyLow">
                                <Description>
                                    <div>
											The Break Low quantity.
											<Validation>
                                    <Condition>
                                    <code>BreakQtyLow</code> cannot be greater than <code>BreakQtyHigh</code>. If invalid:
												</Condition>
                                    <Error
                                    Description="BreakQtyLow cannot be greater than BreakQtyHigh" ErrorCode="OMP10031"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </ItemPriceSetDtl>
            </ItemPriceSetDtlList>
        </ItemPriceSet>
    </ItemPriceSetList>
</PriceSet>
