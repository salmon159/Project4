<!-- getServiceSlotGroupList input XML -->
<ServiceSlotGroup ItemGroupCode="" OrganizationKey="Required"
    ServiceSlotGroupDesc="" ServiceSlotGroupId="" ServiceSlotGroupKey="" xmlns="http://www.sterlingcommerce.com/documentation/INV/getServiceSlotGroupList/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
				A service slot group is a collection of service slots.
				<Validation>
                        <Condition>
							The <b>
                                <code>OrganizationKey</code>
                            </b> attribute is mandatory. If it is not passed:
						</Condition>
                        <Error
                            Description="INV: Missing mandatory attribute" ErrorCode="INV10008"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute Name="OrganizationKey">
                <Description>
                    <div>
					A Service Slot Group is created for a Capacity Organization. An Organization can use service slot groups within the same Capacity Organization.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ServiceSlotList>
        <ServiceSlot CanUseForAppointment="" EndTime="" ParentSlotKey=""
            ServiceSlotDesc="" ServiceSlotGroupKey="" ServiceSlotKey="" StartTime="">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
					A service slot is a defined period of time against which service item promising can be made.
					</div>
                    </Description>
                </Element>
            </yfc:doc>
        </ServiceSlot>
    </ServiceSlotList>
</ServiceSlotGroup>
