<!-- manageAttribute Input XML -->
<AttributeList xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageAttribute/input">
    <Attribute AllowMultipleValues="" AllowedValueLength=""
        AttributeDomainID="" AttributeGroupID="" AttributeID=""
        AttributeKey="" AttributePostFix="" DataType=""
        DerivedFromAttributeKey="" IsAllowedValueDefined=""
        IsValueMandatory="" LongDescription=""
        Operation="Manage|Create|Modify|Delete|" OrganizationCode=""
        SequenceNo="" ShortDescription="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="EnumAttributeDataType" Name="DataType">
                    <Description>
                        <div>The data type allowed for the value of the attribute..
							<Validation>
                                <Condition>
								The <code>DataType</code> of the <code>Attribute</code> must be one of INTEGER, DECIMAL, BOOLEAN, TEXT or DATE.
							 </Condition>
                                <Error
                                    Description="Invalid Attribute Data Type Defined." ErrorCode="YSC84_0009"/>
                            </Validation>
                            <Validation>
                                <Condition>
								If this <code>Attribute</code> is a Derived Attribute, then the  <code>DataType</code> of this <code>Attribute</code> must be one of INTEGER or DECIMAL.
							 </Condition>
                                <Error
                                    Description="Invalid Attribute Data Type Defined For Derived Attribute." ErrorCode="YSC84_0019"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Action" Name="Operation">
                    <Description>
                        <div>Operation to perform
						<br/>Valid Values: 
						<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
						<br/>
                            <b>
                                <code>Create</code>
                            </b> - Creates a new record. 
						<br/>
                            <b>
                                <code>Modify</code>
                            </b> - Change values in an existing record.
						<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
						<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <InsertBefore AttributeDomainID="" AttributeGroupID=""
            AttributeID="" AttributeKey="" OrganizationCode="" SequenceNo="">
            <yfc:doc>
                <Element EntityName="Attribute">
                    <Description>
                        <div>The <code>Attribute</code> before which the current <code>Attribute</code> must be inserted. The <code>SequenceNo</code> will be computed such that the current attribute has <code>SequenceNo</code> less than the <code>SequenceNo</code> of the <code>InsertBefore</code> element.
					<Validation>
                                <Condition>
						Either <code>AttributeKey</code> or a combination of <code>AttributeID</code>, <code>AttributeDomainID</code>, <code>AttributeGroupID</code> and <code>OrganizationCode</code> must be passed to identify the <code>InsertBefore</code> object.
					 </Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                            <Validation>
                                <Condition>
						Passed attributes do not identify an <code>Attribute</code> record.
					 </Condition>
                                <Error Description="No Record Found." ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
            </yfc:doc>
        </InsertBefore>
        <DerivedFromAttribute AttributeDomainID="" AttributeGroupID=""
            AttributeID="" AttributeKey="" OrganizationCode="">
            <yfc:doc>
                <Element EntityName="Attribute">
                    <Description>
                        <div>The <code>Attribute</code> for which the current <code>Attribute</code> defines the range of values is the <code>DerivedFromAttribute</code>.
				<Validation>
                                <Condition>
					Either <code>AttributeKey</code> or a combination of <code>AttributeID</code>, <code>AttributeDomainID</code>, <code>AttributeGroupID</code> and <code>OrganizationCode</code> must be passed to identify the <code>DerivedFromAttribute</code> object.
				 </Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
            </yfc:doc>
        </DerivedFromAttribute>
        <AttributeAllowedValueList Reset="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="Reset">
                        <Description>
                            <div>
					If <code>Reset</code> is passed as <b>
                                    <code>Y</code>
                                </b>, the old <code>AttributeAllowedValue</code> records are removed and replaced with the new ones.
					If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                    <code>N</code>
                                </b>, only the deltas are applied. 
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <AttributeAllowedValue AttributeAllowedValueKey=""
                AttributeKey="" IsDefaultValue="" LongDescription=""
                NumericStep="" Operation="Manage|Create|Modify|Delete|"
                SequenceNo="" ShortDescription="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Number" Name="NumericStep">
                            <Description>
                                <div>The incremental step of the allowed value within the numeric value range. For instance, if the numeric value range is from 0 to 100 and the numeric step is 5, the allowed values are 0, 5, 10, 15, 20, 25, etc. If not passed, it is defaulted to 1.
							<Validation>
                                    <Condition>
								If the <code>DataType</code> of the <code>Attribute</code> is an INTEGER, then the <code>NumericStep</code> must also be an Integer.
							 </Condition>
                                    <Error
                                    Description="Invalid Numeric Step For Data Type." ErrorCode="YSC84_0014"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Action" Name="Operation">
                            <Description>
                                <div>Operation to perform
						<br/>Valid Values: 
						<br/>
                                    <b>
                                    <code>Manage</code>
                                    </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
						<br/>
                                    <b>
                                    <code>Create</code>
                                    </b> - Creates a new record. 
						<br/>
                                    <b>
                                    <code>Modify</code>
                                    </b> - Change values in an existing record.
						<br/>
                                    <b>
                                    <code>Delete</code>
                                    </b> - Deletes an existing record.
						<br/>The default behavior is <b>
                                    <code>Manage</code>
                                    </b>.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-200" Name="Value">
                            <Description>
                                <div>The allowed value of the attribute that the <code>AttributeAllowedValue</code> represents. For an attribute of INTEGER or DECIMAL Datatype, this value may be a list of values or range of values. Each value in the list may be separated by a ';'. A range must be specified by two values separated by a '-'.
							<Validation>
                                    <Condition>
								If the <code>Value</code> attribute is a range, then the two numbers in the range must be separated by a '-'.
							 </Condition>
                                    <Error
                                    Description="Range Not Defined Correctly." ErrorCode="YSC84_0007"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
								If the <code>Value</code> attribute is a range, then the first value must be the smaller value and second value must be the greater one.
							 </Condition>
                                    <Error
                                    Description="Min Value Cannot Be Greater Than Max Value In A Range." ErrorCode="YSC84_0002"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
								If the <code>Attribute</code> is configured with <code>AllowMultipleValues</code> set to 'N' and this <code>AttributeAllowedValue</code> has <code>IsDefaultValue</code> set to 'Y', then the <code>Value</code> must not be a range.
							 </Condition>
                                    <Error
                                    Description="Cannot Define Multiple Default Values For This Attriute." ErrorCode="YSC84_0017"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
								If the <code>AllowedValueLength</code> is configured for this <code>AttributeAllowedValue</code> record, then the length of the <code>Value</code> string must not exceed the <code>AllowedValueLength</code>.
							 </Condition>
                                    <Error
                                    Description="Length Of the Value String Exceeds The Allowed Value Length For The Attribute." ErrorCode="YSC84_0012"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
								The <code>Value</code> string must contain valid Decimal numbers.
							 </Condition>
                                    <Error
                                    Description="Defined Value Not Valid For Decimal Data Type." ErrorCode="YSC84_0010"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
								The <code>Value</code> string must contain valid integers.
							 </Condition>
                                    <Error
                                    Description="Defined Value Not Valid For Integer Data Type." ErrorCode="YSC84_0018"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <AttrAllowValLocaleList Reset="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Flag" Name="Reset">
                                <Description>
                                    <div>
								If <code>Reset</code> is passed as <b>
