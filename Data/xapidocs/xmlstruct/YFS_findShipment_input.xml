<Shipment ActualDeliveryDate="" ActualShipmentDate="" AppointmentNo=""
    BolNo="" BuyerOrganizationCode="" CarrierAccountNo=""
    CarrierServiceCode="" CommercialValue="" Currency="" DeliveryCode=""
    DestinationZone="" DocumentType="Required" DownLoadCount=""
    EnterpriseCode="" ExpectedDeliveryDate="" ExpectedShipmentDate=""
    ExportTaxpayerID="" FreightTerms="" FromAddressKey=""
    FromAppointment="" HazardousMaterialFlag="" HeldForConfirmation=""
    ITDate="" ITNo="" IsSingleOrder="" LevelOfService="" ManifestNo=""
    NumOfCartons="" NumOfPallets="" OrderAvailableOnSystem="" OrderNo=""
    OriginZone="" PackListType="" PickListNo="" PickticketNo="" PodNo=""
    ProNo="" ReceivingNode="" ReleaseNo="" RequestedDeliveryDate=""
    RequestedShipmentDate="" SCAC="" SealNo="" SellerOrganizationCode=""
    ShipMode="" ShipNode="" ShipVia="" ShipmentKey="" ShipmentNo=""
    ShipmentType="" StatusDate="" ToAddressKey="" ToAppointment=""
    TotalVolume="" TotalVolumeUOM="" TotalWeight="" TotalWeightUOM=""
    TrackingNo="" TrailerNo="" TransactionId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/findShipment/input">
    <yfc:doc>
        <Element EntityName="Shipment"/>
        <Attributes>
            <Attribute EntityName="Shipment" Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>
					Indicates a classification of data on which a particular set of business processes must be applied. In this case, it happens to be the document type of the order document - basically the document type of the orders comprising the shipment. A document type derived from the base document type 0001 - ORDER.
						<Validation>
                            <Condition>
                                <code>DocumentType</code> should not be void.
							</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="HeldForConfirmation"/>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid">
                <Description>
                    <div>
					Unique identifier for a transaction.
						<Validation>
                            <Condition>The receipt which can be processed by this transaction will be output. <code>TransactionId</code> should not be void.
							</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                        <Validation>
                            <Condition>Error if <code>TransactionId</code> is not valid.
							</Condition>
                            <Error Description="Invalid Transaction Id." ErrorCode="YCP0068"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Shipment>
