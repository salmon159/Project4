<?xml version="1.0" encoding="UTF-8"?>
<PlannedTasks EntityKey="" EntityName="" Reset="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/managePlannedTasks/input">
    <yfc:doc>
        <Element EntityName="Planned_Task"/>
        <Attributes>
            <Attribute EntityName="Planned_Task" Name="EntityKey" NameInEntity="Entity_Key">
                <Description>
                    <div>
						Identifies a entity key being planned for.
						Passing this attribute at header level implies passing the same for each planned task.
						<Validation>
                            <Condition>If <code>Reset</code> is not passed or passed as <code>Y</code>, EntityKey is mandatory.</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed." ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Planned_Task" Name="EntityName" NameInEntity="Entity_Name">
                <Description>
                    <div>
						Identifies the entity being planned for.
						Passing this attribute at header level implies passing the same for each planned task.
						<Validation>
                            <Condition>If <code>Reset</code> is not passed or passed as <code>Y</code>, EntityName is mandatory.</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed." ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="Reset">
                <Description>
                    <div>The flag indicates whether to reset all the relevant planned tasks.
					If <code>Reset</code> is not passed, then it is defaulted to value <code>
                            <b>Y</b>
                        </code>.
					<br/>Valid values - <code>
                            <b>Y</b>
                        </code>, <code>
                            <b>N</b>
                        </code>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PlannedTask Completed="" EntityKey="" EntityName="" Node=""
        PlannedDate="" PlannedDateType="" PlannedTaskKey=""
        RequiredCapacity="" ResourcePoolKey="" TaskType="" yfc:DTDOccurrence="REQUIRED">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Planned_Tasks_master.xml"
            IncludeXMLPath="/PlannedTask" RemoveDBHouseKeeping="Y"/>
        <yfc:doc>
            <Element EntityName="Planned_Task"/>
            <Attributes>
                <Attribute DataType="Key" EntityName="Planned_Task"
                    Name="Node" NameInEntity="Node_Key">
                    <Description>
                        <div>
							Identifies the node for which planning is being done.
							<Validation>
                                <Condition>If passed<code>Node</code> is void or invalid.</Condition>
                                <Error Description="YFS:Invalid Node." ErrorCode="YFS10048"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Planned_Task"
                    Name="ResourcePoolKey" NameInEntity="Resource_Pool_Key">
                    <Description>
                        <div>
							Identifies the resource pool from which capacity is being consumed.
							<Validation>
                                <Condition>If not passed or an invalid value is passed.</Condition>
                                <Error
                                    Description="INV: Invalid Resource Pool." ErrorCode="INV10014"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </PlannedTask>
</PlannedTasks>
