<GetDeliveryLeadTime CarrierServiceCode="" EnterpriseCode="" SCAC=""
    SellerOrganizationCode="" ShipNode="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getDeliveryLeadTimeUE/input">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
                <div>
			Basic information to calculate delivery lead time.
			</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Key" EntityName="[NONE]" Name="ShipNode">
                <Description>
                    <div>When invoked from create or change order functionality, the Ship Node is retrieved from the 
					order line and order line schedule.  When invoked from the promising module, the Ship Node may 
					be populated using one of the following, depending on its context: 1) The node in the promise line or shipment, 2) The node where inventory exists, or 3) the merge node.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PersonInfoShipTo AddressID="" AddressLine1="" AddressLine2=""
        AddressLine3="" AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" EveningFaxNo=""
        EveningPhone="" FirstName="" IsAddressVerified=""
        IsCommercialAddress="" JobTitle="" LastName="" Latitude=""
        Longitude="" MiddleName="" MobilePhone="" OtherPhone=""
        PersonID="" PersonInfoKey="" State="" Suffix="" TaxGeoCode=""
        Title="" ZipCode="">
        <yfc:includeInParent IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/PersonInfoShipTo"/>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="MasterPersonInfo.xml" IncludeXMLPath="/PersonInfo"/>
        <yfc:doc>
            <Element EntityName="Person_Info">
                <Description>
                    <div>
				Information about the person being shipped to.
				</div>
                </Description>
            </Element>
        </yfc:doc>
    </PersonInfoShipTo>
    <PersonInfoShipFrom AddressID="" AddressLine1="" AddressLine2=""
        AddressLine3="" AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" EveningFaxNo=""
        EveningPhone="" FirstName="" IsAddressVerified=""
        IsCommercialAddress="" JobTitle="" LastName="" Latitude=""
        Longitude="" MiddleName="" MobilePhone="" OtherPhone=""
        PersonID="" PersonInfoKey="" State="" Suffix="" TaxGeoCode=""
        Title="" ZipCode="">
        <yfc:includeInParent IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/PersonInfoShipTo"/>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="MasterPersonInfo.xml" IncludeXMLPath="/PersonInfo"/>
        <yfc:doc>
            <Element EntityName="Person_Info">
                <Description>
                    <div>
				Information about the person who is shipping the order.
				</div>
                </Description>
            </Element>
        </yfc:doc>
    </PersonInfoShipFrom>
    <PossibleCarrierServices>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
						Carrier services available.
					</div>
                </Description>
            </Element>
        </yfc:doc>
        <CarrierService CarrierServiceCode="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Carrier_Service"/>
            </yfc:doc>
        </CarrierService>
    </PossibleCarrierServices>
</GetDeliveryLeadTime>
