<?xml version="1.0" encoding="UTF-8"?>
<Cart BatchNo="" CartId="Required" Node="Required" UserId="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/inductCart/input">
    <yfc:doc>
        <Element EntityName="Cart"/>
        <Attributes>
            <Attribute DataType="SequenceNo" EntityName="Cart"
                Name="BatchNo" NameInEntity="Batch_No">
                <Description>
                    <div> Identifies the batch associated with the cart.
						<Validation>
                            <Condition>Batch with only open tasks should be passed.
							</Condition>
                            <Error
                                Description="Task(s) not open for the batch." ErrorCode="WMS75100302"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" EntityName="Cart"
                Name="CartId" NameInEntity="Cart_Id">
                <Description>
                    <div> Unique Identifier for a cart.Forms unique key of this table in conjunction with <code>NODE_KEY</code>.
						<Validation>
                            <Condition>Cart has open associated with it.
							</Condition>
                            <Error
                                Description="Cart has open batch associated with it." ErrorCode="WMS75100301"/>
                        </Validation>
                        <Validation>
                            <Condition>No available open task to associate with cart.
							</Condition>
                            <Error Description="No open task to induct." ErrorCode="WMS00296"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>CartId</code> must be passed.
							</Condition>
                            <Error
                                Description="Mandatory parameters not passed." ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Tote" Name="Node" NameInEntity="Node_Key">
                <Description>
                    <div> 
					Business key or unique identifier for a ship node.
						<Validation>
                            <Condition>
                                <code>Node</code> must be passed.
							</Condition>
                            <Error
                                Description="Mandatory parameters not passed." ErrorCode="WMS00063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="UserId"/>
        </Attributes>
    </yfc:doc>
</Cart>
