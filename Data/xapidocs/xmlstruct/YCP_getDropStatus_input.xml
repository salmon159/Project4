<GetDropStatusInput BaseDropStatus="Required" FromStatus="Required"
    OwnerKey="" PipelineId="" PipelineKey="" ProcessType=""
    TransactionId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getDropStatus/input">
    <yfc:doc>
        <Element EntityName="Pipeline_Definition">
            <Description>
                <div>
				To uniquely identify a pipeline, pass either <code>PipelineKey</code>
                    <i>or</i>
                    <code>PipelineId</code>, <code>OwnerKey</code> and  <code>ProcessType</code>.
				<Validation>
                        <Condition>If neither  <code>PipelineKey</code>
                            <i>nor</i>
                            <code>PipelineId</code>, <code>OwnerKey</code> and  <code>ProcessType</code> is passed.
					</Condition>
                        <Error
                            Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Pipeline_Definition"
                Name="BaseDropStatus" NameInEntity="Drop_Status"/>
            <Attribute EntityName="Pipeline_Pickup_Tran"
                Name="FromStatus" NameInEntity="Status"/>
            <Attribute EntityName="Pipeline" Name="OwnerKey" NameInEntity="Owner_Key"/>
            <Attribute EntityName="Pipeline" Name="PipelineId" NameInEntity="Pipeline_Id"/>
            <Attribute EntityName="Process_Type" Name="ProcessType" NameInEntity="Process_Type"/>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid">
                <Description>Transaction for which drop status is being determined.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</GetDropStatusInput>
