    <RoutingGuideAttrs>
        <RoutingGuideAttr AttrName="" Include="" yfc:DTDOccurrence="ZeroOrMany">
            <Value EndZip="" StartZip="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Routing_Guide_Attr"/>
                    <Attributes>
                        <Attribute DataType="Text-100" Name="Value">
                            <Description>
                                <div>Value of the attribute.For example if the attribute is <code>FreightTerms</code>, then valid values are <code>COLLECT</code>, <code>PREPAID</code>,<code>TP-COLLECT</code> and <code>TP-PREPAID</code>.
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Value>
        </RoutingGuideAttr>
    </RoutingGuideAttrs>
    <RoutingGuideDetails>
        <RoutingGuideDetail CarrierServiceCode=""
            FreightTermsOverride="" Priority=""
            RevisedShipToConsolNode="" RevisedShipToNode=""
            RevisedShipToStore="" Scac="" VolumeUOM="" WeightUOM="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="CarrierServiceCode">
                        <Description>
							Indicates the kind of service(eg:Ground,Priority Overnight) offered by the carrier.
						</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Priority">
                        <Description>
							Indicates the number to give this rule a relative importance.
							When a shipment is compared to the routing guidelines, there may be two carrier services that could be used. The priority serves as a tie breaker. The carrier service with the lowest number is used.
						</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Scac">
                        <Description>
							The Standard Carrier Alpha Code assigned to identify the carrier.
						</Description>
                    </Attribute>
                    <Attribute Name="VolumeUOM">
                        <Description>
							Volume UOM of the Shipment/Load
						</Description>
                    </Attribute>
                    <Attribute Name="WeightUOM">
                        <Description>
							Weight UOM of the Shipment/Load
						</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
