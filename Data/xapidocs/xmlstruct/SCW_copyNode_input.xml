<Organization ClonedOrganizationCode="Required"
    OrganizationCode="Required" xmlns="http://www.sterlingcommerce.com/documentation/SCW/copyNode/input">
    <yfc:doc>
        <Element EntityName="Organization">
            <Description>
                <div>
					Both <code>OrganizationCode</code> and <code>ClonedOrganizationCode</code> 
					 should be passed.
					<Validation>
                        <Condition>If both <code>ClonedOrganizationCode</code> and <code>OrganizationCode</code> are not passed
						</Condition>
                        <Error
                            Description="Mandatory Parameter(s) not passed." ErrorCode="WMS00063"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Organization"
                Name="ClonedOrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>The existing WMS node configuration that needs to be copied. 
						This should be a networked WMS node.
						<Validation>
                            <Condition>If <code>ClonedOrganizationCode</code> is not a networked WMS node.
							</Condition>
                            <Error
                                Description="Node passed does not exist" ErrorCode="WMS00042"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization" Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>The new WMS node configuration to be created.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Organization>
