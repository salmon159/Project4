<StatusInventoryTypes BaseProcessType="" BuyerSupplyType=""
    DocumentType="Required" Fob="" HasChainedChild=""
    IncrementSellerSupply="" Operation="" ProcessType=""
    ProcessTypeKey="Required" SellerDemandType="" SellerSupplyType=""
    Status="Required" StatusInventoryID="" StatusInventoryTypesKey="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/manageStatusInventoryTypes/input">
    <yfc:doc>
        <Element EntityName="Status_Inventory_Types"/>
        <Attributes>
            <Attribute EntityName="Process_Type" Name="BaseProcessType" NameInEntity="Base_Process_Type">
                <Description>
                    <div>
					If <code>BaseProcessType</code> is passed, <code>DocumentType</code> should also be passed. If <code>ProcessTypeKey</code> or <code>ProcessType</code> is passed,
					its value is used to make the matches, and <code>BaseProcessType</code> is ignored. 
					<Validation>
                            <Condition>Invalid BaseProcessType passed.</Condition>
                            <Error Description="Invalid Process Type" ErrorCode="YCP0062"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="BuyerSupplyType">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Invalid Supply Type passed.</Condition>
                            <Error Description="YFS: Invalid Data" ErrorCode="YFS10190"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="DocumentType">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Invalid <code>DocumentType</code> passed.</Condition>
                            <Error
                                Description="YCP:Invalid Document Type" ErrorCode="YCP0029"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="HasChainedChild">
                <Description>
                    <div>
						Valid Values:
						<br/>
                        <b>
                            <code>Y</code>
                        </b> - For Orders with chained children.
						<br/>
                        <b>
                            <code>N</code>
                        </b> - For Orders without chaining and for Procurement Orders.		
						<Validation>
                            <Condition>Invalid <code>HasChainedChild</code> passed.</Condition>
                            <Error Description="YFS: Invalid Data" ErrorCode="YFS10190"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>
						Operation to perform on this record.
						<br/>
                        <br/>Default: If not passed then API will try to either create or modify depending on the existence of the data.
						<br/>
                        <br/>Valid Values: 
						<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
						<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.					
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Process_Type" Name="ProcessType" NameInEntity="Process_Type">
                <Description>
                    <div>
						If <code>ProcessTypeKey</code> is passed, <code>ProcessType</code> is ignored.
						<Validation>
                            <Condition>Invalid ProcessType passed.</Condition>
                            <Error Description="Invalid Process Type" ErrorCode="YCP0062"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Process_Type" Name="ProcessTypeKey" NameInEntity="Process_Type_Key">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Invalid <code>ProcessTypeKey</code> passed.</Condition>
                            <Error Description="Invalid Process Type" ErrorCode="YCP0062"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="SellerDemandType">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Invalid Demand Type passed.</Condition>
                            <Error Description="YFS: Invalid Data" ErrorCode="YFS10190"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="SellerSupplyType">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Invalid Supply Type passed.</Condition>
                            <Error Description="YFS: Invalid Data" ErrorCode="YFS10190"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="Status">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Invalid <code>Status</code> passed.</Condition>
                            <Error Description="YFS:Invalid Status" ErrorCode="YCP0158"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="StatusInventoryID">
                <Description>
                    <div>
						Valid Values:
						<br/>
                        <b>
                            <code>DEFAULT</code>
                        </b> - For Orders with or without chained children.
						<br/>
                        <b>
                            <code>PROCURE</code>
                        </b> - For Procurement Orders.	
						<Validation>
                            <Condition>Invalid <code>StatusInventoryID</code> passed.</Condition>
                            <Error Description="YFS: Invalid Data" ErrorCode="YFS10190"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>StatusInventoryID</code> is passed as "PROCURE" and <code>HasChainedChild</code> is passed as "Y"</Condition>
                            <Error Description="YFS: Invalid Data" ErrorCode="YFS10190"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</StatusInventoryTypes>
