<CarrierServiceOverrideList CarrierServiceCode="" CarrierServiceKey="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageCarrierServiceOverride/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Text-40" Name="CarrierServiceCode">
                <Description>
                    <div>A code that identifies a service provided by a carrier.
						<br/>If <code>CarrierServiceKey</code> is passed, <code>CarrierServiceCode</code> is ignored.
						<Validation>
                            <Condition>If invalid <code>CarrierServiceCode</code> is provided.</Condition>
                            <Error Description="YFS: Invalid Data." ErrorCode="YFS10190"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Carrier_Service_Override"
                Name="CarrierServiceKey" NameInEntity="Carrier_Service_Key">
                <Description>
                    <div>The key of the carrier service for which this override date is defined.
					<Validation>
                            <Condition>If creating a new record, CarrierServiceKey is not passed OR If updating existing record, new value passed is null. </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <CarrierServiceOverride CanDeliver="" CanTransit=""
        CarrierServiceOverrideKey="" Operation="" OverrideDate="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Action" Name="Operation">
                    <Description>
                        <div>Operation to perform on this record.
				<br/>Valid Values: 
				<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
				<br/>
                            <b>
                                <code>Create</code>
                            </b> - Creates a new record. 
				<br/>
                            <b>
                                <code>Modify</code>
                            </b> - Change values in an existing record.
				<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
				<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
			</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Carrier_Service_Override"
                    Name="OverrideDate" NameInEntity="Override_Date">
                    <Description>
                        <div>The date that has been overridden. This date takes precedence over the carrier service schedule.
					<Validation>
                                <Condition>If creating a new record, OverrideDate is not passed OR If updating existing record, new value passed is null. </Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </CarrierServiceOverride>
</CarrierServiceOverrideList>
