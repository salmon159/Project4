<Receipt DocumentType="Required" ReceiptHeaderKey="Conditional"
    ReceiptNo="Conditional" ReceivingNode="Conditional"
    ShipmentKey="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YFS/verifyReceipt/input">
    <yfc:doc>
        <Element EntityName="Receipt_Header">
            <Description>
                <div>
                    <Validation>
                        <Condition>
                            <Code>ReceiptHeaderKey</Code> attribute OR the <Code>ReceiptNo</Code>, <Code>ReceivingNode</Code> and <Code>ShipmentKey</Code> attributes must be passed and must be valid.
						</Condition>
                        <Error
                            Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Document_Params" Name="DocumentType">
                <Description>
                    <div>
					The Document Type of the order being received.
						<Validation>
                            <Condition>
                                <code>DocumentType</code> should not be void.
							</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Receipt_Header" Name="ReceivingNode" NameInEntity="ReceivingNode_Key">
                <Description>
                    <div>
					The Node that is receiving the shipment.
						<Validation>
                            <Condition>
                                <code>ReceivingNode</code> should be valid. 
							</Condition>
                            <Error Description="Invalid Receiving Node" ErrorCode="YFS10371"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Receipt>
