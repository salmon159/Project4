<!--YCP_resolveException_Input.xml-->
<!-- Provide resolution details -->
<ResolutionDetails AutoResolvedFlag="" IgnoreHookErrors="" ResolvedBy="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/resolveException/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="IgnoreHookErrors">
                <Description>Indicates if the error returned by the exception resolution hook should not be displayed in the output XML. Valid values are Y and N. The default is Y.</Description>
            </Attribute>
            <Attribute EntityName="User" Name="ResolvedBy" NameInEntity="Loginid">
                <Description>User who resolved the exception</Description>
            </Attribute>
        </Attributes>
        <Element EntityName="Inbox"/>
    </yfc:doc>
    <!-- Pass criteria to identify exceptions for resolution-->
    <Inbox ActiveFlag="" ActiveFlagQryType="" AssignedToUserKey=""
        AssignedToUserKeyQryType="" AutoResolvedFlag=""
        AutoResolvedFlagQryType="" BillToID="" ClosedOn=""
        ClosedOnQryType="" Description="" DescriptionQryType=""
        DetailDescription="" DetailDescriptionQryType=""
        EnterpriseKey="" EnterpriseKeyQryType=""
        ExceptionEscalatedFlag="" ExceptionEscalatedFlagQryType=""
        ExceptionType="" ExceptionTypeQryType="" FollowupDate=""
        FollowupDateQryType="" FromActiveFlag=""
        FromAssignedToUserKey="" FromAutoResolvedFlag="" FromClosedOn=""
        FromDescription="" FromDetailDescription="" FromEnterpriseKey=""
        FromExceptionEscalatedFlag="" FromExceptionType=""
        FromFollowupDate="" FromGeneratedOn="" FromInboxKey=""
        FromInboxType="" FromItemId="" FromLastExceedAlert=""
        FromLastUnassignAlert="" FromLastUnresolveAlert=""
        FromLockedByUserKey="" FromLockedFlag="" FromLockedOn=""
        FromLockid="" FromOrderHeaderKey="" FromOrderLineKey=""
        FromOrderNo="" FromOwnerKey="" FromOwnerType=""
        FromParentInboxKey="" FromPriority="" FromQueueId=""
        FromQueueKey="" FromResolutionDate="" FromResolveBy=""
        FromShipnodeKey="" FromStatus="" FromSupplierKey=""
        FromWaveKey="" FromWaveNo="" FromWorkOrderKey=""
        FromWorkOrderNo="" GeneratedOn="" GeneratedOnQryType=""
        InboxKey="" InboxKeyQryType="" InboxType="" InboxTypeQryType=""
        ItemId="" ItemIdQryType="" LastExceedAlert=""
        LastExceedAlertQryType="" LastUnassignAlert=""
        LastUnassignAlertQryType="" LastUnresolveAlert=""
        LastUnresolveAlertQryType="" LockedByUserKey=""
        LockedByUserKeyQryType="" LockedFlag="" LockedFlagQryType=""
        LockedOn="" LockedOnQryType="" Lockid="" LockidQryType=""
        MaximumRecords="" OrderHeaderKey="" OrderHeaderKeyQryType=""
        OrderLineKey="" OrderLineKeyQryType="" OrderNo=""
        OrderNoQryType="" OwnerKey="" OwnerKeyQryType="" OwnerType=""
        OwnerTypeQryType="" ParentInboxKey="" ParentInboxKeyQryType=""
        Priority="" PriorityQryType="" QueueId="Conditional"
        QueueIdQryType="" QueueKey="Conditional" QueueKeyQryType=""
        QueueOwnerKey="Conditional" ResolutionDate=""
        ResolutionDateQryType="" ResolveBy="" ResolveByQryType=""
        ShipnodeKey="" ShipnodeKeyQryType="" Status="" StatusQryType=""
        SupplierKey="" SupplierKeyQryType="" TeamCode="" ToActiveFlag=""
        ToAssignedToUserKey="" ToAutoResolvedFlag="" ToClosedOn=""
        ToDescription="" ToDetailDescription="" ToEnterpriseKey=""
        ToExceptionEscalatedFlag="" ToExceptionType="" ToFollowupDate=""
        ToGeneratedOn="" ToInboxKey="" ToInboxType="" ToItemId=""
        ToLastExceedAlert="" ToLastUnassignAlert=""
        ToLastUnresolveAlert="" ToLockedByUserKey="" ToLockedFlag=""
        ToLockedOn="" ToLockid="" ToOrderHeaderKey="" ToOrderLineKey=""
        ToOrderNo="" ToOwnerKey="" ToOwnerType="" ToParentInboxKey=""
        ToPriority="" ToQueueId="" ToQueueKey="" ToResolutionDate=""
        ToResolveBy="" ToShipnodeKey="" ToStatus="" ToSupplierKey=""
        ToWaveKey="" ToWaveNo="" ToWorkOrderKey="" ToWorkOrderNo=""
        WaveKey="" WaveKeyQryType="" WaveNo="" WaveNoQryType=""
        WorkOrderKey="" WorkOrderKeyQryType="" WorkOrderNo=""
        WorkOrderNoQryType="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Inbox" Name="Lockid" NameInEntity="Lockid">
                    <Description>If this is passed,InboxKey is mandatory.</Description>
                </Attribute>
                <Attribute DataType="Number-11" Name="MaximumRecords">
                    <Description>Maximum number of records matching the filter criteria to be processed by the API.</Description>
                </Attribute>
                <Attribute EntityName="Queue" Name="QueueKey" NameInEntity="Queue_Key">
                    <Description>
                        <div>
                            <code>Queue</code> to which the exception belongs. If <code>QueueId</code> and <code>QueueOwnerKey</code> are also passed, they are used to identify the queue, and the <code>QueueKeyQryType</code>, if passed, is not used. 
				</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Queue" Name="QueueOwnerKey" NameInEntity="Owner_Key"/>
            </Attributes>
            <Element EntityName="Inbox">
                <Description>
                    <div>
				Exception Master table. All exceptions raised by system are stored in this table. Pass either <code>QueueKey</code> or 
				<code>QueueId</code> and <code>QueueOwnerKey</code> to identify the Queue.
				<Validation>
                            <Condition>Cannot find the <code>Queue</code> specified by the <code>QueueKey</code> or <code>QueueID</code> and <code>QueueOwnerKey</code>.
					</Condition>
                            <Error Description="YFS:No Record Found" ErrorCode="YFS10001"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
        <!-- Include any data that will help in querying/resolving the exception as a key-value pair-->
        <InboxReferencesList>
            <InboxReferences FromInboxKey="" FromInboxReferenceKey=""
                FromName="" FromReferenceType="" FromValue=""
                InboxKey="" InboxKeyQryType="" InboxReferenceKey=""
                InboxReferenceKeyQryType="" Name="" NameQryType=""
                ReferenceType="" ReferenceTypeQryType="" ToInboxKey=""
                ToInboxReferenceKey="" ToName="" ToReferenceType=""
                ToValue="" Value="" ValueQryType="" yfc:DTDOccurrence="ZeroOrMany"/>
        </InboxReferencesList>
        <InboxNotesList>
            <InboxNotes AuditTransactionId=""
                AuditTransactionIdQryType="" ContactReference=""
                ContactReferenceQryType="" ContactType=""
                ContactTypeQryType="" ContactUser=""
                ContactUserQryType="" CustomerSatIndicator=""
                CustomerSatIndicatorQryType="" FromAuditTransactionId=""
                FromContactReference="" FromContactType=""
                FromContactUser="" FromCustomerSatIndicator=""
                FromInboxKey="" FromInboxNotesKey="" FromNoteText=""
                FromReasonCode="" FromSequenceNo="" FromTranid=""
                InboxKey="" InboxKeyQryType="" InboxNotesKey=""
                InboxNotesKeyQryType="" NoteText="" NoteTextQryType=""
                ReasonCode="" ReasonCodeQryType="" SequenceNo=""
                SequenceNoQryType="" ToAuditTransactionId=""
                ToContactReference="" ToContactType="" ToContactUser=""
                ToCustomerSatIndicator="" ToInboxKey=""
                ToInboxNotesKey="" ToNoteText="" ToReasonCode=""
                ToSequenceNo="" ToTranid="" Tranid="" TranidQryType="" yfc:DTDOccurrence="ZeroOrMany"/>
        </InboxNotesList>
    </Inbox>
</ResolutionDetails>
