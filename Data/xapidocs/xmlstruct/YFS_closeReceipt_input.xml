<!--closeReceipt Input XML-->
<Receipt DocumentType="Required" ReceiptHeaderKey="Conditional"
    ReceiptNo="Conditional" ReceivingNode="Conditional"
    ShipmentKey="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YFS/closeReceipt/input">
    <yfc:doc>
        <Element EntityName="Receipt_Header"/>
        <Attributes>
            <Attribute EntityName="Document_Params" Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>
						This field indicates the type of document for which parameters are configured.
							<Validation>
                            <Condition>Error if document type not passed.
								</Condition>
                            <Error Description="YCP:Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Receipt_Header"
                Name="ReceiptHeaderKey" NameInEntity="Receipt_Header_Key">
                <Description>
                    <div>
						Primary key attribute of <code>YFS_RECEIPT_HEADER</code> table.
							<Validation>
                            <Condition>Error if failed to find a receipt.
								</Condition>
                            <Error Description="Receipt not found." ErrorCode="YFS10466"/>
                        </Validation>
                        <Validation>
                            <Condition>Error if the receipt status is not eligible for this transaction.
								</Condition>
                            <Error
                                Description="Receipt Status is not in the pickup status for this transaction." ErrorCode="YFS10534"/>
                        </Validation>
                        <Validation>
                            <Condition>Error if multiple receipts are found for the given input criterion.
								</Condition>
                            <Error
                                Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Shipment DocumentType="Conditional" EnterpriseCode="Conditional"
        OrderHeaderKey="Conditional" OrderNo="Conditional"
        OrderReleaseKey="Conditional" ReleaseNo="Conditional"
        SellerOrganizationCode="Conditional" ShipNode="Conditional" ShipmentNo="Conditional"/>
</Receipt>
