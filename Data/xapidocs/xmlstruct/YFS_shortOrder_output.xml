<!-- ShortOrder is not a template-driven element.  It is returned same as what was passed to the api.  However,  OrderAudit element is a template-driven element.   -->
<ShortOrder AuditTransactionId="" DocumentType="" EnterpriseCode=""
    ModificationReasonCode="" ModificationReasonText=""
    ModificationReference1="" ModificationReference2=""
    ModificationReference3="" ModificationReference4=""
    OrderHeaderKey="" OrderNo="" OrderReleaseKey="" ReleaseNo=""
    SelectMethod="" ShortForAllAvailableQty="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/shortOrder/output">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
		The order entity which contains the description of the order.
		<Validation>
                    <Condition>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed and must be valid.
			</Condition>
                    <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                </Validation>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Order_Audit"
                Name="AuditTransactionId" NameInEntity="Audit_Transaction_Id"/>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>The document type used to describe the order. Part of the unique key for the order. Either <code>OrderHeaderKey</code>
                        <i>or</i>
                        <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> of the order must be passed.
					<Validation>
                            <Condition>Either <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code>
                                <i>or</i>
                                <code>OrderHeaderKey</code> of the order is not passed.
					</Condition>
                            <Error Description="Invalid Order" ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="EnterpriseCode" NameInEntity="Order_Header_Key">
                <Description>
                    <div>
					Value that identifies an Enterprise.
					<br/>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed to identify the <code>Order</code>.
					<Validation>
                            <Condition>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed and must be valid.
						</Condition>
                            <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonCode" NameInEntity="Reason_Code"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonText" NameInEntity="Reason_Text"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference1" NameInEntity="Reference_1"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference2" NameInEntity="Reference_2"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference3" NameInEntity="Reference_3"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference4" NameInEntity="Reference_4"/>
            <Attribute EntityName="Order_Header" Name="OrderHeaderKey" NameInEntity="Order_Header_Key">
                <Description>
                    <div>
					Unique key that identifies the order. This value is system-generated.
					<br/>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed to identify the <code>Order</code>.
					<Validation>
                            <Condition>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed and must be valid.
						</Condition>
                            <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No">
                <Description>
                    <div>
                        <br/>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed to identify the <code>Order</code>.
					<Validation>
                            <Condition>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed and must be valid.
						</Condition>
                            <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Release" Name="OrderReleaseKey" NameInEntity="Order_Release_Key"/>
            <Attribute EntityName="Order_Release" Name="ReleaseNo" NameInEntity="Release_No"/>
            <Attribute DataType="SelectMethod" Name="SelectMethod">
                <Description>
                    <div>
				This attribute determines whether database select operations should be carried out in the <b>
                            <code>WAIT</code>
                        </b> or <b>
                            <code>NO_WAIT mode</code>
                        </b>.
				<br/>Default: <b>
                            <code>WAIT</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>WAIT</code>
                        </b> or  <b>
                            <code>NO_WAIT mode</code>
                        </b>.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ShortForAllAvailableQty">
                <Description>
                    <div>
					This attribute determines whether to short all of the available quantities of specified Order's Order Lines.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderLines>
        <OrderLine OrderLineKey="" OrderReleaseKey="" PrimeLineNo=""
            ReleaseNo="" SubLineNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="OrderReleaseKey"/>
                    <Attribute DataType="String" Name="ReleaseNo"/>
                </Attributes>
            </yfc:doc>
        </OrderLine>
    </OrderLines>
    <!-- OrderAudit is a template-driven element.  -->
    <OrderAudit AuditTransactionId="" CreateUserName="" Createts=""
        Createuserid="" ModifyUserName="" Modifyts="" Modifyuserid=""
        NewBuffer="" OldBuffer="" OrderAuditKey="" OrderHeaderKey=""
        OrderLineKey="" OrderReleaseKey="" ReasonCode="" ReasonText=""
        Reference1="" Reference2="" Reference3="" Reference4="" XMLFlag="">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YFS_getOrderAuditDetails_output.xml" IncludeXMLPath="/OrderAudit"/>
        <yfc:doc>
            <Element EntityName="Order_Audit"/>
            <Attributes>
                <Attribute EntityName="User" Name="CreateUserName" NameInEntity="Username">
                    <Description>Name of the user who created the audit.</Description>
                </Attribute>
                <Attribute EntityName="User" Name="ModifyUserName" NameInEntity="Username">
                    <Description>Name of the user who modified the audit.</Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="ReasonCode">
                    <Description>
		Reason Code
		</Description>
                </Attribute>
                <Attribute DataType="Text-100" Name="ReasonText">
                    <Description>
		Reason Text
		</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Order DocumentType="" EnterpriseCode="" OrderNo=""/>
        <OrderAuditLevels>
            <OrderAuditLevel ModificationLevel=""
                ModificationLevelScreenName="" OrderLineKey=""
                OrderReleaseKey="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Text-40" Name="ModificationLevel">
                            <Description>
            Level at which modification is done. Order, Order_Line, Order_Release or Order_Release_Line.
            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ModificationLevelScreenName">
                            <Description>
            Screen Name for the Modification level.
            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <OrderLine OrderLineKey="" OrderedQty="" PrimeLineNo="" SubLineNo="">
                    <Item AlternateItemID=""
                        BundleFulfillmentMode="|00|01|02"
                        CostCurrency="" CountryOfOrigin=""
                        CustomerItem="" CustomerItemDesc="" ECCNNo=""
                        HarmonizedCode="" ISBN="" ItemDesc="" ItemID=""
                        ItemShortDesc="" ItemWeight="" ItemWeightUOM=""
                        ManufacturerItem="" ManufacturerItemDesc=""
                        ManufacturerName="" NMFCClass="" NMFCCode=""
                        NMFCDescription="" ProductClass=""
                        ProductLine="" ScheduleBCode=""
                        SubstituteItemID="" SubstituteItemUOM=""
                        SupplierItem="" SupplierItemDesc=""
                        TaxProductCode="" UPCCode="" UnitCost="" UnitOfMeasure="">
                        <yfc:doc>
                            <Element EntityName="Order_Line"/>
                            <Attributes>
                                <Attribute DataType="Text-40" Name="SubstituteItemID">
                                    <Description>Substituted ItemID. This field is only populated if the item on the Order line is substituted. 
					</Description>
                                </Attribute>
                                <Attribute DataType="Text-40" Name="SubstituteItemUOM">
                                    <Description>Substituted Item UOM. This field is only populated if the item on the Order line is substituted.
					</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <LanguageDescriptionList>
                            <LanguageDescription Description="" LocaleCode=""/>
                        </LanguageDescriptionList>
                    </Item>
                </OrderLine>
                <OrderRelease ReleaseNo=""/>
                <OrderAuditDetails>
                    <OrderAuditDetail Action="" AuditType="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Text-10" Name="Action">
                                    <Description>
						Action 
						</Description>
                                </Attribute>
                                <Attribute DataType="Text-30" Name="AuditType">
                                    <Description>
						Audit Type
						</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <IDs>
                            <!-- ID is not a template-driven element. If the template contains OrderAuditDetail element, this element will come based on AuditType. For AuditTypes like Order, OrderLine this element does not come.-->
                            <ID Name="Required" Value="Required" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="Text-30" Name="Name">
                                    <Description>
											Name of the ID
											</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-30" Name="Value">
                                    <Description>
											Value of the ID
											</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                            </ID>
                        </IDs>
                        <Attributes>
                            <!-- Attribute is not a template-driven element. If the template contains OrderAuditDetail element, this element will appear in the output xml alongwith all its attributes.-->
                            <Attribute Extended="N|Y"
                                ModificationType="" Name="Required"
                                NewValue="Required" OldValue="Required" yfc:DTDOccurrence="OneOrMany">
                                <yfc:doc>
                                    <Element EntityName="[NONE]"/>
                                    <Attributes>
                                    <Attribute DataType="Flag" Name="Extended">
                                    <Description>Determines whether the attribute is an extended field. If this attribute
								is not available, then it means the attribute is not extended.</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-40" Name="ModificationType">
                                    <Description>
							Modification Type used for changing the attribute
							</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-30" Name="Name">
                                    <Description>
							Name of the Attribute Changed
							</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-100" Name="NewValue">
                                    <Description>
							New Value of the attribute modified
							</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-100" Name="OldValue">
                                    <Description>
							Old Value of the attribute modified
							</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                            </Attribute>
                        </Attributes>
                        <!-- CustomAuditDetails is not a template-driven element. If custom entity is modified, this element will appear in the output xml . -->
                        <!-- Nested CustomAuditDetails element will be returned if custom table has a hangoff table. -->
                        <CustomAuditDetails>
                            <OrderAuditDetail Action="" AuditType="" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:includeInParent
                                    IncludeXMLChildNodes="N"
                                    IncludeXMLFileName="YFS_getOrderAuditDetails_output.xml" IncludeXMLPath="/OrderAudit/OrderAuditLevels/OrderAuditLevel/OrderAuditDetails/OrderAuditDetail"/>
                                <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="Text-10" Name="Action">
                                    <Description>
						Action 
						</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-30" Name="AuditType">
                                    <Description>
						Audit Type
						</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                                <IDs>
                                    <yfc:includeInParent
                                    IncludeXMLChildNodes="Y"
                                    IncludeXMLFileName="YFS_getOrderAuditDetails_output.xml" IncludeXMLPath="/OrderAudit/OrderAuditLevels/OrderAuditLevel/OrderAuditDetails/OrderAuditDetail/IDs"/>
                                    <!-- ID is not a template-driven element. If the template contains OrderAuditDetail element, this element will come based on AuditType. For AuditTypes like Order, OrderLine this element does not come.-->
                                    <ID Name="Required" Value="Required" yfc:DTDOccurrence="ZeroOrMany">
                                    <yfc:doc>
                                    <Attributes>
                                    <Attribute
                                    DataType="Text-30" Name="Name">
                                    <Description>
											Name of the ID
											</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-30" Name="Value">
                                    <Description>
											Value of the ID
											</Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                    </ID>
                                </IDs>
                                <Attributes>
                                    <yfc:includeInParent
                                    IncludeXMLChildNodes="Y"
                                    IncludeXMLFileName="YFS_getOrderAuditDetails_output.xml" IncludeXMLPath="/OrderAudit/OrderAuditLevels/OrderAuditLevel/OrderAuditDetails/OrderAuditDetail/Attributes"/>
                                    <!-- Attribute is not a template-driven element. If the template contains OrderAuditDetail element, this element will appear in the output xml alongwith all its attributes.-->
                                    <Attribute Extended="N|Y"
                                    ModificationType=""
                                    Name="Required"
                                    NewValue="Required"
                                    OldValue="Required" yfc:DTDOccurrence="OneOrMany">
                                    <yfc:doc>
                                    <Element EntityName="[NONE]"/>
                                    <Attributes>
                                    <Attribute
                                    DataType="Flag" Name="Extended">
                                    <Description>Determines whether the attribute is an extended field. If this attribute
								is not available, then it means the attribute is not extended.</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-40" Name="ModificationType">
                                    <Description>
							Modification Type used for changing the attribute
							</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-30" Name="Name">
                                    <Description>
							Name of the Attribute Changed
							</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-100" Name="NewValue">
                                    <Description>
							New Value of the attribute modified
							</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-100" Name="OldValue">
                                    <Description>
							Old Value of the attribute modified
							</Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                    </Attribute>
                                </Attributes>
                            </OrderAuditDetail>
                        </CustomAuditDetails>
                    </OrderAuditDetail>
                </OrderAuditDetails>
                <ModificationTypes>
                    <ModificationType Name="" ScreenName="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element EntityName="Order_Audit"/>
                            <Attributes>
                                <Attribute EntityName="Order_Audit"
                                    Name="Name" NameInEntity="Modification_Type"/>
                                <Attribute
                                    EntityName="Status_Modification_Type"
                                    Name="ScreenName" NameInEntity="Modification_Type_Screen_Name"/>
                            </Attributes>
                        </yfc:doc>
                    </ModificationType>
                </ModificationTypes>
            </OrderAuditLevel>
        </OrderAuditLevels>
    </OrderAudit>
</ShortOrder>
