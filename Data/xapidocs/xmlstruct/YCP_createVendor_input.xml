<!-- createVendor Input XML -->
<Vendor OrganizationCode="Required" SellerOrganizationCode=""
    VendorClassificationCode="" VendorID="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/createVendor/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Vendor" Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
				Organization code of organization that defines this vendor.
				<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not provided.
					</Condition>
                            <Error Description="Missing attribute." ErrorCode="YFS10460"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Vendor" Name="SellerOrganizationCode" NameInEntity="Seller_Organization_Code">
                <Description>
                    <div>
                        <code>SellerOrganizationCode</code> attribute must be passed when creating a vendor representing an existing organization. If <code>SellerOrganizationCode</code> is passed, the API will update the organization with information provided in <code>SellerOrganization</code> element.   If <code>SellerOrganizationCode</code> is not passed, the API will create a new organization with information provided in <code>SellerOrganization</code> element.
				<Validation>
                            <Condition>Organization does not exist.
					</Condition>
                            <Error Description="Invalid Organization." ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>SellerOrganizationCode</code> attribute is not passed when creating a vendor representing an existing organization.
					</Condition>
                            <Error
                                Description="Record already exists in the database." ErrorCode="YFC0001"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Vendor" Name="VendorID" NameInEntity="Vendor_ID">
                <Description>
                    <div>
				Identifier for a vendor.  If <code>VendorID</code> is not provided , it will be defaulted as <code>SellerOrganizationCode</code>.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SellerOrganization AccountWithHub="" AuthorityType=""
        BillingAddressKey="" CatalogOrganizationCode=""
        ContactAddressKey="" CorporateAddressKey=""
        CreatorOrganizationKey="" DunsNumber=""
        InventoryOrganizationCode="" InventoryPublished=""
        IsHubOrganization="" IsLegalEntity="" IsSourcingKept=""
        IssuingAuthority="" ItemXrefRule="" LocaleCode=""
        OrganizationCode="Required" OrganizationKey=""
        OrganizationName="" ParentOrganizationCode=""
        PaymentProcessingReqd="" PrimaryEnterpriseKey="Required"
        PrimarySicCode="" PrimaryUrl="" TaxExemptFlag=""
        TaxExemptionCertificate="" TaxJurisdiction="" TaxpayerId=""
        XrefAliasType="" XrefOrganizationCode="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Organization">
                <Description>
                    <div>
				Identify the organization or node which vendor represents.  <code>SellerOrganization</code> is required if <code>SellerOrganizationCode</code> attribute of an existing organization is not provided.
				<Validation>
                            <Condition>If <code>SellerOrganization</code> is not provided.
					</Condition>
                            <Error Description="Missing element in XML." ErrorCode="YFS10507"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Organization"
                    Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
					OrganizationCode which identifies an organization.
					<Validation>
                                <Condition>If <code>OrganizationCode</code> is not passed.
						</Condition>
                                <Error
                                    Description="Missing attribute in input." ErrorCode="YFS10460"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Organization"
                    Name="PrimaryEnterpriseKey" NameInEntity="Primary_Enterprise_Key">
                    <Description>
                        <div>
					Organizations may participate with multiple enterprises one of which is primary. This indicates the primary enterprise.
					<Validation>
                                <Condition>If <code>PrimaryEnterpriseKey</code> is not passed.
						</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Node AcceptanceRequired="" ActivateFlag=""
            AdvanceNotificationTime="" AgentCriteriaGroup=""
            CurrentBolNumber="" DcmIntegrationRealTime=""
            DefaultDeclaredValue="" ExportLicenseExpDate=""
            ExportLicenseNo="" GLN="" GenerateBolNumber=""
            IdentifiedByParentAs="" InheritConfigFromShipNode=""
            InterfaceSubType="" InterfaceSubTypeCode="" InterfaceType=""
            InventoryTracked="" Inventorytype=""
            IsItemBasedAllocationAllowed="" Latitude="" Longitude=""
            MaintainInventoryCost="" MaxDaysToScheduleBefore=""
            MinNotificationTime="" OwnerType="" PicklistType=""
            ProcureToShipAllowed="" ReceiptProcessingTime=""
            ReceiptProcessingTimeForForwarding=""
            ReceivingCalendarKey="" ReceivingNode=""
            RequiresChangeRequest="" ReturnCenterFlag="" ReturnsNode=""
            SerialTracking="" ShipNode="" ShipNodeClass=""
            ShipnodeType="" ShippingCalendarKey="" ShippingNode=""
            StoreInvConfig="" SupplierKey="" ThreePlNode="" TimeDiff=""
            UseEndOfShiftTime="" XDocNode="">
            <yfc:doc>
                <Element EntityName="Ship_Node"/>
                <Attributes>
                    <Attribute DataType="NumberOfDays" Name="AdvanceNotificationTime">
                        <Description>
                            <div>
                                <b>Deprecated.</b> See <code>manageNodeNotification</code> API. The number of 
					days before which the ship node needs notification of an impending receipt or shipment.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="InterfaceSubTypeCode"/>
                    <Attribute DataType="NumberOfDays" Name="MinNotificationTime">
                        <Description>
                            <div>
                                <b>Deprecated.</b> See <code>manageNodeNotification</code> API. The number of 
					days before which the communication to ship the order to the shipping node or supplier should be 
					made. This field is used to determine the earliest ship date on the line.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Node>
