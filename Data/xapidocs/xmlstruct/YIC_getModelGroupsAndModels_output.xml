<Models Country="" Depth="" Group="" Language="" xmlns="http://www.sterlingcommerce.com/documentation/YIC/getModelGroupsAndModels/output">
    <yfc:doc>
        <Element>
            <Description>
                <div>
						The root XML header with criteria for fetching models.  
					</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Text-120" Name="Country">
                <Description>
                    <div>
						The country identifier for the models.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="Depth">
                <Description>
                    <div>
						Indicated the depth of traversal.						
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Group">
                <Description>
                    <div>
						The model group to start looking in. 
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Language">
                <Description>
                    <div>
						The language identifier for the models to search for.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Model Country="" Language="" Name="" Path="" Type="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="masterModel.xml" IncludeXMLPath="/"/>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					A model is a terminal node and represents a configurable product or a reusable building block.  
				</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-120" Name="Country">
                    <Description>
                        <div>
						The country identifier for the model.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" Name="Language">
                    <Description>
                        <div>
						The language identifier for the model.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" Name="Name">
                    <Description>
                        <div>
						The human-readable model name. 
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-480" Name="Path">
                    <Description>
                        <div>
						The configurator path to a model. This path is 'mangled' such that special characters are replaced by their Unicode code preceded by an underscore char.						
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" Name="Type">
                    <Description>
                        <div>
						The type of model.
						<br/>Valid Values: <b>
                                <code>model</code>
                            </b> or <b>
                                <code>ocg</code>
                            </b> or <b>
                                <code>oig</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Model>
    <Group Country="" Language="" Name="" Path="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
						A group is a container of other groups and models.  
					</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-120" Name="Country">
                    <Description>
                        <div>
							The country identifier for the model group.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" Name="Language">
                    <Description>
                        <div>
							The language identifier for the model group.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" Name="Name">
                    <Description>
                        <div>
							The human-readable name of a model group. 
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-480" Name="Path">
                    <Description>
                        <div>
							The configurator path to a model group.						
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Model Country="" Language="" Name="" Path="" Type="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="masterModel.xml" IncludeXMLPath="/"/>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
					A model is a terminal node and represents a configurable product or a reusable building block.  
				</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Text-120" Name="Country">
                        <Description>
                            <div>
						The country identifier for the model.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-120" Name="Language">
                        <Description>
                            <div>
						The language identifier for the model.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-120" Name="Name">
                        <Description>
                            <div>
						The human-readable model name. 
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-480" Name="Path">
                        <Description>
                            <div>
						The configurator path to a model. This path is 'mangled' such that special characters are replaced by their Unicode code preceded by an underscore char.						
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-120" Name="Type">
                        <Description>
                            <div>
						The type of model.
						<br/>Valid Values: <b>
                                    <code>model</code>
                                </b> or <b>
                                    <code>ocg</code>
                                </b> or <b>
                                    <code>oig</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Model>
    </Group>
</Models>
