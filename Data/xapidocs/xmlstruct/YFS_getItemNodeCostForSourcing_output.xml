<!--getItemNodeCostForSourcing Output XML-->
<ItemCostList xmlns="http://www.sterlingcommerce.com/documentation/YFS/getItemNodeCostForSourcing/output">
    <ItemCost ItemID="Required" ProductClass="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Inventory_Item" Name="ItemID" NameInEntity="Item_Id"/>
                <Attribute EntityName="Inventory_Item"
                    Name="ProductClass" NameInEntity="Product_Class"/>
                <Attribute EntityName="Inventory_Item"
                    Name="UnitOfMeasure" NameInEntity="Uom"/>
            </Attributes>
        </yfc:doc>
        <Nodes yfc:DTDOccurrence="REQUIRED">
            <Node Currency="" MaxQty="" MinQty="" ShipNode="Required"
                UnitCost="Required" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Currency" Name="Currency">
                            <Description>
                                <div>
                    Currency in which Unit Cost is returned.  If attribute is not returned, Enterprise's local currency will be asssumed.
                  </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="MaxQty">
                            <Description>
                                <div>
                    Indicates maximum quantity for which Unit Cost is valid.  If not returned, Unit Cost will be valid for any quantity greater than MinQty.
                  </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="MinQty">
                            <Description>
                                <div>
                    Indicates minimum quantity for which Unit Cost is valid.  If not returned, 0 will be assumed.
                  </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Order_Line_Schedule"
                            Name="ShipNode" NameInEntity="Ship_Node"/>
                        <Attribute DataType="Money" Name="UnitCost">
                            <Description>
                                <div>
                    Unit cost for the item at the node.
                  </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Node>
        </Nodes>
    </ItemCost>
</ItemCostList>
