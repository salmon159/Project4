<MasterCatalogList xmlns="http://www.sterlingcommerce.com/documentation/YCM/createMasterCatalog/input">
    <MasterCatalog Description="" MasterCatalogID="Required"
        OrganizationCode="Required" ShortDescription="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Master_Catalog"
                    Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
				The organization for which the master catalog is created.
				<Validation>
                                <Condition>
                                    <code>OrganizationCode</code> is not passed.
					</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YCM0002"/>
                            </Validation>
                            <Validation>
                                <Condition>Not a valid organization code.
					</Condition>
                                <Error
                                    Description="Invalid Organization Code" ErrorCode="YCM0019"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </MasterCatalog>
</MasterCatalogList>
