<!--checkHoldTypesForShipment Input XML-->
<!-- Either ShipmentKey OR (ShipmentNo, ShipNode and SellerOrganizationCode) must be provided -->
<Shipment SellerOrganizationCode="Conditional" ShipmentKey="Conditional"
    ShipmentNo="Conditional" ShipnodeKey="Conditional"
    TransactionId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YDM/checkHoldTypesForShipment/input">
    <yfc:doc>
        <Element EntityName="Shipment">
            <Description>
                <div>
					The <code>ShipmentKey</code>, or  the <code>SellerOrganizationCode</code>, <code>ShipnodeKey</code> and the <code>ShipmentNo</code> must be passed to identify the <code>Order</code>.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="String" Name="ShipnodeKey"/>
            <Attribute DataType="TransactionID" Name="TransactionId">
                <Description>
                    <div>
                        <Validation>
                            <Condition>If not passed.</Condition>
                            <Error Description="Invalid Transaction ID." ErrorCode="YCP0068"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Shipment>
