<OrderRelease BuyerMarkForNodeId="" BuyerOrganizationCode=""
    BuyerReceivingNodeId="" CarrierServiceCode="" Currency=""
    CustReqShipDate="" CustomerPoNo="" DeliveryCode="" DepartmentCode=""
    DoNotConsolidate="" DocumentType="" EnterpriseCode="Required"
    ExternalReleaseIdentifier="" FOB="" GiftFlag=""
    ItemClassification="" LevelOfService="" OrderAvailableOnSystem=""
    OrderHeaderKey="" OrderNo="" OrderReleaseKey="" OrderType=""
    PackAndHold="" PacklistType="" PriorityCode="" ReceivingNode=""
    ReleaseNo="" ReqCancelDate="" ReqDeliveryDate="" ReqShipDate=""
    SCAC="" SellerOrganizationCode="" ShipNode=""
    ShipmentConsolidationGroupId="" ShipmentKey="" ShipmentNo="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/consolidateToShipment/input">
    <yfc:doc>
        <Element EntityName="Order_Release">
            <Description>
                <div>
	   The order release to be consolidated. If <code>Order</code> is available on system, the order release must be identified. Pass  <code>OrderReleaseKey</code>
                    <i>or</i> pass <code>ReleaseNo</code>and <code>OrderHeaderKey</code>  to identify the order release.
	   </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Order_Header" Name="DoNotConsolidate" NameInEntity="Do_Not_Consolidate">
                <Description>
                    <div>Indicates whether consolidation is allowed for this order release based on shipment constraints and routing constraints.  
					 <br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                        <br/>If passed as <b>
                            <code>Y</code>
                        </b> finds shipment to which the order release can be consolidated. If not found a new shipment will be created for the release.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" EntityName="Order_Header"
                Name="EnterpriseCode" NameInEntity="Enterprise_Code">
                <Description>
					Enterprise associated with the order. 
				</Description>
            </Attribute>
            <Attribute EntityName="Shipment_Line"
                Name="ExternalReleaseIdentifier" NameInEntity="External_Release_Identifier"/>
            <Attribute EntityName="Shipment"
                Name="OrderAvailableOnSystem" NameInEntity="Order_Available_On_System">
                <Description>
                    <div>
                 Indicates if order is available on the system.  
				 <br/>Default: <b>
                            <code>Y</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                        <br/>If passed as <b>
                            <code>N</code>
                        </b> Order, OrderLine and OrderRelease validations are not done. Further defaulitng of shipment attributes from Order, OrderLine and OrderRelease are not done.
                </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No">
                <Description>
					Part of the unique key for the order. 
				</Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="ShipNode" NameInEntity="Shipnode_Key">
                <Description>
		    Ship node from where shipment has to be made.
	       </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Shipment"
                Name="ShipmentKey" NameInEntity="Shipment_Key">
                <Description>
		    Primary key attribute of Shipment to which consolidation is requested.
	       </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" EntityName="Shipment"
                Name="ShipmentNo" NameInEntity="Shipment_No">
                <Description>
		    Part of the information that identifies a Shipment.
	       </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderLines>
        <OrderLine CustomerPONo="" DepartmentCode="" KitCode=""
            OrderedQty="Required" PrimeLineNo="" Segment=""
            SegmentType="" SerialNo="" SubLineNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Order_Line">
                    <Description>
                        <div>Each order line will be converted to shipment line in the process of consolidation.</div>
                    </Description>
                </Element>
            </yfc:doc>
            <Item CountryOfOrigin="" ItemDesc="" ItemID="Required"
                ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Item"/>
                    <Attributes>
                        <Attribute DataType="String" Name="ItemDesc"/>
                        <Attribute DataType="String" Name="ProductClass"/>
                    </Attributes>
                </yfc:doc>
            </Item>
            <OrderLineInvAttRequest BatchNo="" LotAttribute1=""
                LotAttribute2="" LotAttribute3="" LotKeyReference=""
                LotNumber="" ManufacturingDate="" RevisionNo="">
                <yfc:doc>
                    <Element EntityName="Order_Line_Req_Tag"/>
                </yfc:doc>
            </OrderLineInvAttRequest>
            <PersonInfoMarkFor AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" ErrorTxt="" EveningFaxNo=""
                EveningPhone="" FirstName="" HttpUrl=""
                IsCommercialAddress="" JobTitle="" LastName=""
                MiddleName="" MobilePhone="" OtherPhone="" PersonID=""
                PersonInfoKey="" PreferredShipAddress="" State=""
                Suffix="" TaxGeoCode="" Title="" UseCount=""
                VerificationStatus="" ZipCode="">
                <yfc:includeInParent
                    IncludeXMLFileName="YFS_Person_Info_tran.xml" IncludeXMLPath="/Address"/>
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                </yfc:doc>
            </PersonInfoMarkFor>
            <Instructions>
                <Instruction InstructionDetailKey="" InstructionText=""
                    InstructionType="" InstructionURL=""
                    InstructionUsage="" ReferenceKey="" SequenceNo=""
                    TableName="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent
                        IncludeXMLFileName="YFS_Instruction_Detail_tran.xml" IncludeXMLPath="/Instruction"/>
                    <yfc:doc>
                        <Element EntityName="Instruction_Detail"/>
                    </yfc:doc>
                </Instruction>
            </Instructions>
        </OrderLine>
    </OrderLines>
    <ToAddress AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" ErrorTxt=""
        EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
        IsCommercialAddress="" JobTitle="" LastName="" MiddleName=""
        MobilePhone="" OtherPhone="" PersonID="" PersonInfoKey=""
        PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
        Title="" UseCount="" VerificationStatus="" ZipCode="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Person_Info_tran.xml" IncludeXMLPath="/Address"/>
        <yfc:doc>
            <Element EntityName="Person_Info"/>
        </yfc:doc>
    </ToAddress>
    <FromAddress AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" ErrorTxt=""
        EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
        IsCommercialAddress="" JobTitle="" LastName="" MiddleName=""
        MobilePhone="" OtherPhone="" PersonID="" PersonInfoKey=""
        PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
        Title="" UseCount="" VerificationStatus="" ZipCode="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Person_Info_tran.xml" IncludeXMLPath="/Address"/>
        <yfc:doc>
            <Element EntityName="Person_Info"/>
        </yfc:doc>
    </FromAddress>
    <MarkForAddress AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" ErrorTxt=""
        EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
        IsCommercialAddress="" JobTitle="" LastName="" MiddleName=""
        MobilePhone="" OtherPhone="" PersonID="" PersonInfoKey=""
        PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
        Title="" UseCount="" VerificationStatus="" ZipCode="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Person_Info_tran.xml" IncludeXMLPath="/Address"/>
        <yfc:doc>
            <Element EntityName="Person_Info"/>
        </yfc:doc>
    </MarkForAddress>
    <Instructions>
        <!-- There are two instances of Instructions in this XML document. Enter element and attribute description in only one, or the information will be generated for each instance in the html document. -->
        <Instruction InstructionDetailKey="" InstructionText=""
            InstructionType="" InstructionURL="" InstructionUsage=""
            ReferenceKey="" SequenceNo="" TableName="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Instruction_Detail_tran.xml" IncludeXMLPath="/Instruction"/>
            <yfc:doc>
                <Element EntityName="Instruction_Detail"/>
            </yfc:doc>
        </Instruction>
    </Instructions>
    <SpecialServices>
        <SpecialService SpecialServicesCode="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Special_Service_Ref">
                    <Description> Should be specified only when Order does not exist on system.</Description>
                </Element>
                <Attributes>
                    <Attribute Name="SpecialServicesCode">
                        <Description>
                            <div>
                                <Validation>
                                    <Condition>
                                    <code>SpecialServicesCode</code> is invalid.
				</Condition>
                                    <Error
                                    Description="Special service code does not exist." ErrorCode="OMP11027"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </SpecialService>
    </SpecialServices>
</OrderRelease>
