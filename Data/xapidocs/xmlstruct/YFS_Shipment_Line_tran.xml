<ShipmentLine BackroomPickComplete="" BackroomPickedQuantity=""
    BatchPickPriority="" BuyerMarkForNodeId="" CountryOfOrigin=""
    CustomerPoLineNo="" CustomerPoNo="" DepartmentCode=""
    DocumentType="Conditional" ExternalReleaseIdentifier="" FifoNo=""
    IsPackComplete="" ItemDesc="" ItemID="Conditional"
    KitCode="LK|| || |Blanks" KitQty="" LevelOfService="" NetWeight=""
    NetWeightUom="" OrderHeaderKey="" OrderLineKey=""
    OrderNo="Conditional" OrderReleaseKey="" PrimeLineNo="Conditional"
    ProductClass="" Quantity="" ReleaseNo="Conditional"
    RequestedSerialNo="" ReturnShippingLabelLevel="" Segment=""
    SegmentType="" ShipAdviceNo="" ShipToCustomerId=""
    ShipmentConsolidationGroupId="" ShipmentLineKey="Conditional"
    ShipmentLineNo="Conditional" ShipmentSubLineNo="Conditional"
    ShortageQty="" StoreBatchKey="" SubLineNo="Conditional"
    UnitOfMeasure="Conditional"
    xmlns="http://www.sterlingcommerce.com/documentation/YFS/Shipment_Line_tran" xmlns:yfc="http://www.yantra.com/documentation">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="DocumentType"/>
            <Attribute Name="KitQty">
                <Description>
                    <div>The number of components required for one kit.  The quantity is used only for bundle components only when the order is not available for the shipment line. When order is available for the shipment line, <code>KitQty</code> is defaulted from order information.
					  </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Release" Name="ShipAdviceNo" NameInEntity="Ship_Advice_No">
                <Description>
                    <div>Ship advice number of the order release.  <code>ShipAdviceNo</code> will only be used when order release identifiers such as <code>OrderReleaseKey</code>
                        <i>or</i>
                        <code>ReleaseNo</code> and <code>OrderHeaderKey</code> are not provided.  <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided) will be used to determine the order release in the system.  If more than one order releases are found in the system with the same <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided), the order release with a pickable status will be used. 
				  <Validation>
                            <Condition>More than one order releases in a pickable status are found with the same  <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided).
					</Condition>
                            <Error
                                Description="Multiple order releases found with ShipAdviceNo." ErrorCode="OMP75_10000"/>
                        </Validation>
                        <Validation>
                            <Condition>System cannot find an order release with the provided <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided).
					</Condition>
                            <Error Description="Invalid ShipAdviceNo." ErrorCode="YDM75_00001"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
        <Element EntityName="Shipment_Line"/>
    </yfc:doc>
    <ShipmentLineInvAttRequest BatchNo="Conditional" LotAttribute1=""
        LotAttribute2="" LotAttribute3="" LotKeyReference=""
        LotNumber="Conditional" ManufacturingDate="" RevisionNo="Conditional">
        <yfc:doc>
            <Element EntityName="Shipment_Line_Req_Tag"/>
        </yfc:doc>
    </ShipmentLineInvAttRequest>
</ShipmentLine>
