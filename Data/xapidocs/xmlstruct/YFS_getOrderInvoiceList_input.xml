        SearchCriteria2QryType="" ShipToID="" ShipToIDQryType=""
        SoldToKey="" SourceKey="" SourceType="" ToBillToID=""
        ToBuyerUserId="" ToCustomerEMailID="" ToHoldReasonCode=""
        ToOrderDate="" ToOrderHeaderKey="" ToOrderName="" ToOrderNo=""
        ToPaymentStatus="" ToReqShipDate="" ToSearchCriteria1=""
        ToSearchCriteria2="" ToShipToID="" yfc:UsedForIndexing="Order">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Key" Name="AuthenticationKey">
                    <Description>Not Used.</Description>
                </Attribute>
                <Attribute DataType="OrgCode" Name="OrganizationCode">
