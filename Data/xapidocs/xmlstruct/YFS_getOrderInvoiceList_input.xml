<!--getOrderInvoiceList Input XML-->
<OrderInvoice ChargeTransactionKey="" ChargeTransactionKeyQryType=""
    Currency="" DocumentType="" EnterpriseCode=""
    FromChargeTransactionKey="" FromInvoiceNo="" FromMasterInvoiceNo=""
    FromOrderHeaderKey="" FromOrderInvoiceKey="" FromReturnReleaseKey=""
    FromShipmentKey="" IgnoreOrdering="" InvoiceCreationReason=""
    InvoiceNo="" InvoiceNoQryType="" InvoiceType="" LatestFirst=""
    MasterInvoiceNo="" MasterInvoiceNoQryType="" MaximumRecords=""
    OrderHeaderKey="" OrderHeaderKeyQryType="" OrderInvoiceKey=""
    OrderInvoiceKeyQryType="" OrderNo="" ReadFromHistory=""
    ReturnReleaseKey="" ReturnReleaseKeyQryType=""
    SellerOrganizationCode="" ShipNode="" ShipmentKey=""
    ShipmentKeyQryType="" ShipmentNo="" ToChargeTransactionKey=""
    ToInvoiceNo="" ToMasterInvoiceNo="" ToOrderHeaderKey=""
    ToOrderInvoiceKey="" ToReturnReleaseKey="" ToShipmentKey="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderInvoiceList/input">
    <yfc:doc>
        <Element EntityName="Order_Invoice"/>
        <Attributes>
            <Attribute DataType="Flag" Name="IgnoreOrdering">
                <Description>
                    <div>
				By default, the output is ordered by <code>ORDER_HEADER_KEY</code> and <code>INVOICE_NO</code>. If <code>IgnoreOrdering</code> is passed as <b>
                            <code>Y</code>
                        </b>, the default ordering is omitted.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="LatestFirst">
                <Description>
                    <div>
				Governs the order in which <code>OrderInvoices</code> are returned.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: 
				<br/>
                        <b>
                            <code>Y</code>
                        </b> :the <code>OrderInvoices</code> are ordered in descending order of<code>ORDER_HEADER_KEY</code> and <code>INVOICE_NO</code>.
				<br/>
                        <b>
                            <code>N</code>
                        </b> :the <code>OrderInvoices</code> are ordered in ascending order of <code>ORDER_HEADER_KEY</code> and <code>INVOICE_NO</code>.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>Specifies the Maximum Number of records to be returned. Default is 500.</Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="ReadFromHistory">
                <Description>
                    <div>
                    Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
                    <br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_ORDER_INVOICE table.
                    <br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_ORDER_INVOICE_H table (the history table).
                    <br/>
                        <b>
                            <code>B</code>
                        </b> - Returns records that satisfy the input criteria from both YFS_ORDER_INVOICE and YFS_ORDER_INVOICE_H tables. Records returned from the current table are first in the output.
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Order AuthenticationKey="" BillToID="" BillToIDQryType=""
        BuyerUserId="" BuyerUserIdQryType="" CustomerContactID=""
