    DemandType="" DisplayLocalizedFieldInLocale="" DistributionRuleId=""
    DocumentType="" EntitlementDate="" ExcludeChildItemsOfModelItems=""
    ExcludeItemsInPriceList="" ExecuteQueries="" FromGlobalItemID=""
