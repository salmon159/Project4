    xmlns="http://www.sterlingcommerce.com/documentation/YFS/getItemListForOrdering/input" yfc:ComplexQuerySupported="Y">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="YFS_getItemListForOrdering_input.xml" IncludeXMLPath="/Item"/>
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="YCM_ItemList_input.xml" IncludeXMLPath="/Item"/>
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="ApplyDeliveryMethodOverrides">
                <Description>
                    <div>This attribute should be passed to the even if the <code>overrideDeliveryMethodForLines</code> user exit is implemented to apply delivery method overrides.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="AuthenticationKey">
                <Description>Not used.</Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="CallingOrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
					The organization code that the API is being invoked for.  This organization will be used to retrieve pricing data and availability.
					<Validation>
                            <Condition>
                                <code>CallingOrganizationCode</code> is not passed.
						</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>CallingOrganizationCode</code> does not identify a valid organization.</Condition>
                            <Error
                                Description="YFS:Invalid organization code" ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderUnassignedModelItemAttrs">
                <Description>
                    <div>If this attribute is <code>Y</code>, then an <code>AssignedValue</code> element with blank attribute values is added in the output at xpath ItemList/AttributeList/Attribute/AssignedValueList. This is done for Attributes of type DISTINCT_ATTRIBUTES if the output has any item(s) for which the value of this particular attribute has an unassigned value. This is intended to be used in CCC item variation selection screens. Default value of this flag is <code>N</code>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Price_Program_Defn" Name="Currency" NameInEntity="Currency">
                <Description>
					The currency to return prices in.  If not passed, this is defaulted to the currency of the <code>CallingOrganizationCode</code>'s primary enterprise's locale.
				</Description>
            </Attribute>
            <Attribute DataType="String" Name="DeliveryMethod"/>
            <Attribute EntityName="Inventory_Demand_Type"
                Name="DemandType" NameInEntity="Demand_Type">
                <Description>
					The demand type to be used when determining availability for items.  If not passed, <code>OPEN_ORDER</code> demand type is assumed.
				</Description>
            </Attribute>
            <Attribute DataType="Locale" Name="DisplayLocalizedFieldInLocale">
                <Description>
                    <div>
						The locale for which output should be returned in. Locale should be passed in the standard format of "language_country_variant" (e.g., "fr_FR", "en_US_CA", etc.).
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="DistributionRuleId" Name="DistributionRuleId">
                <Description>The distribution rule ID to be used to determine the node(s) that 
				will be used for availability checks. 
				</Description>
            </Attribute>
            <Attribute DataType="Text-40" EntityName="Item" Name="DocumentType">
                <Description>
                    <div>The document type used to determine if the item has visible associations.  This is only used when HasVisibleAssociations is in the output template, and if not provided the sales order document type defined will be utilized by default.</div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="EntitlementDate">
                <Description>
					The date to be used when reading entitlements.  If not passed, this date defaults to the current date.
				</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ExcludeChildItemsOfModelItems">
                <Description>
                    <div>
			This flag indicates whether child items of an item with variations should be returned in the search results.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ExcludeItemsInPriceList">
                <Description>
                    <div>Items within this Pricelist will be excluded from the search results.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ExecuteQueries">
                <Description>
					Indicates that query-based associations should be executed, and a list of matching items should be returned under the <code>Association</code> element.  If this flag is passed as <code>
                        <b>N</b>
                    </code>, query-based associations will not be returned.<br/>
					The default value is <code>
                        <b>N</b>
                    </code>.
				</Description>
            </Attribute>
            <Attribute EntityName="Sourcing_Rule_Header"
                Name="FulfillmentType" NameInEntity="Fulfillment_Type">
                <Description>
					The fulfillment type to be used when determining availability for items.  If not passed, the <code>CallingOrganizationCode</code>'s primary enterprise's default distribution rule will be used.
				</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="GetAvailabilityFromCache">
                <Description>
                    <div>
			This flag indicates whether the available quantity for the item must be computed or obtained from cache. When <code>GetAvailabilityFromCache</code> is not passed or is passed as <code>N</code>, the output for the <code>Availability</code> element will only contain the attribute <code>CurrentAvailableQty</code>, if passed in the template.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="GetUnpublishedItems">
                <Description>Indicates if unpublished items needs to be listed. The default value is <code>
                        <b>N</b>
                    </code>.</Description>
            </Attribute>
            <Attribute Name="GlobalItemID" NameInEntity="Global_Item_ID">
                <Description>Unique Global identifier used to cross reference an item with another catalog organization.</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="HideAssociationsWithoutItems">
                <Description>
                    <div>This attribute should be passed with a value <code>Y</code> to list only those associations which has at least one items associated. Default value of this flag is <code>N</code>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreInvalidItems">
                <Description>
                    <div>
						This flag indicates whether invalid items should be returned in the search results.
						Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
						Default value: <b>
                            <code>Y</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreIsSoldSeparately">
                <Description>
                    <div>
						Set this flag to consider all items regardlessly of the <code>IsSoldSeparately</code> flag.
						Valid values: <b>
                            <code>Y</code>
                        </b>, <b>
                            <code>NULL</code>
                        </b>, or <b>
                            <code>N</code>
                        </b>
						Default value: <b>
                            <code>NULL</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreOrdering">
                <Description>
                    <div>
				By default, the output is ordered by <code>ORGANIZATION_CODE</code>, <code>ITEM_ID</code>, and <code>UNIT_OF_MEASURE</code>. Pass <code>IgnoreOrdering</code> as <b>
                            <code>Y</code>
                        </b> to omit ordering.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Item"
                Name="InheritAttributesFromClassification" NameInEntity="Inherit_Attrs_From_Classification"/>
            <Attribute DataType="Flag" Name="IsForOrdering">
                <Description>
                    <div>
			This flag indicates whether the item search is for ordering purpose. It must be set to <code>Y</code> to apply customer entitlements.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>Y</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-20" Name="ItemGroupCode">
                <Description>
                    <div>
					Valid values: <b>
                            <code>PS</code>
                        </b> - Provided Service, <b>
                            <code>DS</code>
                        </b> - Delivery Service, <b>
                            <code>PROD</code>
                        </b> - Product Item
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>Maximum records requested for output. Defaults to 5000.</Description>
            </Attribute>
            <Attribute EntityName="Organization" Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
					The <code>OrganizationCode</code> attribute can be passed to override the catalog organization.  By default, the <code>CallingOrganizationCode</code>'s catalog organization code is used.  The catalog organization code corresponds to the <code>OrganizationCode</code> attribute of the items, and will be used to query the YFS_ITEM table.
				</Description>
            </Attribute>
            <Attribute EntityName="Price_Program" Name="PriceProgramKey" NameInEntity="Price_Program_Key">
                <Description>
					The key of the price program to use to compute prices.  If not passed, this will be defaulted to the <code>CallingOrganizationCode</code>'s default price program if specified.
					<br/>In order to compute prices, the <code>PriceProgramKey</code> or <code>PriceProgramName</code> and <code>CallingOrganizationCode</code> must be passed, if the <code>CallingOrganizationCode</code> does not define a default price program.
				</Description>
            </Attribute>
            <Attribute EntityName="Price_Program"
                Name="PriceProgramName" NameInEntity="Price_Program_Name">
                <Description>
					The name of the price program to use to compute prices.  This value is used along with <code>CallingOrganizationCode</code> to identify a price program.  If not passed, this will be defaulted to the <code>CallingOrganizationCode</code>'s default price program if specified.
					<br/>In order to compute prices, the <code>PriceProgramKey</code> or <code>PriceProgramName</code> and <code>CallingOrganizationCode</code> must be passed, if the <code>CallingOrganizationCode</code> does not define a default price program.
				</Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="PricingDate">
                <Description>
					The date to be used when computing prices.  If not passed, this date defaults to the current date.
				</Description>
            </Attribute>
            <Attribute EntityName="Classification_Purpose"
                Name="PurposeForDisplayClassification" NameInEntity="Classification_Purpose_Code">
                <Description>
					If passed, the corresponding classification path of each item for the specified purpose will be returned in the output as <code>DisplayClassification</code> on the <code>Item</code>'s <code>PrimaryInformation</code> element.  The value passed as <code>PurposeForDisplayClassification</code> should correspond to a valid <code>ClassificationPurposeCode</code> for a classification purpose in the system.  A list of valid <code>ClassificationPurposeCode</code> values can be obtained from the <code>getClassificationPurposeList</code> API.
				</Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="SellerOrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
					The organization code of the Seller Organization.
					<Validation>
                            <Condition>
                                <code>SellerOrganizationCode</code> does not identify a valid organization.</Condition>
                            <Error
                                Description="YFS:Invalid organization code" ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ShowAttributesWithDifferentValuesOnly">
                <Description>
					Flag indicating that only attributes that have different values for different items should be returned when calling the API for item comparison.
					Valid values: <b>
                        <code>Y</code>
                    </b> or <b>
                        <code>N</code>
                    </b>
						Default value: <b>
                        <code>N</code>
                    </b>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ShowFinalSupersededItemOnly">
                <Description>When set to <code>Y</code>, only the final valid superseded item will be returned as the superseded item.
				<br/>Default: <code>N</code>
                    <br/>Valid values: <code>Y</code>, <code>N</code>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ShowKeyItemAttributesOnly">
                <Description>
					Flag indicating that only key item attributes should be returned when the API is called to get items for comparison.
					Valid values: <b>
                        <code>Y</code>
                    </b> or <b>
                        <code>N</code>
                    </b>
						Default value: <b>
                        <code>N</code>
                    </b>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ShowVisibleAssociationsOnly">
                <Description>
                    <div>This attribute should be passed with a value <code>Y</code> to list associated items that are associated to the association type which has <code>ShowInUI</code> flag is Y</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Item" Name="StartItemKey" NameInEntity="Item_Key">
                <Description>To start from a particular Item key.</Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="UserOrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
						Not used by <code>getItemListForOrdering</code>.  <code>CallingOrganizationCode</code> should be used instead.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <BarCode BarCodeData="Required" BarCodeType="Required" MultipleTranslationsError="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="BarCodeData">
                    <Description>
                        <div>BarCode data to be translated.
							<Validation>
                                <Condition>
                                    <code>BarCodeData</code> is mandatory.</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YCP0187"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="BarCode_Translation"
                    Name="BarCodeType" NameInEntity="BarCode_Type">
                    <Description>
                        <div>BarCode Type associated with the data being scanned.
							<Validation>
                                <Condition>
                                    <code>BarCodeType</code> is mandatory.</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YCP0186"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="MultipleTranslationsError">
                    <Description>
                        <div>If this is passed as Y and multiple translations are found for the barcode, then an error <code>YCD00064</code> is thrown. 
						<br/>Default value is N.
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ContextualInfo OrganizationCode="Required">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="BarCode_Translation"
                        Name="OrganizationCode" NameInEntity="Organization_Code">
                        <Description>
                            <div>This is the organization from which bar code set up will be picked up. For node users, this will be the org code corresponding to the node in context.
								<Validation>
                                    <Condition>
                                    <code>OrganizationCode</code> is mandatory.</Condition>
                                    <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10513"/>
                                </Validation>
                                <Validation>
                                    <Condition>
                                    <code>OrganizationCode</code> does not identify a valid organization.</Condition>
                                    <Error
                                    Description="YFS:Invalid organization code" ErrorCode="YFS10395"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ContextualInfo>
    </BarCode>
    <ServiceType IsInStoreService="">
        <yfc:doc>
            <Element EntityName="Service_Type"/>
        </yfc:doc>
    </ServiceType>
    <CustomerInformation BuyerUserId="" CustomerContactID=""
        CustomerID="" CustomerLevel="" CustomerType="" Region=""
        RelationshipType="" Vertical="">
        <yfc:doc>
            <Element EntityName="Entitle_Rule_Assignment">
                <Description>This is used to provide information about the customer for which items are being retrieved when entitlements need to be applied. CustomerID is used to get 
				customer information from the system. If both CustomerID and CustomerType are not populated, CustomerType is
				defaulted to <code>03 - Anonymous</code>. All other attributes are used as an override to apply corresponding entitlements. </Description>
            </Element>
            <Attributes>
                <Attribute DataType="ContactUserId" Name="BuyerUserId">
                    <Description>
                        <div>An identifier of the customer. This is a pass through field available to user exits.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="ID-40" Name="CustomerContactID">
                    <Description>
                        <div>An identifier of the customer. This is a pass through field available to user exits.</div>
                    </Description>
                </Attribute>
                <Attribute Name="CustomerID" NameInEntity="Customer_Id"/>
                <Attribute Name="CustomerLevel" NameInEntity="Customer_Level"/>
                <Attribute Name="CustomerType">
                    <Description>
                        <div> The type of customer.
							<br/>Valid Values:
							<br/>01 - for B2B customer
							<br/>02 - for B2C customer
							<br/>03 - for anonymous customer 
							<br/>04 - for everyone
						</div>
                    </Description>
                </Attribute>
                <Attribute Name="Region">
                    <Description>
                        <div> Region to which entitlement rule is assigned. </div>
                    </Description>
                </Attribute>
                <Attribute Name="RelationshipType" NameInEntity="Relationship_Type"/>
                <Attribute Name="Vertical" NameInEntity="Vertical"/>
            </Attributes>
        </yfc:doc>
    </CustomerInformation>
