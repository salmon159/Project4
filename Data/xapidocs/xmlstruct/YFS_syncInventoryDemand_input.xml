<SyncInventoryDemand xmlns="http://www.sterlingcommerce.com/documentation/YFS/syncInventoryDemand/input">
    <ShipNodes>
        <ShipNode ShipNode="Required">
            <yfc:doc>
                <Element EntityName="Ship_Node"/>
                <Attributes>
                    <Attribute DataType="ShipNode" Name="ShipNode">
                        <Description>
                            <div>
			The ship node associated with this inventory demand, if demand is raised for a particular node.
			<Validation>
                                    <Condition>
                                    <code>ShipNode</code> must be a passed.</Condition>
                                    <Error
                                    Description="No Ship Node is passed." ErrorCode="INV80_003"/>
                                </Validation>
                                <Validation>
                                    <Condition>
                                    <code>ShipNode</code> must be a valid existing <code>ShipNode.</code>
                                    </Condition>
                                    <Error
                                    Description="Invalid shipnode." ErrorCode="YFS10048"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ShipNode>
    </ShipNodes>
    <DemandTypes>
        <DemandType DemandType="Required">
            <yfc:doc>
                <Element EntityName="Inventory_Demand_Type"/>
                <Attributes>
                    <Attribute DataType="Text-40" Name="DemandType" NameInEntity="Demand_Type">
                        <Description>
                            <div>
			The type of demand.
			<Validation>
                                    <Condition>
                                    <code>DemandType</code> must be passed.</Condition>
                                    <Error
                                    Description="No demand type is passed." ErrorCode="INV80_004"/>
                                </Validation>
                                <Validation>
                                    <Condition>
                                    <code>DemandType</code> must be a valid existing <code>DemandType.</code>
                                    </Condition>
                                    <Error
                                    Description="Invalid demand type." ErrorCode="INV80_008"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </DemandType>
    </DemandTypes>
    <Items>
        <Item InventoryOrganizationCode="Required" ItemID="Required"
            ProductClass="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Inventory_Item"/>
                <Attributes>
                    <Attribute DataType="OrgCode" Name="InventoryOrganizationCode"/>
                    <Attribute EntityName="Inventory_Item" Name="ItemID" NameInEntity="Item_Id">
                        <Description>
                            <div>
			The identifer for this inventory item.
			<Validation>
                                    <Condition>
                                    <code>ItemID</code> must be passed.</Condition>
                                    <Error
                                    Description="Required fields are not passed." ErrorCode="INV80_005"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Demand DemandCancelDate="" DemandReference=""
                DemandReferenceType="" DemandShipDate="Required"
                DemandType="" MinShipByDate="" OrganizationCode=""
                Quantity="" Segment="" SegmentType="" ShipNode="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Demand"/>
                    <Attributes>
                        <Attribute EntityName="Inventory_Demand"
                            Name="DemandShipDate" NameInEntity="Demand_Ship_Date">
                            <Description>
                                <div>
			The ship date on this inventory demand.
			<Validation>
                                    <Condition>
                                    <code>DemandShipDate</code> must be passed.</Condition>
                                    <Error
                                    Description="Required fields are not passed." ErrorCode="INV80_005"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute Name="DemandType">
                            <Description>
                                <div>
			The type of demand.
			<Validation>
                                    <Condition>
                                    <code>DemandType</code> must be consistent. DemandType passed for new demand must be consistent with the DemandType for the existing demand.</Condition>
                                    <Error
                                    Description="There is inconsistency in input. DemandType under demand node should be consistent." ErrorCode="INV80_007"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Demand"
                            Name="OrganizationCode" NameInEntity="Owner_Key"/>
                        <Attribute EntityName="Inventory_Demand"
                            Name="ShipNode" NameInEntity="Shipnode_Key">
                            <Description>
                                <div>
			The ship node associated with this inventory demand, if demand is raised for a particular node.
			<Validation>
                                    <Condition>
                                    <code>ShipNode</code> must be consistent. ShipNode passed for new demand must be consistent with the shipNode for the existing demand.</Condition>
                                    <Error
                                    Description="There is inconsistency in input. ShipNode under demand node should be consistent." ErrorCode="INV80_006"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                    LotAttribute3="" LotKeyReference="" LotNumber=""
                    ManufacturingDate="" RevisionNo="">
                    <yfc:doc>
                        <Element EntityName="Inventory_Tag">
                            <Description>
                                <div>
			Tag information is stored only if the Node tracks tag information in inventory.
			<Validation>
                                    <Condition>The <code>Item</code> is Tag controlled and <code>Tag</code> is not passed.
				</Condition>
                                    <Error
                                    Description="No Tag information found" ErrorCode="INV79_002"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>The <code>Item</code> is Tag controlled and all <code>Tag</code> identifiers are not passed.
				</Condition>
                                    <Error
                                    Description="INV: Missing Inventory Identifier Attribute." ErrorCode="INV10029"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Element>
                    </yfc:doc>
                </Tag>
            </Demand>
        </Item>
    </Items>
</SyncInventoryDemand>
