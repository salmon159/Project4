<!-- manageAttributeDomain Input XML -->
<AttributeDomain AttributeDomainID="Required" AttributeDomainKey=""
    LongDescription="" Operation="Manage|Create|Modify|Delete|"
    OrganizationCode="Required" Purpose="Required" ShortDescription="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageAttributeDomain/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
				Attribute Domains are hierarchies of attribute groups which can be created for different usages or purposes.
					<Validation>
                        <Condition>
                            <code>AttributeDomainID</code>, <code>OrganizationCode</code> and <code>Purpose</code> must be passed.
						</Condition>
                        <Error
                            Description="Mandatory parameters for the operation are missing" ErrorCode="YFS10364"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>Operation to perform
					<br/>Valid Values: 
					<br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
					<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
					<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
					<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
					<br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="Purpose">
                <Description>
                    <div>
				The purpose of the attribute domain. This is used to indicate the purpose or usage of the attribute domain. If not passed in the input, it will be defaulted to 'ITEM'.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <AttrDomainLocaleList Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>
                        <div>
							If <code>Reset</code> is passed as <b>
                                <code>Y</code>
                            </b>, the old records are removed and replaced with the new ones.
							If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                <code>N</code>
                            </b>, only the deltas are applied. 
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <AttrDomainLocale AttrDomainLocaleKey="" AttributeDomainKey=""
            Country="" Language="" LongDescription="" Operation=""
            ShortDescription="" Variant="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Action" Name="Operation">
                        <Description>
                            <div>
								Valid value is <b>
                                    <code>Delete</code>
                                </b>. Set <code>Operation</code> = "<b>
                                    <code>Delete</code>
                                </b>" to delete the entity.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </AttrDomainLocale>
    </AttrDomainLocaleList>
</AttributeDomain>
