<StoreBatch BatchNo="" OrganizationCode="" StoreBatchKey="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/getStoreBatchDetails/input">
    <yfc:doc>
        <Element EntityName="Store_Batch"/>
    </yfc:doc>
    <StoreBatchLine BackroomPickComplete="" ItemID="" StagingComplete="" UnitOfMeasure="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="BackroomPickComplete">
                    <Description>
                        <div>
										This attribute is Y if the batch line is completely picked. It can be used to filter <code>StoreBatchLines</code>.
									</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Shipment_Line" Name="ItemID" NameInEntity="Item_Id">
                    <Description>
                        <div>
										Item ID of the batch line. It can be used along with <code>UnitOfMeasure</code> to filter <code>StoreBatchLines</code>.
										The lines will be filtered only if both <code>ItemID</code> and <code>UOM</code> are passed.
									</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="StagingComplete">
                    <Description>
                        <div>
										This attribute is Y if the batch line is completely staged. It can be used to filter <code>StoreBatchLines</code>.
									</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Shipment_Line"
                    Name="UnitOfMeasure" NameInEntity="Uom">
                    <Description>
                        <div>
										Unit of measure of the batch line. It can be used along with <code>ItemID</code> to filter <code>StoreBatchLines</code>.
										The lines will be filtered only if both <code>ItemID</code> and <code>UOM</code> are passed.
									</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </StoreBatchLine>
    <OrderBy>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				This <code>OrderBy</code> element is passed as input to <code>getShipmentLineList</code> API. This can be used to sequence batch lines for picking.
				If OrderBy element is passed, orders returned by the API are sorted in the same order as Attribute elements passed.
				 Having many attributes in OrderBy element may have performance implications when large number of orders are returned by the API.
				</div>
                </Description>
            </Element>
        </yfc:doc>
        <Attribute Desc="" Name="Required" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="Desc">
                        <Description>Sort in decending order.
						Valid values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
						Default value: <b>
                                <code>N</code>
                            </b>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-32" Name="Name">
                        <Description>Name of the attribute to use for sorting the results.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Attribute>
    </OrderBy>
</StoreBatch>
