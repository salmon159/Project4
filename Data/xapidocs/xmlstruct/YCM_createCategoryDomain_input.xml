<!-- create CategoryDomain Input XML -->
<CategoryDomainList xmlns="http://www.sterlingcommerce.com/documentation/YCM/createCategoryDomain/input">
    <CategoryDomain AttributeName="Conditional"
        AuthorizedSubCatalogOrganizationCode=""
        CategoryDomain="Required" Description="" IsClassification=""
        OrganizationCode="Required" ShortDescription="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					The highest level of classification for an item. Multiple domains can be created by an organization. All categorization is defined under a particular domain.
					</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute Name="AttributeName">
                    <Description>
                        <div>
						A pre-defined list of fields that can be used for Item Classification. If <code>AttributeName</code> is passed, it must be a valid attribute name of an item that the system allows. The valid values are: <code>
                                <b>HarmonizedCode</b>,</code>
                            <code>
                                <b>Schedule_B_Code</b>,</code>
                            <code>
                                <b>NMFCCode</b>,</code>
                            <code>
                                <b>NMFCClass</b>,</code>
                            <code>
                                <b>ECCNNo</b>,</code>
                            <code>
                                <b>NAICSCode</b>,</code>
                            <code>
                                <b>UNSPSC</b>,</code>
                            <code>
                                <b>HazmatClass</b>,</code>
                            <code>
                                <b>ProductLine</b>,</code>
                            <code>
                                <b>VelocityCode</b>,</code>
                            <code>
                                <b>TaxProductCode</b>,</code>
                            <code>
                                <b>ItemType</b>,</code>
                            <code>
                                <b>StorageType</b>,</code>
                            <code>
                                <b>PickingType</b>,</code>
                            <code>
                                <b>CommodityCode</b>,</code> and extended attributes of Item.
							<Validation>
                                <Condition>
								If <code>IsClassification</code> is set to <code>
                                    <b>Y</b>
                                    </code>,  <code>AttributeName</code> must be passed. If not passed:
								</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                            </Validation>
                            <Validation>
                                <Condition>
								If passed, <code>AttributeName</code> must be a valid attribute name of an item that the system allows. If not valid:
								</Condition>
                                <Error
                                    Description="Invalid Item Classification Attribute" ErrorCode="YCM0050"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="AuthorizedSubCatalogOrganizationCode">
                    <Description>
                        <div>
						The sub-catalog organization that the catalog organization authorizes to maintain the category domain. 
						<Validation>
                                <Condition>
                                    <code>SubCatalogOrganizationCode</code> or <code>AuthorizedSubCatalogOrganizationCode</code> must be of a valid sub-catalog organization of the catalog organization specified in <code>OrganziationCode</code> field:
							</Condition>
                                <Error
                                    Description="Invalid sub-catalog organization" ErrorCode="YCM83_007"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="CategoryDomain">
                    <Description>
                        <div>
						An identifier for a category domain defined by an organization. This is a required attribute. 
							<Validation>
                                <Condition>
                                    <code>CategoryDomain</code> must be passed. If not passed:
								</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="OrganizationCode">
                    <Description>
                        <div>
						The organization for which the category domain will be created. This is a required attribute. 
							<Validation>
                                <Condition>
                                    <code>OrganizationCode</code> must be passed. If not passed:
								</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>OrganizationCode</code> must be a valid organization code. If not valid:
								</Condition>
                                <Error
                                    Description="Invalid Organization Code" ErrorCode="YCM0019"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <AssetList Reset="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="Reset">
                        <Description>
                            <div>
								If <code>Reset</code> is passed as <b>
                                    <code>Y</code>
                                </b>, the old Asset records are removed and replaced with the new ones.
								If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                    <code>N</code>
                                </b>, only the deltas are applied. 
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Asset AssetID="" AssetKey="" ContentID=""
                ContentLocation="" ContentSize="" Description=""
                Label="" Operation="" Type="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Action" Name="Operation">
                            <Description>
                                <div>
									Valid value is <b>
                                    <code>Delete</code>
                                    </b>. Set <code>Operation</code> = "<b>
                                    <code>Delete</code>
                                    </b>" to delete the entity.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Action" Name="Type">
                            <Description>
                                <div>
									Type of asset.<br/>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <AssetLocaleList Reset="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Flag" Name="Reset">
                                <Description>
                                    <div>
									If <code>Reset</code> is passed as <b>
                                    <code>Y</code>
                                    </b>, the old records are removed and replaced with the new ones.
									If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                    <code>N</code>
                                    </b>, only the deltas are applied. 
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <AssetLocale AssetKey="" AssetLocaleKey=""
                        ContentID="" ContentLocation="" Country=""
                        Description="" Label="" Language="" Operation="" Variant="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Action" Name="Operation">
                                    <Description>
                                    <div>
											Valid value is <b>
                                    <code>Delete</code>
                                    </b>. Set <code>Operation</code> = "<b>
                                    <code>Delete</code>
                                    </b>" to delete the entity.
										</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </AssetLocale>
                </AssetLocaleList>
            </Asset>
        </AssetList>
        <CategoryDomainLocaleList Reset="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="Reset">
                        <Description>
                            <div>
								If <code>Reset</code> is passed as <b>
                                    <code>Y</code>
                                </b>, the old records are removed and replaced with the new ones.
								If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                    <code>N</code>
                                </b>, only the deltas are applied. 
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <CategoryDomainLocale CatDomainLocaleKey=""
                CategoryDomainKey="" Country="" Description=""
                Language="" Operation="" ShortDescription="" Variant="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="CatDomainLocaleKey"/>
                        <Attribute DataType="String" Name="Country"/>
                        <Attribute DataType="String" Name="Language"/>
                        <Attribute DataType="Action" Name="Operation">
                            <Description>
                                <div>
									Valid value is <b>
                                    <code>Delete</code>
                                    </b>. Set <code>Operation</code> = "<b>
                                    <code>Delete</code>
                                    </b>" to delete the entity.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="Variant"/>
                    </Attributes>
                </yfc:doc>
            </CategoryDomainLocale>
        </CategoryDomainLocaleList>
    </CategoryDomain>
</CategoryDomainList>
