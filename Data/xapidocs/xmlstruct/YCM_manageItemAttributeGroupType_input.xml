<ItemAttributeGroupType ClassificationPurposeCode="" Description=""
    DisplayLocalizedFieldInLocale="" IsForMaintenance="" IsForSelling=""
    IsSingleGroup="" IsValidForCategory="" IsValidForClassification=""
    ItemAttributeGroupType="" ItemAttributeGroupTypeKey=""
    Operation="Manage|Create|Modify|Delete|" xmlns="http://www.sterlingcommerce.com/documentation/YCM/manageItemAttributeGroupType/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Locale" Name="DisplayLocalizedFieldInLocale">
                <Description>
                    <div>
							The locale for which output should be returned in. Locale should be passed in the standard format of "language_country_variant" (e.g., "fr_FR", "en_US_CA", etc.).
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>Operation to perform
						<br/>Valid Values: 
						<br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
						<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
						<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
						<br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
						</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ItemAttrGrpTypeLocaleList Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>
                        <div>
									If <code>Reset</code> is passed as <b>
                                <code>Y</code>
                            </b>, the old records are removed and replaced with the new ones.
									If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                <code>N</code>
                            </b>, only the deltas are applied. 
								</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ItemAttrGrpTypeLocale Country="" Description=""
            ItemAttrGroupTypeKey="" ItemAttrGrpTypeLocaleKey=""
            Language="" Operation="" Variant="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Action" Name="Operation">
                        <Description>
                            <div>
										Valid value is <b>
                                    <code>Delete</code>
                                </b>. Set <code>Operation</code> = "<b>
                                    <code>Delete</code>
                                </b>" to delete the entity.
									</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ItemAttrGrpTypeLocale>
    </ItemAttrGrpTypeLocaleList>
    <ItemAttributeGroupList Reset="">
