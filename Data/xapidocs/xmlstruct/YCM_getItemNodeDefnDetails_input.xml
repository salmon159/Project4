<!-- getItemNodeDefnDetails Input XML -->
<ItemNodeDefn ItemID="Conditional" ItemNodeDefnKey="Conditional"
    Node="Conditional" OrganizationCode="Conditional"
    UnitOfMeasure="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YCM/getItemNodeDefnDetails/input">
    <yfc:doc>
        <Element EntityName="Item_Node_Defn">
            <Description>
                <div>
			Provide unique identifications of an ItemNodeDefn.  <code>ItemNodeDefnKey</code> or a combination of  <code>OrganizationCode</code>, <code>Node</code>, <code>ItemID</code>, and <code>UnitOfMeasure</code> can be used to identify an ItemNodeDefn.
			<Validation>
                        <Condition>
                            <code>ItemNodeDefnKey</code> and a combination of  <code>OrganizationCode</code>, <code>Node</code>, <code>ItemID</code>, and <code>UnitOfMeasure</code> are not provided.
				</Condition>
                        <Error
                            Description="Missing mandatory parameters." ErrorCode="YFS10364"/>
                    </Validation>
                    <Validation>
                        <Condition>ItemNodeDefn does not exist.
				</Condition>
                        <Error Description="No record found." ErrorCode="YFS10001"/>
                    </Validation>
                </div>
            </Description>
        </Element>
    </yfc:doc>
</ItemNodeDefn>
