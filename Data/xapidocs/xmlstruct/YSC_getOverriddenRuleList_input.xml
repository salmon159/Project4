<OrderApprovalRule CallingOrganizationCode="" Channel="" DocumentType=""
    Domain="" EffectiveEndDate="" EffectiveStartDate="" IsActive=""
    IsOverride="" LaneType="" Message="" MessageCode=""
    OrderApprovalRuleKey="" OrganizationCode="" RegionKey="" RuleID=""
    StoreType="" Type="" ValidationID="" ValidationKey="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/getOverriddenRuleList/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
	                  This is the table to store Overridden Rule.
	                   </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="String" Name="CallingOrganizationCode">
                <Description>
                    <div>
						 If CallingOrganizationCode is passed, then it will be used as the context organization. This, combined with the configuration data setup for rules, will result in data filtered for access by the given organization. 
					</div>
                    <div>
                        <Validation>
                            <Condition>If OrganizationCode is not passed, CallingOrganizationCode and either (Domain/ValidationID) or (ValidationKey) must be passed. 
							</Condition>
                            <Error
                                Description="Missing Mandatory Parameters" ErrorCode="YFS10364"/>
                        </Validation>
                        <Validation>
                            <Condition>Validation idenitified by the attributes (Domain/ValidationID) or (ValidationKey) must exist.
							</Condition>
                            <Error Description="Validation Not Found" ErrorCode="OMP921_MO03"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Validation" Name="Domain"/>
            <Attribute Name="IsActive">
                <Description>
                    <div>
	                    Indicates when the Overridden rule is active .
	                    </div>
                </Description>
            </Attribute>
            <Attribute Name="IsOverride">
                <Description>
                    <div>
	                    Indicates when approval is required for Overridden rule.
	                    
	                    <br/>Valid Values: 
					<br/>
                        <b>
                            <code>X</code>
                        </b> - Approval is required to proceed with the transaction.
					<br/>
                        <b>
                            <code>Y</code>
                        </b> - Allow the transaction no approval required
					<br/>
                        <b>
                            <code>Z</code>
                        </b> -Do not allow the transaction.
					<br/>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="RuleID">
                <Description>
                    <div>
	                    Overridden rule identifier .
	                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Validation" Name="ValidationID"/>
        </Attributes>
    </yfc:doc>
    <GreexRule>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
	               			The Greex format advanced XML condition associated with this overridden rule.
	                   </div>
                </Description>
            </Element>
        </yfc:doc>
    </GreexRule>
    <ApprovalGroup ApprovalGroupId="" ApprovalGroupKey="" ApprovalGroupName=""/>
</OrderApprovalRule>
