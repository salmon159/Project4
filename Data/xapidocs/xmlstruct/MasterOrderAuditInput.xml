<!-- MasterOrderAuditOutput XML-->
<OrderAudits xmlns="http://www.sterlingcommerce.com/documentation/MasterOrderAuditInput">
    <OrderAudit AuditTransactionId="" Createts="" Createuserid=""
        Modifyuserid="" NewBuffer="" OldBuffer="" OrderHeaderKey=""
        ReasonCode="" ReasonText="" Reference1="" Reference2=""
        Reference3="" Reference4="" XMLFlag="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element EntityName="Order_Audit"/>
            <Attributes>
                <Attribute DataType="String" Name="isHistory"/>
            </Attributes>
        </yfc:doc>
        <OrderAuditLevels>
            <OrderAuditLevel ModificationLevel="" ModificationType=""
                OrderLineKey="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Text-40" Name="ModificationLevel">
                            <Description>
				Level at which modification is done. Order, Order_Line, Order_Release or Order_Release_Line.
				</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <OrderLine PrimeLineNo="" SubLineNo=""
                    TransactionalLineId="" yfc:GroupName="OrderAuditLevel:OrderLine">
                    <yfc:doc>
                        <Element EntityName="Order_Line"/>
                        <Attributes>
                            <Attribute DataType="Text-40" Name="TransactionalLineId">
                                <Description>
                                    <div>
									A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not accepted in input. This <code>TransactionalLineId</code> is not stored in the database and cannot be used after an order has been created.
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </OrderLine>
                <OrderAuditDetails>
                    <OrderAuditDetail Action="" AuditType="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Text-10" Name="Action">
                                    <Description>
						Action 
						</Description>
                                </Attribute>
                                <Attribute DataType="Text-30" Name="AuditType">
                                    <Description>
						Audit Type
						</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <IDs>
                            <ID Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="Text-30" Name="Name">
                                    <Description>
									Name of the ID
									</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-30" Name="Value">
                                    <Description>
									Value of the ID
									</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                            </ID>
                        </IDs>
                        <Attributes>
                            <Attribute Extended="N|Y"
                                ModificationType="" Name="" NewValue=""
                                OldValue="" yfc:DTDOccurrence="OneOrMany">
                                <yfc:doc>
                                    <Element EntityName="[NONE]"/>
                                    <Attributes>
                                    <Attribute DataType="Flag" Name="Extended">
                                    <Description>Determines whether the attribute is an extended field. If this attribute
								is not available, then it means the attribute is not extended.</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-40" Name="ModificationType">
                                    <Description>
							Modification Type used for changing the attribute
							</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-30" Name="Name">
                                    <Description>
							Name of the Attribute Changed
							</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-100" Name="NewValue">
                                    <Description>
							New Value of the attribute modified
							</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-100" Name="OldValue">
                                    <Description>
							Old Value of the attribute modified
							</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                            </Attribute>
                        </Attributes>
                        <CustomAuditDetails>
                            <OrderAuditDetail Action="" AuditType="" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:includeInParent
                                    IncludeXMLChildNodes="N"
                                    IncludeXMLFileName="MasterOrderAuditInput.xml" IncludeXMLPath="/OrderAudits/OrderAudit/OrderAuditLevels/OrderAuditLevel/OrderAuditDetails/OrderAuditDetail"/>
                                <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="Text-10" Name="Action">
                                    <Description>
						Action 
						</Description>
                                    </Attribute>
                                    <Attribute DataType="Text-30" Name="AuditType">
                                    <Description>
						Audit Type
						</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                                <IDs>
                                    <yfc:includeInParent
                                    IncludeXMLChildNodes="Y"
                                    IncludeXMLFileName="MasterOrderAuditInput.xml" IncludeXMLPath="/OrderAudits/OrderAudit/OrderAuditLevels/OrderAuditLevel/OrderAuditDetails/OrderAuditDetail/IDs"/>
                                    <ID Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                                    <yfc:doc>
                                    <Attributes>
                                    <Attribute
                                    DataType="Text-30" Name="Name">
                                    <Description>
									Name of the ID
									</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-30" Name="Value">
                                    <Description>
									Value of the ID
									</Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                    </ID>
                                </IDs>
                                <Attributes>
                                    <yfc:includeInParent
                                    IncludeXMLChildNodes="Y"
                                    IncludeXMLFileName="MasterOrderAuditInput.xml" IncludeXMLPath="/OrderAudits/OrderAudit/OrderAuditLevels/OrderAuditLevel/OrderAuditDetails/OrderAuditDetail/Attributes"/>
                                    <Attribute Extended="N|Y"
                                    ModificationType="" Name=""
                                    NewValue="" OldValue="" yfc:DTDOccurrence="OneOrMany">
                                    <yfc:doc>
                                    <Element EntityName="[NONE]"/>
                                    <Attributes>
                                    <Attribute
                                    DataType="Flag" Name="Extended">
                                    <Description>Determines whether the attribute is an extended field. If this attribute
								is not available, then it means the attribute is not extended.</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-40" Name="ModificationType">
                                    <Description>
							Modification Type used for changing the attribute
							</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-30" Name="Name">
                                    <Description>
							Name of the Attribute Changed
							</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-100" Name="NewValue">
                                    <Description>
							New Value of the attribute modified
							</Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-100" Name="OldValue">
                                    <Description>
							Old Value of the attribute modified
							</Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                    </Attribute>
                                </Attributes>
                            </OrderAuditDetail>
                        </CustomAuditDetails>
                    </OrderAuditDetail>
                </OrderAuditDetails>
                <ModificationTypes>
                    <ModificationType Name="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element EntityName="Order_Audit"/>
                            <Attributes>
                                <Attribute EntityName="Order_Audit"
                                    Name="Name" NameInEntity="Modification_Type"/>
                            </Attributes>
                        </yfc:doc>
                    </ModificationType>
                </ModificationTypes>
            </OrderAuditLevel>
        </OrderAuditLevels>
    </OrderAudit>
</OrderAudits>
