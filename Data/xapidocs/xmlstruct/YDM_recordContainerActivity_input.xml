<Container ContainerNo="" ContainerScm="" LoadKey=""
    ShipmentContainerKey="" ShipmentKey="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/recordContainerActivity/input">
    <yfc:doc>
        <Element EntityName="Shipment_Container">
            <Description>
                <div>
				Stores container header information.
				<Validation>
                        <Condition>Container can be identified by using <code>ShipmentContainerKey</code> or <code>ContainerNo</code> and <code>ShipmentKey</code> or <code>ContainerNo</code> and <code>LoadKey</code>.
				</Condition>
                        <Error
                            Description="Either ContainerNo or ShipmentContainerKey is mandatory." ErrorCode="YDM00066"/>
                    </Validation>
                    <Validation>
                        <Condition>If neither <code>ShipmentKey</code> nor <code>LoadKey</code> is passed or identifiable from the <code>Shipment</code> or <code>Load</code> element passed.
				</Condition>
                        <Error
                            Description="Either shipment or load details must be passed in input." ErrorCode="YDM00267"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="ContainerNo" Name="ContainerNo">
                <Description>
                    <div>Unique key or business key, which by itself identifies a shipment container along with <code>ShipmentKey</code> and <code>LoadKey </code>. 
 				<Validation>
                            <Condition> It should be unique shipment container.
				</Condition>
                            <Error
                                Description="Container does not exist in the system." ErrorCode="YDM000157"/>
                        </Validation>
                        <Validation>
                            <Condition> Should be able to identify either a shipment or load for the container.
				</Condition>
                            <Error
                                Description="Either shipment or load details must be passed in input." ErrorCode="YDM00267"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="UCC128" Name="ContainerScm">
                <Description>
                    <div>A unique number which helps identify the container along with <code>ShipmentKey</code> or <code>LoadKey</code>.
				<Validation>
                            <Condition> It should be unique shipment container.
				</Condition>
                            <Error
                                Description="Container does not exist in the system." ErrorCode="YDM000157"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="LoadKey">
                <Description>
                    <div>Specifies the load to which the container belongs.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="ShipmentContainerKey">
                <Description>
                    <div>Primary key attribute of YFS_SHIPMENT_CONTAINER table. 
				<Validation>
                            <Condition> Must be a valid ShipmentContainerKey.
				</Condition>
                            <Error
                                Description="Specified Container does not exist." ErrorCode="YDM000157"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="ShipmentKey">
                <Description>
                    <div>Specifies the shipment to which the container belongs.</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Shipment SellerOrganizationCode="" ShipNode="" ShipmentNo="">
        <yfc:doc>
            <Element EntityName="Shipment"/>
        </yfc:doc>
    </Shipment>
    <Load LoadNo="">
        <yfc:doc>
            <Element EntityName="Load"/>
        </yfc:doc>
    </Load>
    <ContainerActivities>
        <ContainerActivity ActivityCode="" ActivityTimeStamp=""
            ActivityUserId="" ContainerActivityKey="" Createprogid=""
            Createts="" Createuserid="" IsCommercialAddress=""
            IsException="" Lockid="" Modifyprogid="" Modifyts=""
            Modifyuserid="" Notes="" ShipmentContainerKey="" TaxGeoCode="">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="yfs_container_activity_master.xml" IncludeXMLPath="/ContainerActivity"/>
            <yfc:doc>
                <Element EntityName="Container_Activity"/>
                <Attributes>
                    <Attribute DataType="Flag" EntityName="Person_Info"
                        Name="IsCommercialAddress" NameInEntity="Is_Commercial_Address">
                        <Description>
                            <div>
							Indicates whether the address is commercial, residential or none. Values of this field can be Y is Commercial address. N is Residential address. NULL is None. Default value is Null.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Person_Info"
                        Name="TaxGeoCode" NameInEntity="Tax_Geo_Code"/>
                </Attributes>
            </yfc:doc>
            <ActivityLocation ActivityLocationId="" ActivityNodeKey=""
                AddressLine1="" AddressLine2="" AddressLine3=""
                AddressLine4="" AddressLine5="" AddressLine6="" City=""
                Company="" Country="" FirstName=""
                IsCommercialAddress="" LastName="" MiddleName=""
                State="" TaxGeoCode="" ZipCode="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="IsCommercialAddress"/>
                        <Attribute DataType="String" Name="TaxGeoCode"/>
                    </Attributes>
                </yfc:doc>
            </ActivityLocation>
        </ContainerActivity>
    </ContainerActivities>
</Container>
