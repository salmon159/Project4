<TaskSummary CurrentTime="" EndOfDay="" Node="" NumberOfUsersLoggedIn="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getTaskSummary/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="CurrentTime">
                <Description>
                    <div> Current system time</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="EndOfDay">
                <Description>
                    <div> End of day.  Its 23:59 hours of today.</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Ship_Node" Name="Node" NameInEntity="Ship_Node"/>
            <Attribute DataType="String" Name="NumberOfUsersLoggedIn">
                <Description>
                    <div> Number of users currently working</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ByTaskTypeList>
        <ByTaskType NumberOfCompletedTasks="" NumberOfOpenBatches=""
            NumberOfOpenTasks="" NumberOfOpenTasksPastDue=""
            NumberOfTasksExpectedToBePastDue="" PersonHoursReqd=""
            PersonHoursReqdInTimeFormat="" StandardRate=""
            StandardRateUom="" TaskType="" TaskTypeName=""
            UsersLoggedInWithSkillSet="" UsersWithSkillSet="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="NumberOfCompletedTasks">
                        <Description>
                            <div> Completed tasks</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="NumberOfOpenBatches">
                        <Description>
                            <div> Total number of batches in 'open' status</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="NumberOfOpenTasks">
                        <Description>
                            <div> Total number of open tasks.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="NumberOfOpenTasksPastDue">
                        <Description>
                            <div> Past due open tasks </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="NumberOfTasksExpectedToBePastDue">
                        <Description>
                            <div> Tasks may not get completed prior to due date</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="PersonHoursReqd">
                        <Description>
                            <div> Total man hours required to complete all open tasks of this task type.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="PersonHoursReqdInTimeFormat">
                        <Description>
                            <div> PersonHoursReqd in time format.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="StandardRate">
                        <Description>
                            <div> Rate in which tasks can be completed in given unit.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="StandardRateUom">
                        <Description>
                            <div> Unit of measure of the tasks can be completed. </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="TaskType" NameInEntity="Task_Type">
                        <Description>
                            <div>Task type of the task. </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="TaskTypeName" NameInEntity="Task_Type_Name">
                        <Description>
                            <div> Task type name. </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="UsersLoggedInWithSkillSet">
                        <Description>
                            <div> Currenly logged in number of users with required skill sets to complete a task. </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="UsersWithSkillSet">
                        <Description>
                            <div> Total number of users with required skill</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <OpenTasks>
                <OpenTask ExpectedBacklogTaskCount=""
                    ExpectedFinishNoLaterThan=""
                    ExpectedStartNoEarlierThan="" FinishNoLaterThan=""
                    PersonHoursAvailable="" PersonHoursReqd=""
                    PersonHoursReqdInTimeFormat="" RaiseAlert=""
                    SeqNo="" StartNoEarlierThan="" TaskCount="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="ExpectedBacklogTaskCount">
                                <Description>
                                    <div> Total expected number of tasks that may not be completed per schedule</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="ExpectedFinishNoLaterThan">
                                <Description>
                                    <div> Expected end time of the tasks.</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="ExpectedStartNoEarlierThan">
                                <Description>
                                    <div> Expected start time of the tasks</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="FinishNoLaterThan">
                                <Description>
                                    <div> Scheduled end time of the tasks.</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="PersonHoursAvailable"/>
                            <Attribute DataType="String" Name="PersonHoursReqd"/>
                            <Attribute DataType="String" Name="PersonHoursReqdInTimeFormat"/>
                            <Attribute DataType="String" Name="RaiseAlert">
                                <Description>
                                    <div> Indicates if the task can be complted or not.
										Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: 
										<br/>
                                    <b>
                                    <code>N</code>
                                    </b> - Task can be completed in alloted time.
										<br/>
                                    <b>
                                    <code>Y</code>
                                    </b> - Task can not be completed in alloted time. Hense raise alert ..
									</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="SeqNo"/>
                            <Attribute DataType="String" Name="StartNoEarlierThan">
                                <Description>
                                    <div> Scheduled start time of the tasks</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="TaskCount">
                                <Description>
                                    <div> Number of open tasks</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </OpenTask>
            </OpenTasks>
            <ByZoneList>
                <ByZone TotalOriginatingTasks=""
                    TotalTerminatingTasks="" Zone="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="TotalOriginatingTasks">
                                <Description>
                                    <div> Number of tasks originating in this zone.</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="TotalTerminatingTasks">
                                <Description>
                                    <div> Number of tasks will complete in this zone.</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Zone" Name="Zone" NameInEntity="Zone_Id"/>
                        </Attributes>
                    </yfc:doc>
                </ByZone>
            </ByZoneList>
        </ByTaskType>
    </ByTaskTypeList>
</TaskSummary>
