<!--getReleaseList Input XML-->
<OrderRelease BillToID="" BuyerOrganizationCode="" CustomerRewardsNo=""
    DocumentType="" FromOrderReleaseKey="" FromReleaseNo=""
    FromStatus="" IgnoreOrdering="" LatestFirst="" MaximumRecords=""
    OrderHeaderKey="" OrderReleaseKey="" OrderReleaseKeyQryType=""
    ReadFromHistory="" ReceivingNode="" ReleaseNo="" ReleaseNoQryType=""
    SalesOrderNo="" SellerOrganizationCode="" ServiceLineKey=""
    ShipNode="" Status="" StatusQryType="" ToOrderReleaseKey=""
    ToReleaseNo="" ToStatus="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderReleaseList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="BuyerOrganizationCode">
                <Description>
                    <div>
			Organization Code of the Buyer.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="CustomerRewardsNo"/>
            <Attribute DataType="Flag" Name="IgnoreOrdering">
                <Description>
                    <div>
				By default, the output is ordered by <code>OrderHeaderKey</code>, <code>PrimeLineNo</code>, <code>SublineNo</code>. If <code>IgnoreOrdering</code> is passed as <b>
                            <code>Y</code>
                        </b>, this ordering is not performed.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="LatestFirst">
                <Description>
                    <div>
				Governs the order in which <code>OrderLines</code> are returned. Default is earliest first: the first <code>OrderLine</code> created is the first in the list. Pass <code>LatestFirst</code> as <b>
                            <code>Y</code>
                        </b> to list the most recently created <code>OrderLine</code> first.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>
                    <div>
			Specifies the Maximum Number of records to be returned. Default is 500.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="ReadFromHistory">
                <Description>
                    <div>
					Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
					<br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_ORDER_RELEASE table.
					<br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_ORDER_RELEASE_H table (the history table).
					<br/>
                        <b>
                            <code>B</code>
                        </b> - Returns records that satisfy the input criteria from both YFS_ORDER_RELEASE and YFS_ORDER_RELEASE_H tables. Records returned from the current table are first in the output.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="SellerOrganizationCode">
                <Description>
                    <div>
			Organization Code of the Seller.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ServiceLineKey"/>
            <Attribute DataType="Status" Name="Status"/>
        </Attributes>
    </yfc:doc>
    <Order AuthenticationKey="" BillToID="" BillToIDQryType=""
        BuyerUserId="" BuyerUserIdQryType="" CustomerContactID=""
        CustomerEMailID="" CustomerEMailIDQryType=""
        CustomerRewardsNo="" DocumentType="" EnterpriseCode=""
        FromBillToID="" FromBuyerUserId="" FromCustomerEMailID=""
        FromHoldReasonCode="" FromOrderDate="" FromOrderHeaderKey=""
        FromOrderName="" FromOrderNo="" FromPaymentStatus=""
        FromReqDeliveryDate="" FromReqShipDate="" FromSearchCriteria1=""
        FromSearchCriteria2="" FromShipToID="" HoldReasonCode=""
        HoldReasonCodeQryType="" InvoiceComplete="" OrderDate=""
        OrderDateQryType="" OrderHeaderKey="" OrderHeaderKeyQryType=""
        OrderName="" OrderNameQryType="" OrderNo="" OrderNoQryType=""
        PaymentStatus="" PaymentStatusQryType="" ReqDeliveryDate=""
        ReqDeliveryDateQryType="" ReqShipDate="" ReqShipDateQryType=""
        SearchCriteria1="" SearchCriteria1QryType="" SearchCriteria2=""
        SearchCriteria2QryType="" ShipToID="" ShipToIDQryType=""
        ToBillToID="" ToBuyerUserId="" ToCustomerEMailID=""
        ToHoldReasonCode="" ToOrderDate="" ToOrderHeaderKey=""
        ToOrderName="" ToOrderNo="" ToPaymentStatus=""
        ToReqDeliveryDate="" ToReqShipDate="" ToSearchCriteria1=""
        ToSearchCriteria2="" ToShipToID="" yfc:UsedForIndexing="Order">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Key" Name="AuthenticationKey"/>
            </Attributes>
        </yfc:doc>
    </Order>
    <OrderLine DerivedFromOrderHeaderKey="" ShipNode="">
        <Item ItemID=""/>
        <DerivedFrom DocumentType="" EnterpriseCode=""
            EnterpriseCodeQryType="" FromEnterpriseCode=""
            FromOrderHeaderKey="" FromOrderNo="" OrderHeaderKey=""
            OrderHeaderKeyQryType="" OrderNo="" OrderNoQryType=""
            ToEnterpriseCode="" ToOrderHeaderKey="" ToOrderNo="">
            <yfc:doc>
                <Element EntityName="Order_Header"/>
            </yfc:doc>
        </DerivedFrom>
    </OrderLine>
</OrderRelease>
