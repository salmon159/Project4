<Task AssignedToUserId="" AssignmentNo="" AssignmentStatus=""
    BatchHold="" DependencyHold="" EndTimeStamp="" EnterpriseKey=""
    EquipmentId="" EquipmentLocationId="" ExceptionHold=""
    FinishNoLaterThan="" HoldReasonCode="" HoldReasonText=""
    IsSummaryTask="" Node="" OrganizationCode="" ParentTaskId=""
    PredecessorTaskId="" ProductivityKey="" ReasonCode="" ReasonText=""
    RootTaskId="" SourceAisle="" SourceLocationId=""
    SourceSortSequence="" SourceZoneId="" StartNoEarlierThan=""
    StartTimeStamp="" TargetAisle="" TargetCaseId="" TargetLocationId=""
    TargetPalletId="" TargetSortSequence="" TargetZoneId="" TaskId=""
    TaskKey="" TaskPriority="" TaskStatus="" TaskType="" isHistory="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/CANCEL_TASK/TASK_CANCELED">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="MasterTask.xml" IncludeXMLPath="/Task"/>
    <yfc:doc>
        <Attributes>
            <Attribute DataType="LPNNo" Name="TargetCaseId" NameInEntity="Target_LPN_No">
                <Description>
                    <div>Target Case Id of the task</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="TargetPalletId" NameInEntity="Target_LPN_No">
                <Description>
                    <div>Target Pallet Id of the task</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="isHistory">
                <Description>
                    <div>returns <code>'Y'</code>: if task is purged and exists in history table.<br/>
				returns <code>'N'</code>: if task is not purged.</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Inventory CountryOfOrigin="" FifoNo="" InventoryStatus="" ItemId=""
        ProductClass="" Quantity="" ReceiptHeaderKey="" Segment=""
        SegmentType="" SerialNo="" ShipByDate="" SourceCaseId=""
        SourcePalletId="" TagNumber="" UnitOfMeasure="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="MasterTask.xml" IncludeXMLPath="/Task/Inventory"/>
        <yfc:doc>
            <Attributes>
                <Attribute DataType="LPNNo" Name="SourceCaseId" NameInEntity="Source_LPN_No">
                    <Description>
                        <div>Source Case Id of the task</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="SourcePalletId" NameInEntity="Source_LPN_No">
                    <Description>
                        <div>Source Pallet Id of the task</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <TagAttributes BatchNo="" LotAttribute1="" LotAttribute2=""
            LotAttribute3="" LotKeyReference="" LotNumber=""
            ManufacturingDate="" RevisionNo="" TagNumber="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterTask.xml" IncludeXMLPath="/Task/Inventory/TagAttributes"/>
            <yfc:doc>
                <Element EntityName="Inventory_Tag"/>
            </yfc:doc>
        </TagAttributes>
    </Inventory>
    <TaskReferences BatchNo="" BolNo="" ContainerNo=""
        CountRequestKey="" CountRequestNo="" DocumentType="" LoadNo=""
        ManifestNo="" MoveRequestKey="" MoveRequestLineKey=""
        MoveRequestNo="" OrderHeaderKey="" OrderLineKey="" OrderNo=""
        OrderReleaseKey="" PrimeLineNo="" ReceiptNo="" ReleaseNo=""
        Scac="" ShipmentContainerKey="" ShipmentKey="" ShipmentNo=""
        SubLineNo="" TrailerNo="" WaveNo="" WorkOrderKey="" WorkOrderNo="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="MasterTask.xml" IncludeXMLPath="/Task/TaskReferences"/>
    </TaskReferences>
    <TaskStatusAudits>
        <TaskStatusAudit NewStatus="" NewStatusDescription=""
            OldStatus="" OldStatusDescription="" ReasonCode=""
            ReasonText="" TaskKey="" TaskStatusAuditKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="NewStatusDescription">
                        <Description>
                            <div>						 Status Description of New Status.						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="OldStatusDescription">
                        <Description>
                            <div>						 Status Description of Old Status.						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </TaskStatusAudit>
    </TaskStatusAudits>
</Task>
