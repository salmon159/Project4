<PricelistLine AbsoluteAdjustment="" EndDateActive="" FromQuantity=""
    ItemID="" ListPrice="" MassAuditID="" PercentAdjustment=""
    PriceTypeName="" PricelistHeaderKey="" PricelistLineKey=""
    PricingStatus="" StartDateActive="" ToQuantity="" UnitOfMeasure=""
    xmlns="http://www.sterlingcommerce.com/documentation/YPM/getPricelistLineList/input" yfc:ComplexQuerySupported="Y">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="YPM_Pricelist_Line_master.xml" IncludeXMLPath="/PricelistLine"/>
    <yfc:doc>
        <Element EntityName="Pricelist_Line"/>
    </yfc:doc>
    <Item ItemGroupCode="" OrganizationCode="Required">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YPM_Item_master.xml" IncludeXMLPath="/Item"/>
        <yfc:doc>
            <Element EntityName="Item_Vw">
                <Description>
                    <div>The view is a subset of YFS_ITEM table. This element is considered for filtering price list lines only if <code>OrganizationCode</code> is passed in the input. This element results in check for existence of item in catalog.</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute Name="ItemGroupCode">
                    <Description>
                        <div>Item group code is used to identify if the item is a Product, Provided Service, Provided Service Option, Delivery Service or Delivery Service Option.
						</div>
                    </Description>
                </Attribute>
                <Attribute Name="OrganizationCode">
                    <Description>
                        <div>Organization code is used to derive the catalog organization code; organization in which items are defined. <code>Item</code> element is completely ignored if this attribute is not passed. <code>QryType</code> is not supported on this attibute.
							<Validation>
                                <Condition>
                                    <code>OrganizationCode</code> is not valid.</Condition>
                                <Error
                                    Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <PrimaryInformation ShortDescription=""/>
        <CategoryFilter CategoryPath="">
            <yfc:doc>
                <Element EntityName="Category_Item_Vw">
                    <Description>Used to filter items based on category. This element is not enabled for complex query.</Description>
                </Element>
            </yfc:doc>
        </CategoryFilter>
    </Item>
    <PricelistHeader AbsoluteAdjustmentOverride="" Currency=""
        Description="" EndDateActive="" InheritFromPricelistHeaderKey=""
        MarkedForListPrice="" OrganizationCode=""
        PercentAdjustmentOverride="" PricelistHeaderKey=""
        PricelistName="" PricingStatus="" SellerOrganizationCode="" StartDateActive="">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YPM_Pricelist_Header_master.xml" IncludeXMLPath="/PricelistHeader"/>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YPM_Pricelist_Header_master_noOrgCode.xml" IncludeXMLPath="/PricelistHeader"/>
        <yfc:doc>
            <Element EntityName="Pricelist_Header">
                <Description>
                    <div>The price list to which this line belongs.</div>
                </Description>
            </Element>
        </yfc:doc>
    </PricelistHeader>
</PricelistLine>
