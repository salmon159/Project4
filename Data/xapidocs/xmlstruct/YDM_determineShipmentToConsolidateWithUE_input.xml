<DetermineShipmentToConsolidateWith xmlns="http://www.sterlingcommerce.com/documentation/YDM/determineShipmentToConsolidateWithUE/input">
    <Shipment ActualDeliveryDate="" ActualFreightCharge=""
        ActualShipmentDate="" AppointmentNo="" AssociationAction=""
        BolNo="" BuyerOrganizationCode="" CarrierAccountNo=""
        CarrierServiceCode="" CarrierType="" Code="" CommercialValue=""
        Currency="" DeliveryPlanKey="" DeliveryTS="" DestinationZone=""
        DocumentType="" DownLoadCount="" EnterpriseCode=""
        ExpectedDeliveryDate="" ExpectedShipmentDate=""
        ExportTaxpayerID="" FreightTerms="" FromAddressKey=""
        FromAppointment="" HasOtherShipments="" HazardousMaterialFlag=""
        HeldForConfirmation="" ITDate="" ITNo="" InvoiceComplete=""
        IsSingleOrder="" LevelOfService="" LinesEntered=""
        ManifestKey="" ManifestNo="" ManuallyEntered="" MergeNode=""
        NumOfCartons="" NumOfPallets="" OrderAvailableOnSystem=""
        OrderHeaderKey="" OrderNo="" OrderReleaseKey="" OriginZone=""
        OverrideManualShipmentEntry="" PackListType=""
        ParentShipmentKey="" PickListNo="" PickticketNo=""
        PipelineKey="" PodNo="" ProNo="" ReceivingNode="" ReleaseNo=""
        RequestedDeliveryDate="" RequestedShipmentDate=""
        RequiresApptConfirmation="" SCAC="" ScacIntegrationRequired=""
        SealNo="" SellerOrganizationCode="" ShipDate="" ShipMode=""
        ShipNode="" ShipVia="" ShipmentClosedFlag=""
        ShipmentConfirmUpdatesDone="" ShipmentContainerizedFlag=""
        ShipmentDeliverUpdatesDone="" ShipmentKey="" ShipmentNo=""
        ShipmentPlannedFlag="" ShipmentType="" Status="" StatusDate=""
        ToAppointment="" TotalActualCharge="" TotalEstimatedCharge=""
        TotalVolume="" TotalVolumeUOM="" TotalWeight=""
        TotalWeightUOM="" TrackingNo="" TrailerNo="" yfc:DTDOccurrence="REQUIRED">
        <yfc:includeInParent IncludeXMLFileName="YFS_Shipment_tran.xml" IncludeXMLPath="/Shipment"/>
        <yfc:doc>
            <Element EntityName="Shipment"/>
            <Attributes>
                <Attribute DataType="String" Name="HeldForConfirmation"/>
            </Attributes>
        </yfc:doc>
        <ShipmentLine ActualQuantity="" BatchPickPriority=""
            BuyerMarkForNodeId="" ChainedFromOrderHeaderKey=""
            ChainedFromOrderLineKey="" CountryOfOrigin=""
            CustomerPoLineNo="" CustomerPoNo="" DocumentType=""
            ExternalReleaseIdentifier="" FifoNo=""
            GroupSequenceNumber="" IsHazmat="" IsPickable="" ItemDesc=""
            ItemID="" KitCode="LK|| |Blanks" LevelOfService=""
            NetWeight="" NetWeightUom="" OrderAvailableOnSystem=""
            OrderHeaderKey="" OrderLineKey="" OrderNo=""
            OrderReleaseKey="" OverShipQuantity="" PickLocation=""
            PickLocationSeq="" PlacedQuantity="" PrimeLineNo=""
            ProductClass="" Quantity="" ReceivedQuantity="" ReleaseNo=""
            RequestedSerialNo="" ReturnShippingLabelLevel="" Segment=""
            SegmentType="" ShipToCustomerId=""
            ShipmentConsolidationGroupId="" ShipmentKey=""
            ShipmentLineKey="" ShipmentLineNo="" ShipmentSubLineNo=""
            ShortageQty="" StoreBatchKey="" SubLineNo=""
            UnitOfMeasure="" WaveNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Shipment_Line_master.xml" IncludeXMLPath="/ShipmentLine"/>
            <yfc:doc>
                <Element EntityName="Shipment_Line"/>
                <Attributes>
                    <Attribute DataType="Quantity" Name="ActualQuantity">
                        <Description>Actual quantity of the shipment line shipped.
				</Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="ChainedFromOrderHeaderKey" NameInEntity="ChainedFromOrderHeaderKey"/>
                    <Attribute EntityName="Order_Line"
                        Name="ChainedFromOrderLineKey" NameInEntity="ChainedFromOrderLineKey"/>
                    <Attribute DataType="ID-40" Name="DocumentType">
                        <Description>The document type of the orders comprising the shipment.
				</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="OrderAvailableOnSystem">
                        <Description>Indicates whether the order is available on the system or not.
				</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="PlacedQuantity">
                        <Description>The sum of all placed quantity in the container details for the shipment line.				</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="ReceivedQuantity">
                        <Description>Quantity received for the shipment line.
				</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ShipmentLineInvAttRequest BatchNo="" LotAttribute1=""
                LotAttribute2="" LotAttribute3="" LotKeyReference=""
                LotNumber="" ManufacturingDate="" RevisionNo="">
                <yfc:doc>
                    <Element EntityName="Shipment_Line_Req_Tag"/>
                </yfc:doc>
            </ShipmentLineInvAttRequest>
            <ShipmentTagSerials>
                <ShipmentTagSerial BatchNo="" ContainerDetailKey=""
                    LotAttribute1="" LotAttribute2="" LotAttribute3=""
                    LotKeyReference="" LotNumber="" ManufacturingDate=""
                    Quantity="" RevisionNo="" SerialNo="" ShipByDate=""
                    ShipmentLineKey="" ShipmentTagSerialKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent
                        IncludeXMLFileName="YFS_Shipment_Tag_Serial_master.xml" IncludeXMLPath="/ShipmentTagSerial"/>
                    <yfc:doc>
                        <Element EntityName="Shipment_Tag_Serial">
                            <Description>
                                <div>
				Stores additional information related to tag number, serial number, ship by date for a shipment line or container detail. Tag information is stored only if the <code>Node</code> tracks tag information in inventory or the <code>Buyer</code> mandates tag information.
				</div>
                            </Description>
                        </Element>
                    </yfc:doc>
                </ShipmentTagSerial>
            </ShipmentTagSerials>
        </ShipmentLine>
    </Shipment>
    <Shipments>
        <Shipment SellerOrganizationCode="Conditional"
            ShipNode="Conditional" ShipmentKey="Conditional"
            ShipmentNo="Conditional" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Shipment_keys.xml" IncludeXMLPath="/Shipment"/>
            <yfc:doc>
                <Element EntityName="Shipment">
                    <Description>
                        <div>
       Pass  <code>ShipmentKey</code>
                            <i>or</i> pass <code>ShipmentNo</code>, <code>ShipNode</code> and <code>SellerOrganizationCode</code>  to identify the shipment.
       </div>
                    </Description>
                </Element>
            </yfc:doc>
        </Shipment>
    </Shipments>
</DetermineShipmentToConsolidateWith>
