<NodeSCACAccount SCAC="Required" ShipNode="Required" xmlns="http://www.sterlingcommerce.com/documentation/YDM/getNodeSCACAccountList/input">
    <yfc:doc>
        <Element EntityName="SCAC_Ex"/>
        <Attributes>
            <Attribute DataType="OrgCode" EntityName="SCAC_Ex"
                Name="SCAC" NameInEntity="Scac_Key">
                <Description>
                    <div>Organization code of an organization that plays the role of <code>CARRIER</code>.
				<Validation>
                            <Condition>SCAC cannot be blank.
				</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ShipNode" EntityName="SCAC_Ex"
                Name="ShipNode" NameInEntity="Record_Key">
                <Description>
                    <div>The participating <code>ShipNode</code> of the <code>SCAC</code>.
				<Validation>
                            <Condition> ShipNode cannot be blank.
				</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</NodeSCACAccount>
