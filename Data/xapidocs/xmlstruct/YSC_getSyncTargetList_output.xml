<SyncTargetList TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/getSyncTargetList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>Total number of records that match the input criteria.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SyncTarget LongDescription="" OrganizationCode=""
        ShortDescription="" SyncSubscriptionKey="" SyncTargetID=""
        SyncTargetKey="" TargetingOrg="">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="MasterSyncSubscription.xml" IncludeXMLPath="/SyncSubscription/SyncTargetList/SyncTarget"/>
        <yfc:doc>
            <Element EntityName="Sync_Target">
                <Description>
                    <div>
					This table holds information about SyncTargets.  One of SyncTargetKey or both SyncTargetID and OrganizationCode must be passed to uniquely identify this SyncTarget. 
							<Validation>
                            <Condition>
                                <code>OrganizationCode</code> passed should be a valid OrganizationCode.</Condition>
                            <Error
                                Description="OrganizationCode does not exist." ErrorCode="YSC921_0007"/>
                        </Validation>
                        <Validation>
                            <Condition>Pass either <Code>SyncTargetKey</Code>, or both <Code>SyncTargetID</Code> and <Code>OrganizationCode</Code>
                            </Condition>
                            <Error
                                Description="Mandatory SyncTarget attributes missing." ErrorCode="YSC921_0008"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>TargetingOrg</code> passed must have its primary enterprise as the <Code>OrganizationCode</Code> on the SyncTarget</Condition>
                            <Error
                                Description="TargetingOrg passed in the input must have its PrimaryEnterprise as the OrganizationCode on the SyncTarget." ErrorCode="YSC921_0011"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="OrgCode" Name="TargetingOrg">
                    <Description>The Organization that is assigned to this SyncTarget</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <SyncSubscription SyncSubscriptionID=""/>
    </SyncTarget>
</SyncTargetList>
