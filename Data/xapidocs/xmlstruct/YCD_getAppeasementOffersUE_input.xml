<AppeasementOffers xmlns="http://www.sterlingcommerce.com/documentation/YCD/getAppeasementOffersUE/input">
    <Order DocumentType="" EnterpriseCode="" OrderHeaderKey="Required" OrderNo="">
        <AppeasementReason ReasonCode="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="ReasonCode">
                        <Description>
                            <div>
										The reason of appeasement.
									</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </AppeasementReason>
        <OrderLines>
            <OrderLine ItemGroupCode="" OpenQty=""
                OrderLineKey="Required" OrderedQty="" OtherCharges=""
                RemainingQty="" Status="" StatusQuantity="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="OpenQty">
                            <Description>
                                <div>
									Total quantity that can be cancelled from the line.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="RemainingQty">
                            <Description>
                                <div>
									Remaining quantity.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-100" Name="Status">
                            <Description>
                                <div>
									Status description of the order line.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Order_Release_Status"
                            Name="StatusQuantity" NameInEntity="Status_Quantity">
                            <Description>
                                <div>
									Quantity on the order line in the current status.
								</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Item ItemDesc="" ItemID="" ItemShortDesc=""
                    ItemWeight="" UnitCost="" UnitOfMeasure="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute EntityName="Order_Line"
                                Name="ItemDesc" NameInEntity="Item_Description"/>
                            <Attribute EntityName="Order_Line"
                                Name="ItemShortDesc" NameInEntity="Item_Short_Description"/>
                            <Attribute EntityName="Order_Line"
                                Name="ItemWeight" NameInEntity="Item_Weight"/>
                        </Attributes>
                    </yfc:doc>
                </Item>
                <LinePriceInfo ActualPricingQty="" LineTotal=""
                    OrderedPricingQty="" UnitPrice=""/>
                <LineOverallTotals Charges="" Discount=""
                    ExtendedPrice="" LineTotal="" OptionPrice=""
                    PricingQty="" Tax="" UnitPrice="">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
									Contains information about the price and total amount details for the order line.
									</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Money" Name="Charges">
                                <Description>Charges.
											</Description>
                            </Attribute>
                            <Attribute DataType="Money" Name="Discount">
                                <Description>Discount amount.
											</Description>
                            </Attribute>
                            <Attribute DataType="Money" Name="ExtendedPrice">
                                <Description>Extended price.
											</Description>
                            </Attribute>
                            <Attribute DataType="Money" Name="LineTotal">
                                <Description>Line total amount.
											</Description>
                            </Attribute>
                            <Attribute DataType="Money" Name="OptionPrice">
                                <Description>Total Option amount for the line.
											</Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="PricingQty">
                                <Description>Pricing Quantity.
											</Description>
                            </Attribute>
                            <Attribute DataType="Money" Name="Tax">
                                <Description>Tax amount.
											</Description>
                            </Attribute>
                            <Attribute DataType="Money" Name="UnitPrice">
                                <Description>Unit price.
											</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </LineOverallTotals>
                <LineCharges>
                    <LineCharge ChargeAmount="" ChargeCategory=""
                        ChargeName="" ChargeNameKey="" ChargePerLine=""
                        ChargePerUnit="" InvoicedChargeAmount=""
                        InvoicedChargePerLine=""
                        InvoicedChargePerUnit="" Reference=""
                        RemainingChargeAmount=""
                        RemainingChargePerLine="" RemainingChargePerUnit="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute EntityName="Charge_Name"
                                    Name="ChargeNameKey" NameInEntity="Charge_Name_Key"/>
                                <Attribute DataType="Money" Name="InvoicedChargeAmount">
                                    <Description>
                                    <div>
													Invoiced charge amount.
												</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Money" Name="InvoicedChargePerUnit">
                                    <Description>
                                    <div>
													Invoiced charge per unit.
												</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Money" Name="RemainingChargeAmount">
                                    <Description>
                                    <div>
													Remaining Charge Amount.
												</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Money" Name="RemainingChargePerLine">
                                    <Description>
                                    <div>
													Remaining charge per line.
												</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Money" Name="RemainingChargePerUnit">
                                    <Description>
                                    <div>
													Remaining charge per unit.
												</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </LineCharge>
                </LineCharges>
                <LineTaxes/>
            </OrderLine>
        </OrderLines>
        <PriceInfo Currency="" TotalAmount=""/>
        <OverallTotals GrandCharges="" GrandDiscount="" GrandTax=""
            GrandTotal="" HdrCharges="" HdrDiscount="" HdrTax=""
            HdrTotal="" LineSubTotal="">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
						  Contains information about the price and total amount details for the order.
						  </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Money" Name="GrandCharges">
                        <Description>
                            <div>
							  Total Charges
							  </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="GrandDiscount">
                        <Description>
                            <div>
							  Total Discount
							  </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="GrandTax">
                        <Description>
                            <div>
							  Grand total Tax Amount for the order
							  </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="GrandTotal">
                        <Description>
                            <div>
							  Grand total amount for the Order
							  </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="HdrCharges">
                        <Description>
                            <div>
							  Order Header Level Charges
							  </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="HdrDiscount">
                        <Description>
                            <div>
							  Order Header Level Discount
							  </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="HdrTax">
                        <Description>
                            <div>
							  Order Header Level Tax
							  </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="HdrTotal">
                        <Description>
                            <div>
							  Order Header Level Total amount
							  </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="LineSubTotal">
                        <Description>
                            <div>
							  Sub Total amount for all the lines
							  </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </OverallTotals>
        <HeaderCharges>
            <HeaderCharge ChargeAmount="" ChargeCategory=""
                ChargeName="" ChargeNameKey="" InvoicedChargeAmount=""
                Reference="" RemainingChargeAmount="">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Charge_Name"
                            Name="ChargeNameKey" NameInEntity="Charge_Name_Key"/>
                        <Attribute DataType="Money" Name="RemainingChargeAmount">
                            <Description>
                                <div>
										Remaining Charge Amount.
									</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </HeaderCharge>
        </HeaderCharges>
        <HeaderTaxes/>
    </Order>
</AppeasementOffers>
