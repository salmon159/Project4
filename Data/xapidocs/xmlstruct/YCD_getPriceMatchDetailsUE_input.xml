<!--getPriceMatchDetails UE Input XML-->
<PriceMatch PriceMatchKey="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCD/getPriceMatchDetailsUE/input">
    <yfc:doc>
        <Element EntityName="Price_Match">
            <Description>
                <div>
			The details of the Price Match. To uniquely identify the Price Match, <code>PriceMatchKey</code> should be passed
				<Validation>
                        <Condition>
                            <code>PriceMatchKey</code> is not passed.
					</Condition>
                        <Error Description="PriceMatchKey not passed." ErrorCode="YCD00010"/>
                    </Validation>
                </div>
            </Description>
        </Element>
    </yfc:doc>
</PriceMatch>
