<User IsValid="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/validateTokenUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="TFBoolean" Name="IsValid">
                <Description>
                    <div>Determines whether the user/token combination is valid.</div>
