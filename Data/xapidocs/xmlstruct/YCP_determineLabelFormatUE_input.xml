<GetLabelFormat LabelFormatId="" PrintDocumentId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/determineLabelFormatUE/input">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="YCP_getLabelFormat_input.xml" IncludeXMLPath="/GetLabelFormat"/>
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Label_Format" Name="LabelFormatId" NameInEntity="Label_Format_Id">
                <Description>
                    <div>
					Unique identifier of a label format.
						<Validation>
                            <Condition>If specified should be a valid label format.
							</Condition>
                            <Error
                                Description="Invalid label format ID." ErrorCode="YCP0179"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Print_Document"
                Name="PrintDocumentId" NameInEntity="Print_Document_Id">
                <Description>
                    <div>
					The Document to print.
						<Validation>
                            <Condition>
                                <code>PrintDocumentId</code> should be a valid document.
							</Condition>
                            <Error
                                Description="Invalid Print Document Identifier." ErrorCode="YCP0185"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <LabelPreference BuyerOrganizationCode="" EnterpriseCode=""
        EquipmentType="" Node="" SCAC="" SellerOrganizationCode=""/>
</GetLabelFormat>
