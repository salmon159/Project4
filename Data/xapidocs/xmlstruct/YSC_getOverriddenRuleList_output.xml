<OrderApprovalRuleList TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/getOverriddenRuleList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="TotalNumberOfRecords"/>
        </Attributes>
    </yfc:doc>
    <OrderApprovalRule Channel="" DocumentType="" EffectiveEndDate=""
        EffectiveStartDate="" IsActive="" IsOverride="" LaneType=""
        Message="" MessageCode="" OrderApprovalRuleKey=""
        OrganizationCode="" RegionKey="" RuleID="" StoreType="" Type="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute Name="IsActive">
                    <Description>
                        <div>
	                    Indicates when the Overridden rule is active .
	                    </div>
                    </Description>
                </Attribute>
                <Attribute Name="IsOverride">
                    <Description>
                        <div>
	                    Indicates when approval is required for Overridden rule.
	                    <br/>Valid Values: 
					<br/>
                            <b>
                                <code>X</code>
                            </b> - Approval is required to proceed with the transaction.
					<br/>
                            <b>
                                <code>Y</code>
                            </b> - Allow the transaction no approval required
					<br/>
                            <b>
                                <code>Z</code>
                            </b> -Do not allow the transaction.
					<br/>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="RuleID">
                    <Description>
                        <div>
	                    Overridden rule identifier .
	                    </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <GreexRule yfc:AllowAnyElement="skip">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
	               			The Greex format advanced XML condition associated with this overridden rule.
	                   </div>
                    </Description>
                </Element>
            </yfc:doc>
        </GreexRule>
        <Validation CanApprovalBePostponed="" ConfigurationGroup=""
            Data_Capture="" Description="" Domain="" ErrorCode=""
            ExternalInitPerform="" InputTemplate=""
            IsApprovalRequiredOnReoccurence="" IsOverridable=""
            IsSystemDefined="" IsUserExitActive="" ModelTemplate=""
            OrganizationCode="" ValidationID="" ValidationKey=""
            ValidationType="" ViolationEntity="" yfc:AllowAnyElement="skip">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterValidation.xml" IncludeXMLPath="/Validation"/>
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Validation" Name="IsApprovalRequiredOnReoccurence">
                        <Description>
                            <div>
	                    Indicates if each transaction violation for this validation needs to be approved (Y) or if subsequent violations for the same validation do not need to be approved (N).
	                    </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Validation" Name="IsOverridable">
                        <Description>
                            <div>
	                    This flag is mainly used by SBC to indicates that the validation can be overridden by overridden rules.
	                     </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Validation" Name="ViolationEntity">
                        <Description>
                            <div>
						This specifies the violation entity for storing the violations and approver information.Violation entity should have corresponding transaction info entity.  
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Validation>
        <ApprovalGroupList>
            <ApprovalGroup ApprovalGroupId="" ApprovalGroupKey=""
                ApprovalGroupName="" DocumentType=""
                IsSignatureMandatory="" MaxApprover="" MinApprover=""
                OrganizationCode="" SignaturePolicy="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Text-1"
                            EntityName="Approval_Rule_Assoc" Name="IsSignatureMandatory">
                            <Description>
                                <div>Indicates in case exception is caused by a person higher or equal to Min Approver role, the system will enforce minimum of self signature as approval mechanism..
						<br/>Valid Values: 
						<br/>
                                    <b>
                                    <code>Y</code>
                                    </b>
                                    <br/>
                                    <b>
                                    <code>N</code>
                                    </b>
                                    <br/>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Approval_Rule_Assoc" Name="MaxApprover"/>
                        <Attribute EntityName="Approval_Rule_Assoc" Name="MinApprover"/>
                        <Attribute DataType="Text-20"
                            EntityName="Approval_Rule_Assoc" Name="SignaturePolicy">
                            <Description>
                                <div>Indicates if approval signature is required by every one in the approval plan sequence or approval by any one in the approval plan sequence.
						<br/>Valid Values: 
						<br/>
                                    <b>
                                    <code>ANYONE</code>
                                    </b>
                                    <br/>
                                    <b>
                                    <code>EVERYONE</code>
                                    </b>
                                    <br/>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ApprovalGroup>
        </ApprovalGroupList>
        <Region CanAggregate="" ParentRegionKey="" RegionDescription=""
            RegionKey="" RegionLevelName="" RegionName="">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
							Information about the Region associated with the Overridden Rule.
						</div>
                    </Description>
                </Element>
            </yfc:doc>
        </Region>
    </OrderApprovalRule>
</OrderApprovalRuleList>
