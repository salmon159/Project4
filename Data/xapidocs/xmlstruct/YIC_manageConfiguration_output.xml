<!-- manageConfiguration output XML-->
<manageConfiguration configId="" createDate="" partial="" sku="" type=""
    updateDate="" xmlns="http://www.sterlingcommerce.com/documentation/YIC/manageConfiguration/output">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					The root node of the XML document. This has the header information of the saved configurator BOM.  
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Key" EntityName="BOM_Header"
                Name="configId" NameInEntity="BOM_Id">
                <Description>
                    <div>
						The ID of the saved configuration.						
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" EntityName="BOM_Header"
                Name="createDate" NameInEntity="Createts">
                <Description>
                    <div>
						The date of creation of the record.						
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="partial">
                <Description>
                    <div>
						This is a derived attribute. The value is <code>Y</code>, if the configuraton has errors. The value of the attribute is <code>N</code> otherwise.
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" EntityName="BOM_Header"
                Name="sku" NameInEntity="SKU">
                <Description>
                    <div>
						The SKU from the root node of the configuration.						
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" EntityName="BOM_Header"
                Name="type" NameInEntity="Type">
                <Description>
                    <div>
						A caller supplied identifier string used to group saved configurations.						
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" EntityName="BOM_Header"
                Name="updateDate" NameInEntity="Modifyts">
                <Description>
                    <div>
						The date of subsequent update of the record.
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</manageConfiguration>
