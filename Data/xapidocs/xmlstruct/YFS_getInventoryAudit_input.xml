<!--getInventoryAudit Input XML-->
<InventoryAudit IgnoreOrdering="" ItemID="" Modifyts=""
    OrganizationCode="Required" ProductClass="" Segment=""
    SegmentType="" ShipNode="" SupplyType="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getInventoryAudit/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="IgnoreOrdering">
                <Description>
                    <div>
				By default, the API output is ordered by INVENTORY_ITEM_KEY in ascending order and INVENTORY_AUDIT_KEY in descending order. If <code>IgnoreOrdering</code> is passed as <b>
                            <code>Y</code>
                        </b>,this default ordering is omitted by the API.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Audit" Name="ItemID" NameInEntity="Item"/>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>
				Indicates the organization.
				<Validation>
                            <Condition>Must be a valid organization.
					</Condition>
                            <Error
                                Description="Invalid organization code" ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> must be passed.
						</Condition>
                            <Error
                                Description="OrganizationCode is mandatory" ErrorCode="YFS10435"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderBy>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				If OrderBy element is passed, inventory audits returned by the API are sorted in the same order as Attribute elements passed.
				</div>
                </Description>
            </Element>
        </yfc:doc>
        <Attribute Desc="" Name="Required" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="Desc">
                        <Description>Sort in decending order.
						Valid values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
						Default value: <b>
                                <code>N</code>
                            </b>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-32" Name="Name">
                        <Description>Name of the attribute to use for sorting the results.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Attribute>
    </OrderBy>
</InventoryAudit>
