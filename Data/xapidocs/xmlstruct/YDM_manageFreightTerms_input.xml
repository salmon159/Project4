<FreightTerms Action="" ChargesPaidBy="" ConsiderBuyersRoutingGuide=""
    Description="" EnterpriseCode="Required" FreightTerms="Required"
    FreightTermsKey="" ShortDescription="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/manageFreightTerms/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Text-8" Name="Action">
                <Description>
                    <div>
                        <br/>Valid Values:
						<br/>
                        <b>
                            <code>CREATE</code>
                        </b> - Creates a Record in YFS_FREIGHT_TERMS table.
						<br/>
                        <b>
                            <code>MODIFY</code>
                        </b> - Modifies a record in YFS_FREIGHT_TERMS table.
						<br/>
                        <b>
                            <code>DELETE</code>
                        </b> - Deletes a record in YFS_FREIGHT_TERMS table.
						<br/>
                        <b>Default Action is <code>MODIFY</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-8" Name="ChargesPaidBy">
                <Description>
                    <div>
                        <br/>Valid Values:
						<br/>
                        <b>
                            <code>BYUER</code>
                        </b>
                        <br/>
                        <b>
                            <code>SHIPPER</code>
                        </b>
                        <br/>If no value is passed then BYUER is DEFAULTED
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="ConsiderBuyersRoutingGuide">
                <Description>
                    <div>
                        <br/>Valid Values:
						<br/>
                        <b>
                            <code>Y</code>
                        </b> - Sets the Transportation_Preference field in YFS_FREIGHT_TERMS to "01"
						<br/>
                        <b>
                            <code>N</code>
                        </b> - Sets the Transportation_Preference field in YFS_FREIGHT_TERMS to "02"
						<br/>If no value is passed then "02" is defaulted
						</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</FreightTerms>
