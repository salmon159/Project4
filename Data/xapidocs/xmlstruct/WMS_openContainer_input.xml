<?xml version="1.0" encoding="UTF-8"?>
<OpenContainer CaseId="" EnterpriseCode="Required"
    IgnoreExistingContainer="" LocationId="" Node="Required" PalletId=""
    TargetLocationId="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/openContainer/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>
                    <div> A case identifier. 
					<Validation>
                            <Condition> Both<code>CaseId</code> and <code>PalletId</code> should not be passed.
						</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                        <Validation>
                            <Condition> Either <code>CaseId</code> or <code>PalletId</code> should be passed.
						</Condition>
                            <Error
                                Description="CaseId/PalletId not passed in the input." ErrorCode="WMS00011"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="EnterpriseCode">
                <Description>
                    <div> Enterprise Code for which the case is created in inventory. 
				<Validation>
                            <Condition> Mandatory.
				</Condition>
                            <Error
                                Description="EnterpriseCode has to be      passed in the input" ErrorCode="WMS00030"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreExistingContainer">
                <Description>
                    <div> If this flag is passed as 'N' then this Api will throw error if the LPN already exists.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="LocationId" Name="LocationId">
                <Description>
                    <div> The location where the license plate is created.
				<Validation>
                            <Condition> Must be a valid <code>Node</code> and <code>LocationId</code> combination.
				</Condition>
                            <Error
                                Description="Invalid Node and LocationId      combination" ErrorCode="WMS00037"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="Node">
                <Description>
                    <div> The shipnode where the license plate is created.
				<Validation>
                            <Condition> Mandatory.
				</Condition>
                            <Error Description="Node not passed." ErrorCode="WMS00054"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>
                    <div>A pallet identifier.
						<Validation>
                            <Condition> Both<code>CaseId</code> and <code>PalletId</code> should not be passed.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                        <Validation>
                            <Condition> Either <code>CaseId</code> or <code>PalletId</code> should be passed.
							</Condition>
                            <Error
                                Description="CaseId/PalletId not passed in the input." ErrorCode="WMS00011"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LocationId" Name="TargetLocationId">
                <Description>
                    <div> Location to which the container should move the Inventory.
				<Validation>
                            <Condition> If Passed it must be a valid <code>Node</code> and <code>LocationId</code> combination.
				</Condition>
                            <Error
                                Description="Invalid Node and LocationId      combination" ErrorCode="WMS00037"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</OpenContainer>
