<WorkOrder AdditionalQuantityRequested=""
    ConsiderSupplementalCapacity="" DelayAppointmentTaking=""
    EnterpriseCode="Conditional" FinishNoLaterThan=""
    IsMultiApptRequired="" IsPlanningComplete=""
    IsQuantityRequestedOverridden="" NodeKey="Conditional"
    OverrideModificationRules="" PreCallStatus="" Priority=""
    PromisedApptEndDate="" PromisedApptStartDate=""
    QuantityRequested="Conditional" RetainAnswerSets=""
    ServiceLocationAddressKey="" ServiceSupervisorId=""
    StartNoEarlierThan="" WorkOrderKey="Conditional"
    WorkOrderNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/VAS/modifyWorkOrder/input">
    <yfc:includeInParent IncludeXMLFileName="YFS_Work_Order_keys.xml" IncludeXMLPath="/WorkOrder"/>
    <yfc:doc>
        <Element EntityName="Work_Order">
            <Description>
                <div>The work order to be modified. Pass  <code>WorkOrderKey</code>
                    <i>or</i>
                    <code>WorkOrderNo</code>, <code>NodeKey</code> and <code>EnterpriseCode</code>  to identify the work order.
			<Validation>
                        <Condition>The passed attributes are unable to identify an existing work order.
				</Condition>
                        <Error Description="Invalid Work Order." ErrorCode="VAS00013"/>
                    </Validation>
                    <Validation>
                        <Condition>Fulfillment type on the service lines are not the same. 
					Lines with different fulfillment types can never be fulfilled
					together.</Condition>
                        <Error
                            Description="Fulfillment Type on the service lines should be same." ErrorCode="VAS00065"/>
                    </Validation>
                    <Validation>
                        <Condition>Service addresses are different for the lines and the work 
					order. Lines with different addresses can never be fulfilled
					together.</Condition>
                        <Error
                            Description="Mismatch on the service addresses of work order and the lines added to the work order." ErrorCode="VAS00052"/>
                    </Validation>
                    <Validation>
                        <Condition>Ship nodes are different for the lines and the work order.
					Lines with different ship nodes can never be fulfilled together.</Condition>
                        <Error
                            Description="Mismatch on the ship nodes of work order and the lines added to the work order." ErrorCode="VAS00049"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="String" Name="AdditionalQuantityRequested"/>
            <Attribute DataType="Flag" Name="ConsiderSupplementalCapacity">
                <Description>
                    <div>Indicates whether the supplemental capacity should be added to the normal capacity for availability calculation while allocating appointments within the work order.
					<br/>If <code>ConsiderSupplementalCapacity</code> flag is not passed as <b>
                            <code>Y</code>
                        </b>, then the value of the <code>CanConsumeSupplementalCapacity</code> attribute of the customer (if existed) in the order will be used.
					<br/>This value is not persisted in the system and must be passed while allocating new appointments or changing existing appointments.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Work_Order" Name="FinishNoLaterThan" NameInEntity="Finish_No_Later_Than">
                <Description>
                    <div>Pass through fields to be stamped on each task.
						<Validation>
                            <Condition>Passed value should not be less than the <code>StartNoEarlierThan</code> date.</Condition>
                            <Error
                                Description="Finish no later than date cannot be before Start no earlier than date." ErrorCode="VAS00044"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="OverrideModificationRules">
                <Description>
                    <div>Pass as <b>
                            <code>Y</code>
                        </b> to  overrides the statuses of work order that can be considered for various modifications.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Work_Order" Name="PreCallStatus" NameInEntity="Pre_Call_Status">
                <Description>
                    <div>Status of the pre-call process.
						<div
                            class="innerpanel" width="30%">
                            <table border="0" cellpadding="0"
                                cellspacing="0" class="table">
                                <thead>
                                    <tr>
                                    <td class="tablecolumnheader">PreCallStatus</td>
                                    <td class="tablecolumnheader">Meaning</td>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                    <td class="tablecolumn">NOT_APPLICABLE</td>
                                    <td class="tablecolumn">Pre-call process is not applicable.</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">NOT_REQUIRED</td>
                                    <td class="tablecolumn">Pre-call process is not required.</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">PENDING</td>
                                    <td class="tablecolumn">Pre-call process is pending.</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">SUCCESS</td>
                                    <td class="tablecolumn">Successful pre-call process is done.</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn">FAILURE</td>
                                    <td class="tablecolumn">Failed pre-call process.</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Work_Order" Name="Priority" NameInEntity="Priority">
                <Description>
                    <div>An integer priority, Requests will be processed in the order of this priority. System defined Common Code with CodeType <code>PRIORITY</code>. If priority is not passed, then it is defaulted to value <code>
                            <b>3</b>
                        </code> i.e. <code>Normal</code>
                        <br/>Valid values - <code>
                            <b>1</b>
                        </code>, <code>
                            <b>2</b>
                        </code>, <code>
                            <b>3</b>
                        </code>, <code>
                            <b>4</b>
                        </code>. These values correspond to <code>Urgent</code>, <code>High</code>, <code>Normal</code> and <code>Low</code> values respectively.
						<Validation>
                            <Condition>
                                <code>Priority</code> is not a valid priority.</Condition>
                            <Error
                                Description="YFS:Invalid Priority No." ErrorCode="YFS10017"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Work_Order"
                Name="PromisedApptEndDate" NameInEntity="Promised_Appt_End_Date">
                <Description>
                    <div>Promised End date of the Appointment. Passed while changing appointment in case of provided service or delivery service work order.
						<Validation>
                            <Condition>Passed value should not be less than the <code>PromisedApptStartDate</code>.</Condition>
                            <Error
                                Description="Appointment end date cannot be before appointment start date." ErrorCode="VAS00045"/>
                        </Validation>
                        <Validation>
                            <Condition>If the date corresponds to a slot which cannot be used for appointment planning.	</Condition>
                            <Error
                                Description="Slot based Capacity Consumption cannot be done on slots that cannot be used for taking appointments." ErrorCode="INV80_002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" EntityName="Order_Line"
                Name="PromisedApptStartDate" NameInEntity="Promised_Appt_Start_Date">
                <Description>
                    <div>Promised Start date of the Appointment. Passed while changing appointment in case of provided service or delivery service work order.
				<Validation>
                            <Condition>If the date corresponds to a slot which cannot be used for appointment planning.
					</Condition>
                            <Error
                                Description="Slot based Capacity Consumption cannot be done on slots that cannot be used for taking appointments." ErrorCode="INV80_002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="RetainAnswerSets">
                <Description>This flag indicates if upon service location address change, the answer sets for the old address should be copied to be used for the new address.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <WorkOrderActivities Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>
                        <div>Indicates if information in the database must be replaced by the information specified in this node in this run of the API.
						<br/>Default: <b>
                                <code>N</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <WorkOrderActivity Action="" ActivityCode="" ActivitySeqNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Work_Order_Activity"/>
                <Attributes>
                    <Attribute DataType="ID-40" Name="Action">
                        <Description>
                            <div>Indicates the action that can be perfomed.
						<br/>Default: <b>
                                    <code>Create-Modify</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Create</code>
                                </b>, <b>
                                    <code>Modify</code>
                                </b> or <b>
                                    <code>Delete</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Work_Order_Activity"
                        Name="ActivitySeqNo" NameInEntity="Activity_Seq_No">
                        <Description>
                            <div>Identifies the sequence in which this activity needs to be performed.
							<Validation>
                                    <Condition>Should be unique for a Work Order.</Condition>
                                    <Error
                                    Description="Duplicate Activity Sequence No." ErrorCode="VAS00009"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <WorkOrderActivityDtls Reset="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Reset">
                            <Description>
                                <div>Indicates if information in the database must be replaced by the information specified in this node in this run of the API.
							<br/>Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <WorkOrderActivityDtl Action="" ActivityLocationId=""
                    QuantityRequested="" yfc:DTDOccurrence="OneOrMany">
                    <yfc:doc>
                        <Element EntityName="Work_Order_Activity_Dtl"/>
                        <Attributes>
                            <Attribute DataType="ID-40" Name="Action">
                                <Description>
                                    <div>Indicates the action that can be perfomed.
							<br/>Default: <b>
                                    <code>Create-Modify</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Create</code>
                                    </b>, <b>
                                    <code>Modify</code>
                                    </b> or <b>
                                    <code>Delete</code>
                                    </b>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </WorkOrderActivityDtl>
            </WorkOrderActivityDtls>
            <WorkOrderActivityInstructions Reset="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Reset">
                            <Description>
                                <div>Indicates if information in the database must be replaced by the information specified in this node in this run of the API.
						<br/>Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Instruction Action="" InstructionDetailKey=""
                    InstructionText="" InstructionType=""
                    InstructionURL="" InstructionUsage="" SequenceNo="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="ID-40" Name="Action">
                                <Description>
                                    <div>Indicates the action that can be perfomed.
							<br/>Default: <b>
                                    <code>Create-Modify</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Create</code>
                                    </b>, <b>
                                    <code>Modify</code>
                                    </b> or <b>
                                    <code>Delete</code>
                                    </b>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Instruction>
            </WorkOrderActivityInstructions>
        </WorkOrderActivity>
    </WorkOrderActivities>
    <WorkOrderServiceLines>
        <WorkOrderServiceLine Action="" CauseAppointmentChange=""
            LineNo="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element EntityName="Work_Order_Service_Line"/>
                <Attributes>
                    <Attribute DataType="ID-40" Name="Action">
                        <Description>
                            <div>Indicates the action that can be perfomed. If action is Create or blank, Order Line element needs to be passed. Line No is generated if not passed. If action is Delete, LineNo is mandatory.
								<br/>Default: <b>
                                    <code>Create</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Create</code>
                                </b> or <b>
                                    <code>Delete</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="CauseAppointmentChange">
                        <Description>
                            <div>
									The <code>CauseAppointmentChange</code> flag value in the input xml is considered only when the enterprise is configured to maintain this flag externally.								
									<br/>Valid Values: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <OrderLine OrderLineKey="" PrimeLineNo="" SubLineNo="" yfc:DTDOccurrence="REQUIRED">
                <Order DocumentType="" EnterpriseCode=""
                    OrderHeaderKey="" OrderNo=""/>
            </OrderLine>
        </WorkOrderServiceLine>
    </WorkOrderServiceLines>
    <WorkOrderProdDeliveries>
        <WorkOrderProdDelivery Action="" CauseAppointmentChange=""
            LineNo="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element EntityName="Work_Order_Prod_Del"/>
                <Attributes>
                    <Attribute DataType="ID-40" Name="Action">
                        <Description>
                            <div>Indicates the action that can be perfomed. If action is Create or blank, Order Line element needs to be passed. Line No is generated if not passed. If action is Delete, LineNo is mandatory.
								<br/>Default: <b>
                                    <code>Create</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Create</code>
                                </b> or <b>
                                    <code>Delete</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="CauseAppointmentChange">
                        <Description>
                            <div>
									The <code>CauseAppointmentChange</code> flag value in the input xml is considered only when the enterprise is configured to maintain this flag externally.
									<br/>Default: <b>
                                    <code>N</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <OrderLine OrderLineKey="" PrimeLineNo="" SubLineNo="" yfc:DTDOccurrence="REQUIRED">
                <Order DocumentType="" EnterpriseCode=""
                    OrderHeaderKey="" OrderNo=""/>
            </OrderLine>
        </WorkOrderProdDelivery>
    </WorkOrderProdDeliveries>
    <WorkOrderAppointments>
        <WorkOrderAppointment Action="" AdditionalEquipmentUsed=""
            AlertSales="" AlertScheduler="" ApptSeq=""
            AttentionRequired="" ConsiderCurrentSlot=""
            ExpectedApptEndDate="" ExpectedApptStartDate=""
            IgnoreMinNotificationTime="" IsAppointmentOverridden=""
            IsRoutePlanningComplete="" ItemsNotUsed=""
            NonInventoryItemsReturned="" PromisedApptEndDate=""
            PromisedApptStartDate="" RequestedQuantity=""
            RequestedVolume="" RequestedWeight="" ResourcePoolId=""
            ServiceResourceId="" StopNumber="" VehicleNumber=""
            VehicleOdometerReading="" VehicleServiceRequest="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element EntityName="Work_Order_Appt"/>
                <Attributes>
                    <Attribute DataType="ID-40" Name="Action">
                        <Description>
                            <div>Indicates the action that can be perfomed. If action is Create-Modify, the appointment will be created if it does not already exists, else it will be modified. ApptSeq is generated if not passed. If action is Cancel, the appointment will be moved to Cancel Status, and the resource allocations will be removed.
								<br/>Default: <b>
                                    <code>Create-Modify</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Create-Modify</code>
                                </b> or <b>
                                    <code>Cancel</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="ConsiderCurrentSlot">
                        <Description>
                            <div>
									The <code>ConsiderCurrentSlot</code> flag indicates that system should consider the available capacity of the current slots, which the current timestamp falls under.
									<br/>Default: <b>
                                    <code>N</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IgnoreMinNotificationTime">
                        <Description>
                            <div>
              If set to Y, minimum notification time will be ignored for product and services.  If not passed, assumed N.
              </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="RequestedVolume">
                        <Description>
                            <div>The quantity requested in the work order's volume unit of measure, which is specified in the <code>VolumeUOM</code> field of the work order</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="RequestedWeight">
                        <Description>
                            <div>The quantity requested in the work order's weight unit of measure, which is specified in the <code>WeightUOM</code> field of the work order</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <WorkOrderAppointmentUsers Reset="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Reset">
                            <Description>
                                <div>
						  If <code>Reset</code> is passed as <b>
                                    <code>Y</code>
                                    </b>, the old appointment users are removed and replaced with the new ones.   
						  </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <WorkOrderAppointmentUser Action="" UserId="" yfc:DTDOccurrence="OneOrMany">
                    <yfc:doc>
                        <Element EntityName="Wo_Appt_User"/>
                        <Attributes>
                            <Attribute DataType="ID-40" Name="Action">
                                <Description>
                                    <div>Indicates the action that can be perfomed. If action is Delete, the user will be removed.
								<br/>Default: <b>
                                    <code>Create</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Create</code>
                                    </b> or <b>
                                    <code>Delete</code>
                                    </b>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </WorkOrderAppointmentUser>
            </WorkOrderAppointmentUsers>
        </WorkOrderAppointment>
    </WorkOrderAppointments>
    <Instructions>
        <Instruction InstructionText="" InstructionType=""
            InstructionURL="" InstructionUsage="" SequenceNo="" yfc:DTDOccurrence="OneOrMany"/>
    </Instructions>
    <WorkOrderServiceTools>
        <WorkOrderServiceTool Action="" Comments="" ItemID=""
            QuantityRequired="" UnitOfMeasure="" WorkOrderKey=""
            WorkOrderServiceToolKey="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element EntityName="Work_Order_Service_Tool"/>
                <Attributes>
                    <Attribute DataType="ID-40" Name="Action">
                        <Description>
                            <div>Indicates the action that can be perfomed. If action is Delete, the service tool will be removed.
								<br/>Default: <b>
                                    <code>Create-Modify</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Create-Modify</code>
                                </b> or <b>
                                    <code>Delete</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </WorkOrderServiceTool>
    </WorkOrderServiceTools>
    <WorkOrderStatusAudit AuditTransactionId="" AuditUserId=""
        Comments="" ReasonCode="" WorkOrderKey="" WorkOrderStatusAuditKey="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Work_Order_Status_Audit_tran.xml" IncludeXMLPath="/WorkOrderStatusAudit"/>
        <yfc:doc>
            <Element EntityName="Work_Order_Status_Audit"/>
        </yfc:doc>
    </WorkOrderStatusAudit>
    <WorkOrderHoldTypes>
        <WorkOrderHoldType HoldType="Required" ReasonText="" Status="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Wo_Hold_Type">
                    <Description>
                        <div>
			Optional. Stores hold type information. Multiple <code>HoldType</code>s can be applied/ removed for a single work order.
			</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute Name="HoldType">
                        <Description>
                            <div>
					The code identifying the hold type to be applied/ removed for a Work Order.
					<Validation>
                                    <Condition>
                                    <code>HoldType</code> must be defined in the configuration
						</Condition>
                                    <Error
                                    Description="Hold Type is not setup." ErrorCode="OMP11035"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute Name="Status">
                        <Description>
                            <div>
					   This attribute specifies the status of the order hold type.  
			<br/>Valid Values: 
				<br/>
                                <b>
                                    <code>1100</code>
                                </b> - To apply a <code>HoldType</code> to order.
				<br/>
                                <b>
                                    <code>1200</code>
                                </b> - To reject a  <code>HoldType</code> from order.
				<br/>
                                <b>
                                    <code>1300</code>
                                </b> - To resolve a  <code>HoldType</code> from order.
			<br/>Default: Hold type is always created in '1100' status.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </WorkOrderHoldType>
    </WorkOrderHoldTypes>
    <WorkOrderNotesList>
        <WorkOrderNotes ContactReference="" ContactTime=""
            ContactType="" ContactUser="" NoteText="Required"
            ReasonCode="" VisibleToAll="" yfc:DTDOccurrence="ZeroOrMany"/>
    </WorkOrderNotesList>
