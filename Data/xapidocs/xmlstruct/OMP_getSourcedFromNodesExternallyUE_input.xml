<Promise CarrierServiceCode="" CustomerID="" DemandType=""
    DistanceToConsider="" DistanceUOMToConsider="" EnterpriseCode=""
    IgnorePromised="" LevelOfService="" OrderReference=""
    OrganizationCode="" ReceivingNode="" SCAC="" ScacAndServiceKey=""
    Segment="" SegmentType="" xmlns="http://www.sterlingcommerce.com/documentation/OMP/getSourcedFromNodesExternallyUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Text-40" Name="CarrierServiceCode">
                <Description>
                    <div>
				Carrier service code. If transfer schedule is present between two nodes , the transfer schedule's transit days are used to calculate the transit time, otherwise <code>CarrierServiceCode</code> is used for calculating the transit time.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-24" Name="CustomerID">
                <Description>
                    <div>
				Indicates the Customer ID on which behalf inquiry is being made.  Customer preferences are used in capacity computations.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="DemandType">
                <Description>
                    <div>
				Specifies a <code>DemandType</code> used in the inventory availability calculations.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Distance" Name="DistanceToConsider">
                <Description>
                    <div>
				Indicates radius within which nodes can be considered for final leg sourcing.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="DistanceUOMToConsider">
                <Description>
                    <div>
				Indicates radius unit of measure.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="EnterpriseCode">
                <Description>
                    <div>
						The <code>EnterpriseCode</code> on which behalf inquiry is being made. 
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnorePromised">
                <Description>
                    <div>If <code>IgnorePromised</code> is passed as <b>
                            <code>Y</code>
                        </b>, promised demands are not considered during inventory availability calculations.
				<BR/>Default: <code>N</code>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ID-40" Name="LevelOfService">
                <Description>
                    <div>
					Determines the level of service to be used for the lines if not passed on the lines.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="OrderReference"/>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>
			Inventory is being checked on behalf of the organization specified by <code>OrganizationCode</code>.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="ShipNode" Name="ReceivingNode">
                <Description>
                    <div>If the <code>AllocationRule</code>'s  <code>UseGeography</code> flag is set to <b>
                            <code>Y</code>
                        </b>, the <code>ReceivingNode</code>'s location is used to calculate the priority of the <code>Option</code>.				
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="SCAC">
                <Description>
                    <div>
				Carrier to be used
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="ScacAndServiceKey">
                <Description>
                    <div>
				Scac And Service Key
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Segment" Name="Segment">
                <Description>
                    <div>
				Inventory segment
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="SegmentType" Name="SegmentType">
                <Description>
                    <div>
				Inventory Segment type
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ShipToAddress AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" EveningFaxNo=""
        EveningPhone="" FirstName="" IsCommercialAddress="" JobTitle=""
        LastName="" MiddleName="" MobilePhone="" OtherPhone=""
        PersonID="" State="" Suffix="" TaxGeoCode="" Title="" ZipCode="">
        <yfc:doc>
            <Element EntityName="Person_Info"/>
        </yfc:doc>
    </ShipToAddress>
    <PromiseLines TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="TotalNumberOfRecords"/>
            </Attributes>
        </yfc:doc>
        <PromiseLine BundleParentLineId="" CarrierServiceCode=""
            DepartmentCode="" FreightTerms="" GiftWrap="" GroupID=""
            ItemID="" KitCode="" KitQty="" LevelOfService="" LineId=""
            OrderLineReference="" OriginalLineQuantity=""
            ReceivingNode="" RequiredQty="" SCAC="" ScacAndServiceKey=""
            Segment="" SegmentType="" ShipNode="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Order_Line">
                    <Description>
                        <div>
							This table stores order line information, including item details
							and shipping information.
							Multiple lines can be created for a
							single order defined in the
							<code>YFS_ORDER_HEADER</code>
							table.
						</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="BundleParentLineId"/>
                    <Attribute DataType="String" Name="GroupID"/>
                    <Attribute DataType="Text-40" Name="LineId">
                        <Description>
                            <div>
								Unique identifier for the line. 
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="OrderLineReference"/>
                    <Attribute DataType="Quantity" Name="OriginalLineQuantity">
                        <Description>
                            <div>
								Total Required Quantity of Line.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="RequiredQty">
                        <Description>
                            <div>
								Required Quantity.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="ScacAndServiceKey">
                        <Description>
                            <div>
								Scac And Service Key
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="ShipNode" Name="ShipNode">
                        <Description>
                            <div>Shipping node. </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ShipToAddress AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" EveningFaxNo="" EveningPhone=""
                FirstName="" IsCommercialAddress="" JobTitle=""
                LastName="" MiddleName="" MobilePhone="" OtherPhone=""
                PersonID="" State="" Suffix="" TaxGeoCode="" Title="" ZipCode="">
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                </yfc:doc>
            </ShipToAddress>
            <SourcingRuleDetail DistributionRuleId="" FromNodeKey=""
                FutureInventoryWindow="" ProcureToShipAllowed=""
                SeqNo="" SubstitutionAllowed="" WorkOrderCreationAllowed="">
                <yfc:doc>
                    <Element EntityName="Sourcing_Rule_Detail"/>
                </yfc:doc>
                <SourcingTemplate>
                    <AdditionalData ExternallyDefinedReasonCode=""
                        IdleAsset="" Radius="" RadiusUOM="">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>
							Additional flags and information to use when sourcing.
						</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Text-40" Name="ExternallyDefinedReasonCode">
                                    <Description>
                                    <div>
									 Reason Code to determine nodes for sourcing externally.
									</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="IdleAsset">
                                    <Description>
                                    <div>
								If set to <code>
                                    <b>Y</b>
                                    </code>, indicates that only nodes where the item being sourced is an idle asset should be considered.  Default value is <code>
                                    <b>N</b>
                                    </code>.
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Number" Name="Radius">
                                    <Description>
                                    <div>
								Indicates the radius from the ship to address that a node must fall within to be considered for sourcing.  
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute EntityName="Uom"
                                    Name="RadiusUOM" NameInEntity="Uom">
                                    <Description>
                                    <div>
								The unit of measure that the <code>Radius</code> is specified in.
							</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </AdditionalData>
                </SourcingTemplate>
                <SourcingRuleHeader FulfillmentType=""
                    ItemClassification="" ItemClassification2=""
                    ItemClassification3="" ItemGroupCode="" ItemID=""
                    NodeTypeID="" OrderSourcingClassification=""
                    Purpose="" RegionKey="" SellerOrganizationCode="" ToNodeKey="">
                    <yfc:doc>
                        <Element EntityName="Sourcing_Rule_Header"/>
                    </yfc:doc>
                </SourcingRuleHeader>
            </SourcingRuleDetail>
            <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotKeyReference="" LotNumber=""
                ManufacturingDate="" RevisionNo="">
                <yfc:doc>
                    <Element EntityName="Order_Line_Req_Tag"/>
                </yfc:doc>
            </Tag>
        </PromiseLine>
    </PromiseLines>
    <PromiseServiceLines>
        <PromiseServiceLine ItemGroupCode="" ItemID="" LevelOfService=""
            LineId="" OrderLineReference="" ReceivingNode=""
            RequiredCapacity="" RequiredQty="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Order_Line"/>
                <Attributes>
                    <Attribute DataType="Text-40" Name="LineId">
                        <Description>
                            <div>
								Unique identifier for the line. 
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="OrderLineReference"/>
                    <Attribute DataType="String" Name="RequiredCapacity"/>
                    <Attribute DataType="Quantity" Name="RequiredQty">
                        <Description>
                            <div>
								Required Quantity.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ShipToAddress AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" EveningFaxNo="" EveningPhone=""
                FirstName="" IsCommercialAddress="" JobTitle=""
                LastName="" MiddleName="" MobilePhone="" OtherPhone=""
                PersonID="" State="" Suffix="" TaxGeoCode="" Title="" ZipCode="">
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                </yfc:doc>
            </ShipToAddress>
            <SourcingRuleDetail DistributionRuleId="" FromNodeKey=""
                FutureInventoryWindow="" ProcureToShipAllowed=""
                SeqNo="" SubstitutionAllowed="" WorkOrderCreationAllowed="">
                <yfc:doc>
                    <Element EntityName="Sourcing_Rule_Detail"/>
                </yfc:doc>
                <SourcingTemplate>
                    <AdditionalData ExternallyDefinedReasonCode=""
                        IdleAsset="" Radius="" RadiusUOM="">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>
							Additional flags and information to use when sourcing.
						</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Text-40" Name="ExternallyDefinedReasonCode">
                                    <Description>
                                    <div>
									 Reason Code to determine nodes for sourcing externally.
									</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="IdleAsset">
                                    <Description>
                                    <div>
								If set to <code>
                                    <b>Y</b>
                                    </code>, indicates that only nodes where the item being sourced is an idle asset should be considered.  Default value is <code>
                                    <b>N</b>
                                    </code>.
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Number" Name="Radius">
                                    <Description>
                                    <div>
								Indicates the radius from the ship to address that a node must fall within to be considered for sourcing.  
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute EntityName="Uom"
                                    Name="RadiusUOM" NameInEntity="Uom">
                                    <Description>
                                    <div>
								The unit of measure that the <code>Radius</code> is specified in.
							</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </AdditionalData>
                </SourcingTemplate>
                <SourcingRuleHeader FulfillmentType=""
                    ItemClassification="" ItemClassification2=""
                    ItemClassification3="" ItemGroupCode="" ItemID=""
                    NodeTypeID="" OrderSourcingClassification=""
                    Purpose="" RegionKey="" SellerOrganizationCode="" ToNodeKey="">
                    <yfc:doc>
                        <Element EntityName="Sourcing_Rule_Header"/>
                    </yfc:doc>
                </SourcingRuleHeader>
            </SourcingRuleDetail>
        </PromiseServiceLine>
    </PromiseServiceLines>
    <ServiceAssociations>
        <ServiceAssociation ProductLineId="" ServiceLineId=""
            ServiceTimeOffsetHours="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Order_Prod_Ser_Assoc"/>
                <Attributes>
                    <Attribute DataType="Text-40" Name="ProductLineId">
                        <Description>
                            <div>
								Unique identifier for the Product Line.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ServiceLineId">
                        <Description>
                            <div>
								Unique identifier for the Service Line. 
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="ServiceTimeOffsetHours">
                        <Description>
                            <div>
								Specifies the time difference needed in Hours between the
								delivery of the	Product and performing the Service on it.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ServiceAssociation>
    </ServiceAssociations>
</Promise>
