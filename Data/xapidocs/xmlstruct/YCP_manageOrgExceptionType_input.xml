<!-- YCP_manageOrgExceptionType_Input.xml -->
<OrgExceptionType Action="" Active="" ExceptionType=""
    OrganizationCode="" Priority="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageOrgExceptionType/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Action" Name="Action">
                <Description>
                    <div>Action to take
					<br/>Valid Values: 
					<br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist or modifies an existing record if it does.
					<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
					<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Changes the value of an existing record.
					<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
					<br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</OrgExceptionType>
