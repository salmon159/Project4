<!-- modifyCustomer Input XML -->
<Customer BuyerOrganizationCode="" CanConsumeSupplementalCapacity=""
    CommitmentTime="" CustomerClassificationCode="" CustomerID=""
    CustomerKey="" ExternalCustomerID="" FuncAckTime=""
    OrganizationCode="" SendsAsn="" SendsCommitment="" SendsFuncAck=""
    SlotPreferenceType="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/modifyCustomer/input">
    <yfc:doc>
        <Element EntityName="Customer">
            <Description>
                <div>
				Specify the customer information to be modified.  <code>CustomerKey</code> or a combination of <code>OrganizationCode</code> and <code>CustomerID</code> can be specified to identify a customer.
				<Validation>
                        <Condition>If no customer matches the criteria.
					</Condition>
                        <Error Description="No Record Found." ErrorCode="YFS10001"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Customer" Name="SlotPreferenceType" NameInEntity="Slot_Preference_Type">
                <Description>
                    <div>
				Slot preference type of the customer.  Valid values are: <code>
                            <b>HARD</b>
                        </code> - Only the preferred slots are to be considered.  <code>
                            <b>SOFT</b>
                        </code> - Other slots will also be considered.  <code>
                            <b>NONE</b>
                        </code> - No preferences.  Default value: <code>
                            <b>NONE</b>
                        </code>
                        <Validation>
                            <Condition>
                                <code>SlotPreferenceType</code> is not a valid value.
					</Condition>
                            <Error
                                Description="Invalid SlotPreferenceType." ErrorCode="YCP73_00022"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>SlotPreferenceType</code> is <code>
                                    <b>NONE</b>
                                </code> and <code>CustomerSlotPreferenceList</code> is passed.
					</Condition>
                            <Error
                                Description="Cannot pass CustomerSlotPreferenceList for NONE SlotPreferenceType." ErrorCode="YCP73_00023"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Consumer>
        <yfc:doc>
            <Element EntityName="Consumer">
                <Description>
                    <div>
				Identify the consumer which customer represents.  <code>Consumer</code> is required if customer is a consumer customer.  
				<Validation>
                            <Condition>If <code>Consumer</code> element is not provided.
					</Condition>
                            <Error Description="Missing element in XML." ErrorCode="YFS10507"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
        <BillingPersonInfo AddressID="" AddressLine1="" AddressLine2=""
            AddressLine3="" AddressLine4="" AddressLine5=""
            AddressLine6="" AlternateEmailID="" Beeper="" City=""
            Company="" Country="" DayFaxNo="" DayPhone="" Department=""
            EMailID="" ErrorTxt="" EveningFaxNo="" EveningPhone=""
            FirstName="" HttpUrl="" IsCommercialAddress="" JobTitle=""
            LastName="" Latitude="" Longitude="" MiddleName=""
            MobilePhone="" OtherPhone="" PersonID=""
            PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
            Title="" UseCount="" VerificationStatus="" ZipCode="">
            <yfc:doc>
                <Element EntityName="Person_Info">
                    <Description>
                        <div>
				Billing information of a consumer.  <code>BillingPersonInfo</code> is required for a customer.
				<Validation>
                                <Condition>If <code>BillingPersonInfo</code> is not provided.
					</Condition>
                                <Error
                                    Description="Missing BillingPersonInfo." ErrorCode="YCP0214"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Person_Info" Name="PersonID" NameInEntity="Person_ID">
                        <Description>
                            <div>
				ID identifying a person information
				<Validation>
                                    <Condition>If <code>CustomerID</code> is blank and <code>PersonID</code> is not provided.
					</Condition>
                                    <Error
                                    Description="Missing attribute." ErrorCode="YFS10460"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </BillingPersonInfo>
    </Consumer>
    <CustomerSlotPreferenceList Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>Indicates if all existing child node definition in the database need to be deleted and replace with the provided child nodes.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <CustomerSlotPreference EndTime="" FridayPreferred=""
            MondayPreferred="" Operation="" SaturdayPreferred=""
            StartTime="" SundayPreferred="" ThursdayPreferred=""
            TuesdayPreferred="" WednesdayPreferred="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
					Identify the preferred day of week and time slot that the customer would like to consider for appointment planning.
						<Validation>
                                <Condition>
                                    <code>StartTime</code> or <code>EndTime</code> is not passed.
								</Condition>
                                <Error
                                    Description="StartTime and EndTime must be passed." ErrorCode="YCP73_00024"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Action" Name="Operation">
                        <Description>
                            <div>
					 Valid value is <b>
                                    <code>Delete</code>
                                </b> or blank.  If not provided the information will be created or modified.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </CustomerSlotPreference>
    </CustomerSlotPreferenceList>
    <CustomerSchedulingPreferences CancelOrderOnBackorder="Y|N"
        IsDropShipAllowed="Y|N" IsLineShipComplete="Y|N"
        IsLineShipSingleNode="Y|N" IsShipComplete="Y|N"
        IsShipSingleNode="Y|N" IsSubstitutionAllowed="Y|N" OptimizationType="">
        <yfc:doc>
            <Attributes>
                <Attribute Name="OptimizationType">
                    <Description>This value indicates indicates what scheduling will first optimize on, for this customer.  If not specified scheduling rule will be used to determine optimization type.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </CustomerSchedulingPreferences>
    <CustomerContactList>
        <CustomerContact Company="" CustomerContactID="Required"
            DateOfBirth="" DayFaxNo="" DayPhone="" EMailID=""
            EveningFaxNo="" EveningPhone="" FirstName="" LastName=""
            MiddleName="" MobilePhone="" SpouseDateOfBirth="" Title=""
            WeddingAnniversaryDate="" yfc:DTDOccurrence="ZeroOrMany"/>
    </CustomerContactList>
    <CustomerAdditionalAddressList>
        <CustomerAdditionalAddress AddressType=""
            CustomerAdditionalAddressID="Required" yfc:DTDOccurrence="ZeroOrMany">
            <PersonInfo AddressID="" AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" ErrorTxt="" EveningFaxNo=""
                EveningPhone="" FirstName="" HttpUrl="" JobTitle=""
                LastName="" Latitude="" Longitude="" MiddleName=""
                MobilePhone="" OtherPhone="" PersonID=""
                PreferredShipAddress="" State="" Suffix="" Title=""
                UseCount="" VerificationStatus="" ZipCode=""/>
        </CustomerAdditionalAddress>
    </CustomerAdditionalAddressList>
    <CustomerPaymentMethodList>
        <CustomerPaymentMethod AccountLimit="" AccountLimitCurrency=""
            CreditCardExpDate="" CreditCardName="" CreditCardNo=""
            CreditCardType="" CustomerAccountNo="" DebitCardNo=""
            DisplayCreditCardNo="" DisplayCustomerAccountNo=""
            DisplayDebitCardNo="" DisplayPaymentReference1=""
            PaymentReference1="" PaymentReference2=""
            PaymentReference3="" PaymentType="Required" yfc:DTDOccurrence="ZeroOrMany">
            <PersonInfoBillTo AddressID="" AddressLine1=""
                AddressLine2="" AddressLine3="" AddressLine4=""
                AddressLine5="" AddressLine6="" AlternateEmailID=""
                Beeper="" City="" Company="" Country="" DayFaxNo=""
                DayPhone="" Department="" EMailID="" ErrorTxt=""
                EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
                JobTitle="" LastName="" Latitude="" Longitude=""
                MiddleName="" MobilePhone="" OtherPhone="" PersonID=""
                PreferredShipAddress="" State="" Suffix="" Title=""
                UseCount="" VerificationStatus="" ZipCode=""/>
        </CustomerPaymentMethod>
    </CustomerPaymentMethodList>
    <AnswerSets>
        <AnswerSet Action="" AnswerSetMasterKey="" QuestionType="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Answer_Set_Master"/>
                <Attributes>
                    <Attribute DataType="Text-10" Name="Action">
                        <Description>
                            <div>
							Specify <code>Action</code> = <code>
                                    <b>REMOVE</b>
                                </code> to remove the entity. Leave <code>Action</code> blank or don't include it for modifying or creating a new entity.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Answer_Set_Master"
                        Name="QuestionType" NameInEntity="Question_Type">
                        <Description>
                            <div>The question type that this answer set is for. 
							<Validation>
                                    <Condition>
                                    <code>QuestionType</code> not passed.
									</Condition>
                                    <Error
                                    Description="Mandatory attributes for answer set are missing." ErrorCode="YCP73_00005"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Answers Reset="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Reset">
                            <Description>Indicates if all existing child node definition in the database need to be scrapped and have only these definitions supplied in the XML.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Answer Action="" AnswerMasterKey=""
                    AnswerSetMasterKey="" AnswerValue="" QuestionID="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Answer_Master"/>
                        <Attributes>
                            <Attribute DataType="Text-10" Name="Action">
                                <Description>
                                    <div>
								Specify <code>Action</code> = <code>
                                    <b>REMOVE</b>
                                    </code> to remove the entity. Leave <code>Action</code> blank or don't include it for modifying or creating a new entity.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute Name="AnswerValue">
                                <Description>
                                    <div>The answer for the question.
									<Validation>
                                    <Condition>
                                    <code>AnswerValue</code> passed should be a valid answer for the question it belongs to.</Condition>
                                    <Error
                                    Description="Answer is not one of the valid pre-defined answer options." ErrorCode="YCP73_00007"/>
                                    <Error
                                    Description="Answer for this question should be an integer number." ErrorCode="YCP73_00020"/>
                                    <Error
                                    Description="Answer for this question should be a decimal number." ErrorCode="YCP73_00021"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute Name="QuestionID">
                                <Description>
                                    <div>"Identifier for the question that this answer is for.
									<Validation>
                                    <Condition>
                                    <code>QuestionID</code> should be valid.</Condition>
                                    <Error
                                    Description="Question does not exist in the database." ErrorCode="YCP73_00006"/>
                                    <Error
                                    Description="Question should only be answered when parent answer option is selected." ErrorCode="YCP73_00009"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Answer>
            </Answers>
        </AnswerSet>
    </AnswerSets>
</Customer>
