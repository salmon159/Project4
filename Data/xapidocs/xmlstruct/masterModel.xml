<Model Country="" Language="" Name="" Path="" Type="" xmlns="http://www.sterlingcommerce.com/documentation/masterModel">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					A model is a terminal node and represents a configurable product or a reusable building block.  
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Text-120" Name="Country">
                <Description>
                    <div>
						The country identifier for the model.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Language">
                <Description>
                    <div>
						The language identifier for the model.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Name">
                <Description>
                    <div>
						The human-readable model name. 
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-480" Name="Path">
                <Description>
                    <div>
						The configurator path to a model. This path is 'mangled' such that special characters are replaced by their Unicode code preceded by an underscore char.						
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Type">
                <Description>
                    <div>
						The type of model.
						<br/>Valid Values: <b>
                            <code>model</code>
                        </b> or <b>
                            <code>ocg</code>
                        </b> or <b>
                            <code>oig</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Model>
