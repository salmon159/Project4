<!--sendOrder Input XML-->
<Order DocumentType="" EnterpriseCode=""
    IgnoreTransactionDependencies="" OrderHeaderKey="" OrderNo=""
    SelectMethod="WAIT|NO_WAIT" TransactionId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/sendOrder/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="IgnoreTransactionDependencies">
                <Description>
                    <div>Flag to override any transaction dependencies that may be defined for the transaction this API invokes.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>. When passed as <b>
                            <code>Y</code>
                        </b>, transaction dependencies will not be evaluated, and thus will not prevent the API from processing.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-10" Name="SelectMethod">
                <Description>
                    <div>
					This attribute determines whether database select operations are carried out in the <b>
                            <code>WAIT</code>
                        </b> or <b>
                            <code>NO_WAIT mode</code>
                        </b>.
					<br/>Default: <b>
                            <code>NO_WAIT</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TransactionID" Name="TransactionId">
                <Description>
                    <div>
				Identifies the derived transaction from the <code>SEND_ORDER</code> base transaction.
				<Validation>
                            <Condition>
                                <code>TransactionID</code> not passed.
						</Condition>
                            <Error Description="Blank TransactionId." ErrorCode="YFS10171"/>
                        </Validation>
                        <Validation>
                            <Condition>Invalid<code>TransactionID</code> passed.
						</Condition>
                            <Error Description="Invalid TransactionId." ErrorCode="YCP0063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Order>
