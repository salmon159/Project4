<?xml version="1.0" encoding="UTF-8"?>
<!-- Either WaveKey or (WaveNo, Node) is mandatory -->
<Wave Node="" WaveKey="" WaveNo="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/acceptWave/input">
    <yfc:doc>
        <Element EntityName="Wave">
            <Description>
                <div>
                    <Validation>
                        <Condition>
                            <code>WaveKey</code> attribute OR the <code>WaveNo</code> and <code>Node</code> attributes must be passed and must be valid. 
					</Condition>
                        <Error
                            Description="WMS: Wave does not exist for the input passed." ErrorCode="WMS00129"/>
                    </Validation>
                    <Validation>
                        <Condition>
                            <code>Wave</code> should be in <code>Created(1100)</code> or any extended status of <code>Created</code>.
					</Condition>
                        <Error
                            Description="WMS: Wave is not ready for the operation." ErrorCode="WMS00159"/>
                    </Validation>
                </div>
            </Description>
        </Element>
    </yfc:doc>
</Wave>
