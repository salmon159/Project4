<GetLinesToReceive CanAddLines="Y|N" EnterpriseCode="" ShipmentKey=""
    ShipmentNo="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getLinesToReceive/output">
    <yfc:doc>
        <Element EntityName="Shipment"/>
        <Attributes>
            <Attribute DataType="Flag" Name="CanAddLines">
                <Description>Indicates whether you can add lines to the shipment.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ReceivableLineList>
        <ReceivableLine AvailableToReceiveQuantity="" EnterpriseCode=""
            ExpectedQuantity="" ItemID="" KitCode="" KitQty=""
            LineType="" OrderHeaderKey="" OrderLineKey="" OrderNo=""
            OrderReleaseKey="" PrimeLineNo="" ProductClass=""
            ReceivedQuantity="" ReleaseNo="" RequestedSerialNo=""
            RequiresDetailAttributes="Y|N" Segment="" SegmentType=""
            ShipmentLineKey="" ShipmentLineNo="" SubLineNo=""
            SuggestedDispositionCode="" TotalQuantity=""
            UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Shipment_Line"/>
                <Attributes>
                    <Attribute DataType="Quantity" Name="AvailableToReceiveQuantity">
                        <Description>The quantity that can be still received against the line.</Description>
                    </Attribute>
                    <Attribute EntityName="Shipment" Name="EnterpriseCode"/>
                    <Attribute DataType="Quantity" Name="ExpectedQuantity">
                        <Description>If shipment doesn't have lines this is the ordered quantity in the order line. Otherwise it is the actual quantity in the shipment line.</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="LineType"/>
                    <Attribute DataType="Quantity" Name="ReceivedQuantity">
                        <Description>Quantity received for the Line.</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="RequiresDetailAttributes">
                        <Description>Indicates whether the item in the line requires detailed attributes (like Serial Number, Lot Number ... ) while receiving.</Description>
                    </Attribute>
                    <Attribute EntityName="Receipt_Line"
                        Name="SuggestedDispositionCode" NameInEntity="Disposition_Code"/>
                    <Attribute DataType="Quantity" Name="TotalQuantity">
                        <Description>Total Quantity of the Line.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ReceivableLine>
        <KitLines>
            <ReceivableLine AvailableToReceiveQuantity=""
                EnterpriseCode="" ItemID="" KitCode="" KitQty=""
                OrderHeaderKey="" OrderLineKey="" OrderNo=""
                OrderReleaseKey="" PrimeLineNo="" ProductClass=""
                ReceivedQuantity="" ReleaseNo="" RequestedSerialNo=""
                RequiresDetailAttributes="" ShipmentLineKey=""
                ShipmentLineNo="" SubLineNo=""
                SuggestedDispositionCode="" TotalQuantity=""
                UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="AvailableToReceiveQuantity"/>
                        <Attribute DataType="OrgCode" Name="EnterpriseCode"/>
                        <Attribute DataType="ItemID" Name="ItemID"/>
                        <Attribute DataType="Text-20" Name="KitCode"/>
                        <Attribute DataType="Quantity" Name="KitQty"/>
                        <Attribute DataType="Key" Name="OrderHeaderKey"/>
                        <Attribute DataType="Key" Name="OrderLineKey"/>
                        <Attribute DataType="VarcharDocumentNo" Name="OrderNo"/>
                        <Attribute DataType="Key" Name="OrderReleaseKey"/>
                        <Attribute DataType="LineNo" Name="PrimeLineNo"/>
                        <Attribute DataType="ProductClass" Name="ProductClass"/>
                        <Attribute DataType="Quantity" Name="ReceivedQuantity"/>
                        <Attribute DataType="Count" Name="ReleaseNo"/>
                        <Attribute DataType="SerialNo" Name="RequestedSerialNo"/>
                        <Attribute DataType="Flag" Name="RequiresDetailAttributes"/>
                        <Attribute DataType="Key" Name="ShipmentLineKey"/>
                        <Attribute DataType="LineNo" Name="ShipmentLineNo"/>
                        <Attribute DataType="LineNo" Name="SubLineNo"/>
                        <Attribute DataType="DispositionCode" Name="SuggestedDispositionCode"/>
                        <Attribute DataType="Quantity" Name="TotalQuantity"/>
                        <Attribute DataType="UOM" Name="UnitOfMeasure"/>
                    </Attributes>
                </yfc:doc>
            </ReceivableLine>
        </KitLines>
    </ReceivableLineList>
</GetLinesToReceive>
