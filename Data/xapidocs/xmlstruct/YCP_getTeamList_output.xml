<TeamList xmlns="http://www.sterlingcommerce.com/documentation/YCP/getTeamList/output">
    <Team CustomerAccessMode="" Description="" DocumentTypeAccessMode=""
        EnterpriseAccessMode="" OrganizationCode="" ParentGroupKey=""
        ShipNodeAccessMode="" TeamId="" TeamKey="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute Name="CustomerAccessMode">
                    <Description>
                        <div>
                            Access mode for Customer. <br/>
                            Valid Values:<br/>
                            01 : Normal access mode. The team can access all customer data.<br/>
                            02 : Restricted access mode. Access to customer data is restricted based on customer assignments.<br/>
                            03: Restricted based on team.  Access to customer data is restricted for the team regardless of the Enterprise's manual assignment rule.<br/>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="TeamId" NameInEntity="DATA_SECURITY_GROUP_ID">
                    <Description>
                        <div>Team Id.</div>
                    </Description>
                </Attribute>
                <Attribute Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY"/>
            </Attributes>
            <Element EntityName="Data_Security_Group"/>
        </yfc:doc>
        <TeamEnterpriseList>
            <TeamEnterprise EnterpriseOrgCode="" TeamEnterpriseKey="" TeamKey="">
                <yfc:doc>
                    <Attributes>
                        <Attribute Name="TeamEnterpriseKey" NameInEntity="DATA_SECURITY_ENTERPRISE_KEY"/>
                        <Attribute Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY"/>
                    </Attributes>
                    <Element EntityName="Data_Security_Enterprise">
                        <Description>
                            <div>
								This tables stores the enterprises that are
								accessible to users associated to this team.
							</div>
                        </Description>
                    </Element>
                </yfc:doc>
            </TeamEnterprise>
        </TeamEnterpriseList>
        <TeamDocTypeList>
            <TeamDocType ApplicationCode="" DefaultForModule=""
                DocumentType="" ModuleCode="" TeamDocTypeKey="" TeamKey="">
                <yfc:doc>
                    <Attributes>
                        <Attribute Name="TeamDocTypeKey" NameInEntity="DATA_SECURITY_DOC_TYPE_KEY"/>
                        <Attribute Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY"/>
                    </Attributes>
                    <Element EntityName="Data_Security_Doc_Type">
                        <Description>
                            <div>
								This table stores the document types (by
								application) that are accessible by users
								associated to the team.
							</div>
                        </Description>
                    </Element>
                </yfc:doc>
            </TeamDocType>
        </TeamDocTypeList>
        <TeamNodesList>
            <TeamNodes ShipnodeKey="" TeamKey="" TeamNodeKey="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY"/>
                        <Attribute Name="TeamNodeKey" NameInEntity="DATA_SECURITY_NODE_KEY"/>
                    </Attributes>
                    <Element EntityName="Data_Security_Nodes">
                        <Description>
                            <div>
								Team Nodes that are used to control end-user
								access to data elements are stored here.
							</div>
                        </Description>
                    </Element>
                </yfc:doc>
            </TeamNodes>
        </TeamNodesList>
        <SubTeamList>
            <SubTeam>
                <Team Description="" DocumentTypeAccessMode=""
                    EnterpriseAccessMode="" OrganizationCode=""
                    ParentGroupKey="" ShipNodeAccessMode="" TeamId="" TeamKey="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute Name="TeamId" NameInEntity="DATA_SECURITY_GROUP_ID">
                                <Description>
                                    <div>Team Id.</div>
                                </Description>
                            </Attribute>
                            <Attribute Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY"/>
                        </Attributes>
                        <Element EntityName="Data_Security_Group"/>
                    </yfc:doc>
                    <TeamEnterpriseList>
                        <TeamEnterprise EnterpriseOrgCode=""
                            TeamEnterpriseKey="" TeamKey="">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute Name="TeamEnterpriseKey" NameInEntity="DATA_SECURITY_ENTERPRISE_KEY"/>
                                    <Attribute Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY"/>
                                </Attributes>
                                <Element EntityName="Data_Security_Enterprise">
                                    <Description>
                                    <div>
											This tables stores the
											enterprises that are accessible
											to users associated to this
											team.
										</div>
                                    </Description>
                                </Element>
                            </yfc:doc>
                        </TeamEnterprise>
                    </TeamEnterpriseList>
                    <TeamDocTypeList>
                        <TeamDocType ApplicationCode=""
                            DefaultForModule="" DocumentType=""
                            ModuleCode="" TeamDocTypeKey="" TeamKey="">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute Name="TeamDocTypeKey" NameInEntity="DATA_SECURITY_DOC_TYPE_KEY"/>
                                    <Attribute Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY"/>
                                </Attributes>
                                <Element EntityName="Data_Security_Doc_Type">
                                    <Description>
                                    <div>
											This table stores the document
											types (by application) that are
											accessible by users associated
											to the team.
										</div>
                                    </Description>
                                </Element>
                            </yfc:doc>
                        </TeamDocType>
                    </TeamDocTypeList>
                    <TeamNodesList>
                        <TeamNodes ShipnodeKey="" TeamKey=""
                            TeamNodeKey="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY"/>
                                    <Attribute Name="TeamNodeKey" NameInEntity="DATA_SECURITY_NODE_KEY"/>
                                </Attributes>
                                <Element EntityName="Data_Security_Nodes">
                                    <Description>
                                    <div>
											Team Nodes that are used to
											control end-user access to data
											elements are stored here.
										</div>
                                    </Description>
                                </Element>
                            </yfc:doc>
                        </TeamNodes>
                    </TeamNodesList>
                </Team>
            </SubTeam>
        </SubTeamList>
    </Team>
</TeamList>
