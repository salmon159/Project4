<!-- ON_CHANGE event XML   -->
<Attribute AllowMultipleValues="" AllowedValueLength=""
    AttributeDomainID="" AttributeGroupID="" AttributeID=""
    AttributeKey="" AttributePostFix="" DataType=""
    IsAllowedValueDefined="" IsValueMandatory="" LongDescription=""
    Operation="" OrganizationCode="" SequenceNo="" ShortDescription="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/ATTRIBUTE_DEFINITION/ON_CHANGE">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>Operation to perform
						<br/>Valid Values: 
						<br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
						<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
						<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
						<br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
						</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <AttributeReference AttributeKey="" AttributeReferenceKey=""
        ReferenceName="" TableName=""/>
    <AttributeAllowedValueList>
        <AttributeAllowedValue AttributeAllowedValueKey=""
            AttributeKey="" IsDefaultValue="" LongDescription=""
            NumericStep="" SequenceNo="" ShortDescription="" Value=""/>
    </AttributeAllowedValueList>
</Attribute>
