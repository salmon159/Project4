<!--ON_RESERVATION_ACCEPTANCE output XML-->
<OrderLineStatus OrderHeaderKey="" OrderLineKey=""
    OrderLineScheduleKey="" OrderReleaseStatusKey="" PipelineKey=""
    Status="" StatusDate="" StatusDescription="" StatusQty=""
    StatusReason="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/DRAFT_ORDER_CONFIRM/ON_AWAITING_RESERVATION_ACCEPTANCE">
    <yfc:doc>
        <Element EntityName="Order_Release_Status"/>
        <Attributes>
            <Attribute DataType="Text-100" Name="StatusDescription">
                <Description>Description of Order Line Status
		  </Description>
            </Attribute>
            <Attribute DataType="String" Name="StatusReason"/>
        </Attributes>
    </yfc:doc>
    <Schedule BatchNo="" ExpectedShipmentDate="" LotNumber=""
        MergeNode="" OrderLineScheduleKey="" ProcureFromNode=""
        ReceivingNode="" RevisionNo="" ShipByDate="" ShipNode="" TagNumber=""/>
    <OrderLine OrderHeaderKey="" OrderLineKey="" OrderedQty=""
        OriginalOrderedQty="" PrimeLineNo="" ProcureFromNode=""
        ShipNode="" SubLineNo="">
        <OrderLineTranQuantity OrderedQty="" OriginalOrderedQty="" TransactionalUOM="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Quantity" Name="OrderedQty">
                        <Description>The quantity of the the line item that has been ordered. If part of line is cancelled, this number is reduced appropriately. If an order line is returned, this number is not reduced.
						  </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="OriginalOrderedQty">
                        <Description>Quantity which was ordered during order creation. This is not modified after order confirmation.
						  </Description>
                    </Attribute>
                    <Attribute DataType="UOM" Name="TransactionalUOM">
                        <Description>UOM on which all the transactional quantities are specified.
						  </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </OrderLineTranQuantity>
        <Item ItemID="" ItemShortDesc="" ProductClass="" UnitOfMeasure="">
            <yfc:doc>
                <Element EntityName="Order_Line"/>
            </yfc:doc>
        </Item>
    </OrderLine>
</OrderLineStatus>
