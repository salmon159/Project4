<Task FromStartNoEarlierThan="" Node="!OrganizationCode!|Required"
    OrganizationCode="" ToStartNoEarlierThan="" WaveNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getTaskSummary/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Task" Name="FromStartNoEarlierThan" NameInEntity="Start_No_Earlier_Than"/>
            <Attribute Name="Node">
                <Description>
                    <div>
				 Task type level attributes are maintained at the node level. 
				 <Validation>
                            <Condition>
                                <code>Node</code> passed is not found in the YFS_SHIP_NODE table.
					</Condition>
                            <Error Description="No record found." ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Task" Name="ToStartNoEarlierThan" NameInEntity="Start_No_Earlier_Than"/>
            <Attribute EntityName="Task" Name="WaveNo" NameInEntity="WaveNo">
                <Description>
                    <div>
                        <Validation>
                            <Condition> Either <code>WaveNo</code> or at least one <code>TaskType</code> must be passed. </Condition>
                            <Error
                                Description="Either TaskType or WaveNo should be passed" ErrorCode="YCP0157"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <EligibleTaskTypeList>
        <EligibleTaskType TaskType="Conditional" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="TaskType">
                        <Description>
                            <div>
					Summary information is returned for the <code>TaskType</code> passed.
					<Validation>
                                    <Condition>
                                    <code>TaskType</code> passed does not exist in YFS_TASK_TYPE table.
						</Condition>
                                    <Error
                                    Description="No record found." ErrorCode="YCP0045"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </EligibleTaskType>
    </EligibleTaskTypeList>
</Task>
