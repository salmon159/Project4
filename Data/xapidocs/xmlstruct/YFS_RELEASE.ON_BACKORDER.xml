<!-- RELEASE.ON_BACKORDER event xml -->
<Order BuyerOrganizationCode="" DocumentType="" EnterpriseCode=""
    OrderHeaderKey="" OrderNo="" SellerOrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/RELEASE/ON_BACKORDER">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="YFS_SCHEDULE.ON_BACKORDER.xml" IncludeXMLPath="/Order"/>
    <OrderLines>
        <OrderLine OrderLineKey="" OrderedQty="" OriginalOrderedQty=""
            PrimeLineNo="" SubLineNo="" yfc:DTDOccurrence="ZeroOrMany">
            <Item ItemID="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Order_Line"/>
                </yfc:doc>
            </Item>
            <StatusBreakupForBackOrderedQty>
                <BackOrderedFrom BackOrderedQuantity=""
                    OrderLineScheduleKey="" OrderReleaseKey=""
                    OrderReleaseStatusKey="" Status="" StatusDate=""
                    StatusDescription="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
							The status the quantity was backordered from.
						</Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="BackOrderedQuantity">
                                <Description>
						Backordered Quantity 
						</Description>
                            </Attribute>
                            <Attribute EntityName="Order_Release_Status"
                                Name="OrderLineScheduleKey" NameInEntity="Order_Line_Schedule_Key"/>
                            <Attribute EntityName="Order_Release_Status"
                                Name="OrderReleaseKey" NameInEntity="Order_Release_Key"/>
                            <Attribute EntityName="Order_Release_Status"
                                Name="OrderReleaseStatusKey" NameInEntity="Order_Release_Status_Key"/>
                            <Attribute EntityName="Order_Release_Status"
                                Name="Status" NameInEntity="Status"/>
                            <Attribute EntityName="Order_Release_Status"
                                Name="StatusDate" NameInEntity="Status_Date"/>
                            <Attribute DataType="Text-100" Name="StatusDescription">
                                <Description>
						Status Description
						</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <Details ExpectedDeliveryDate=""
                        ExpectedShipmentDate="" OverrideItemID=""
                        OverrideProductClass="" OverrideUnitOfMeasure=""
                        ReceivingNode="" ShipByDate="" ShipNode="" TagNumber="">
                        <yfc:includeInParent IncludeXMLChildNodes="N"
                            IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/OrderStatuses/OrderStatus/Details"/>
                        <yfc:doc>
                            <Element EntityName="Order_Line_Schedule"/>
                            <Attributes>
                                <Attribute DataType="TimeStamp"
                                    EntityName="Order_Line_Schedule"
                                    Name="ExpectedDeliveryDate" NameInEntity="ExpectedDeliveryDate"/>
                                <Attribute DataType="TimeStamp"
                                    EntityName="Order_Line_Schedule"
                                    Name="ExpectedShipmentDate" NameInEntity="ExpectedShipmentDate"/>
                                <Attribute DataType="ShipNode"
                                    EntityName="Order_Line_Schedule"
                                    Name="ReceivingNode" NameInEntity="ReceivingNode"/>
                                <Attribute DataType="Date"
                                    EntityName="Order_Line_Schedule"
                                    Name="ShipByDate" NameInEntity="ShipByDate"/>
                                <Attribute DataType="ShipNode"
                                    EntityName="Order_Line_Schedule"
                                    Name="ShipNode" NameInEntity="ShipNode"/>
                                <Attribute DataType="Text-40"
                                    EntityName="Order_Line_Schedule"
                                    Name="TagNumber" NameInEntity="TagNumber"/>
                            </Attributes>
                        </yfc:doc>
                    </Details>
                </BackOrderedFrom>
            </StatusBreakupForBackOrderedQty>
        </OrderLine>
    </OrderLines>
</Order>
