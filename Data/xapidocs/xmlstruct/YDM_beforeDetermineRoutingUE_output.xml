<RoutingGuide BuyerOrganizationCode="Required" EnterpriseCode="Required"
    ExpectedDeliveryDate="" ExpectedShipDate="Required" LoadNo=""
    SellerOrganizationCode="Required" ShipmentNo="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/beforeDetermineRoutingUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Date" Name="ExpectedDeliveryDate">
                <Description>
                    <div>
						Expected Delivery Date of the Shipment.
						</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Load" Name="LoadNo" NameInEntity="Load_No">
                <Description>
                    <div>Indicates the Number of the Load, for which routing is being performed.</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="ShipmentNo" NameInEntity="Shipment_No">
                <Description>
                    <div>Indicates the Number of the Shipment, for which routing is being performed.</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SelectionCriteria DepartmentCode="" FreightTerms="" ItemClassification=""/>
    <RoutingGuideDetails>
        <RoutingGuideDetail DynamicRouting="">
            <ShipFrom ShipFromCity="Conditional"
                ShipFromCountry="Conditional" ShipFromNode="Conditional"
                ShipFromState="Conditional" ShipFromZip="Conditional">
                <yfc:doc>
                    <Element EntityName="Routing_Guide_Detail">
                        <Description>
                            <div>
						Origin Address information for which we need to determine SCAC and Carrier Service Code.
						<Validation>
                                    <Condition> An exception will be thrown if all the attributes in the <code>ShipFrom</code> element are void. </Condition>
                                    <Error
                                    Description="at least one ShipFrom attributes must be passed." ErrorCode="YDM000140"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                </yfc:doc>
            </ShipFrom>
            <ShipTo IsCommercialAddress="" ShipToCity="Conditional"
                ShipToCountry="Conditional" ShipToNode="Conditional"
                ShipToState="" ShipToStore="Conditional"
                ShipToZip="Conditional" TaxGeoCode="">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
						Destination Address information for which we need to determine SCAC and Carrier Service Code.
						<Validation>
                                    <Condition> An exception will be thrown if all the attributes in the <code>ShipTo</code> element are void. </Condition>
                                    <Error
                                    Description="at least one ShipTo attributes must be passed." ErrorCode="YDM000141"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Flag"
                            EntityName="Person_Info"
                            Name="IsCommercialAddress" NameInEntity="Is_Commercial_Address">
                            <Description>
                                <div>
Indicates whether the address is commercial, residential or none. Values of this field can be Y is Commercial address. N is Residential address. NULL is None. Default value is Null.
</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Person_Info"
                            Name="TaxGeoCode" NameInEntity="Tax_Geo_Code"/>
                    </Attributes>
                </yfc:doc>
            </ShipTo>
            <ShippingParameters HandlingUnits=""
                InputCarrierServiceCode="" ShipmentType="" Volume=""
                VolumeUOM="" Weight="" WeightUOM=""/>
        </RoutingGuideDetail>
    </RoutingGuideDetails>
</RoutingGuide>
