<?xml version="1.0" encoding="UTF-8"?>
<overridePickLocationAssignment Activity="" ActivityGroup=""
    EnterpriseCode="" Node="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/overridePickLocationAssignmentUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Activity" Name="Activity" NameInEntity="Activity_Code"/>
            <Attribute EntityName="Activity" Name="ActivityGroup" NameInEntity="Activity_Group_Id"/>
            <Attribute EntityName="Enterprise" Name="EnterpriseCode" NameInEntity="Enterprisecode"/>
            <Attribute EntityName="Ship_Node" Name="Node" NameInEntity="Ship_Node"/>
        </Attributes>
    </yfc:doc>
    <Target LocationId="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Location">
                <Description>
					Contains information about the target where inventory needs to be replenished.
				</Description>
            </Element>
        </yfc:doc>
        <Inventory CountryOfOrigin="" FifoNo="" InventoryItemKey=""
            InventoryStatus="" InventoryTagKey="" Quantity="Required"
            ReceiptHeaderKey="" Segment="" SegmentType="" ShipByDate="">
            <yfc:doc>
                <Element EntityName="Location_Inventory"/>
            </yfc:doc>
            <Receipt ReceiptNo="">
                <yfc:doc>
                    <Element EntityName="Receipt_Header"/>
                </yfc:doc>
            </Receipt>
            <InventoryItem ItemID="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Inventory_Item"/>
                </yfc:doc>
            </InventoryItem>
            <TagAttributes BatchNo="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotKeyReference="" LotNumber=""
                ManufacturingDate="" RevisionNo="">
                <yfc:doc>
                    <Element EntityName="Inventory_Tag"/>
                </yfc:doc>
            </TagAttributes>
        </Inventory>
    </Target>
    <Suggestion>
        <SourceList>
            <Source CrossDockLocation="" DedicatedLocation=""
                DynamicLocation="" LocationId="Required"
                OverAllocatedQty="" OverAllocating="" PickQuantity=""
                TaskType="Required" ZoneId="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Location">
                        <Description>
					Contains information about the source inventory is being adjusted.
				</Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Flag" Name="CrossDockLocation">
                            <Description>Indicates if the chosen location is a cross dock location</Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="DedicatedLocation">
                            <Description>Indicates if the chosen location is a dedicated location</Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="DynamicLocation">
                            <Description>Indicates if the chosen location is a dynamic slot location</Description>
                        </Attribute>
                        <Attribute EntityName="Location"
                            Name="LocationId" NameInEntity="Location_Id">
                            <Description>
                                <div>
                                    <Validation>
                                    <Condition>
                                    <code>LocationId</code> must be passed.
								</Condition>
                                    <Error
                                    Description="Source location is mandatory for tasks of this task type." ErrorCode="WMS00215"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
                                    <code>LocationId</code> should be a valid location in the warehouse.
								</Condition>
                                    <Error
                                    Description="Location does not exist." ErrorCode="WMS00005"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="OverAllocatedQty">
                            <Description>Indicates if the quantity that is overallocated.</Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="OverAllocating">
                            <Description>Indicates if the chosen location is a allocated beyond the inventory available at the location. This is 'Y' when a dedicated location is overallocated and when location is of type dynamic slot location.</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="PickQuantity">
                            <Description>Indicates the quantity that will be picked from this location</Description>
                        </Attribute>
                        <Attribute EntityName="Task_Type"
                            Name="TaskType" NameInEntity="Task_Type">
                            <Description>
                                <div>
                                    <Validation>
                                    <Condition>Must be a valid <code>TaskType</code> , in the table <code>YFS_TASK_TYPE</code>.
					</Condition>
                                    <Error
                                    Description="Invalid task type" ErrorCode="YCP0130"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
                                    <code>TaskType</code> not passed.
					</Condition>
                                    <Error
                                    Description="Task type determinations not set up for the task type group." ErrorCode="YCP0134"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Inventory CountryOfOrigin="" FifoNo=""
                    InventoryItemKey="" InventoryStatus=""
                    InventoryTagKey="" Quantity="Required"
                    ReceiptHeaderKey="" Segment="" SegmentType="" ShipByDate="">
                    <yfc:doc>
                        <Element EntityName="Location_Inventory"/>
                    </yfc:doc>
                    <Receipt ReceiptNo="">
                        <yfc:doc>
                            <Element EntityName="Receipt_Header"/>
                        </yfc:doc>
                    </Receipt>
                    <InventoryItem ItemID="" ProductClass="" UnitOfMeasure="">
                        <yfc:doc>
                            <Element EntityName="Inventory_Item"/>
                        </yfc:doc>
                    </InventoryItem>
                    <TagDetail BatchNo="" LotAttribute1=""
                        LotAttribute2="" LotAttribute3=""
                        LotKeyReference="" LotNumber=""
                        ManufacturingDate="" RevisionNo="">
                        <yfc:doc>
                            <Element EntityName="Inventory_Tag"/>
                        </yfc:doc>
                    </TagDetail>
                </Inventory>
            </Source>
        </SourceList>
        <ActivityDemandList>
            <ActivityDemand CaseId="" DemandLocationId="" DemandQty=""
                EnterpriseCode="Required" ForActivityCode="Required"
                ForMoveRequestLineKey="" ForShipmentLineKey=""
                ForWorkOrderKey="" FromActivityGroup="Required"
                InventoryStatus="" ItemId="" Node="Required"
                OrderHeaderKey="" PalletId="" Priority=""
                ProductClass="" Segment="" SegmentType="" ShipmentKey=""
                UnitOfMeasure="" WorkOrderKey="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="WMS_createActivityDemand_input.xml" IncludeXMLPath="/ActivityDemand"/>
                <yfc:doc>
                    <Element EntityName="Activity_Demand">
                        <Description>
                            <div>
			Demand on inventory may be placed for an activity from antother activity group. This entity stores information about such demands placed. Following attributes together form the Unique key of the table: <code>Node</code>, <code>FromActivityGroup</code>, <code>ForActivityCode</code>, <code>ShipmentKey</code>, <code>WorkOrderKey</code>, <code>ForShipmentLineKey</code>, <code>ForWorkOrderKey</code>, <code>ForMoveRequestLineKey</code>, <code>LPNNo</code>, <code>LPNType</code>, <code>OrganizationCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code>, <code>ProductClass</code>, <code>InventoryStatus</code>, <code>SegmentType</code>, <code>Segment</code>, <code>DemandLocationId</code>, <code>OrderHeaderKey</code>, <code>OrderNo</code>, <code>DocumentType</code>. Unique Keys of the <code>YFS_Activity_Demand</code> table should not be violated.
				<Validation>
                                    <Condition>activity demand being created should not already be existing in the database.
					</Condition>
                                    <Error
                                    Description="Record Already Exists." ErrorCode="YCP0051"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="LPNNo" Name="CaseId">
                            <Description>
                                <div>
                                    <code>CaseId</code> against which activity demand has been placed. <code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code> define a SKU. Pallet/Case are LPN types and <code>PalletId</code>/<code>CaseId</code> represents an LPN. Either of SKU details or (<code>PalletId</code>/<code>CaseId</code>) must be passed.
						<Validation>
                                    <Condition>Either of (<code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code>) or (<code>PalletId</code>/<code>CaseId</code>) should be passed.
							</Condition>
                                    <Error
                                    Description="Either of CaseId/PalletId or SKU Details must be passed." ErrorCode="WMS00051"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="DemandLocationId" NameInEntity="Demand_Location_Id">
                            <Description>
                                <div>
					Location Identifier. The location Id to which the demanded product must be diverted. If passed, then during put away, location considered for the demand fulfillment would be this location.
						<Validation>
                                    <Condition>Location Id passed should be valid location in the node.
							</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>Location Id passed should not be of type <code>INTRANSIT</code> or <code>VIRTUAL</code>.
							</Condition>
                                    <Error
                                    Description="Invalid Location Type." ErrorCode="WMS00067"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="DemandQty" NameInEntity="Demand_Qty">
                            <Description>
                                <div>
					Quantity of the product that is demanded for the activity. If demand quantity is not passed then demand is considered to be infinite ans this demand never gets completed. For a demand without inbound shipment/order information, quantity is mandatory. 
						<Validation>
                                    <Condition>If passed, the value passed must be greater than zero. For a <code>CaseId</code>/<code>PalletId</code>, it is defaulted to zero.
							</Condition>
                                    <Error
                                    Description="Quantity passed cannot be negative." ErrorCode="WMS00057"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>If not passed, then inbound shipment/order information should be passed. 
							</Condition>
                                    <Error
                                    Description="Cannot create an infinite activity demand for item." ErrorCode="WMS00275"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Zone_Enterprise" Name="EnterpriseCode">
                            <Description>
                                <div>
					Unique key attribute of the <code>YFS_ENTERPRISE_CODE</code> table. This is used to identify the enterprise for which the demand(<code>SKU</code>/<code>CaseId</code>/<code>PalletId</code>) is being placed. It is de-referenced to inventory organization code in <code>YFS_Activity_Demand</code> table. This is done so because put away is performed for the stock based on activity demand and the inventory picture.
						<Validation>
                                    <Condition>
                                    <code>EnterpriseCode</code> should be passed.
							</Condition>
                                    <Error
                                    Description="EnterpriseCode has to be passed in the input." ErrorCode="WMS00030"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
                                    <code>EnterpriseCode</code> passed should be a valid node.
							</Condition>
                                    <Error
                                    Description="Invalid Enterprise Code." ErrorCode="YCP0011"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="ForActivityCode" NameInEntity="For_Activity_Code">
                            <Description>
                                <div>
					Identifies the department/purpose for which demand was created.
						<Validation>
                                    <Condition>
                                    <code>ForActivityCode</code> should be passed.
							</Condition>
                                    <Error
                                    Description="ActivityCode not passed." ErrorCode="WMS00049"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
                                    <code>ForActivityCode</code> must be valid.
							</Condition>
                                    <Error
                                    Description="No Record Found." ErrorCode="YCP0045"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="ForShipmentLineKey" NameInEntity="For_Shipment_Line_Key">
                            <Description>
                                <div>
                                    <code>ForShipmentLineKey</code> identifies the shipment line for which the demand was placed. Primary key attribute of <code>YFS_SHIPMENT_LINE</code> table. 
						<Validation>
                                    <Condition>
                                    <code>ForShipmentLineKey</code> if passed should be a valid key.
							</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="ForWorkOrderKey" NameInEntity="For_Work_Order_Key">
                            <Description>
                                <div>
                                    <code>ForWorkOrderKey</code> identifies the work order for which the demand was placed. Primary key attribute of <code>YFS_WORK_ORDER</code> table. 
						<Validation>
                                    <Condition>
                                    <code>ForWorkOrderKey</code> if passed should be a valid key.
							</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="FromActivityGroup" NameInEntity="From_Activity_Group">
                            <Description>
                                <div>
					Identifies the department from which demand was requested.
						<Validation>
                                    <Condition>
                                    <code>FromActivityGroup</code> should be passed.
							</Condition>
                                    <Error
                                    Description="Module Code must be given." ErrorCode="WMS00053"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
                                    <code>FromActivityGroup</code> must be valid.
							</Condition>
                                    <Error
                                    Description="No Record Found." ErrorCode="YCP0045"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="ItemId" NameInEntity="Item_Id">
                            <Description>
                                <div>
                                    <code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code> define a SKU. Pallet/Case are LPN types and <code>PalletId</code>/<code>CaseId</code> represents an LPN. Either of SKU details or (<code>PalletId</code>/<code>CaseId</code>) must be passed.
						<Validation>
                                    <Condition>Either of (<code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code>) or (<code>PalletId</code>/<code>CaseId</code>) should be passed.
							</Condition>
                                    <Error
                                    Description="Either of CaseId/PalletId or SKU Details must be passed." ErrorCode="WMS00051"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="Node" NameInEntity="Node_Key">
                            <Description>
                                <div>
					Primary key attribute of <code>YFS_SHIP_NODE</code> table. Required to uniquely identify a location within the node.
						<Validation>
                                    <Condition>
                                    <code>Node</code> should be passed.
							</Condition>
                                    <Error
                                    Description="Node not passed." ErrorCode="WMS00054"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
                                    <code>Node</code> passed should be a valid node.
							</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="OrderHeaderKey" NameInEntity="Order_Header_Key">
                            <Description>
                                <div>
                                    <code>OrderHeaderKey</code> identifies an order against which the Activity Demand is placed. Primary key attribute of <code>YFS_ORDER_HEADER</code> table.
					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="LPNNo" Name="PalletId">
                            <Description>
                                <div>
                                    <code>PalletId</code> against which activity demand has been placed. <code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code> define a SKU. Pallet/Case are LPN types and <code>PalletId</code>/<code>CaseId</code> represents an LPN. Either of SKU details or (<code>PalletId</code>/<code>CaseId</code>) must be passed.
						<Validation>
                                    <Condition>Either of (<code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code>) or (<code>PalletId</code>/<code>CaseId</code>) should be passed.
							</Condition>
                                    <Error
                                    Description="Either of CaseId/PalletId or SKU Details must be passed." ErrorCode="WMS00051"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="Priority" NameInEntity="Priority">
                            <Description>
                                <div>
					Identifies the Priority with which the demand is created. 
						<Validation>
                                    <Condition>
                                    <code>Priority</code> if passed should be a valid value.
							</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="ProductClass" NameInEntity="Product_Class">
                            <Description>
                                <div>
                                    <code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code> define a SKU. Pallet/Case are LPN types and <code>PalletId</code>/<code>CaseId</code> represents an LPN. Either of SKU details or (<code>PalletId</code>/<code>CaseId</code>) must be passed.
						<Validation>
                                    <Condition>Either of (<code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code>) or (<code>PalletId</code>/<code>CaseId</code>) should be passed.
							</Condition>
                                    <Error
                                    Description="Either of CaseId/PalletId or SKU Details must be passed." ErrorCode="WMS00051"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="ShipmentKey" NameInEntity="Shipment_Key">
                            <Description>
                                <div>
                                    <code>ShipmentKey</code> helps identifying all stock attached to a shipment against which the demand is placed. Primary key attribute of <code>YFS_SHIPMENT</code> table.
					</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="UnitOfMeasure" NameInEntity="Uom">
                            <Description>
                                <div>
                                    <code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code> define a SKU. Pallet/Case are LPN types and <code>PalletId</code>/<code>CaseId</code> represents an LPN. Either of SKU details or (<code>PalletId</code>/<code>CaseId</code>) must be passed.
						<Validation>
                                    <Condition>Either of (<code>EnterpriseCode</code>, <code>ItemId</code>, <code>UnitOfMeasure</code> and <code>ProductClass</code>) or (<code>PalletId</code>/<code>CaseId</code>) should be passed.
							</Condition>
                                    <Error
                                    Description="Either of CaseId/PalletId or SKU Details must be passed." ErrorCode="WMS00051"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Activity_Demand"
                            Name="WorkOrderKey" NameInEntity="Work_Order_Key">
                            <Description>
                                <div>
                                    <code>WorkOrderKey</code> identifies a work order in context for the Activity Demand. The demand is created against this work order. Primary key attribute of <code>YFS_WORK_ORDER</code> table.
					</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <WorkOrder EnterpriseCode="" NodeKey="" WorkOrderNo="">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="YFS_Activity_Demand_master.xml"
                        IncludeXMLPath="/ActivityDemand/WorkOrder" RemoveDBHouseKeeping="Y"/>
                    <yfc:doc>
                        <Element EntityName="Work_Order"/>
                        <Attributes>
                            <Attribute EntityName="Work_Order"
                                Name="NodeKey" NameInEntity="Node_Key">
                                <Description>
                                    <div>
                                    <code>NodeKey</code> identifies node, where work orders would be executed.
							<Validation>
                                    <Condition>Unique key should be passed.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00171"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Work_Order"
                                Name="WorkOrderNo" NameInEntity="Work_Order_No">
                                <Description>
                                    <div>
                                    <code>WorkOrderNo</code> is a part of unique key.
							<Validation>
                                    <Condition>Unique key should be passed.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00171"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>Passed <code>WorkOrderNo</code> should exist in the database.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>Multiple records should not exist for the passed <code>WorkOrderNo</code>.
								</Condition>
                                    <Error
                                    Description="Record cannot be uniquely determined." ErrorCode="WMS00074"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </WorkOrder>
                <Shipment SellerOrganizationCode="" ShipNode="" ShipmentNo="">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="YFS_Activity_Demand_master.xml"
                        IncludeXMLPath="/ActivityDemand/Shipment" RemoveDBHouseKeeping="Y"/>
                    <yfc:doc>
                        <Element EntityName="Shipment"/>
                        <Attributes>
                            <Attribute EntityName="Shipment"
                                Name="SellerOrganizationCode" NameInEntity="Seller_Organization_Code">
                                <Description>
                                    <div>
						Seller organization to which this shipment belongs. <code>ShipNode</code> and <code>SellerOrganizationCode</code> identify the origin of the shipment (i.e The place from where the shipment was shipped).
							<Validation>
                                    <Condition>Unique key should be passed.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00171"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Shipment"
                                Name="ShipNode" NameInEntity="Shipnode_Key">
                                <Description>
                                    <div>
                                    <code>ShipNode</code> and <code>SellerOrganizationCode</code> identify the origin of the shipment (i.e The place from where the shipment was shipped).
							<Validation>
                                    <Condition>Unique key should be passed.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00171"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Shipment"
                                Name="ShipmentNo" NameInEntity="Shipment_No">
                                <Description>
                                    <div>
                                    <code>ShipmentNo</code> is a part of unique key.
							<Validation>
                                    <Condition>Unique key should be passed.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00171"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>Passed <code>ShipmentNo</code> should exist in the database.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>Multiple records should not exist for the passed <code>ShipmentNo</code>.
								</Condition>
                                    <Error
                                    Description="Record cannot be uniquely determined." ErrorCode="WMS00074"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Shipment>
                <Order DocumentType="" OrderNo="">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="YFS_Activity_Demand_master.xml"
                        IncludeXMLPath="/ActivityDemand/Order" RemoveDBHouseKeeping="Y"/>
                    <yfc:doc>
                        <Element EntityName="Order_Header"/>
                        <Attributes>
                            <Attribute Name="DocumentType" NameInEntity="Document_Type">
                                <Description>
                                    <div>
                                    <code>OrderNo</code>, <code>DocumentType</code> and <code>EnterpriseCode</code> identify the order.
							<Validation>
                                    <Condition>Unique key should be passed.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00171"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute Name="OrderNo" NameInEntity="Order_No">
                                <Description>
                                    <div>
                                    <code>OrderNo</code> is a part of unique key. Required, if OrderHeaderKey is not passed.
							<Validation>
                                    <Condition>Unique key should be passed.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00171"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>Multiple records should not exist for the passed <code>OrderNo</code>.
								</Condition>
                                    <Error
                                    Description="Record cannot be uniquely determined." ErrorCode="WMS00074"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Order>
            </ActivityDemand>
        </ActivityDemandList>
    </Suggestion>
</overridePickLocationAssignment>
