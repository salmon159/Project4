<!--checkNoHasslePriceMatchUE Output XML-->
<Order OrderHeaderKey="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCD/checkNoHasslePriceMatchUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Order_Header" Name="OrderHeaderKey" NameInEntity="Order_Header_Key"/>
        </Attributes>
    </yfc:doc>
    <OrderLines Competitor="Required">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" EntityName="Price_Match"
                    Name="Competitor" NameInEntity="Competitor"/>
            </Attributes>
        </yfc:doc>
        <OrderLine MeetsNoHassleCriteria="Required"
            OrderLineKey="Required" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="MeetsNoHassleCriteria">
                        <Description>
                            <div>
						This is the flag which says whether Price match meets the criteria or not, possible values are Y or N.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="OrderLineKey" NameInEntity="Order_Line_Key"/>
                </Attributes>
            </yfc:doc>
        </OrderLine>
    </OrderLines>
</Order>
