<Task OrganizationCode="" TaskId="Conditional" TaskKey="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getTaskDetails/input">
    <yfc:doc>
        <Attributes>
            <Attribute Name="TaskId">
                <Description>
                    <div>
			The ID which identifies the task. Pass either the <code>TaskKey</code> or <code>TaskId</code> to identify the task.
			<Validation>
                            <Condition>Neither <code>TaskKey</code> nor <code>TaskId</code> are passed. 
				</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                        <Validation>
                            <Condition>Passed value does not identify an existing task. 
				</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="TaskKey">
                <Description>
                    <div>
			The key which identifies the task. Pass either the <code>TaskKey</code> or <code>TaskId</code> to identify the task.
			<Validation>
                            <Condition>Neither <code>TaskKey</code> nor <code>TaskId</code> are passed. 
				</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                        <Validation>
                            <Condition>Passed value does not identify an existing task. 
				</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Task>
