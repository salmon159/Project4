<StopDeliveryRequest Comments="" DocumentType="" EnterpriseCode=""
    Operation="Manage|Create|Modify|Delete" OrderHeaderKey="" OrderNo=""
    RequestDate="" StopDeliveryRequestHeaderKey=""
    StopDeliveryRequestID="" xmlns="http://www.sterlingcommerce.com/documentation/OMP/manageStopDeliveryRequest/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Order_Header" Name="DocumentType"/>
            <Attribute EntityName="Order_Header" Name="EnterpriseCode"/>
            <Attribute DataType="String" Name="Operation">
                <Description>
                    <div>Determines what action to take on the <code>StopDeliveryRequest</code>. <br/>Default: <code>
                            <b>Manage</b>
                        </code>
                        <br/>Valid Values:<br/>
                        <code>Manage</code> - Create if the data is not found, otherwise modify.<br/>
                        <code>Create</code> - Create a new stop delivery.<br/>
                        <code>Modify</code> - Modify an existing stop delivery.<code>Delete</code> - Delete a stop delivery.</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="OrderNo"/>
        </Attributes>
    </yfc:doc>
    <StopDeliveryRequestDetails>
        <StopDeliveryRequestDetail Comments=""
            Operation="Manage|Create|Modify|Delete" OrderLineKey=""
            PrimeLineNo="" RequestedQty=""
            StopDeliveryRequestDetailKey="" SubLineNo=""
            SuccessfulQty="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="Operation">
                        <Description>
                            <div>Determines what action to take on the <code>StopDeliveryRequestDetail</code>. <br/>Default: <code>
                                    <b>Manage</b>
                                </code>
                                <br/>Valid Values:<br/>
                                <code>Manage</code> - Create if the data is not found, otherwise modify.<br/>
                                <code>Create</code> - Create a new detail.<br/>
                                <code>Modify</code> - Modify an existing detail.<br/>
                                <code>Delete</code> - Delete a detail.</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line" Name="PrimeLineNo"/>
                    <Attribute EntityName="Order_Line" Name="SubLineNo"/>
                </Attributes>
            </yfc:doc>
        </StopDeliveryRequestDetail>
    </StopDeliveryRequestDetails>
</StopDeliveryRequest>
