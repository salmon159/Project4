<Order BuyerOrganizationCode="" DocumentType="" EnterpriseCode=""
    OrderHeaderKey="" OrderNo="" SellerOrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/SCHEDULE/ON_CANCEL">
    <OrderLines>
        <!--ChangeInOrderedQty is returned with sign. In case of this event it should always come with negative sign. -->
        <OrderLine ChangeInOrderedQty="" OrderLineKey="" OrderedQty=""
            PrimeLineNo="" SubLineNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="ChangeInOrderedQty"/>
                </Attributes>
            </yfc:doc>
            <OrderLineTranQuantity ChangeInOrderedQty="" OrderedQty="" TransactionalUOM="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="ChangeInOrderedQty">
                            <Description>Change In Ordered Qty
				  </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="OrderedQty">
                            <Description>Ordered Quantity
				  </Description>
                        </Attribute>
                        <Attribute DataType="UOM" Name="TransactionalUOM">
                            <Description>UOM on which all the transactional quantities are specified.
				  </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </OrderLineTranQuantity>
            <Item ItemID="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="ProductClass"/>
                    </Attributes>
                </yfc:doc>
            </Item>
            <!--ChangeInLineTotal is returned with sign. -->
            <LinePriceInfo ChangeInLineTotal="" LineTotal=""
                TaxExemptionCertificate="" TaxableFlag="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Money" Name="ChangeInLineTotal">
                            <Description>Change In Line Total
				  </Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="LineTotal">
                            <Description>Line Total
				  </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </LinePriceInfo>
            <StatusBreakupForCanceledQty>
                <CanceledFrom OrderLineScheduleKey="" OrderReleaseKey=""
                    OrderReleaseStatusKey="" Quantity="" Status=""
                    StatusDate="" StatusDescription="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute EntityName="Order_Release_Status"
                                Name="OrderLineScheduleKey" NameInEntity="Order_Line_Schedule_Key"/>
                            <Attribute EntityName="Order_Release_Status"
                                Name="OrderReleaseKey" NameInEntity="Order_Release_Key"/>
                            <Attribute EntityName="Order_Release_Status"
                                Name="OrderReleaseStatusKey" NameInEntity="Order_Release_Status_Key"/>
                            <Attribute DataType="String" Name="Quantity">
                                <Description>
						Quantity Canceled
						</Description>
                            </Attribute>
                            <Attribute EntityName="Order_Release_Status"
                                Name="Status" NameInEntity="Status"/>
                            <Attribute EntityName="Order_Release_Status"
                                Name="StatusDate" NameInEntity="Status_Date"/>
                            <Attribute DataType="Text-100" Name="StatusDescription">
                                <Description>
						Status Description
						</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <CanceledFromTranQuantity Quantity="" TransactionalUOM="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Quantity" Name="Quantity">
                                    <Description>Quantity Canceled
						  </Description>
                                </Attribute>
                                <Attribute DataType="UOM" Name="TransactionalUOM">
                                    <Description>UOM on which all the transactional quantities are specified.
						  </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </CanceledFromTranQuantity>
                    <Details ExpectedDeliveryDate=""
                        ExpectedShipmentDate="" OverrideItemID=""
                        OverrideProductClass="" OverrideUnitOfMeasure=""
                        ReceivingNode="" ShipByDate="" ShipNode="" TagNumber="">
                        <yfc:includeInParent IncludeXMLChildNodes="N"
                            IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/OrderStatuses/OrderStatus/Details"/>
                        <yfc:doc>
                            <Element EntityName="Order_Line_Schedule"/>
                            <Attributes>
                                <Attribute DataType="TimeStamp"
                                    EntityName="Order_Line_Schedule"
                                    Name="ExpectedDeliveryDate" NameInEntity="ExpectedDeliveryDate"/>
                                <Attribute DataType="TimeStamp"
                                    EntityName="Order_Line_Schedule"
                                    Name="ExpectedShipmentDate" NameInEntity="ExpectedShipmentDate"/>
                                <Attribute DataType="ShipNode"
                                    EntityName="Order_Line_Schedule"
                                    Name="ReceivingNode" NameInEntity="ReceivingNode"/>
                                <Attribute DataType="Date"
                                    EntityName="Order_Line_Schedule"
                                    Name="ShipByDate" NameInEntity="ShipByDate"/>
                                <Attribute DataType="ShipNode"
                                    EntityName="Order_Line_Schedule"
                                    Name="ShipNode" NameInEntity="ShipNode"/>
                                <Attribute DataType="Text-40"
                                    EntityName="Order_Line_Schedule"
                                    Name="TagNumber" NameInEntity="TagNumber"/>
                            </Attributes>
                        </yfc:doc>
                    </Details>
                </CanceledFrom>
            </StatusBreakupForCanceledQty>
        </OrderLine>
    </OrderLines>
    <PriceInfo ChangeInTotalAmount="" Currency="" EnterpriseCurrency=""
        ReportingConversionDate="" ReportingConversionRate="" TotalAmount="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Money" Name="ChangeInTotalAmount">
                    <Description>
	Change In Total Amount
	</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </PriceInfo>
</Order>
