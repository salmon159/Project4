<!--getItemNodeCostForSourcing Input XML-->
<ItemCostList EnterpriseCode="" OrderReference=""
    OrganizationCode="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getItemNodeCostForSourcing/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="EnterpriseCode">
                <Description>
                    <div>
                  Enterprise Code.
                </div>
                </Description>
            </Attribute>
            <Attribute DataType="ID-24" Name="OrderReference">
                <Description>
                    <div>
					The unique identifier for the order. The <code>OrderReference</code> is the Order Header Key.	
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item"
                Name="OrganizationCode" NameInEntity="Organization_Code"/>
        </Attributes>
    </yfc:doc>
    <ItemCost ItemID="Required" ProductClass="Required"
        UnitOfMeasure="Required" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Inventory_Item" Name="ItemID" NameInEntity="Item_Id"/>
                <Attribute EntityName="Inventory_Item"
                    Name="ProductClass" NameInEntity="Product_Class"/>
                <Attribute EntityName="Inventory_Item"
                    Name="UnitOfMeasure" NameInEntity="Uom"/>
            </Attributes>
        </yfc:doc>
        <Nodes yfc:DTDOccurrence="REQUIRED">
            <Node Currency="" InventoryUnitCost="" Qty="Required"
                ShipNode="Required" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Currency" Name="Currency">
                            <Description>
                                <div>
                  Currency for <code>InventoryItemCost</code>.  It Will always be enterprise' local currency.
                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="InventoryUnitCost">
                            <Description>
                                <div>
                  Indicates inventory item cost at a node.  If average costing is maintained internally, then cost will be internally computed, and passed as input to user exit.
                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="Qty">
                            <Description>
                                <div>
                  Total quantity that is being scheduled.
                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Order_Line_Schedule"
                            Name="ShipNode" NameInEntity="Ship_Node"/>
                    </Attributes>
                </yfc:doc>
            </Node>
        </Nodes>
    </ItemCost>
    <PromiseLines>
        <PromiseLine ItemID="Required" LineId="Conditional"
            OrderLineReference="Conditional" ProductClass="" ShipNode=""
            UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Order_Line"/>
                <Attributes>
                    <Attribute DataType="Text-128" Name="LineId">
                        <Description>
                            <div>
						Unique identifier for the line. 
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="OrderLineReference">
                        <Description>
                            <div>
	            		Reference to the orderLine.
	            	  </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ShipToAddress AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" ErrorTxt="" EveningFaxNo=""
                EveningPhone="" FirstName="" HttpUrl=""
                IsAddressVerified="" IsCommercialAddress="" JobTitle=""
                LastName="" Latitude="" Longitude="" MiddleName=""
                MobilePhone="" OtherPhone="" PersonID=""
                PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
                Title="" UseCount="" VerificationStatus="" ZipCode="" isHistory="">
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                    <Attributes>
                        <Attribute DataType="Flag" Name="isHistory">
                            <Description>
						Flag to indicate if the address is in history tables.
					</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ShipToAddress>
        </PromiseLine>
    </PromiseLines>
</ItemCostList>
