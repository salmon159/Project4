<Order DocumentType="" EnterpriseCode="" OrderHeaderKey="" OrderNo="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getCarrierServiceOptionsForOrdering/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					Pass either the <code>EnterpriseCode</code> or the <code>OrderHeaderKey</code> to be able to search for the item in the catalog. If <code>EnterpriseCode</code> is not passed, it is defaulted from the enterprise of the <code>OrderHeaderKey</code>.
					<Validation>
                        <Condition>
                            <code>EnterpriseCode</code> or <code>OrderHeaderKey</code> must be passed and must be valid.
						</Condition>
                        <Error
                            Description="YCM:Missing Mandatory parameters." ErrorCode="YCM0002"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute Name="EnterpriseCode" NameInEntity="Enterprise_Key"/>
            <Attribute Name="OrderHeaderKey" NameInEntity="Order_Header_Key"/>
            <Attribute Name="OrderNo" NameInEntity="Order_No"/>
        </Attributes>
    </yfc:doc>
    <PersonInfoShipTo AddressID="" AddressLine1="" AddressLine2=""
        AddressLine3="" AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" EveningFaxNo=""
        EveningPhone="" FirstName="" IsCommercialAddress="" JobTitle=""
        LastName="" MiddleName="" MobilePhone="" OtherPhone=""
        PersonID="" State="" Suffix="" TaxGeoCode="" Title="" ZipCode="">
        <yfc:doc>
            <Element EntityName="Person_Info"/>
        </yfc:doc>
    </PersonInfoShipTo>
    <OrderLines>
        <OrderLine EarliestShipDate="" ItemID="" OrderLineKey=""
            PrimeLineNo="" ProductClass="" ShipNode="" SubLineNo=""
            UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute Name="EarliestShipDate">
                        <Description>
							The earliest date that the item or the order line can be shipped.
						</Description>
                    </Attribute>
                    <Attribute Name="ItemID" NameInEntity="Item_Id"/>
                    <Attribute Name="OrderLineKey" NameInEntity="Order_Line_Key"/>
                    <Attribute Name="PrimeLineNo" NameInEntity="Prime_Line_No"/>
                    <Attribute Name="ProductClass" NameInEntity="Product_Class"/>
                    <Attribute Name="ShipNode">
                        <Description>
							The address to which the order line or the item needs to be shipped.
						</Description>
                    </Attribute>
                    <Attribute Name="SubLineNo" NameInEntity="Sub_Line_No"/>
                    <Attribute Name="UnitOfMeasure" NameInEntity="Uom"/>
                </Attributes>
            </yfc:doc>
            <PersonInfoShipTo AddressID="" AddressLine1=""
                AddressLine2="" AddressLine3="" AddressLine4=""
                AddressLine5="" AddressLine6="" AlternateEmailID=""
                Beeper="" City="" Company="" Country="" DayFaxNo=""
                DayPhone="" Department="" EMailID="" EveningFaxNo=""
                EveningPhone="" FirstName="" IsCommercialAddress=""
                JobTitle="" LastName="" MiddleName="" MobilePhone=""
                OtherPhone="" PersonID="" State="" Suffix=""
                TaxGeoCode="" Title="" ZipCode="">
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                </yfc:doc>
            </PersonInfoShipTo>
        </OrderLine>
    </OrderLines>
</Order>
