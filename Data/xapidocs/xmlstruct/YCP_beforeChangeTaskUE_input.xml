<Task Action="" AssignedToUserId="" BatchHold="" EndTimeStamp=""
    EquipmentId="" EquipmentLocationId="" ExceptionHold=""
    FinishNoLaterThan="" HoldReasonCode="" HoldReasonText="" Node=""
    ParentTaskId="" PredecessorTaskId="" ProductivityKey=""
    ReasonCode="" ReasonText="" ReferenceSortSequence="" SourceAisle=""
    SourceLocationId="" SourceSortSequence="" SourceZoneId=""
    StartNoEarlierThan="" StartTimeStamp="" TargetAisle=""
    TargetLocationId="" TargetSortSequence="" TargetZoneId=""
    TaskId="Conditional" TaskKey="Conditional" TaskPriority="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/beforeChangeTaskUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute Name="Action">
                <Description>
                    <div>
				 The action to perform. If passed as <b>
                            <code>CANCEL</code>
                        </b> then changes are applied to the passed quantity. The rest of the task quantity is canceled. Any other value for this attribute is ignored.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Task" Name="AssignedToUserId" NameInEntity="AssignedToUserId">
                <Description>
                    <div>
					User does not exist in master table (YFS_USER).
					<Validation>
                            <Condition>
                                <code>AssignedToUserId</code> must exists in system. 
						</Condition>
                            <Error
                                Description="Invalid User identifiers passed" ErrorCode="YCP0084"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Task" Name="BatchHold" NameInEntity="BatchHold">
                <Description>
                    <div>Specifies if task is held on batch.
					<Validation>
                            <Condition> If<code>BatchHold</code> is set for task and trying to hold the task for batch again. 
						</Condition>
                            <Error
                                Description="Task is already batch held." ErrorCode="YCP0227"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Task" Name="ExceptionHold" NameInEntity="ExceptionHold">
                <Description>
                    <div>Specifies if task is held on exception.
					<Validation>
                            <Condition> If<code>ExceptionHold</code> is set for task and trying to hold the task for exception again. 
						</Condition>
                            <Error
                                Description="Task is already exception held." ErrorCode="YCP0228"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Task" Name="ParentTaskId" NameInEntity="ParentTaskId">
                <Description>
                    <div>
				Parent task id of this task. 
				<Validation>
                            <Condition>If a differnt <code>ParentTaskId</code> is passed. 
					</Condition>
                            <Error
                                Description="Cannot change Parent Task Id." ErrorCode="YCP0232"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="TaskId">
                <Description>
                    <div>
				The key which identifies the task. Pass either the <code>TaskKey</code> or <code>TaskId</code> to identify the task.
				<Validation>
                            <Condition>Neither <code>TaskKey</code> nor <code>TaskId</code> are passed. 
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                        <Validation>
                            <Condition>Pass value does not identify an existing task. 
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Task" Name="TaskKey" NameInEntity="TaskKey">
                <Description>
                    <div>
				The key which identifies the task. Pass either the <code>TaskKey</code> or <code>TaskId</code> to identify the task.
				<Validation>
                            <Condition>Neither <code>TaskKey</code> nor <code>TaskId</code> are passed. 
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                        <Validation>
                            <Condition>Pass value does not identify an existing task. 
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Inventory ItemId="" ProductClass="" Quantity="" ReceiptHeaderKey=""
        ShipByDate="" SourceCaseId="" SourcePalletId="" TargetCaseId=""
        TargetPalletId="" UnitOfMeasure="">
        <yfc:doc>
            <Element EntityName="Task"/>
            <Attributes>
                <Attribute Name="Quantity">
                    <Description>
                        <div>
                            <Validation>
                                <Condition>Can not cancel requested quantity. </Condition>
                                <Error Description="Not Enough Quantity" ErrorCode="YFS10131"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="SourceCaseId" NameInEntity="Source_LPN_No">
                    <Description>
                        <div>Source Case Id of the task</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="SourcePalletId" NameInEntity="Source_LPN_No">
                    <Description>
                        <div>Source Pallet Id of the task</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="TargetCaseId" NameInEntity="Target_LPN_No">
                    <Description>
                        <div>Target Case Id of the task</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="TargetPalletId" NameInEntity="Target_LPN_No">
                    <Description>
                        <div>Target Pallet Id of the task</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <TagAttributes BatchNo="" LotAttribute1="" LotAttribute2=""
            LotAttribute3="" LotKeyReference="" LotNumber=""
            ManufacturingDate="" RevisionNo="">
            <yfc:doc>
                <Element EntityName="Inventory_Tag"/>
            </yfc:doc>
        </TagAttributes>
    </Inventory>
    <TaskReferences BatchNo="" BolNo="" ContainerNo=""
        CountRequestKey="" CountRequestNo="" LoadNo="" ManifestNo=""
        MoveRequestKey="" MoveRequestLineKey="" MoveRequestNo=""
        OrderHeaderKey="" OrderLineKey="" OrderNo="" OrderReleaseKey=""
        PrimeLineNo="" ReceiptNo="" ReleaseNo="" Scac=""
        ShipmentContainerKey="" ShipmentKey="" ShipmentNo=""
        SubLineNo="" TrailerNo="" WaveNo="" WorkOrderKey="" WorkOrderNo=""/>
</Task>
