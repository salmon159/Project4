<!--getReceiptLinesForTransaction Input XML-->
<Receipt DocumentType="" EnterpriseCode="" OrderHeaderKey=""
    OrderLineKey="" OrderNo="" OrderReleaseKey="" PrimeLineNo=""
    ReleaseNo="" SubLineNo="" TransactionId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getReceiptLinesForTransaction/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Order_Header" Name="DocumentType"/>
            <Attribute EntityName="Order_Header" Name="EnterpriseCode"/>
            <Attribute DataType="String" Name="OrderHeaderKey"/>
            <Attribute EntityName="Order_Line" Name="OrderLineKey"/>
            <Attribute EntityName="Order_Header" Name="OrderNo"/>
            <Attribute DataType="String" Name="OrderReleaseKey"/>
            <Attribute EntityName="Order_Line" Name="PrimeLineNo"/>
            <Attribute EntityName="Order_Release" Name="ReleaseNo"/>
            <Attribute EntityName="Order_Line" Name="SubLineNo"/>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid">
                <Description>
                    <div>
					Unique identifier for a transaction. The receipt lines which can be processed by this transaction will be output.
						<Validation>
                            <Condition>Error if <code>TransactionId</code> is not valid.
							</Condition>
                            <Error Description="Invalid Transaction Id." ErrorCode="YCP0068"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Receipt>
