<OrderAudit AuditTransactionId="" EnterpriseCode="" MaximumRecords=""
    ModificationType="" OrderAuditKey="" OrderHeaderKey=""
    OrderLineKey="" OrderReleaseKey="" ReadFromHistory="" Reference1=""
    Reference2="" Reference3="" Reference4="" SelectMethod="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderAuditList/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Enterprise" Name="EnterpriseCode" NameInEntity="Enterprisecode"/>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>Specifies the Maximum Number of records to be returned. Default is 500.</Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="ReadFromHistory">
                <Description>
                    <div>
                Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
                <br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_AUDIT table.
                <br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_AUDIT_H table (the history table).
                <br/>
                        <b>
                            <code>B</code>
                        </b> - Returns records that satisfy the input criteria from both YFS_AUDIT and YFS_AUDIT_H tables. Records returned from the current table are first in the output.
                </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-20" Name="SelectMethod">
                <Description>
                    <div>
			  Indicates if database operations should be performed in the
			  <code>WAIT</code> or <code>NO_WAIT</code> or <code>NO_LOCK</code> mode.
			  <br/>
			  Default: <code>WAIT</code>
                        <br/>
			  Valid Values:
			  <br/>
                        <b>
                            <code>WAIT</code>
                        </b> - The select operation waits until database resource becomes available.
			  <br/>
                        <b>
                            <code>NO_WAIT</code>
                        </b> - Peform a select immediately regardless the database resource availability.
			  <br/>
                        <b>
                            <code>NO_LOCK</code>
                        </b> - SQL will not put a lock on DB records.
			  <br/>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</OrderAudit>
