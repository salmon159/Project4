<!--changeReceipt Input XML-->
<Receipt ArrivalDateTime="" DriverName="" NumOfCartons=""
    NumOfPallets="" OverrideModificationRules="" ReceiptDate=""
    ReceiptHeaderKey="Conditional" ReceiptNo="Conditional"
    ReceivingDock="" ReceivingNode="Conditional"
    ShipmentKey="Conditional" TrailerLPNNo="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/changeReceipt/input">
    <yfc:doc>
        <Element EntityName="Receipt_Header"/>
        <Attributes>
            <Attribute DataType="Flag" Name="OverrideModificationRules">
                <Description> Indicates whether modification rule override is allowed or not.</Description>
            </Attribute>
            <Attribute EntityName="Receipt_Header"
                Name="ReceiptHeaderKey" NameInEntity="Receipt_Header_Key">
                <Description>
                    <div>
						Primary key attribute of <code>YFS_RECEIPT_HEADER</code> table.
							<Validation>
                            <Condition>Modification of <code>ReceiptHeaderKey</code> is not allowed.
								</Condition>
                            <Error
                                Description="Key Fields can not be modified." ErrorCode="YFS10415"/>
                        </Validation>
                        <Validation>
                            <Condition>The receipt that is to be modified should be open and the status modification rules should allow this operation.
								</Condition>
                            <Error
                                Description="Closed receipt cannot be modified." ErrorCode="OMP10051"/>
                            <Error
                                Description="Receipt modification rules do not permit modification of receipt in its current status." ErrorCode="OMP10039"/>
                        </Validation>
                        <Validation>
                            <Condition>Either <code>ReceiptHeaderKey</code> or <code>ReceiptNo</code>, <code>ReceivingNode</code> and <code>ShipmentKey</code> has to be passed to identify the receipt. Errors if not valid.
								</Condition>
                            <Error
                                Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                            <Error Description="Receipt not found." ErrorCode="YFS10466"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Receipt_Header" Name="ReceiptNo" NameInEntity="Receipt_No">
                <Description>
                    <div>
						Unique key or business key, which by itself identifies a receipt along with <code>ReceivingNode</code>.
							<Validation>
                            <Condition>Modification of <code>ReceiptNo</code> is not allowed.
								</Condition>
                            <Error
                                Description="Key Fields can not be modified." ErrorCode="YFS10415"/>
                        </Validation>
                        <Validation>
                            <Condition>The receipt that is to be modified should be open and the status modification rules should allow this operation.
								</Condition>
                            <Error
                                Description="Closed receipt cannot be modified." ErrorCode="OMP10051"/>
                            <Error
                                Description="Receipt modification rules do not permit modification of receipt in its current status." ErrorCode="OMP10039"/>
                        </Validation>
                        <Validation>
                            <Condition>Either <code>ReceiptHeaderKey</code> or <code>ReceiptNo</code>, <code>ReceivingNode</code> and <code>ShipmentKey</code> has to be passed to identify the receipt. Errors if not valid.
								</Condition>
                            <Error
                                Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                            <Error Description="Receipt not found." ErrorCode="YFS10466"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" EntityName="Receipt_Header"
                Name="ReceivingNode" NameInEntity="Receiving_Node">
                <Description>
                    <div>
						The Receiving Node.
							<Validation>
                            <Condition>Modification of <code>ReceivingNode</code> is not allowed.
								</Condition>
                            <Error
                                Description="Key Fields can not be modified." ErrorCode="YFS10415"/>
                        </Validation>
                        <Validation>
                            <Condition>The receipt that is to be modified should be open and the status modification rules should allow this operation.
								</Condition>
                            <Error
                                Description="Closed receipt cannot be modified." ErrorCode="OMP10051"/>
                            <Error
                                Description="Receipt modification rules do not permit modification of receipt in its current status." ErrorCode="OMP10039"/>
                        </Validation>
                        <Validation>
                            <Condition>Either <code>ReceiptHeaderKey</code> or <code>ReceiptNo</code>, <code>ReceivingNode</code> and <code>ShipmentKey</code> has to be passed to identify the receipt. Errors if not valid.
								</Condition>
                            <Error
                                Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                            <Error Description="Receipt not found." ErrorCode="YFS10466"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Receipt_Header" Name="ShipmentKey" NameInEntity="Shipment_Key">
                <Description>
                    <div>
						Primary key attribute of <code>YFS_SHIPMENT</code> table. Specifies the shipment being received.
							<Validation>
                            <Condition>Modification of <code>ShipmentKey</code> is not allowed.
								</Condition>
                            <Error
                                Description="Key Fields can not be modified." ErrorCode="YFS10415"/>
                        </Validation>
                        <Validation>
                            <Condition>The receipt that is to be modified should be open and the status modification rules should allow this operation.
								</Condition>
                            <Error
                                Description="Closed receipt cannot be modified." ErrorCode="OMP10051"/>
                            <Error
                                Description="Receipt modification rules do not permit modification of receipt in its current status." ErrorCode="OMP10039"/>
                        </Validation>
                        <Validation>
                            <Condition>Either <code>ReceiptHeaderKey</code> or <code>ReceiptNo</code>, <code>ReceivingNode</code> and <code>ShipmentKey</code> has to be passed to identify the receipt. Errors if not valid.
								</Condition>
                            <Error
                                Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                            <Error Description="Receipt not found." ErrorCode="YFS10466"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ReceiptLines>
        <ReceiptLine DispositionLocation="" NetWeight="" NetWeightUom="" ReceiptLineNo=""/>
    </ReceiptLines>
    <Instructions>
        <Instruction Action="" InstructionDetailKey=""
            InstructionText="" InstructionType="" InstructionURL=""
            InstructionUsage="" SequenceNo="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Action" Name="Action">
                        <Description> An Action. </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Instruction>
    </Instructions>
    <Shipment DocumentType="Conditional" EnterpriseCode="Conditional"
        OrderHeaderKey="Conditional" OrderNo="Conditional"
        OrderReleaseKey="Conditional" ReleaseNo="Conditional"
        SellerOrganizationCode="Conditional" ShipNode="Conditional" ShipmentNo="Conditional"/>
</Receipt>
