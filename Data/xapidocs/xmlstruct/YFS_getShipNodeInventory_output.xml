<!--getShipNodeInventory Output XML-->
<ShipNodeInventory xmlns="http://www.sterlingcommerce.com/documentation/YFS/getShipNodeInventory/output">
    <Item ConsiderAllNodes="" ConsiderAllSegments=""
        ConsiderInventoryNodeControl="" Description=""
        DistributionRuleId="" ItemID="" OrganizationCode=""
        ProductClass="" Segment="" SegmentType="" ShipDate=""
        TrackedEverywhereFlag="" UnitOfMeasure="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="ConsiderAllNodes"/>
                <Attribute DataType="Flag" Name="ConsiderAllSegments">
                    <Description>
                        <div>
				Default is <code>N</code>.   
				<br/>If set to <b>
                                <code>Y</code>
                            </b>, <code>SegmentType</code> and <code>Segment</code> are ignored.  
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="ConsiderInventoryNodeControl">
                    <Description>
                        <div>
                Indicates whether inventory node control data were used in the inventory reporting.
                </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Item_Ship_Node"
                    Name="DistributionRuleId" NameInEntity="Distribution_Rule_Id"/>
                <Attribute EntityName="Inventory_Item" Name="ItemID"/>
                <Attribute DataType="OrgCode" Name="OrganizationCode"/>
                <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
                <Attribute DataType="CommonCode" Name="Segment"/>
                <Attribute DataType="SegmentType" Name="SegmentType"/>
                <Attribute DataType="Date" Name="ShipDate"/>
                <Attribute DataType="Flag" Name="TrackedEverywhereFlag"/>
                <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
            </Attributes>
        </yfc:doc>
        <PrimaryInformation Description="" ShortDescription=""/>
        <LanguageDescriptionList>
            <LanguageDescription Description="" ExtendedDescription=""
                LocaleCode="" ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
        </LanguageDescriptionList>
        <ShipNodes yfc:DTDOccurrence="REQUIRED">
            <ShipNode Description="" ExternalNode=""
                IdentifiedByParentAs="" OwnerKey="" ShipNode=""
                TotalDemand="" TotalSupply="" Tracked="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="ExternalNode"/>
                        <Attribute DataType="Quantity" Name="TotalDemand">
                            <Description>
                                <div>
							Total demand at this <code>ShipNode</code>.
							</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="TotalSupply">
                            <Description>
                                <div>
							Total supply at this ShipNode.
							</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="Tracked">
                            <Description>
                                <div>
                                    <b>
                                    <code>Y</code>
                                    </b> - Inventory is tracked at this <code>ShipNode</code>.
							<br/>
                                    <b>
                                    <code>N</code>
                                    </b> - Inventory is not tracked at this <code>ShipNode</code>.
							</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Supplies TotalSupply="" yfc:DTDOccurrence="REQUIRED">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="TotalSupply">
                                <Description>
                                    <div>
								Quantity of supplies with all supplies.
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <InventorySupplyType OnhandSupply="" Quantity=""
                        SupplyType="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Quantity" Name="Quantity">
                                    <Description>
                                    <div>
								Quantity of supplies of the  specified <code>SupplyType</code>.
								</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </InventorySupplyType>
                </Supplies>
                <Demands TotalDemand="" yfc:DTDOccurrence="REQUIRED">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="TotalDemand">
                                <Description>
                                    <div>
								Total demand at this <code>ShipNode</code>.
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <InventoryDemandType AllocatedDemand=""
                        DemandType="" PromisedDemand="" Quantity="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Flag" Name="AllocatedDemand">
                                    <Description>
                                    <div>
										Value is <b>
                                    <code>Y</code>
                                    </b> if demand is allocated demand.
										</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="PromisedDemand">
                                    <Description>
                                    <div>
										 Value is  <b>
                                    <code>Y</code>
                                    </b> if the demand is promised demand.
										</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Quantity" Name="Quantity">
                                    <Description>
                                    <div>
										Quantity of demands with the specified <code>DemandType</code>.
										</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </InventoryDemandType>
                </Demands>
            </ShipNode>
        </ShipNodes>
    </Item>
</ShipNodeInventory>
