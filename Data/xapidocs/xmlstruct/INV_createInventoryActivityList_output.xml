<InventoryActivityList TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/INV/createInventoryActivityList/output">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="INV_createInventoryActivity_output.xml" IncludeXMLPath="/InventoryActivityList"/>
    <yfc:doc>
        <Element>
            <Description>
                <div>	
				When <code>DistributionRuleId</code> is passed in the input XML, inventory activities will be created with the provided item for each node configured within the distribution rule.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>The total number of activities created.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <InventoryActivity InventoryActivityKey="" InventoryItemKey=""
        ItemID="" Node="" OrganizationCode="" ProcessedFlag=""
        ProductClass="" Purpose="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Inventory_Item" Name="ItemID" NameInEntity="Item_Id"/>
                <Attribute EntityName="Inventory_Item"
                    Name="OrganizationCode" NameInEntity="Organization_Code"/>
                <Attribute EntityName="Inventory_Activity"
                    Name="ProcessedFlag" NameInEntity="Processed_Flag">
                    <Description>
						The flag indicates whether this inventory activity has been picked up for processing by the Real-Time Availability Monitor.  Valid values: [F,N] Default: N
						<br/>Specify:<br/>
                        <b>
                            <code>F</code>
                        </b> for Force Publish. Indicating that inventory activity has not been processed and the Real-Time Availability Monitor should publish the availability picture regardless whether it has changed from the previously published availability.<br/>
                        <b>
                            <code>N</code>
                        </b> otherwise. 
					</Description>
                </Attribute>
                <Attribute EntityName="Inventory_Item"
                    Name="ProductClass" NameInEntity="Product_Class"/>
                <Attribute EntityName="Inventory_Item"
                    Name="UnitOfMeasure" NameInEntity="Uom"/>
            </Attributes>
        </yfc:doc>
    </InventoryActivity>
</InventoryActivityList>
