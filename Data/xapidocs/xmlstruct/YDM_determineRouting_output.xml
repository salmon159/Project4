<RoutingGuide Comments="" DepartmentCode="" EffectiveDate=""
    ExpirationDate="" FreightTerms="" ItemClassification=""
    OrganizationCode="" PublishDate="" RoutingGuideName=""
    RoutingGuideNo="" SupercedesRoutingGuideNo="" Type="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/determineRouting/output">
    <!-- If routing guide details record can not be determined for given criteria, RoutingGuideDetailList node will not be returned even if its in a template. Attributes  ShipmentType and ShipMode are not template driven. -->
    <RoutingGuideDetail FreightTermsOverride=""
        RevisedShipToConsolNode="" RevisedShipToNode="" RevisedShipToStore="">
        <ShipFrom ShipFromCity="" ShipFromCountry="" ShipFromNode=""
            ShipFromState="" ShipFromZip=""/>
        <ShipTo ShipToCity="" ShipToConsolNode="" ShipToCountry=""
            ShipToNode="" ShipToState="" ShipToStore="" ShipToZip=""/>
        <ShippingParameters MaxHandlingUnits="" MaxVolume=""
            MaxWeight="" MinHandlingUnits="" MinVolume="" MinWeight=""
            VolumeUOM="" WeightUOM=""/>
        <RoutingGuideDetailCarrierList>
            <RoutingGuideDetailCarrier AlternateOption="" BestOption=""
                CarrierServiceCode="" ContactKey="" Priority="" Scac=""
                ScacCode="" ShipMode="" ShipmentType="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="AlternateOption">
                            <Description>Indicates that the selected routing guide detail is an alternate option and not the best.
							</Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="BestOption">
                            <Description>Indicates that the selected routing guide detail is the best option.
							</Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="CarrierServiceCode">
                            <Description>A code that identifies a service provided by a carrier.
							</Description>
                        </Attribute>
                        <Attribute DataType="OrgCode" Name="Scac">
                            <Description>The code of the organization plays the role of carrier.
							</Description>
                        </Attribute>
                        <Attribute DataType="Text-4" Name="ScacCode">
                            <Description>Code that uniquely identifies the combination of SCAC and Carrier Service Code.
							</Description>
                        </Attribute>
                        <Attribute DataType="ShipMode" Name="ShipMode">
                            <Description>Shipment mode serviced - PARCEL, TL, etc. 
							</Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ShipmentType">
                            <Description>Indicates the type of Shipment.GOH -Garment on Hangers/Reefer/Blanket wrap - this affects the type of trailer needed.
							</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </RoutingGuideDetailCarrier>
        </RoutingGuideDetailCarrierList>
    </RoutingGuideDetail>
    <Trace>
        <yfc:doc>
            <Element>
                <Description>
                    <div>This element should be provided in the input template to get this node in the output XML. Also this node will come only if no routing guide is found for the given input criteria.					
					</div>
                </Description>
            </Element>
        </yfc:doc>
        <LogMessage Message="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="Message">
                        <Description>This gives a log message indicating the reason for not finding a routing guide for given input criteria.
					</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </LogMessage>
    </Trace>
</RoutingGuide>
