<Batch BatchKey="Conditional" BatchNo="Conditional" BatchStatusDesc=""
    CountRequestNo="" EquipmentType="" ManuallyCreated=""
    MoveRequestNo="" Node="" OrganizationCode="" RemainingMinutes=""
    ShipmentNo="" Status="" TaskType="" WaveNo="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/changeBatch/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="MasterBatch.xml" IncludeXMLPath="/Batch"/>
    <yfc:doc>
        <Attributes>
            <Attribute Name="BatchKey">
                <Description>
                    <div>
				Pass the <code>BatchKey</code> or <code>BatchNo</code> to identify the Batch.
				<Validation>
                            <Condition>
                                <code>BatchKey</code> passed does not exist in the YFS_BATCH table.
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="BatchNo">
                <Description>
                    <div>
				Pass the <code>BatchKey</code> or <code>BatchNo</code> to identify the Batch.
				<Validation>
                            <Condition>
                                <code>BatchNo</code> passed does not exist in the YFS_BATCH table.
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="BatchStatusDesc">
                <Description>
                    <div>Batch Status Description</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Ship_Node" Name="Node" NameInEntity="Ship_Node"/>
        </Attributes>
    </yfc:doc>
    <Tasks yfc:DTDOccurrence="REQUIRED">
        <Task Action="" TaskId="Conditional" TaskKey="Conditional" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute Name="Action">
                        <Description>
                            <div>
					Indicates action to perform
					<br/> Valid values are:
					<br/>
                                <b>
                                    <code>ADD</code>
                                </b> - Adds the task to the batch
					<br/>
                                <b>
                                    <code>REMOVE</code>
                                </b> - Removes the task from the batch 
					</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="TaskKey">
                        <Description>
                            <div>
					Pass either <code>TaskKey</code> or <code>TaskId</code> to identify the task.
					<Validation>
                                    <Condition>
                                    <code>TaskKey</code> passed does not exist in the YFS_TASK table.
						</Condition>
                                    <Error Description="No record found" ErrorCode="YCP0045"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Task>
    </Tasks>
</Batch>
