<!-- ON_ASSIGNMENTS_CHANGE event XML   -->
<CustomerAssignment AssignmentType="" CustomerClassificationCode=""
    CustomerID="" CustomerKey="" CustomerOrganizationCode=""
    OrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/CUSTOMER_DEFINITION/ON_ASSIGNMENTS_CHANGE">
    <yfc:doc>
        <Attributes>
            <Attribute Name="AssignmentType">
                <Description>
                    <div>The type of assignments which need to be synchronized for the customer.  The <code>AssignmentType</code> can be either <code>BUYER</code> or <code>SELLER</code>.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="CustomerClassificationCode"/>
            <Attribute DataType="CustomerId" Name="CustomerID">
                <Description>CustomerId for which the assignments need to be synchronized.</Description>
            </Attribute>
            <Attribute DataType="Key" Name="CustomerKey">
                <Description>Total unique identifier of the customer.</Description>
            </Attribute>
            <Attribute DataType="String" Name="CustomerOrganizationCode">
                <Description>
                    <div>Organization code of organization that defines the Customer. If CustomerID is passed then a valid CustomerOrganizationCode has to be passed.</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</CustomerAssignment>
