    <yfc:includeInParent IncludeXMLChildNodes="Y" IncludeXMLFileName="MasterChangeProject_Input.xml"/>
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Operation" Name="Operation">
                <Description>
                    <div>
						Operation to take
						<br/>
						Valid Values:
						<br/>
                        <b>
                            <code>Manage</code>
                        </b>
						- Creates a new record if it does not exist or modifies an
						existing record if it does.
						<br/>
                        <b>
                            <code>Create</code>
                        </b>
						- Creates a new record.
						<br/>
                        <b>
                            <code>Modify</code>
                        </b>
						- Changes the value of an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b>
						- Deletes an existing record.
						<br/>
						The default behavior is
						<b>
                            <code>Manage</code>
                        </b>
						.
						<Validation>
                            <Condition>The record has invalid values or references to
								non-existing records.</Condition>
                            <Error
                                Description="Change Project cannot be created or updated." ErrorCode="YSC91A_0001"/>
                        </Validation>
                        <Validation>
                            <Condition>Change Project cannot be created or updated in this
								status.</Condition>
                            <Error
                                Description="Change Project cannot be created or updated." ErrorCode="YSC91A_0003"/>
                        </Validation>
                        <Validation>
                            <Condition>Change Project has entity changes associated to it and
								cannot be deleted.</Condition>
                            <Error
                                Description="Change Project cannot be deleted." ErrorCode="YSC91A_0005"/>
                        </Validation>
                        <Validation>
                            <Condition>User is not allowed to perform this operation on the
								Change Project</Condition>
                            <Error
                                Description="Current operation could not be completed." ErrorCode="YSC91A_0006"/>
                        </Validation>
                        <Validation>
                            <Condition>Change Project doesn't have any changes to approve.</Condition>
                            <Error
                                Description="Current operation could not be completed." ErrorCode="YSC91A_0011"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
