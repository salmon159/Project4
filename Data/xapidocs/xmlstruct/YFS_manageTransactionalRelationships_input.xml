<TransactionalRelationships xmlns="http://www.sterlingcommerce.com/documentation/YFS/manageTransactionalRelationships/input">
    <TransactionalRelationship HierarchyLevel="" IsDependencyResolved=""
        Operation="" RelatedTableKey="" RelatedTableName=""
        RelationshipKey="" RelationshipType="Required" RootNodeKey=""
        TableKey="Required" TableName="Required">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="MasterOrderRelationshipInput.xml" IncludeXMLPath="/"/>
        <yfc:doc>
            <Element EntityName="Transactional_Relationship">
                <Description>
                    <div>This table stores details about the relationship between two transactional records.</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Action" Name="Operation">
                    <Description>
                        <div>
                            <p>Operation to perform on this record. Default: If not passed then API will try to either create or modify depending on the existence of the data.</p>
                            <p>Valid Values: </p>
                            <p>
                                <b>
                                    <code>Manage</code>
                                </b> - Manage is the default operation, it will either create or modify depending on the existence of the data.</p>
                            <p>
                                <b>
                                    <code>Create</code>
                                </b> - Create a new record.</p>
                            <p>
                                <b>
                                    <code>Modify</code>
                                </b> - Modify an existing record.</p>
                            <p>
                                <b>
                                    <code>Delete</code>
                                </b> - Delete an existing record.</p>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="RelatedTableName">
                    <Description>
                        <div>The primary key of the related transactional record.<br/>This attribute corresponds to the PrimaryKey Fact. 
						<Validation>
                                <Condition>
								The RelatedTableName must be valid.
							</Condition>
                                <Error
                                    Description="The RelatedTableName is not valid." ErrorCode="OMP956_0002"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="RelationshipType">
                    <Description>
                        <div>The type of relationship between the two transactional records.
						<Validation>
                                <Condition>
								The RelationshipType must be passed.
							</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="TableKey">
                    <Description>
                        <div>The primary key of the transactional record for which this relationship is defined.
						<Validation>
                                <Condition>
								The TableKey must be passed.
							</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="TableName">
                    <Description>
                        <div>The TableName of the transactional record for which this relationship is defined. 
						<Validation>
                                <Condition>
								The TableName must be passed.
							</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                            <Validation>
                                <Condition>
								The TableName must be valid.
							</Condition>
                                <Error
                                    Description="The TableName is not valid." ErrorCode="OMP956_0001"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </TransactionalRelationship>
</TransactionalRelationships>
