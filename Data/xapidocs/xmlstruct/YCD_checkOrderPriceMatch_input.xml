<!--checkOrderPriceMatch Input XML-->
<Order DocumentType="" EnterpriseCode="" OrderHeaderKey="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCD/checkOrderPriceMatch/input">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
                <div/>
            </Description>
        </Element>
    </yfc:doc>
    <OrderLines yfc:DTDOccurrence="REQUIRED">
        <OrderLine OrderLineKey="Required"
            PriceMatchCompetitor="Required" PriceMatchCurrency=""
            PriceMatchMinQuantity="" PriceMatchPrice="Required"
            PriceMatchShippingCharge="" RegionCity="" RegionState=""
            UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Price_Match">
                    <Description>
                        <div/>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Order_Line"
                        Name="OrderLineKey" NameInEntity="Order_Line_Key">
                        <Description>This is the Order Line key Of the OrderLine  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match"
                        Name="PriceMatchCompetitor" NameInEntity="Competitor">
                        <Description>This is the Competitor  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="PriceMatchCurrency" NameInEntity="Currency">
                        <Description>This is the Currency of the Price of the Item  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="PriceMatchMinQuantity" NameInEntity="Minimum_Quantity">
                        <Description>This is the Minimum Quantity for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match" Name="PriceMatchPrice" NameInEntity="New_Unit_Price">
                        <Description>This is the Competitor's Price of the Item  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match"
                        Name="PriceMatchShippingCharge" NameInEntity="Shipping_Charges">
                        <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="RegionCity" NameInEntity="Region_City">
                        <Description>This is the Region City where the Price Match is gettting performed </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="RegionState" NameInEntity="Region_State">
                        <Description>This is the Region State where the Price Match is gettting performed </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="UnitOfMeasure" NameInEntity="Unit_Of_Measure">
                        <Description>This is the Unit Of Measure Of Item which is getting Price Matched </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </OrderLine>
    </OrderLines>
</Order>
