<ManageServiceGrouping EnterpriseCode="Required"
    OrderHeaderKey="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCD/manageServiceGrouping/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
			        The <code>OrderHeaderKey</code> attribute <i>or</i> the <code>EnterpriseCode</code> attributes must be passed to identify the Order.
			 		<Validation>
                        <Condition>The order cannot be identified.</Condition>
                        <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Text-24" Name="EnterpriseCode">
                <Description>
                    <div>The enterprise that is associated with this order.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-24" Name="OrderHeaderKey">
                <Description>
                    <div>The primary key for the YFS_ORDER_HEADER table.</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderLines>
        <OrderLine CurrentWorkOrderGroup="Required"
            CurrentWorkOrderKey="Required" DSOrderLineKey=""
            ItemGroupCode="Required" LineNo="Required"
            NewWorkOrderGroup="Required" OrderLineKey="Required"
            Timezone="" WorkOrderNo="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="PositiveInteger" Name="CurrentWorkOrderGroup">
                        <Description>
                            <div> A number refer as group no., assigned to work order at the time of WO creation.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="DSOrderLineKey">
                        <Description>
                            <div>Order line key of the DS line associated to the PROD line.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="LineNo" Name="LineNo">
                        <Description>
                            <div>The order line number.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="PositiveInteger" Name="NewWorkOrderGroup">
                        <Description>
                            <div> A new number assigned to the WO to create a new WO or move the existing WO to a different group. </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="WorkOrderNo">
                        <Description>
                            <div>The work order number. Identifies a work order within the context of a node and enterprise.</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <BundleParentLine OrderLineKey="" PrimeLineNo=""
                SubLineNo="" TransactionalLineId="">
                <yfc:doc>
                    <Element EntityName="Order_Line">
                        <Description>
					Identifies the bundle parent order line for this line.
					<Validation>
                                <Condition>
                                    <code>OrderLineKey</code>
                                    <i>or</i>
                                    <code>PrimeLineNo</code> and <code>SubLineNo</code>
                                    <i>or</i>
                                    <code>TransactionalLineId</code> must be passed, and must identify a valid order line.
						</Condition>
                                <Error Description="Invalid order line" ErrorCode="OMP10009"/>
                            </Validation>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute EntityName="Order_Line"
                            Name="OrderLineKey" NameInEntity="Order_Line_Key">
                            <Description>
                                <div>
					Primary key generated by the system. May be passed to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                                    <i>or</i>
                                    <br/>the <code>PrimeLineNo</code> and the <code>SubLineNo</code> can be passed.

					</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Order_Line"
                            Name="PrimeLineNo" NameInEntity="Prime_Line_No">
                            <Description>
                                <div>
					The order line number. Must be passed with the <code>SubLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code> can be passed.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Order_Line"
                            Name="SubLineNo" NameInEntity="Sub_Line_No">
                            <Description>
                                <div>
					Must be passed with the <code>PrimeLineNo</code> to identify the <code>OrderLine</code>. Alternatively, the <code>TransactionalLineId</code>
                                    <i>or</i> the <code>OrderLineKey</code> can be passed.
					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="TransactionalLineId">
                            <Description>
                                <div>
					A virtual field meant to identify order lines when <code>PrimeLineNo</code> and <code>SubLineNo</code> are not passed. Used in Line Associations. This value is not stored in the database. While creating a new line and associating it with another line, the same attribute must be passed in the <code>OrderLine</code> element and in this element to uniquely identify a line if <code>OrderLineKey</code> or <code>PrimeLineNo</code> and <code>SubLineNo</code> are not present.
					</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </BundleParentLine>
            <Item ItemDesc="" ItemID="Required" ItemShortDesc=""
                ProductClass="" UnitOfMeasure="Required">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="ItemDesc">
                            <Description>
                                <div> The short description of the item. </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="ItemShortDesc">
                            <Description>
                                <div> The short description of the item. </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-10" Name="ProductClass">
                            <Description>
                                <div> The product class of an item. </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Item>
            <WorkOrderAppointments>
                <WorkOrderAppointment ApptSeq="" ApptStatus=""
                    PromisedApptEndDate="" PromisedApptStartDate="" WorkOrderApptKey=""/>
            </WorkOrderAppointments>
        </OrderLine>
    </OrderLines>
</ManageServiceGrouping>
