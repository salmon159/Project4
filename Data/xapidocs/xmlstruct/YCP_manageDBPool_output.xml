<DBPool PoolId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageDBPool/output">
    <yfc:doc>
        <Element EntityName="DbPool"/>
    </yfc:doc>
    <ParamList>
        <yfc:doc>
            <Element>
                <Description>
                    <div>List of connection parameters. Any supported property in <code>jdbc.properties</code> may be specified here.</div>
                </Description>
            </Element>
        </yfc:doc>
        <Param IsSuppressed="" Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>Connection parameter</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Boolean" Name="IsSuppressed">
                        <Description>
                            <div>Flag to indicate whether the value was filtered from output.</div>
                        </Description>
                        <Options IsEnumeration="Y">
                            <Option
                                Description="Value is not suppressed (default)"
                                IsDefault="Y" Value="N"/>
                            <Option
                                Description="The value attribute was suppressed" Value="Y"/>
                        </Options>
                    </Attribute>
                    <Attribute DataType="String" Name="Name">
                        <Description>
                            <div>Parameter name</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Value">
                        <Description>
                            <div>Parameter value. This attribute is not set if the value is suppressed for some reason.</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Param>
    </ParamList>
</DBPool>
