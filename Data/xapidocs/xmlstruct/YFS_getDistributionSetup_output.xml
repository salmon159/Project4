<Item AvailableQty="" ConsiderAllNodes="" ConsiderAllSegments=""
    ConsiderInventoryNodeControl="" ConsiderUnassignedDemand=""
    Description="" DistributionRuleId="" ExtendedDescription=""
    ItemID="" ProductClass="" ShortDescription="" TotalAvailableQty=""
    UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getDistributionSetup/output">
    <yfc:doc>
        <Element EntityName="Inventory_Item"/>
        <Attributes>
            <Attribute DataType="Quantity" Name="AvailableQty">
                <Description>
                    <div>
				Returned as 0 if <code>RequiredQty</code> was not passed in the input xml. Otherwise <code>AvailableQty</code> is set to how much is available for the required quantity.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderAllNodes">
                <Description>
                    <div>
				Indicates whether all nodes were used in the inventory reporting.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderAllSegments">
                <Description>
                    <div>
				Indicates whether all segments are to be considered in the availability checking.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderInventoryNodeControl">
                <Description>
                    <div>
				Whether Inventory Node Control records are considered.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderUnassignedDemand">
                <Description>
                    <div>
				Indicates whether unassigned demands were used in the inventory reporting.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Item" Name="Description" NameInEntity="Description"/>
            <Attribute EntityName="Item_Ship_Node"
                Name="DistributionRuleId" NameInEntity="Distribution_Rule_Id"/>
            <Attribute EntityName="Item" Name="ExtendedDescription" NameInEntity="Extended_Description"/>
            <Attribute EntityName="Item" Name="ShortDescription" NameInEntity="Short_Description"/>
            <Attribute DataType="Quantity" Name="TotalAvailableQty">
                <Description>
                    <div>
				Total available quantity of all the nodes.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ShipNodes>
        <ShipNode AvailableQty="" Priority="" ShipNode="" Tracked="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Item_Ship_Node"/>
                <Attributes>
                    <Attribute DataType="Quantity" Name="AvailableQty">
                        <Description>
                            <div>
						Available quantity at the nodes.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Item_Ship_Node"
                        Name="ShipNode" NameInEntity="Shipnode_Key"/>
                    <Attribute DataType="Flag" Name="Tracked">
                        <Description>
                            <div>
						Indicates if inventory is tracked at the node.
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ShipNode>
    </ShipNodes>
</Item>
