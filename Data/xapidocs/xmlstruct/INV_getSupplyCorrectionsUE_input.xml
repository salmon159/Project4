<!--getSupplyCorrections Input XML-->
<Items OrderReference="" xmlns="http://www.sterlingcommerce.com/documentation/INV/getSupplyCorrectionsUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="ID-24" Name="OrderReference">
                <Description>
                    <div>
						The <code>OrderReference</code> is the Order Header Key.The substitutes can be adjusted based on order information.	
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Item ItemID="Required" OrganizationCode="Required" ProductClass=""
        UnitOfMeasure="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Element EntityName="Inventory_Item"/>
            <Attributes>
                <Attribute EntityName="Inventory_Item"
                    Name="OrganizationCode" NameInEntity="Organization_Code"/>
            </Attributes>
        </yfc:doc>
        <Supplies>
            <Supply AvailabilityType="" ETA="" Quantity="" Segment=""
                SegmentType="" ShipByDate="" ShipNode=""
                SupplyLineReference="" SupplyReference=""
                SupplyReferenceType="" SupplyType="" yfc:DTDOccurrence="OneOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply"/>
                    <Attributes>
                        <Attribute EntityName="Inventory_Supply"
                            Name="AvailabilityType" NameInEntity="Availability_Type">
                            <Description>
                                <div>
						This field indicates the availability type for this inventory supply.
						<br/>Default: <b>
                                    <code>TRACK</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>TRACK</code>
                                    </b> or <b>
                                    <code>INFINITE</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Supply"
                            Name="ETA" NameInEntity="Eta">
                            <Description>
                                <div>
						The date when this inventory supply will be first available.  If not provided, <code>ETA</code> will be defaulted to a low date of <code>19000101</code>.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Supply"
                            Name="ShipByDate" NameInEntity="Ship_By_Date">
                            <Description>
                                <div>
						The date when this inventory supply will be last available.  If not provided, <code>ShipByDate</code> will be defaulted to a high date of <code>25000101</code>.
						</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                    LotAttribute3="" LotKeyReference="" LotNumber=""
                    ManufacturingDate="" RevisionNo="" TagNumber="">
                    <yfc:doc>
                        <Element EntityName="Inventory_Tag"/>
                    </yfc:doc>
                </Tag>
            </Supply>
        </Supplies>
        <DeliveryMethodForSupplyList>
            <DeliveryMethodForSupply DeliveryMethod="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Text-40" Name="DeliveryMethod">
                            <Description>
                                <div>
						The <code>DeliveryMethod</code> requested for the promise line. Possible values include Delivery DEL, Ship SHP or Pick Up PICK
						</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </DeliveryMethodForSupply>
        </DeliveryMethodForSupplyList>
    </Item>
</Items>
