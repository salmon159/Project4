<GetSurroundingNodeList DistanceToConsider="Required"
    DistanceToConsiderUOM="" FulfillmentType="" NodeType=""
    OrganizationCode="Required" xmlns="http://www.sterlingcommerce.com/documentation/OMP/getSurroundingNodeList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Number-22" Name="DistanceToConsider">
                <Description>
                    <div>
				Nodes in this radius from <code>ShipToAddress</code> will be returned.
				<Validation>
                            <Condition>
                                <code>DistanceToConsider</code> is not passed. 
					</Condition>
                            <Error
                                Description="DistanceToConsider should be greater than 0." ErrorCode="YCP77_00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="UOM" Name="DistanceToConsiderUOM">
                <Description>
                    <div>
				Specifies the UOM for <code>DistanceToConsider</code>. Defaulted to <code>MILE</code> if not passed.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="ID-40" Name="FulfillmentType">
                <Description>
                    <div>
				If passed, sourcing rules for the <code>OrganizationCode</code>'s primary enterprise will be read to determine the valid nodes to be returned. The <code>OrganizationCode</code> will be used as the seller organization. Only those nodes that can be fulfilled by the sourcing rule and those within the specified range will be returned. 
			</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Ship_Node" Name="NodeType" NameInEntity="Node_Type"/>
            <Attribute EntityName="Organization" Name="OrganizationCode">
                <Description>
                    <div>
				Identifies the organization. 
				<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not passed.
					</Condition>
                            <Error
                                Description="Organization code is mandatory for this operation." ErrorCode="YCP0154"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> passed is not in the YFS_ORGANIZATION table.
					</Condition>
                            <Error
                                Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ShipToAddress AddressID="" AddressLine1="" AddressLine2=""
        AddressLine3="" AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" ErrorTxt=""
        EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
        IsCommercialAddress="" JobTitle="" LastName="" Latitude=""
        Longitude="" MiddleName="" MobilePhone="" OtherPhone=""
        PersonID="" PreferredShipAddress="" State="" Suffix=""
        TaxGeoCode="" Title="" UseCount="" VerificationStatus="" ZipCode="">
        <yfc:doc>
            <Element EntityName="Person_Info">
                <Description>
                    <div>
			Address from which distance needs to be considered. If the <code>DistanceFromShipToAddress</code> is to be 
			considered, then the attributes for <code>ZipCode</code> and <code>Country</code> must be passed in the input.
				<Validation>
                            <Condition>
                                <code>ShipToAddress</code> is not passed.
					</Condition>
                            <Error
                                Description="Ship to address not passed." ErrorCode="YCP77_00003"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Person_Info" Name="Country" NameInEntity="Country">
                    <Description>
                        <div>
					The country in the address. Mandatory if <code>DistanceFromShipToAddress</code> is to be considered.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Person_Info" Name="Latitude" NameInEntity="Latitude"/>
                <Attribute EntityName="Person_Info" Name="Longitude" NameInEntity="Longitude"/>
                <Attribute EntityName="Person_Info" Name="ZipCode" NameInEntity="ZipCode">
                    <Description>
                        <div>
					The zip code in the address. Mandatory if <code>DistanceFromShipToAddress</code> is to be considered.
					</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </ShipToAddress>
</GetSurroundingNodeList>
