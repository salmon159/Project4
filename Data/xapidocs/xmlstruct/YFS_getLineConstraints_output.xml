<!-- YFS_getLineConstraints_output.xml -->
<OrderLine DocumentType="" EnterpriseCode="" OrderHeaderKey=""
    OrderLineKey="" OrderNo="" PrimeLineNo="" SubLineNo="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getLineConstraints/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute DataType="OrgCode" Name="EnterpriseCode">
                <Description>
	Enterprise Code
	</Description>
            </Attribute>
            <Attribute DataType="DocumentNo" Name="OrderNo">
                <Description>
	Order Number
	</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Item ItemDesc="" ItemID="" UnitOfMeasure="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="ItemDesc" Name="ItemDesc">
                    <Description>
	Item Description
	</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Item>
    <Constraints>
        <CannotScheduleBeforeConstraints>
            <Constraint ConstrainingDate="" ConstrainingOrderLineKey=""
                ConstraintDate="" ConstraintReason="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="TimeStamp" Name="ConstrainingDate">
                            <Description>
				Date which is causing the constraint
				</Description>
                        </Attribute>
                        <Attribute DataType="Key" Name="ConstrainingOrderLineKey">
                            <Description>
				Primary Key of the Order Line which has the Constraining Date
				</Description>
                        </Attribute>
                        <Attribute DataType="TimeStamp" Name="ConstraintDate">
                            <Description>
				Date on which a constraint has been created. For ex: If Delivery Date is Today but delivery cannot be made before tomorrow, then Constraining Date is tomorrow's date and constraint date is today's date.
				</Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ConstraintReason">
                            <Description>
				Reason due to which constraint has been created.
				</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <OrderLine ItemGroupCode="" OrderLineKey=""
                    PrimeLineNo="" SubLineNo="">
                    <Item ItemDesc="" ItemID="" UnitOfMeasure="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="ItemDesc" Name="ItemDesc"/>
                            </Attributes>
                        </yfc:doc>
                    </Item>
                </OrderLine>
            </Constraint>
        </CannotScheduleBeforeConstraints>
        <CannotScheduleAfterConstraints>
            <Constraint ConstrainingDate="" ConstrainingOrderLineKey=""
                ConstraintDate="" ConstraintReason="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="TimeStamp" Name="ConstrainingDate"/>
                        <Attribute DataType="Key" Name="ConstrainingOrderLineKey"/>
                        <Attribute DataType="TimeStamp" Name="ConstraintDate"/>
                        <Attribute DataType="Text-40" Name="ConstraintReason"/>
                    </Attributes>
                </yfc:doc>
                <OrderLine ItemGroupCode="" OrderLineKey=""
                    PrimeLineNo="" SubLineNo="">
                    <Item ItemDesc="" ItemID="" UnitOfMeasure="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="ItemDesc" Name="ItemDesc"/>
                            </Attributes>
                        </yfc:doc>
                    </Item>
                </OrderLine>
            </Constraint>
        </CannotScheduleAfterConstraints>
        <CannotScheduleConstraints>
            <Constraint ConstrainingDate="" ConstrainingOrderLineKey=""
                ConstraintDate="" ConstraintReason="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="TimeStamp" Name="ConstrainingDate"/>
                        <Attribute DataType="Key" Name="ConstrainingOrderLineKey"/>
                        <Attribute DataType="TimeStamp" Name="ConstraintDate"/>
                        <Attribute DataType="Text-40" Name="ConstraintReason"/>
                    </Attributes>
                </yfc:doc>
                <OrderLine ItemGroupCode="" OrderLineKey=""
                    PrimeLineNo="" SubLineNo="">
                    <Item ItemDesc="" ItemID="" UnitOfMeasure="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="ItemDesc" Name="ItemDesc"/>
                            </Attributes>
                        </yfc:doc>
                    </Item>
                </OrderLine>
            </Constraint>
        </CannotScheduleConstraints>
    </Constraints>
</OrderLine>
