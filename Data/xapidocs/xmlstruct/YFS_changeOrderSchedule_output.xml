<!-- ChangeSchedule is not a template-driven element. It is returned same as what was passed to the api. -->
<ChangeSchedule AuditTransactionId="" DocumentType="" EnterpriseCode=""
    ModificationReasonCode="" ModificationReasonText=""
    ModificationReference1="" ModificationReference2=""
    ModificationReference3="" ModificationReference4=""
    OrderHeaderKey="" OrderNo="" Override="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/changeOrderSchedule/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Order_Audit"
                Name="AuditTransactionId" NameInEntity="Audit_Transaction_Id"/>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute DataType="String" EntityName="Order_Header"
                Name="EnterpriseCode" NameInEntity="Enterprise_Code"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonCode" NameInEntity="Reason_Code">
                <Description>
			Modification Reason Code
			</Description>
            </Attribute>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonText" NameInEntity="Reason_Text">
                <Description>
			Modification Reason Text
			</Description>
            </Attribute>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference1" NameInEntity="Reference_1"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference2" NameInEntity="Reference_2"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference3" NameInEntity="Reference_3"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference4" NameInEntity="Reference_4"/>
            <Attribute EntityName="Order_Header" Name="OrderHeaderKey" NameInEntity="Order_Header_Key"/>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No"/>
            <Attribute DataType="Flag" Name="Override">
                <Description>Whether or not the status change validation was overridden.
			  </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <FromSchedules>
        <!-- FromSchedule is not a template-driven element. It is returned same as what was passed to the api. -->
        <FromSchedule FromBatchNo="" FromExpectedDeliveryDate=""
            FromExpectedShipDate="" FromLotNo="" FromLotNumber=""
            FromRevisionNo="" FromStatus="Required" OrderHeaderKey=""
            OrderLineKey="" OrderLineScheduleKey="" OrderReleaseKey=""
            PrimeLineNo="" ReleaseNo="" SubLineNo="">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
				Contains the schedule information from which the quantity needs to be moved. This is the input data.
				</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Text-40"
                        EntityName="Order_Line_Schedule"
                        Name="FromBatchNo" NameInEntity="Batch_No"/>
                    <Attribute DataType="Date"
                        EntityName="Order_Line_Schedule"
                        Name="FromExpectedDeliveryDate" NameInEntity="Expected_Delivery_Date">
                        <Description>
                            <div>
				Specifies the ExpectedShipmentDate on the order line schedule record before the change
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Date"
                        EntityName="Order_Line_Schedule"
                        Name="FromExpectedShipDate" NameInEntity="Expected_Shipment_Date"/>
                    <Attribute DataType="Text-40"
                        EntityName="Order_Line_Schedule"
                        Name="FromLotNo" NameInEntity="Lot_No">
                        <Description>
                            <div>
				Specifies the BatchNo before the change
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40"
                        EntityName="Order_Line_Schedule"
                        Name="FromLotNumber" NameInEntity="Lot_Number">
                        <Description>
                            <div>
				Specifies the LotNo before the change
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40"
                        EntityName="Order_Line_Schedule"
                        Name="FromRevisionNo" NameInEntity="Revision_No">
                        <Description>
                            <div>
				Specifies the RevisionNo before the change
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Release_Status"
                        Name="FromStatus" NameInEntity="Status">
                        <Description>
                            <div>
				Specifies the status before the change
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Header"
                        Name="OrderHeaderKey" NameInEntity="Order_Header_Key"/>
                    <Attribute DataType="Key"
                        EntityName="Order_Line_Schedule"
                        Name="OrderLineKey" NameInEntity="OrderLineKey"/>
                    <Attribute DataType="Key"
                        EntityName="Order_Line_Schedule"
                        Name="OrderLineScheduleKey" NameInEntity="Order_Line_Schedule_Key"/>
                    <Attribute DataType="Key" EntityName="Order_Release"
                        Name="OrderReleaseKey" NameInEntity="OrderReleaseKey"/>
                    <Attribute DataType="LineNo" EntityName="Order_Line"
                        Name="PrimeLineNo" NameInEntity="PrimeLineNo"/>
                    <Attribute DataType="Count"
                        EntityName="Order_Release" Name="ReleaseNo" NameInEntity="ReleaseNo"/>
                    <Attribute DataType="LineNo" EntityName="Order_Line"
                        Name="SubLineNo" NameInEntity="SubLineNo"/>
                </Attributes>
            </yfc:doc>
            <ToSchedules>
                <!-- ToSchedule is not a template-driven element. It is returned same as what was passed to the api. -->
                <ToSchedule Quantity="Required" ToBatchNo=""
                    ToExpectedDeliveryDate="" ToExpectedShipDate=""
                    ToLotNumber="" ToRevisionNo="">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
				Contains the schedule information to which the quantity needs to be moved. This is the input data.
				</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="Quantity">
                                <Description>
                                    <div>
						Specifies the Quantity associated with new schedule record.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Text-40"
                                EntityName="Order_Line_Schedule"
                                Name="ToBatchNo" NameInEntity="Batch_No">
                                <Description>
                                    <div>
						Specifies the new BatchNo
						</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Date"
                                EntityName="Order_Line_Schedule"
                                Name="ToExpectedDeliveryDate" NameInEntity="Expected_Delivery_Date">
                                <Description>
                                    <div>
						Specifies the new ExpectedDeliveryDate on the Order Line Schedule record
						</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Date"
                                EntityName="Order_Line_Schedule"
                                Name="ToExpectedShipDate" NameInEntity="Expected_Shipment_Date">
                                <Description>
                                    <div>
						Specifies the new ExpectedShipDate on the Order Line Schedule record
						</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Text-40"
                                EntityName="Order_Line_Schedule"
                                Name="ToLotNumber" NameInEntity="Lot_Number">
                                <Description>
                                    <div>
						Specifies the new LotNO
						</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Text-40"
                                EntityName="Order_Line_Schedule"
                                Name="ToRevisionNo" NameInEntity="Revision_No">
                                <Description>
                                    <div>
						Specifies the new RevisionNo on the Order Line Schedule record
						</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <ToScheduleTranQuantity Quantity="" TransactionalUOM="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Quantity" Name="Quantity">
                                    <Description>Quantity in the Transactional UOM.
					  </Description>
                                </Attribute>
                                <Attribute DataType="UOM" Name="TransactionalUOM">
                                    <Description>UOM on which all the transactional quantities are specified.
					  </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </ToScheduleTranQuantity>
                </ToSchedule>
            </ToSchedules>
        </FromSchedule>
    </FromSchedules>
    <!-- OrderAudit is a template-driven element.  -->
