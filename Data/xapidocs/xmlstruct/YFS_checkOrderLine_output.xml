<CheckOrderLine xmlns="http://www.sterlingcommerce.com/documentation/YFS/checkOrderLine/output">
    <InventoryInformation yfc:DTDOccurrence="REQUIRED">
        <Item AllocationDate="" AvailableToSell="" Description=""
            EndDate="" InventoryIndicator="" ItemID="" LeadTime=""
            PeriodicalLength="" ProcessingTime="" ProductClass=""
            ReservationDate="" ShipDate="" ShortDescription=""
            TotalAllocated="" TotalAvailable="" TotalBackOrdered=""
            TotalIncoming="" TotalOnHand="" TotalOnHandAvailable=""
            TotalOnHandHeld="" TotalOpenOrder="" TotalOutgoing=""
            TotalOutgoingForSegments="" TotalOutgoingOrders=""
            TotalReservedForOrders="" TotalReservedForSegments=""
            TrackedEverywhere="" UnitOfMeasure="" yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="AllocationDate"/>
                    <Attribute DataType="String" Name="AvailableToSell"/>
                    <Attribute DataType="String" Name="EndDate"/>
                    <Attribute DataType="String" Name="InventoryIndicator"/>
                    <Attribute DataType="String" Name="PeriodicalLength"/>
                    <Attribute DataType="String" Name="ProductClass"/>
                    <Attribute DataType="String" Name="ReservationDate"/>
                    <Attribute DataType="String" Name="ShipDate"/>
                    <Attribute DataType="String" Name="TotalAllocated"/>
                    <Attribute DataType="String" Name="TotalAvailable"/>
                    <Attribute DataType="String" Name="TotalBackOrdered"/>
                    <Attribute DataType="String" Name="TotalIncoming"/>
                    <Attribute DataType="String" Name="TotalOnHand"/>
                    <Attribute DataType="String" Name="TotalOnHandAvailable"/>
                    <Attribute DataType="String" Name="TotalOnHandHeld"/>
                    <Attribute DataType="String" Name="TotalOpenOrder"/>
                    <Attribute DataType="String" Name="TotalOutgoing"/>
                    <Attribute DataType="String" Name="TotalOutgoingForSegments"/>
                    <Attribute DataType="String" Name="TotalOutgoingOrders"/>
                    <Attribute DataType="String" Name="TotalReservedForOrders"/>
                    <Attribute DataType="String" Name="TotalReservedForSegments"/>
                    <Attribute DataType="String" Name="TrackedEverywhere"/>
                </Attributes>
            </yfc:doc>
            <GlobalInventory yfc:DTDOccurrence="REQUIRED">
                <ShipNodes yfc:DTDOccurrence="REQUIRED">
                    <ShipNode ShipNode="" ShipNodeDescription=""
                        TotalAllocated="" TotalAvailable=""
                        TotalOnHandAvailable="" TotalOnHandHeld=""
                        Tracked="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="String" Name="ShipNodeDescription"/>
                                <Attribute DataType="String" Name="TotalAllocated"/>
                                <Attribute DataType="String" Name="TotalAvailable"/>
                                <Attribute DataType="String" Name="TotalOnHandAvailable"/>
                                <Attribute DataType="String" Name="TotalOnHandHeld"/>
                                <Attribute DataType="String" Name="Tracked"/>
                            </Attributes>
                        </yfc:doc>
                    </ShipNode>
                </ShipNodes>
            </GlobalInventory>
            <AvailableToPromiseInventory AvailableQuantity="" EndDate=""
                FromDate="" IncomingQty="" OutgoingQty="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="AvailableQuantity"/>
                        <Attribute DataType="String" Name="EndDate"/>
                        <Attribute DataType="String" Name="FromDate"/>
                        <Attribute DataType="String" Name="IncomingQty"/>
                        <Attribute DataType="String" Name="OutgoingQty"/>
                    </Attributes>
                </yfc:doc>
            </AvailableToPromiseInventory>
            <InventoryCalculator RequiredQty="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="RequiredQty"/>
                    </Attributes>
                </yfc:doc>
                <Inventory AvailableQuantity="" Date="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="AvailableQuantity"/>
                            <Attribute DataType="String" Name="Date"/>
                        </Attributes>
                    </yfc:doc>
                </Inventory>
            </InventoryCalculator>
            <IncomingPurchaseOrders yfc:DTDOccurrence="REQUIRED">
                <IncomingPurchaseOrder AvailableReference=""
                    CanAllocateDate="" ETA="" ExpirationDate=""
                    Quantity="" ShipNode="" ShipNodeDescription="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="AvailableReference"/>
                            <Attribute DataType="String" Name="CanAllocateDate"/>
                            <Attribute DataType="String" Name="ETA"/>
                            <Attribute DataType="String" Name="ExpirationDate"/>
                            <Attribute DataType="String" Name="Quantity"/>
                            <Attribute DataType="String" Name="ShipNode"/>
                            <Attribute DataType="String" Name="ShipNodeDescription"/>
                        </Attributes>
                    </yfc:doc>
                </IncomingPurchaseOrder>
            </IncomingPurchaseOrders>
            <ItemReservations yfc:DTDOccurrence="REQUIRED">
                <ItemReservation AllocationDate="" ReservationDate=""
                    ReservationID="" ReservationPool=""
                    ReservedQuantity="" ShipDate="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="AllocationDate"/>
                            <Attribute DataType="String" Name="ReservationDate"/>
                            <Attribute DataType="String" Name="ReservationID"/>
                            <Attribute DataType="String" Name="ReservationPool"/>
                            <Attribute DataType="String" Name="ReservedQuantity"/>
                            <Attribute DataType="String" Name="ShipDate"/>
                        </Attributes>
                    </yfc:doc>
                </ItemReservation>
            </ItemReservations>
            <AvailableToSegment EndDate="" FromDate=""
                ReservedQuantity="" TotalAvailableQty="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="EndDate"/>
                        <Attribute DataType="String" Name="FromDate"/>
                        <Attribute DataType="String" Name="ReservedQuantity"/>
                        <Attribute DataType="String" Name="TotalAvailableQty"/>
                    </Attributes>
                </yfc:doc>
            </AvailableToSegment>
        </Item>
    </InventoryInformation>
    <LinePriceInfo DiscountPercentage="" FixedHandlingCharges=""
        FixedPersonalizationCharges="" FixedShippingCharges=""
        InvoicedPricingQty="" IsLinePriceForInformationOnly=""
        LineTotal="" ListPrice="" PricingQty=""
        PricingQtyConversionFactor="" PricingUOM="" RetailPrice=""
        Tax="" TaxPercentage="" TaxableFlag="" TotalHandlingCharges=""
        TotalPersonalizationCharges="" TotalShippingCharges=""
        UnitHandlingCharges="" UnitPersonalizationCharges=""
        UnitPrice="" UnitShippingCharges="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="DiscountPercentage"/>
                <Attribute DataType="String" Name="FixedHandlingCharges"/>
                <Attribute DataType="String" Name="FixedPersonalizationCharges"/>
                <Attribute DataType="String" Name="FixedShippingCharges"/>
                <Attribute DataType="String" Name="InvoicedPricingQty"/>
                <Attribute DataType="String" Name="IsLinePriceForInformationOnly"/>
                <Attribute DataType="String" Name="LineTotal"/>
                <Attribute DataType="String" Name="ListPrice"/>
                <Attribute DataType="String" Name="PricingQty"/>
                <Attribute DataType="String" Name="PricingQtyConversionFactor"/>
                <Attribute DataType="String" Name="PricingUOM"/>
                <Attribute DataType="String" Name="RetailPrice"/>
                <Attribute DataType="String" Name="Tax"/>
                <Attribute DataType="String" Name="TaxPercentage"/>
                <Attribute DataType="String" Name="TaxableFlag"/>
                <Attribute DataType="String" Name="TotalHandlingCharges"/>
                <Attribute DataType="String" Name="TotalPersonalizationCharges"/>
                <Attribute DataType="String" Name="TotalShippingCharges"/>
                <Attribute DataType="String" Name="UnitHandlingCharges"/>
                <Attribute DataType="String" Name="UnitPersonalizationCharges"/>
                <Attribute DataType="String" Name="UnitPrice"/>
                <Attribute DataType="String" Name="UnitShippingCharges"/>
            </Attributes>
        </yfc:doc>
    </LinePriceInfo>
</CheckOrderLine>
