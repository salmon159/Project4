<!-- Either ShipmentKey or ShipmentNo is required -->
<!-- OverrideModificationRules is used to override the statuses of shipment that can be considered for pack. This is determined using the modification_type SHIPMENT_PACK -->
<Shipment OverrideModificationRules="Y|N|"
    SellerOrganizationCode="Conditional" ShipNode="Conditional"
    ShipmentKey="Conditional" ShipmentNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YDM/beforePackShipmentUE/input">
    <yfc:includeInParent IncludeXMLFileName="YFS_Shipment_keys.xml" IncludeXMLPath="/Shipment"/>
    <yfc:doc>
        <Element EntityName="Shipment">
            <Description>
                <div>
       Pass  <code>ShipmentKey</code>
                    <i>or</i> pass <code>ShipmentNo</code>, <code>ShipNode</code> and <code>SellerOrganizationCode</code>  to identify the shipment.
       </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Flag" Name="OverrideModificationRules">
                <Description>OverrideModificationRules is used to override the statuses of shipment that can be considered for packing. This is determined using the modification_type PACK_SHIPMENT. Valid values are Y, N and Void. Void implies N. 
			</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Containers yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
                        <Validation>
                            <Condition>If element is not passed
							</Condition>
                            <Error
                                Description="Missing element in input XML." ErrorCode="YFS10507"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
        <!-- Only one Container will be processed in one call to this API. All other Container nodes will be ignored. Read Javadocs for additional details. -->
        <Container ActualFreightCharge="" AppliedWeight=""
            AppliedWeightUOM="" AstraCode="" BarcodeDiscount=""
            BasicFreightCharge="" CarriageValue="" CarrierBillAccount=""
            CarrierLocationID="" CommitmentCode=""
            ContainerGrossWeight="" ContainerGrossWeightUOM=""
            ContainerHeight="" ContainerHeightUOM="" ContainerLength=""
            ContainerLengthUOM="" ContainerNetWeight=""
            ContainerNetWeightUOM="" ContainerNo="" ContainerScm=""
            ContainerSeqNo="" ContainerType="" ContainerWidth=""
            ContainerWidthUOM="" CustCarrierAccountNo="" CustomsValue=""
            DeclaredValue="" DeliverBy="" DeliveryDay="" DimmedFlag=""
            DiscountAmount="" ExportLicenseExpDate="" ExportLicenseNo=""
            FormID="" OversizedFlag="" ParentContainerNo=""
            RoutingCode="" ServiceType="" ShipDate=""
            SpecialServicesSurcharge="" TrackingNo="" Zone="">
            <yfc:doc>
                <Element EntityName="Shipment_Container"/>
                <Attributes>
                    <Attribute EntityName="Shipment_Container"
                        Name="ContainerScm" NameInEntity="Container_Scm">
                        <Description>
                            <div>
						An unique number which helps identify the shipper as well as the goods.
							<Validation>
                                    <Condition>
                                    <code>ContainerScm</code>Should be unique in the <code>YFS_Shipment_Container</code> table.
								</Condition>
                                    <Error
                                    Description="Node not passed." ErrorCode="YDM00132"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ContainerDetails>
                <!--One amongst ShipmentLineKey OR ShipmentLine child element must be passed-->
                <ContainerDetail ContainerDetailsKey="" Quantity=""
                    ShipmentLineKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <ShipmentLine DocumentType="" ItemID="" OrderNo=""
                        PrimeLineNo="" ProductClass="" ReleaseNo=""
                        ShipmentLineNo="Required" SubLineNo="" UnitOfMeasure="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="String" Name="DocumentType"/>
                            </Attributes>
                        </yfc:doc>
                    </ShipmentLine>
                    <ShipmentTagSerials>
                        <ShipmentTagSerial BatchNo="" LotAttribute1=""
                            LotAttribute2="" LotAttribute3=""
                            LotKeyReference="" LotNumber=""
                            ManufacturingDate="" Quantity=""
                            RevisionNo="" SerialNo="" ShipByDate=""
                            ShipmentTagSerialKey="" yfc:DTDOccurrence="ZeroOrMany"/>
                    </ShipmentTagSerials>
                </ContainerDetail>
            </ContainerDetails>
            <Instructions>
                <Instruction InstructionText="" InstructionType=""
                    InstructionURL="" InstructionUsage="" SequenceNo="" yfc:DTDOccurrence="ZeroOrMany"/>
            </Instructions>
            <AdditionalAttributes>
                <AdditionalAttribute Name="" Value="" yfc:DTDOccurrence="ZeroOrMany"/>
            </AdditionalAttributes>
            <SpecialServices>
                <SpecialService SpecialServiceSurcharge=""
                    SpecialServicesCode="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Special_Service_Ref"/>
                    </yfc:doc>
                </SpecialService>
            </SpecialServices>
        </Container>
    </Containers>
</Shipment>
