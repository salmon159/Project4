<ValidationList TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/getValidationList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="TotalNumberOfRecords"/>
        </Attributes>
    </yfc:doc>
    <Validation CanApprovalBePostponed="" ConfigurationGroup=""
        Data_Capture="" Description="" Domain="" ErrorCode=""
        ExternalInitPerform="" InputTemplate=""
        IsApprovalRequiredOnReoccurence="" IsOverridable=""
        IsSystemDefined="" IsUserExitActive="" ModelTemplate=""
        OrganizationCode="" ValidationID="" ValidationKey=""
        ValidationType="" ViolationEntity="" yfc:AllowAnyElement="skip" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="MasterValidation.xml" IncludeXMLPath="/Validation"/>
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Validation" Name="IsApprovalRequiredOnReoccurence">
                    <Description>
                        <div>
	                    Indicates if each transaction violation for this validation needs to be approved (Y) or if subsequent violations for the same validation do not need to be approved (N).
	                    </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Validation" Name="IsOverridable">
                    <Description>
                        <div>
	                    This flag is mainly used by SBC to indicates that the validation can be overridden by overridden rules.
	                     </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Validation" Name="ViolationEntity">
                    <Description>
                        <div>
						This specifies the violation entity for storing the violations and approver information.Violation entity should have corresponding transaction info entity.  
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ValidationReferenceList TotalNumberOfRecords="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="TotalNumberOfRecords"/>
                </Attributes>
            </yfc:doc>
            <ValidationReference DataType="" IsPrimaryReference=""
                ReferenceName="" ValidationReferenceKey="" yfc:DTDOccurrence="ZeroOrMany"/>
        </ValidationReferenceList>
        <InputTemplate>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
	                 Input template to be used in business API.</div>
                    </Description>
                </Element>
            </yfc:doc>
        </InputTemplate>
        <ModelTemplate>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
	                 Model template to be used in SBC.
	                   </div>
                    </Description>
                </Element>
            </yfc:doc>
        </ModelTemplate>
        <PreDefinedGreexRule>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
	               			The pre-defined greex rules for the validation. This can also be used as a starting point for creating the override rules for the validation that is externally performed.
	                   </div>
                    </Description>
                </Element>
            </yfc:doc>
        </PreDefinedGreexRule>
        <DataCapture>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
	               			This is used to indicate the attributes whose values need to be captured to be used as part of the greex condition defined in the pre-defined greex rule.
	                   </div>
                    </Description>
                </Element>
            </yfc:doc>
        </DataCapture>
    </Validation>
</ValidationList>
