<!-- YCP_getAccessTokenDetails_Output.xml -->
<AccessToken AccessTokenKey="" ActiveFlag="" ApplicationName=""
    IsValid="Required" SessionId="" TokenExpireTS=""
    TokenValue="Required" UserId="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getAccessTokenDetails/output">
    <yfc:doc>
        <Element>
            <Description>
                <div/>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Access_Token" Name="AccessTokenKey" NameInEntity="Access_Token_Key"/>
            <Attribute EntityName="Access_Token" Name="ActiveFlag" NameInEntity="Active_Flag">
                <Description>
                    <div>The attribute will have "Y" is the token is active. "N" if the token is inactive. This flag does indicate whether the token has expired.</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Access_Token" Name="ApplicationName" NameInEntity="Application_Name"/>
            <Attribute DataType="Boolean" Name="IsValid">
                <Description>
                    <div>Flag to indicate whether the token is valid or not.</div>
                </Description>
                <Options IsEnumeration="Y">
                    <Option
                        Description="The token is expired or inactive." Value="N"/>
                    <Option
                        Description="The token is active and not yet expired." Value="Y"/>
                </Options>
            </Attribute>
            <Attribute EntityName="Access_Token" Name="SessionId" NameInEntity="Session_Id"/>
            <Attribute EntityName="Access_Token" Name="TokenExpireTS" NameInEntity="Token_Expire_TS"/>
            <Attribute EntityName="Access_Token" Name="TokenValue" NameInEntity="Token_Value"/>
            <Attribute EntityName="Access_Token" Name="UserId" NameInEntity="User_Id"/>
        </Attributes>
    </yfc:doc>
</AccessToken>
