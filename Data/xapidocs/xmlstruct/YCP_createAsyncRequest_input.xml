<CreateAsyncRequest xmlns="http://www.sterlingcommerce.com/documentation/YCP/createAsyncRequest/input">
    <API IsService="" Name="Required">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="IsService">
                    <Description>
                        <div>Whether to call an API or Service.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="Name">
                    <Description>
                        <div>Name of the API or Service to call.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <!-- The Input element must contain a child element that corresponds to
 the document element of the api input -->
        <Input yfc:AllowAnyElement="skip" yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element>
                    <Description>The Input element must contain a child element that corresponds to
	 the document element of the api input.</Description>
                </Element>
            </yfc:doc>
        </Input>
    </API>
</CreateAsyncRequest>
