<!-- processRoutingExternally UserExit Input XML -->
<PromiseShipments CustomerId="" EnterpriseCode="" OptimizationType=""
    OptionNo="" xmlns="http://www.sterlingcommerce.com/documentation/OMP/processRoutingExternally/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Text-24" Name="CustomerId">
                <Description>
                    <div>
						Indicates the Customer ID on which behalf inquiry is being
						made. Customer
						preferences are used in capacity computations.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="EnterpriseCode">
                <Description>
                    <div>
						Inventory is being checked on behalf of the organization specified
						by
						<code>EnterpriseCode</code>
						.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-2" Name="OptimizationType">
                <Description>
                    <div>
						This value determines the optimization rules to be applied to the
						scheduling process.
						<br/>
						Default:
						<b>
                            <code>01</code>
                        </b>
                        <br/>
						Valid Values:
						<br/>
                        <b>
                            <code>01</code>
                        </b>
						: Optimize on date.
						<br/>
                        <b>
                            <code>02</code>
                        </b>
						: Optimize on ShipNode priority.
						<br/>
                        <b>
                            <code>03</code>
                        </b>
						: Optimize on number of shipments.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Number" Name="OptionNo">
                <Description>
                    <div>
		Option Number
		</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PromiseShipment Currency="" DeliveryMethod="" GroupID=""
        HandlingUnits="" ShipNode="" ShipmentId="Required"
        TotalVolume="" TotalWeight="" VolumeUOM="" WeightUOM="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Text-24" Name="Currency">
                    <Description>
                        <div>
							Currency in which Unit Cost is returned. If attribute is not
							returned,
							Enterprise's local currency will be asssumed.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-24" Name="DeliveryMethod">
                    <Description>
                        <div/>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-24" Name="GroupID">
                    <Description>
                        <div/>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-24" Name="HandlingUnits">
                    <Description>
                        <div/>
                    </Description>
                </Attribute>
                <Attribute DataType="ShipNode" Name="ShipNode">
                    <Description>
                        <div>
							Shipping node from which the inventory should be found.   
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="ID-10" Name="ShipmentId">
                    <Description>
                        <div>
							Unique Identifier for each Promise Shipment.
							<code>ShipmentId</code>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-24" Name="TotalVolume">
                    <Description>
                        <div/>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-24" Name="TotalWeight">
                    <Description>
                        <div/>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-24" Name="VolumeUOM">
                    <Description>
                        <div/>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-24" Name="WeightUOM">
                    <Description>
                        <div/>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ShipToAddress AddressLine1="" AddressLine2="" AddressLine3=""
            AddressLine4="" AddressLine5="" AddressLine6=""
            AlternateEmailID="" Beeper="" City="" Company="" Country=""
            DayFaxNo="" DayPhone="" Department="" EMailID=""
            EveningFaxNo="" EveningPhone="" FirstName=""
            IsCommercialAddress="" JobTitle="" LastName="" MiddleName=""
            MobilePhone="" OtherPhone="" PersonID="" State="" Suffix=""
            TaxGeoCode="" Title="" ZipCode="">
            <yfc:doc>
                <Element EntityName="Person_Info"/>
            </yfc:doc>
        </ShipToAddress>
        <CarrierOption BreakBulkNode="" CarrierServiceCode="" SCAC="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="BreakBulkNode">
                        <Description>
                            <div/>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="CarrierServiceCode">
                        <Description>
                            <div>
								Carrier service code.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="SCAC">
                        <Description>
                            <div>
								SCAC
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </CarrierOption>
        <PossibleCarriers>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
							Carrier services available for this assignment. You may filter out these in the UE output.
						</div>
                    </Description>
                </Element>
            </yfc:doc>
            <Carrier CarrierServiceCode="" ExpectedDeliveryDate=""
                ExpectedShipmentDate="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Carrier_Service"/>
                    <Attributes>
                        <Attribute DataType="TimeStamp" Name="ExpectedDeliveryDate">
                            <Description>
                                <div>Expected delivery date. </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="TimeStamp" Name="ExpectedShipmentDate">
                            <Description>
                                <div>Expected shipment date. </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Carrier>
        </PossibleCarriers>
        <PromiseShipmentLines TotalNumberOfRecords="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-24" Name="TotalNumberOfRecords">
                        <Description>
                            <div/>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <PromiseShipmentLine AvailableQuantity=""
                BundleParentLineId="" CurrentItemCost=""
                FulfillmentType="" IsBundleParent="" ItemGroupCode=""
                ItemID="" ItemUnitCost="" ItemUnitHeight=""
                ItemUnitHeightUOM="" ItemUnitLength=""
                ItemUnitLengthUOM="" ItemUnitVolume=""
                ItemUnitVolumeUOM="" ItemUnitWeight=""
                ItemUnitWeightUOM="" ItemUnitWidth=""
                ItemUnitWidthUOM="" ItemUnitlWeightUOM=""
                NodeProcessingDate="" PromiseShipmentLineKey="" UnitOfMeasure="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="AvailableQuantity">
                            <Description>
                                <div>
									Quantity available at the ship node without any
									substitutiion and procurement.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="BundleParentLineId">
                            <Description>
                                <div>
									Unique identifier for the parent line.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="CurrentItemCost">
                            <Description>
                                <div/>
                            </Description>
                        </Attribute>
                        <Attribute DataType="ID-40" Name="FulfillmentType">
                            <Description>
                                <div>
									Depicts the method of fulfilling the order line during
									scheduling.
									This field can be matched with the corresponding
									field in
									YFS_Sourcing_Rule_Hdr table and appropriate sourcing
									rule can
									be selected. 
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsBundleParent">
                            <Description>
                                <div>
									Flag to indicate if the promise line is a bundle parent.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="ItemGroupCode" Name="ItemGroupCode">
                            <Description>
                                <div>
									Item Group Code of the Item present in line. Possible values
									are Product
									<b>PROD</b>
									, Delivery
									<b>DS</b>
									or Provided Service
									<b>PS</b>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemID">
                            <Description>
                                <div>
									The ItemId of the original item.
         						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitCost">
                            <Description>
                                <div>
									Unit cost of the item
            					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitHeight">
                            <Description>
                                <div>
									Height of the unit item
          						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitHeightUOM">
                            <Description>
                                <div>
									Unit of measure of the height
          						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitLength">
                            <Description>
                                <div>
									length of the unit item
           						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitLengthUOM">
                            <Description>
                                <div>
									Unit of measure of the length
            					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitVolume">
                            <Description>
                                <div>
									Volume of the unit item
            					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitVolumeUOM">
                            <Description>
                                <div>
									Unit of measure of the volume
         						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitWeight">
                            <Description>
                                <div>
									Unit of measure of the weight
            					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="ItemUnitWeightUOM"/>
                        <Attribute DataType="Text-40" Name="ItemUnitWidth">
                            <Description>
                                <div>
									width of the unit item
            					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitWidthUOM">
                            <Description>
                                <div>
									Unit of measure of the width
            					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="ItemUnitlWeightUOM">
                            <Description>
                                <div>
									Unit of measure of the weight
            					</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Date" Name="NodeProcessingDate">
                            <Description>
                                <div>
									Date on which inventory and node capacity are available at the suggested ShipNode. This is applicable only for <Code>findInventory</Code> api when <code>DetermineCarrierService</code> flag is set to <code>
                                    <b>Y</b>
                                    </code>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="PromiseShipmentLineKey">
                            <Description>
                                <div>
									Unique identifier for each shipment line (choice).
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Text-40" Name="UnitOfMeasure">
                            <Description>
                                <div/>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Substitutions>
                    <Substitution AvailableQuantity="" ItemID=""
                        ProductClass="" UOM="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Quantity" Name="AvailableQuantity">
                                    <Description>
                                    <div>
											Quantity available to be substituted
								</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="ItemID" Name="ItemID">
                                    <Description>
                                    <div>
											ItemID of the substitute Item
										</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="ProductClass" Name="ProductClass">
                                    <Description>
                                    <div>
											Product class of the substitute Item
										</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="UOM" Name="UOM">
                                    <Description>
                                    <div>
											Unit Of Measure of the substitute Item
										</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <Procurements>
                            <Procurement AvailableQuantity="" ProcureFromNode="">
                                <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="Quantity" Name="AvailableQuantity">
                                    <Description>
                                    <div>
													Quantity available to be substituted
												</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="ShipNode" Name="ProcureFromNode">
                                    <Description>
                                    <div>
													Indicates node from which Substitute is being procured.
												</div>
                                    </Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                            </Procurement>
                        </Procurements>
                    </Substitution>
                </Substitutions>
                <Procurements>
                    <Procurement AvailableQuantity="" ProcureFromNode="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Quantity" Name="AvailableQuantity">
                                    <Description>
                                    <div>
											Quantity available to be substituted
										</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="ShipNode" Name="ProcureFromNode">
                                    <Description>
                                    <div>
											Indicates node from which item is being procured.
										</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </Procurement>
                </Procurements>
            </PromiseShipmentLine>
        </PromiseShipmentLines>
    </PromiseShipment>
</PromiseShipments>
