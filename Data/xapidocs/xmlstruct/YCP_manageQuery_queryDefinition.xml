<QueryDefinition QueryKey="" QueryName="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageQuery_queryDefinition">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="QueryKey">
                <Description>
                    <div>Unique identifier for the Query</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="QueryName">
                <Description>
                    <div>Name of the Query</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ConditionList>
        <Condition DisplayName="" EntityName="" TableName="" UserInfo="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="DisplayName">
                        <Description>
                            <div>Name shown in UI.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="EntityName">
                        <Description>
                            <div>Name of entity that the query is designed to work with.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="TableName">
                        <Description>
                            <div>Name of table that the query is designed to work with.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="UserInfo"/>
                </Attributes>
            </yfc:doc>
            <Statement Attribute="" ColumnName="" DataType=""
                DisplayName="" DisplayPrefix="" Id="" Prefix=""
                QryTypeDisplayName="" QryTypeId="" QryTypeName=""
                Suffix="" UserInfo="" ValueFrom="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="Attribute"/>
                        <Attribute DataType="String" Name="ColumnName"/>
                        <Attribute DataType="String" Name="DataType"/>
                        <Attribute DataType="String" Name="DisplayName"/>
                        <Attribute DataType="String" Name="DisplayPrefix"/>
                        <Attribute DataType="String" Name="Id"/>
                        <Attribute DataType="String" Name="Prefix"/>
                        <Attribute DataType="String" Name="QryTypeDisplayName">
                            <Description>
                                <div>Query Type for the DisplayName</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="QryTypeId">
                            <Description>
                                <div>Query Type for the Id</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="QryTypeName">
                            <Description>
                                <div>Query Type for the Name</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="Suffix"/>
                        <Attribute DataType="String" Name="UserInfo"/>
                        <Attribute DataType="String" Name="ValueFrom"/>
                    </Attributes>
                </yfc:doc>
                <ValueList>
                    <Value Attribute="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="String" Name="Attribute"/>
                            </Attributes>
                        </yfc:doc>
                    </Value>
                </ValueList>
            </Statement>
        </Condition>
    </ConditionList>
</QueryDefinition>
