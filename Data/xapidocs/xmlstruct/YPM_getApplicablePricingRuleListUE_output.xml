<PricingRuleList TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YPM/getApplicablePricingRuleListUE/output">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="YPM_getApplicablePricingRuleList_output.xml" IncludeXMLPath="/PricingRuleList"/>
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>Total number of records that match the input criteria.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PricingRule Currency="" Description="" EnableMixNMatchItems=""
        EndDateActive="" EndTime="" ExclusivityId="" ExclusivityType=""
        IsActiveOnFriday="" IsActiveOnMonday="" IsActiveOnSaturday=""
        IsActiveOnSunday="" IsActiveOnThursday="" IsActiveOnTuesday=""
        IsActiveOnWednesday="" IsItemRule="" OrganizationCode=""
        PaymentType="" PricingRuleKey="" PricingRuleName=""
        PricingStatus="" Priority="" RuleCategory="" RuleType=""
        StartDateActive="" StartTime="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Pricing_Rule" Name="Currency" NameInEntity="Currency">
                    <Description>
                        <div>Currency in which this rule is applicable.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule" Name="Description" NameInEntity="Description">
                    <Description>
                        <div>Description of the rule.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="EnableMixNMatchItems" NameInEntity="Enable_Mix_N_Match_Items">
                    <Description>
                        <div>
                            <p>Indicates that discount can be applied in the scenario where the combinations can not satisfied by single item, but can be satisfied by including Items which belongs to same category or having same attribute values defined in the rule. Default value is null.<br/>
							When this flag is enabled for Quantity rules and <code>WHEN_TO_APPLY=SINGLE_ITEM</code>, then field <code>TargetItemDeterminationRule</code> value is used for selecting to which item adjustment should be applied. If the <code>TargetItemDeterminationRule</code> value is null, this value will be defaulted to <code>HIGH_PRICED_ITEM</code>. 
							This flag is not applicable for getItemPrice API.
							Note: Item aggregated quantity will not be distributed among the rule's criteria to satisfy the criteria condition.
						 </p>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="EndDateActive" NameInEntity="End_Date_Active">
                    <Description>
                        <div>After this date the rule is not active.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule" Name="EndTime" NameInEntity="End_Time">
                    <Description>
                        <div>If not void indicates that the rule is applicable till this time.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="IsActiveOnFriday" NameInEntity="Is_Active_On_Friday">
                    <Description>
                        <div>If set indicates that the rule is applicable on Friday.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="IsActiveOnMonday" NameInEntity="Is_Active_On_Monday">
                    <Description>
                        <div>If set indicates that the rule is applicable on Monday.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="IsActiveOnSaturday" NameInEntity="Is_Active_On_Saturday">
                    <Description>
                        <div>If set indicates that the rule is applicable on Saturday.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="IsActiveOnSunday" NameInEntity="Is_Active_On_Sunday">
                    <Description>
                        <div>If set indicates that the rule is applicable on Sunday.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="IsActiveOnThursday" NameInEntity="Is_Active_On_Thursday">
                    <Description>
                        <div>If set indicates that the rule is applicable on Thursday.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="IsActiveOnTuesday" NameInEntity="Is_Active_On_Tuesday">
                    <Description>
                        <div>If set indicates that the rule is applicable on Tuesday.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="IsActiveOnWednesday" NameInEntity="Is_Active_On_Wednesday">
                    <Description>
                        <div>If set indicates that the rule is applicable on Wednesday.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule" Name="IsItemRule" NameInEntity="Is_Item_Rule">
                    <Description>
                        <div>Indicates whether the rule is targeted on items or not.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>Pricing organization to which the configuration belongs.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule" Name="PaymentType" NameInEntity="Payment_Type">
                    <Description>
                        <div>The rule is applicable for this Payment Type.
						</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="PricingRuleKey" NameInEntity="Pricing_Rule_Key">
                    <Description>
                        <div>Primary key attribute of YPM_PRICING_RULE table.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="PricingRuleName" NameInEntity="Pricing_Rule_Name">
                    <Description>
                        <div>Name of the pricing rule.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="PricingStatus" NameInEntity="Pricing_Status">
                    <Description>
                        <div>Indicates the status of the pricing rule ACTIVE or INACTIVE.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule" Name="RuleCategory" NameInEntity="Rule_Category">
                    <Description>
                        <div>Indicates the category to which the rule belongs PRICING or SHIPPING.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule" Name="RuleType" NameInEntity="Rule_Type">
                    <Description>
                        <div>Indicates the type of the rule COMBINATION ORDER_TOTAL ITEM_QUANTITY or SURCHARGE</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule"
                    Name="StartDateActive" NameInEntity="Start_Date_Active">
                    <Description>
                        <div>Date from which the rule is active.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Pricing_Rule" Name="StartTime" NameInEntity="Start_Time">
                    <Description>
                        <div>If not void indicates that the rule is applicable from this time.
					</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <PricingRuleItemList>
            <PricingRuleItem IncludeOrExclude="" ItemID=""
                PricingRuleItemKey="" PricingRuleKey="" Quantity=""
                UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YPM_Pricing_Rule_Item_master.xml" IncludeXMLPath="/PricingRuleItem"/>
                <yfc:doc>
                    <Element EntityName="Pricing_Rule_Item"/>
                </yfc:doc>
            </PricingRuleItem>
        </PricingRuleItemList>
        <PricingRuleTargetItemList>
            <PricingRuleTargetItem IncludeOrExclude="" PricingRuleKey=""
                PricingRuleTargetItemKey="" Quantity="" TargetItemID=""
                TargetUnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YPM_Pricing_Rule_Target_Item_master.xml" IncludeXMLPath="/PricingRuleTargetItem"/>
                <yfc:doc>
                    <Element EntityName="Pricing_Rule_Target_Item"/>
                </yfc:doc>
            </PricingRuleTargetItem>
        </PricingRuleTargetItemList>
        <PricingRuleTargetAttributeValueList>
            <PricingRuleTargetAttributeValue GroupId=""
                IncludeOrExclude="" PricingRuleKey=""
                PricingRuleTargetAttributeValueKey="" Quantity=""
                TargetAttributeDomainID="" TargetAttributeDoubleValue=""
                TargetAttributeGroupID="" TargetAttributeID=""
                TargetAttributeIntegerValue=""
                TargetAttributeMaxValue="" TargetAttributeMinValue=""
                TargetAttributeValue="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YPM_Pricing_Rule_Target_Attribute_Value_master.xml" IncludeXMLPath="/PricingRuleTargetAttributeValue"/>
                <yfc:doc>
                    <Element EntityName="Pricing_Rule_Target_Attribute_Value"/>
                </yfc:doc>
            </PricingRuleTargetAttributeValue>
        </PricingRuleTargetAttributeValueList>
        <PricingRuleAttributeValueList>
            <PricingRuleAttributeValue GroupId="" IncludeOrExclude=""
                PricingRuleAttributeValueKey="" PricingRuleKey=""
                Quantity="" TriggerAttributeDomainID=""
                TriggerAttributeDoubleValue=""
                TriggerAttributeGroupID="" TriggerAttributeID=""
                TriggerAttributeIntegerValue=""
                TriggerAttributeMaxValue="" TriggerAttributeMinValue=""
                TriggerAttributeValue="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YPM_Pricing_Rule_Attribute_Value_master.xml" IncludeXMLPath="/PricingRuleAttributeValue"/>
                <yfc:doc>
                    <Element EntityName="Pricing_Rule_Attribute_Value"/>
                </yfc:doc>
            </PricingRuleAttributeValue>
        </PricingRuleAttributeValueList>
        <PricingRuleCategoryList>
            <PricingRuleCategory IncludeOrExclude=""
                PricingRuleCategoryKey="" Quantity=""
                TriggerCategoryID="" TriggerCategoryPath="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YPM_Pricing_Rule_Category_master.xml" IncludeXMLPath="/PricingRuleCategory"/>
                <yfc:doc>
                    <Element EntityName="Pricing_Rule_Category"/>
                </yfc:doc>
            </PricingRuleCategory>
        </PricingRuleCategoryList>
        <PricingRuleTargetCategoryList>
            <PricingRuleTargetCategory IncludeOrExclude=""
                PricingRuleTargetCategoryKey="" Quantity=""
                TargetCategoryID="" TargetCategoryPath="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YPM_Pricing_Rule_Target_Category_master.xml" IncludeXMLPath="/PricingRuleTargetCategory"/>
                <yfc:doc>
                    <Element EntityName="Pricing_Rule_Target_Category"/>
                </yfc:doc>
            </PricingRuleTargetCategory>
        </PricingRuleTargetCategoryList>
        <PricingRuleActionList>
            <PricingRuleAction Adjustment="" AdjustmentType=""
                ApplyAdjustmentToRemainder="" ChargeCategory=""
                MaxAbsoluteAdjustment="" PricingRuleActionKey=""
                PricingRuleKey="" QualifierAmount=""
                QualifierAmountType="" Quantity=""
                TargetItemDeterminationRule="" TargetOccurrenceLimit=""
                WhenToApply="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="Adjustment" NameInEntity="Adjustment">
                            <Description>
                                <div>Adjustment to be applied as part of this action.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="AdjustmentType" NameInEntity="Adjustment_Type">
                            <Description>
                                <div>Type of adjustment. FLAT_FEE, PERCENT or ABSOLUTE.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="ApplyAdjustmentToRemainder" NameInEntity="Apply_Adjustment_To_Remainder">
                            <Description>
                                <div>Indicates that adjustments should be applied only to the remainder of the transaction.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="ChargeCategory" NameInEntity="Charge_Category">
                            <Description>
                                <div>Unique identifier of the Charge Name. Category to which the charge name belongs to.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="MaxAbsoluteAdjustment" NameInEntity="Max_Absolute_Adjustment">
                            <Description>
                                <div>Maximum absolute adjustment that can be provided for this pricing rule action within a transaction.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="PricingRuleActionKey" NameInEntity="Pricing_Rule_Action_Key">
                            <Description>
                                <div>Primary key attribute of YPM_PRICING_RULE_ACTION table.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="PricingRuleKey" NameInEntity="Pricing_Rule_Key">
                            <Description>
                                <div>The rule this action is associated.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="QualifierAmount" NameInEntity="Qualifier_Amount">
                            <Description>
                                <div>A number that needs to be satisfied in conjunction with the QUALIFIER_AMOUNT_TYPE for the rule condition to satisfy.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="QualifierAmountType" NameInEntity="Qualifier_Amount_Type">
                            <Description>
                                <div>Specifies qualifier amount type of the action. Qualifier amount type can be based on minimum unit price or minimum total extended price.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="Quantity" NameInEntity="Quantity">
                            <Description>
                                <div>Quantity of triggering item for tiered actions. Applicable only for rule type item quantity.</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String"
                            EntityName="Pricing_Rule_Action"
                            Name="TargetOccurrenceLimit" NameInEntity="Target_Occurence_Limit">
                            <Description>
                                <div>Maximum Number of occurrences of adjustment because of this rule in an order.</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Pricing_Rule_Action"
                            Name="WhenToApply" NameInEntity="When_To_Apply">
                            <Description>
                                <div>Indicates how to apply the action. As SINGLE_ITEM, ALL or EACH_COMBINATION.</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </PricingRuleAction>
        </PricingRuleActionList>
        <PricingRuleLocaleList>
            <PricingRuleLocale Country="" Description="" Language=""
                PricingRuleKey="" PricingRuleLocaleKey="" Variant="" yfc:DTDOccurrence="OneOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YPM_Pricing_Rule_Locale_master.xml" IncludeXMLPath="/PricingRuleLocale"/>
                <yfc:doc>
                    <Element EntityName="Pricing_Rule_Locale"/>
                </yfc:doc>
            </PricingRuleLocale>
        </PricingRuleLocaleList>
        <PricingRulePricelistExclusionList>
            <PricingRulePricelistExclusion PricelistHeaderKey=""
                PricingRuleKey="" PricingRulePricelistExclusionKey="" yfc:DTDOccurrence="OneOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YPM_Pricing_Rule_Pricelist_Exclusion_master.xml" IncludeXMLPath="/PricingRulePricelistExclusion"/>
                <yfc:doc>
                    <Element EntityName="Pricing_Rule_Pricelist_Exclusion"/>
                </yfc:doc>
            </PricingRulePricelistExclusion>
        </PricingRulePricelistExclusionList>
    </PricingRule>
</PricingRuleList>
