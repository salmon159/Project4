<ProductivitySummary EndDate="Required" StartDate="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getProductivitySummary/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="EndDate">
                <Description>
                    <div>
				End date for the time period to be considered to calculate the productivity.
				<Validation>
                            <Condition>
                                <code>EndDate</code> is not passed.
					</Condition>
                            <Error
                                Description="StartTime and EndTime must be passed." ErrorCode="YCP73_00024"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="StartDate">
                <Description>
                    <div>
				Start date for the time period to be considered to calculate the productivity.
				<Validation>
                            <Condition>
                                <code>StartDate</code> is not passed.
					</Condition>
                            <Error
                                Description="StartTime and EndTime must be passed." ErrorCode="YCP73_00024"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ProductivityType EquipmentType="" OrganizationCode="Required" ProductivityType="Required">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> and <code>ProductivityType</code> attributes must be passed and must be valid. 
					</Condition>
                            <Error
                                Description="Productivity Type Details are not passed." ErrorCode="YCPPY0006"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
        <EstimatedSAM AllowedMinsConstant="" AllowedMinsPerCase=""
            AllowedMinsPerItem="" AllowedMinsPerPallet=""
            AllowedMinsPerSourceAisle="" AllowedMinsPerSourceLevel=""
            AllowedMinsPerSourceLocn="" AllowedMinsPerTargetAisle=""
            AllowedMinsPerTargetLevel="" AllowedMinsPerTargetLocn=""
            AllowedMinsPerTask="" AllowedMinsPerUnit=""
            AllowedMinsPerUnitHorizontalDistance=""
            AllowedMinsPerUnitVerticalDistance="" AllowedMinsPerUnitWeight="">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
					User can pass estimated Standard Allowable minutes to calculate estimated productivity.  These values can be different from the actual values of the Productivity Type.  
					</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsConstant" NameInEntity="Allowed_Mins_Constant"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerCase" NameInEntity="Allowed_Mins_Per_Case"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerItem" NameInEntity="Allowed_Mins_Per_Item"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerPallet" NameInEntity="Allowed_Mins_Per_Pallet"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerSourceAisle" NameInEntity="Allowed_Mins_Per_Source_Aisle"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerSourceLevel" NameInEntity="Allowed_Mins_Per_Source_Level"/>
                    <Attribute DataType="String"
                        EntityName="Productivity_Type"
                        Name="AllowedMinsPerSourceLocn" NameInEntity="Allowed_Mins_Per_SourceLocn"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerTargetAisle" NameInEntity="Allowed_Mins_Per_Target_Aisle"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerTargetLevel" NameInEntity="Allowed_Mins_Per_Target_Level"/>
                    <Attribute DataType="String"
                        EntityName="Productivity_Type"
                        Name="AllowedMinsPerTargetLocn" NameInEntity="Allowed_Mins_Per_TargetLocn"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerTask" NameInEntity="Allowed_Mins_Per_Task"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerUnit" NameInEntity="Allowed_Mins_Per_Unit"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerUnitHorizontalDistance" NameInEntity="Allowed_Mins_Per_Unit_Horizontal_Distance"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerUnitVerticalDistance" NameInEntity="Allowed_Mins_Per_Unit_Vertical_Distance"/>
                    <Attribute EntityName="Productivity_Type"
                        Name="AllowedMinsPerUnitWeight" NameInEntity="Allowed_Mins_Per_Unit_Weight"/>
                </Attributes>
            </yfc:doc>
        </EstimatedSAM>
    </ProductivityType>
</ProductivitySummary>
