<?xml version="1.0" encoding="UTF-8"?>
<MoveLocationInventory EnterpriseCode="Required" Node="Required" xmlns="http://www.sterlingcommerce.com/documentation/WMS/beforeMoveLocationInventoryUE/output">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="WMS_moveLocationInventory_input.xml" IncludeXMLPath="/MoveLocationInventory"/>
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Inventory_Item" Name="EnterpriseCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
					Inventory Organization Code. The Enterprise whose product information is being stored.
						<Validation>
                            <Condition>
                                <code>EnterpriseCode</code> must be passed. 
							</Condition>
                            <Error
                                Description="EnterpriseCode has to be passed in the input." ErrorCode="WMS00030"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Node</code> and <code>EnterpriseCode</code> combination should be valid in the system.
							</Condition>
                            <Error
                                Description="Invalid Node and EnterpriseCode combination." ErrorCode="WMS00040"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>EnterpriseCode</code> passed but does not exist. 
							</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                        <Validation>
                            <Condition>Error if zone in which adjustment is being done is not dedicated to this, but dedicated to other Enterprises. 
							</Condition>
                            <Error
                                Description="Zone-Enterprise constraint violated." ErrorCode="WMS00091"/>
                        </Validation>
                        <Validation>
                            <Condition>Error if inventory adjustment violates the storage constraints defined for the zone.
							</Condition>
                            <Error
                                Description="Storage Constraint for the zone is violated." ErrorCode="WMS00102"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Ship_Node" Name="Node" NameInEntity="Ship_Node">
                <Description>
                    <div>
					Business key or unique identifier for a ship node.
						<Validation>
                            <Condition>
                                <code>Node</code> and <code>EnterpriseCode</code> combination should be valid in the system.
							</Condition>
                            <Error
                                Description="Invalid Node and EnterpriseCode combination." ErrorCode="WMS00040"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Node</code> must be passed. 
							</Condition>
                            <Error Description="Node not passed." ErrorCode="WMS00054"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Node</code> passed but does not exist. 
							</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Source CaseId="" LocationId="" PalletId="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="LPN_Location">
                <Description>
                    <div>
						Contains information about the source from where inventory will be moved. Source may be an LPN or a location.
							<Validation>
                            <Condition>Inventory must exists at the source to pick for move.
								</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                        <Validation>
                            <Condition>Inventory for the SKU should exist in the location or Case/Pallet to perform the inventory operation. 
								</Condition>
                            <Error
                                Description="Inventory for the SKU does not exist in the location or Case/Pallet to perform the inventory operation." ErrorCode="WMS00227"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description>
                        <div>
							A CaseId. Gives the LPN information for adjustment.
								<Validation>
                                <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
									</Condition>
                                <Error
                                    Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                            </Validation>
                            <Validation>
                                <Condition>Both <code>CaseId</code> and <code>PalletId</code> must exist if passed.
									</Condition>
                                <Error
                                    Description="CaseId/PalletId does not exist." ErrorCode="WMS00004"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="LPN_Location" Name="LocationId" NameInEntity="Location_Id">
                    <Description>
                        <div>
							Indicates the location from where inventory will be picked. Becomes mandatory if CaseId/PalletId is not passed. 
								<Validation>
                                <Condition>
                                    <code>LocationId</code> should be a valid location in the warehouse.
									</Condition>
                                <Error
                                    Description="Location does not exist." ErrorCode="WMS00005"/>
                            </Validation>
                            <Validation>
                                <Condition>If both <code>ParentLPN</code>/<code>Location</code> are passed, location is validated against <code>ParentLPN</code> location.
									</Condition>
                                <Error
                                    Description="CaseId/PalletId exists at a different location." ErrorCode="WMS00014"/>
                            </Validation>
                            <Validation>
                                <Condition>If both <code>ParentLPN</code>/<code>Location</code> are not passed, Error is thrown since LPNs location is not determinable.
									</Condition>
                                <Error
                                    Description="Either CaseId/PalletId or location information is not passed in the input." ErrorCode="WMS00015"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description>
                        <div>
						A PalletId. Gives the LPN information for adjustment.
							<Validation>
                                <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
								</Condition>
                                <Error
                                    Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                            </Validation>
                            <Validation>
                                <Condition>Both <code>CaseId</code> and <code>PalletId</code> must exist if passed.
								</Condition>
                                <Error
                                    Description="CaseId/PalletId does not exist." ErrorCode="WMS00004"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Inventory CountryOfOrigin="" FifoNo="" InventoryItemKey=""
            InventoryStatus="" InventoryTagKey="" Quantity="Required"
            ReceiptHeaderKey="" Segment="" SegmentType="" ShipByDate="">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="WMS_Move_Inventory_master.xml" IncludeXMLPath="/Inventory"/>
            <yfc:doc>
                <Element EntityName="Location_Inventory">
                    <Description>
                        <div>
					Contains information for the inventory that needs to be moved.
						<Validation>
                                <Condition>Inventory must exists with these attributes.
							</Condition>
                                <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Location_Inventory"
                        Name="CountryOfOrigin" NameInEntity="Country_Of_Origin">
                        <Description>
                            <div>
						Country in which the item was manufactured.
							<Validation>
                                    <Condition>Should be a valid country of origin.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Location_Inventory"
                        Name="FifoNo" NameInEntity="Fifo_No">
                        <Description>
                            <div>
						The first-in-first-out number to determine sequence of inventory move. A user exit <code>getFifoNumber</code> is called to obtain <code>FifoNo</code>, if new inventory is being created, <code>FifoNo</code> is not passed and item is FIFO tracked.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Location_Inventory"
                        Name="InventoryItemKey" NameInEntity="Inventory_Item_Key">
                        <Description>
                            <div>
						Gives the unique key for the inventory item.
							<Validation>
                                    <Condition>Should be a valid <code>InventoryItemKey</code> in the system.
								</Condition>
                                    <Error
                                    Description="Invalid InventoryItemKey." ErrorCode="WMS00017"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Location_Inventory"
                        Name="InventoryStatus" NameInEntity="Inventory_Status">
                        <Description>
                            <div>
						Gives the status of the inventory. Only one level <code>InventoryStatus</code> transitions will happen for the inventory.
							<Validation>
                                    <Condition>Error if status not passed and status transition is not specified for blank status.
								</Condition>
                                    <Error
                                    Description="Inventory Status not passed and transition is not specified for blank Status." ErrorCode="WMS00023"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Location_Inventory"
                        Name="InventoryTagKey" NameInEntity="Inventory_Tag_Key">
                        <Description>
                            <div>
						Gives the unique key for the tag associated with inventory.
							<Validation>
                                    <Condition>Should be a valid <code>InventoryTagKey</code> in the system.
								</Condition>
                                    <Error
                                    Description="Invalid InventoryTagKey." ErrorCode="WMS00070"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Location_Inventory"
                        Name="Quantity" NameInEntity="Quantity">
                        <Description>
                            <div>
						Gives the quantity for the inventory in the LPN.
							<Validation>
                                    <Condition>
                                    <code>Quantity</code> should be passed and should not be zero.
								</Condition>
                                    <Error
                                    Description="Quantity not passed in the input." ErrorCode="WMS00021"/>
                                </Validation>
                                <Validation>
                                    <Condition>
                                    <code>Quantity</code> should not be more than existing quantity.
								</Condition>
                                    <Error
                                    Description="Not enough quantity for inventory operation." ErrorCode="WMS00019"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Location_Inventory"
                        Name="ReceiptHeaderKey" NameInEntity="Receipt_Header_Key">
                        <Description>
                            <div>
						Specifies the receipt to which inventory belongs.
							<Validation>
                                    <Condition>If passed, should be a valid <code>ReceiptHeaderKey</code> in the system.
								</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Location_Inventory"
                        Name="Segment" NameInEntity="Segment">
                        <Description>
                            <div>
                                <code>Segment</code> for particular enterprise or organization. <code>Segment</code> becomes mandatory if <code>SegmentType</code> is passed.
							<Validation>
                                    <Condition>If <code>SegmentType</code> is passed, <code>Segment</code> must also be passed.
								</Condition>
                                    <Error
                                    Description="Both Segment and SegmentType are required." ErrorCode="WMS00101"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Location_Inventory"
                        Name="SegmentType" NameInEntity="Segment_Type">
                        <Description>
                            <div>
                                <code>SegmentType</code> for particular enterprise or organization. <code>SegmentType</code> becomes mandatory if <code>Segment</code> is passed.
							<Validation>
                                    <Condition>If <code>Segment</code> is passed, <code>SegmentType</code> must also be passed.
								</Condition>
                                    <Error
                                    Description="Both Segment and SegmentType are required." ErrorCode="WMS00101"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Location_Inventory"
                        Name="ShipByDate" NameInEntity="Ship_By_Date">
                        <Description>
                            <div>
						Date by which inventory should be shipped.
							<Validation>
                                    <Condition>Mandatory if the item is time sensitive.
								</Condition>
                                    <Error
                                    Description="Blank Ship By Date." ErrorCode="YFSI10002"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Receipt ReceiptNo="">
                <yfc:doc>
                    <Element EntityName="Receipt_Header"/>
                    <Attributes>
                        <Attribute EntityName="Receipt_Header"
                            Name="ReceiptNo" NameInEntity="Receipt_No">
                            <Description>
                                <div>
								Indicates the receipt to which inventory belongs.
									<Validation>
                                    <Condition>If passed, should be a valid receipt number.
										</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Receipt>
            <InventoryItem ItemID="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Inventory_Item">
                        <Description>
                            <div>
                                <Validation>
                                    <Condition>Item should exist in the catalog.
									</Condition>
                                    <Error
                                    Description="Invalid Item. Organization Code: {OrganizationCode}, Item ID: {ItemID} and UOM: {UnitOfMeasure}." ErrorCode="WMS00209"/>
                                </Validation>
                                <Validation>
                                    <Condition>Item cannot be a logical kit.
										</Condition>
                                    <Error
                                    Description="Item cannot be a logical kit" ErrorCode="WMS00214"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute EntityName="Inventory_Item"
                            Name="ItemID" NameInEntity="Item_ID">
                            <Description>
                                <div>
								Item identity for the inventory.
									<Validation>
                                    <Condition>if <code>InventoryItemKey</code> is not passed, <code>ItemID</code> must be passed.
										</Condition>
                                    <Error
                                    Description="ItemId has to be passed in the input." ErrorCode="WMS00031"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Item"
                            Name="ProductClass" NameInEntity="Product_Class">
                            <Description>
                                <div>
								Gives the ProductClass for the item.
									<Validation>
                                    <Condition>if <code>InventoryItemKey</code> is not passed, <code>ProductClass</code> must be passed.
										</Condition>
                                    <Error
                                    Description="ProductClass has to be passed in the input." ErrorCode="WMS00033"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Item"
                            Name="UnitOfMeasure" NameInEntity="Uom">
                            <Description>
                                <div>
								Unit of measure for the item.
									<Validation>
                                    <Condition>if <code>InventoryItemKey</code> is not passed, <code>UnitOfMeasure</code> must be passed.
										</Condition>
                                    <Error
                                    Description="UnitOfMeasure has to be passed in the input." ErrorCode="WMS00032"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </InventoryItem>
            <TagDetail BatchNo="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotKeyReference="" LotNumber=""
                ManufacturingDate="" RevisionNo="">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/TagDetail"/>
                <yfc:doc>
                    <Element EntityName="Inventory_Tag">
                        <Description>
                            <div>
							Gives the information about the tag to which inventory belongs. Attributes passed in this element will be used to identify a tag for the item in inventory based on attributes marked as unique tag attributes in the item catalog. If a tag is present, it will be used and the existing tag will be updated with the other non-unique tag attributes passed in the TagDetail element (if different from the existing tag record). Tag information is stored only if the <code>Node</code> tracks tag information in inventory.
								<Validation>
                                    <Condition>Mandatory if item is tag tracked. Error if mandatory and not passed.
										</Condition>
                                    <Error Description="No Tag Number." ErrorCode="YFSI10004"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                </yfc:doc>
            </TagDetail>
            <SerialList>
                <SerialDetail SerialNo="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Global_Serial_Num"/>
                        <Attributes>
                            <Attribute DataType="SerialNo" Name="SerialNo">
                                <Description>
                                    <div>
										Unique identity for each serial.
											<Validation>
                                    <Condition>If passed, should be a valid serial number.
												</Condition>
                                    <Error
                                    Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
                                    <code>Serial</code> should be in Location or Case/Pallet.
												</Condition>
                                    <Error
                                    Description="Serial not in location or Case/Pallet." ErrorCode="WMS00221"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>If <code>SerialList</code> and item details are passed, then it is validated that serial belongs to same item.
												</Condition>
                                    <Error
                                    Description="Serial mismatch occured." ErrorCode="WMS00010"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </SerialDetail>
            </SerialList>
        </Inventory>
    </Source>
    <Destination CaseId="" LocationId="" PalletId="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="LPN_Location"/>
            <Attributes>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description>
                        <div>
						A CaseId. Gives the LPN information for adjustment. If the LPN doesn't exist then it will be created in the destination location if passed, else in the source location with the specified LPN identifier.
							<Validation>
                                <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
								</Condition>
                                <Error
                                    Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                            </Validation>
                            <Validation>
                                <Condition>If passed, means that the inventory will move to <code>LPN</code>. If inventory/LPN being moved has <code>Segment</code> or <code>SegmentType</code> and this <code>LPN</code> has <code>Segment</code> or <code>SegmentType</code> respectively, then both should be same.
								</Condition>
                                <Error
                                    Description="Segment cannot be mixed within a Case/Pallet." ErrorCode="WMS00096"/>
                                <Error
                                    Description="SegmentTypes cannot be mixed within a Case/Pallet." ErrorCode="WMS00097"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Location" Name="LocationId" NameInEntity="Location_Id">
                    <Description>
                        <div>
						Location Identifier. Forms unique key of this table in conjunction with <code>NODE_KEY</code>. Indicates the location for deposit. <code>LocationId</code> becomes mandatory, if <code>CaseId</code>/<code>PalletId</code> is not passed. 
							<Validation>
                                <Condition>
                                    <code>LocationId</code> should be a valid location in the warehouse.
								</Condition>
                                <Error
                                    Description="Location does not exist." ErrorCode="WMS00005"/>
                            </Validation>
                            <Validation>
                                <Condition>If both <code>CaseId</code>/<code>PalletId</code> and <code>LocationId</code> are passed, location is validated against LPN location.
								</Condition>
                                <Error
                                    Description="CaseId/PalletId exists at a different location." ErrorCode="WMS00014"/>
                            </Validation>
                            <Validation>
                                <Condition>Error if both <code>CaseId</code>/<code>PalletId</code> and <code>LocationId</code> are not passed.
								</Condition>
                                <Error
                                    Description="Either CaseId/PalletId or location information is not passed in the input." ErrorCode="WMS00015"/>
                            </Validation>
                            <Validation>
                                <Condition>Error if location is not dedicated to SKU that is being adjusted, but dedicated ot other SKU.
								</Condition>
                                <Error
                                    Description="SKU Dedication violated for location." ErrorCode="WMS00090"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description>
                        <div>
						A PalletId. Gives the LPN information for adjustment. If the LPN doesn't exist then it will be created in the destination location if passed, else in the source location with the specified LPN identifier.
							<Validation>
                                <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
								</Condition>
                                <Error
                                    Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                            </Validation>
                            <Validation>
                                <Condition>If passed, means that the inventory will move to <code>LPN</code>. If inventory/LPN being moved has <code>Segment</code> or <code>SegmentType</code> and this <code>LPN</code> has <code>Segment</code> or <code>SegmentType</code> respectively, then both should be same.
								</Condition>
                                <Error
                                    Description="Segment cannot be mixed within a Case/Pallet." ErrorCode="WMS00096"/>
                                <Error
                                    Description="SegmentTypes cannot be mixed within a Case/Pallet." ErrorCode="WMS00097"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Destination>
    <Audit CompleteInprogressTasks="" DocumentType="" ProcessAllTasks=""
        ReasonCode="" ReasonText="" Reference1="" Reference2=""
        Reference3="" Reference4="" Reference5="">
        <yfc:doc>
            <Element EntityName="Location_Inventory_Audit"/>
            <Attributes>
                <Attribute DataType="Flag" Name="CompleteInprogressTasks">
                    <Description>Pass this flag as N to prevent auto-completion of ONLY in-progress tasks associated to LPN when LPN is being deleted from the system after moving inventory.</Description>
                </Attribute>
                <Attribute DataType="Flag" Name="ProcessAllTasks">
                    <Description>Pass this flag as N to prevent auto-cancellation of all tasks associated 
					to LPN when LPN is being deleted from the system after moving inventory.</Description>
                </Attribute>
                <Attribute EntityName="Location_Inventory_Audit"
                    Name="ReasonCode" NameInEntity="Reason_Code">
                    <Description>
                        <div>
							Gives the reason code for the inventory transaction. The inventory adjustments wil be done against this reason code. Although this is not mandatory, it is recommended that it be passed.
							<Validation>
                                <Condition>Should be a valid reason code in the system.
								</Condition>
                                <Error Description="Invalid ReasonCode." ErrorCode="WMS00071"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>ReasonCode</code> should not be associated with a bin location.
								</Condition>
                                <Error
                                    Description="Reason Code associated to a bin location." ErrorCode="WMS00192"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Audit>
</MoveLocationInventory>
