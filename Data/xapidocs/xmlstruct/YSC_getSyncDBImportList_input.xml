<SyncDBImport DBImportTime="" Operation="" OrganizationCode=""
    ProfileOrganizationCode="" SequenceNo="" SyncDBImportKey=""
    SyncProfileID="" SyncTargetID="" SyncType="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/getSyncDBImportList/input">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="YSC_manageSyncDBImport_input.xml" IncludeXMLPath="/SyncDBImport"/>
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Operation" Name="Operation">
                <Description>
                    <div>
							Operation to take
							<br/>
							Valid Values:
							<br/>
                        <b>
                            <code>Manage</code>
                        </b>
							- Creates a new record if it does not exist or modifies an
							existing record if it does.
							<br/>
                        <b>
                            <code>Create</code>
                        </b>
							- Creates a new record.
							<br/>
                        <b>
                            <code>Modify</code>
                        </b>
							- Changes the value of an existing record.
							<br/>
                        <b>
                            <code>Delete</code>
                        </b>
							- Deletes an existing record.
							<br/>
							The default behavior is
							<b>
                            <code>Manage</code>
                        </b>
							.
						</div>
                </Description>
            </Attribute>
            <Attribute Name="ProfileOrganizationCode" NameInEntity="Profile_Organization_Code">
                <Description>
                    <div>
					 The organization that owns the profile. 
					 	<Validation>
                            <Condition>The ProfileOrganizationCode is missing in the input</Condition>
                            <Error
                                Description="ProfileOrganizationCode is mandatory" ErrorCode="YSC921_0018"/>
                        </Validation>
                        <Validation>
                            <Condition>The ProfileOrganizationCode is not a known organization code</Condition>
                            <Error
                                Description="ProfileOrganizationCode does not exist" ErrorCode="YSC921_0019"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="SyncType" NameInEntity="Sync_Type">
                <Description>
                    <div>
					 The type of synchronization (FULL or DELTA). 
						<Validation>
                            <Condition>The SyncType is missing in the input</Condition>
                            <Error Description="SyncType is mandatory" ErrorCode="YSC921_0005"/>
                        </Validation>
                        <Validation>
                            <Condition>SyncType is neither FULL or DELTA status.</Condition>
                            <Error
                                Description="Invalid SyncType passed in the input" ErrorCode="YSC921_0004"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</SyncDBImport>
