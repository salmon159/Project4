<Condition ConditionID="" OwnerKey="" ProcessTypeKey="" ReturnType="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/evaluateCondition/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Condition" Name="ConditionID" NameInEntity="ConditionID">
                <Description>
                    <div>
						Indicates the Condition Id.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Condition" Name="OwnerKey" NameInEntity="OwnerKey">
                <Description>
                    <div>
						Indicates the owner organization associated with the Condition Id.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Condition" Name="ProcessTypeKey" NameInEntity="ProcessTypeKey">
                <Description>
                    <div>
						Indicates the process type key associated with the condition Id.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Condition" Name="ReturnType" NameInEntity="ReturnType">
                <Description>
                    <div>
						Indicates the Return type.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Output yfc:AllowAnyElement="skip">PCDATA<yfc:doc>
            <Element>
                <Description>
                    <div>
				If the return type is an XML, the output returned after evaluating the condition is returned as an element to Output.
				If the return type is of type String or Boolean, the returned value would be set to the Value attribute of the Output element.
				</div>
                </Description>
            </Element>
        </yfc:doc>
    </Output>
</Condition>
