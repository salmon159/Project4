<TaskList UserId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/acceptTasks/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="User" Name="UserId" NameInEntity="Loginid">
                <Description>
                    <div>User who accepted task(s).
						<Validation>
                            <Condition>
                                <code>UserId</code> value does not exist in YFS_USER table.
							</Condition>
                            <Error
                                Description="Invalid User identifiers passed" ErrorCode="YCP0084"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Task IsConsolidatedTask="" OrganizationCode="" TaskId="Conditional"
        TaskKey="Conditional" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute Name="IsConsolidatedTask">
                    <Description>
                        <div>
				Flag to indicate that the task being accepted is a consolidated task.
			</div>
                    </Description>
                </Attribute>
                <Attribute Name="TaskId">
                    <Description>
                        <div>
			The Id which identifies the task. Pass either the <code>TaskKey</code> or <code>TaskId</code> to identify the task.
			<Validation>
                                <Condition>Neither <code>TaskKey</code> nor <code>TaskId</code> are passed. 
				</Condition>
                                <Error Description="No record found" ErrorCode="YCP0045"/>
                            </Validation>
                            <Validation>
                                <Condition>Passed value does not identify an existing task. 
				</Condition>
                                <Error Description="No record found" ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="TaskKey">
                    <Description>
                        <div>
			The key which identifies the task. Pass either the <code>TaskKey</code> or <code>TaskId</code> to identify the task.
			<Validation>
                                <Condition>Neither <code>TaskKey</code> nor <code>TaskId</code> are passed. 
				</Condition>
                                <Error Description="No record found" ErrorCode="YCP0045"/>
                            </Validation>
                            <Validation>
                                <Condition>Passed value does not identify an existing task. 
				</Condition>
                                <Error Description="No record found" ErrorCode="YCP0045"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Task>
</TaskList>
