<InvokeUE IsUserExitImplemented="" TransactionId="" UserExit="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/invokeUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="IsUserExitImplemented">
                <Description>
                    <div>
						Indicates if the User Exit is implemented or not. 
						This attribute will appear only when this API is invoked by passing <code>CheckIfUserExitIsImplemented</code> as <b>
                            <code> 
						Y </code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid">
                <Description>
                    <div>
						Indicates the transaction associated to the
						User Exit.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="User_Exit" Name="UserExit" NameInEntity="UserExitName">
                <Description>
                    <div>
						Indicates the name of the User Exit that was invoked.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <XMLData yfc:AllowAnyElement="skip">PCDATA</XMLData>
</InvokeUE>
