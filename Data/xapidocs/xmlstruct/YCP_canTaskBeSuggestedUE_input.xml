<Task AssignedToUserId="" EnterpriseKey="" EquipmentId=""
    ExceptionHold="" FinishNoLaterThan="" HoldReasonCode=""
    HoldReasonText="" OrganizationCode="Required" SourceAisle=""
    SourceLocationId="" SourceSortSequence="" SourceZoneId=""
    StartNoEarlierThan="" TargetAisle="" TargetLocationId=""
    TargetSortSequence="" TargetZoneId="" TaskId="" TaskKey=""
    TaskPriority="" TaskStatus="" TaskType="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/canTaskBeSuggestedUE/input">
    <yfc:doc>
        <Element>
            <Description>
                <div/>
            </Description>
        </Element>
        <Attributes>
            <Attribute Name="OrganizationCode">
                <Description>
                    <div>
				Identifies the organization. 
				<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not passed.
					</Condition>
                            <Error
                                Description="Organization code is mandatory for this operation." ErrorCode="YCP0154"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> passed is not in the YFS_ORGANIZATION table.
					</Condition>
                            <Error
                                Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="TaskType">
                <Description>
                    <div>
				Specifies the type of task.
				<br/>
                        <b>Task type specific validations</b>
                        <br/>Task Type should belongs to <code>RECEIPT or PACK or VAS or SHIP</code> Activity Groups.
				<Validation>
                            <Condition>
                                <code>TaskType</code> is not passed. 
					</Condition>
                            <Error Description="Task type not passed." ErrorCode="YCP0131"/>
                        </Validation>
                    </div>
                    <Validation>
                        <Condition>
                            <code>TaskType</code> value does not exist in the YFS_TASK_TYPE table.
					</Condition>
                        <Error Description="Invalid task type." ErrorCode="YCP0130"/>
                    </Validation>
                    <Validation>
                        <Condition>
                            <code>TaskType</code> should belongs to <code>RECEIPT or PACK or VAS or SHIP</code> Activity Groups.
					</Condition>
                        <Error
                            Description="createTask not supported for this kind of task type." ErrorCode="YCP0194"/>
                    </Validation>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Inventory InventoryStatus="" ItemId="" ProductClass="" Quantity=""
        ReceiptHeaderKey="" Segment="" SegmentType="" SerialNo=""
        ShipByDate="" UnitOfMeasure="">
        <TagAttributes BatchNo="" LotAttribute1="" LotAttribute2=""
            LotAttribute3="" LotKeyReference="" LotNumber=""
            ManufacturingDate="" RevisionNo="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterTaskForInput.xml" IncludeXMLPath="/Task/Inventory/TagAttributes"/>
            <yfc:doc>
                <Element EntityName="Inventory_Tag"/>
            </yfc:doc>
        </TagAttributes>
    </Inventory>
    <TaskReferences BatchNo="" BolNo="" ContainerNo="" DocumentType=""
        LoadNo="" ManifestNo="" OrderHeaderKey="" OrderLineKey=""
        OrderNo="" OrderReleaseKey="" PrimeLineNo="" ReleaseNo=""
        Scac="" ShipmentContainerKey="" ShipmentKey="" ShipmentNo=""
        SubLineNo="" TrailerNo="" WaveNo=""/>
</Task>
