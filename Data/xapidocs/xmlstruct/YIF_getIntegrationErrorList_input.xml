<!-- getIntegrationErrorList Input XML -->
<IntegrationError ApiName="" ApiNameQryType="" Createts=""
    CreatetsQryType="" ErrorReference="" ErrorReferenceQryType=""
    FlowName="" FlowNameQryType="" FromApiName="" FromCreatets=""
    FromErrorReference="" FromFlowName="" FromSubFlowName=""
    FromSystemName="" MaximumRecords="" State="" SubFlowName=""
    SubFlowNameQryType="" SystemName="" SystemNameQryType=""
    ToApiName="" ToCreatets="" ToErrorReference="" ToFlowName=""
    ToSubFlowName="" ToSystemName="" xmlns="http://www.sterlingcommerce.com/documentation/YIF/getIntegrationErrorList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="MaximumRecords"/>
            <Attribute Name="State">
                <Description>
                    <div>
					The filter criteria is passed in the input XML via the <code>State</code> attribute.
					When the <code>State</code> attribute is passed as:
					<ul>
                            <b>
                                <code>Initial</code>
                            </b> - errors that are in "Initial" state are returned.<br>
                                <b>
                                    <code>Ignored</code>
                                </b> - errors that are completely in "Ignored" state are returned.</br>
                            <b>
                                <code>Saved</code>
                            </b> - errors that have been Saved or Reprocessed one or more times are returned. When a error is saved, a new error is created with "Saved" state. The state of the original error changes to "Modified" state.<br>
                                <b>
                                    <code>PendingReprocess</code>
                                </b> - errors that are submitted for Reprocessing but not yet processed are returned.</br>
                            <b>
                                <code>Fixed</code>
                            </b> - errors that are completely in "Fixed" state are returned.
					</ul>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Queue QueueKey=""/>
</IntegrationError>
