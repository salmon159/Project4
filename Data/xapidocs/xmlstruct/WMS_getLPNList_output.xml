<?xml version="1.0" encoding="UTF-8"?>
<LPNs xmlns="http://www.sterlingcommerce.com/documentation/WMS/getLPNList/output">
    <LPN CaseId="" DimensionUom="" FreezeOnVariance="" Height=""
        IsOutboundContainer="" LPNHdrKey="" Length=""
        OrganizationCode="" OuterMostCaseId="" OuterMostPalletId=""
        PalletId="" ParentCaseId="" ParentPalletId=""
        ReceiptHeaderKey="" Segment="" SegmentType="" Volume=""
        Weight="" WeightUom="" Width="">
        <yfc:doc>
            <Element EntityName="LPN_Hdr"/>
            <Attributes>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description>
                        <div>
						Indicates the Id of the Case.											
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="DimensionUom"/>
                <Attribute DataType="LPNNo" Name="OuterMostCaseId">
                    <Description>
                        <div>
						Indicates the Id of the Outer Most Case.													
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="OuterMostPalletId">
                    <Description>
                        <div>
						Indicates the Id of the Outer Most Pallet.													
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description>
                        <div>
						Indicates the Id of the Pallet.													
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="ParentCaseId">
                    <Description>
                        <div>
						Indicates the Id of the Parent Case.						
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="ParentPalletId">
                    <Description>
                        <div>
						Indicates the Id of the Parent Pallet.													
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="Volume"/>
                <Attribute DataType="String" Name="WeightUom"/>
            </Attributes>
        </yfc:doc>
        <Receipt ReceiptNo="">
            <yfc:doc>
                <Element EntityName="Receipt_Header"/>
                <Attributes>
                    <Attribute EntityName="Receipt_Header"
                        Name="ReceiptNo" NameInEntity="Receipt_No">
                        <Description>
                            <div>
								Indicates the receipt to which <code>LPN</code> belongs.							
								</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Receipt>
        <LPNLocation LocationId="" Node="" ZoneId="">
            <yfc:doc>
                <Element EntityName="LPN_Location"/>
                <Attributes>
                    <Attribute EntityName="Location" Name="LocationId" NameInEntity="Location_Id">
                        <Description>
                            <div>
							Location where the <code>LPN</code> is residing.							
							</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="LPN_Location" Name="Node" NameInEntity="Node_Key">
                        <Description>
                            <div>
						The Node in which the <code>LPN</code> exists.						
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </LPNLocation>
    </LPN>
</LPNs>
