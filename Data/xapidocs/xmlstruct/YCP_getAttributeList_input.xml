<Attribute AllowMultipleValues="" AllowedValueLength=""
    AttributeDomainID="" AttributeGroupID="" AttributeID=""
    AttributeKey="" AttributePostFix="" CallingOrganizationCode=""
    DataType="" DerivedFromAttributeKey=""
    DisplayLocalizedFieldInLocale="" IsAllowedValueDefined=""
    IsValueMandatory="" LongDescription="" OrganizationCode=""
    Purpose="" SequenceNo="" ShortDescription="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getAttributeList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="CallingOrganizationCode">
                <Description>
                    <div>
					If <code>CallingOrganizationCode</code> is passed, the attribute list for the organization that has configured attributes for this <code>CallingOrganizationCode</code> is returned.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Locale" Name="DisplayLocalizedFieldInLocale">
                <Description>
                    <div>
						The locale for which output should be returned in. Locale should be passed in the standard format of "language_country_variant" (e.g., "fr_FR", "en_US_CA", etc.).
					</div>
                </Description>
            </Attribute>
            <Attribute Name="OrganizationCode">
                <Description>
                    <div>
					The organization that has defined the attributes.
					If <code>CallingOrganizationCode</code> is not passed in the input, <code>OrganizationCode</code> is obtained from the logged in user. If <code>OrganizationCode</code> cannot be obtained from the logged in user, then this <code>OrganizationCode</code> passed in the input is used to fetch the <code>Attribute</code> list.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Attribute_Domain" Name="Purpose" NameInEntity="Purpose">
                <Description>
                    <div>
					The <code>Purpose</code> of the <code>AttributeDomain</code> to which this attribute belongs. If not passed in the input, a default value of 'ITEM' is used to fetch the atribute list.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <AttributeAllowedValueList>
        <AttributeAllowedValue AttributeAllowedValueKey=""
            AttributeKey="" IsDefaultValue="" LongDescription=""
            NumericStep="" SequenceNo="" ShortDescription="" Value="" yfc:DTDOccurrence="ZeroOrMany">
            <AssetList>
                <Asset AssetID="" AssetKey="" ContentID=""
                    ContentLocation="" ContentSize="" Description=""
                    Label="" Type="">
                    <AssetLocaleList>
                        <AssetLocale AssetKey="" AssetLocaleKey=""
                            ContentID="" ContentLocation="" Country=""
                            Description="" Label="" Language="" Variant=""/>
                    </AssetLocaleList>
                </Asset>
            </AssetList>
        </AttributeAllowedValue>
    </AttributeAllowedValueList>
    <AttributeReference AttributeKey="" AttributeReferenceKey=""
        ReferenceName="" TableName=""/>
    <AttributeLocaleList>
        <AttributeLocale AttributeKey="" AttributeLocaleKey=""
            Country="" Language="" LongDescription=""
            ShortDescription="" Variant=""/>
    </AttributeLocaleList>
    <AssetList>
        <Asset AssetID="" AssetKey="" ContentID="" ContentLocation=""
            ContentSize="" Description="" Label="" Type="">
            <AssetLocaleList>
                <AssetLocale AssetKey="" AssetLocaleKey="" ContentID=""
                    ContentLocation="" Country="" Description=""
                    Label="" Language="" Variant=""/>
            </AssetLocaleList>
        </Asset>
    </AssetList>
</Attribute>
