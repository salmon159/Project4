<Order xmlns="http://www.sterlingcommerce.com/documentation/OMP/determineBestMultistopTransferUE/output">
    <Lines>
        <Line LineId="Required" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="LineId">
                        <Description>
                            <div>Unique identifier for the line. It is mandatory to pass this attribute.
	                        </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <MultiStopTransfers>
                <MultiStopTransfer DestinationNode="Required"
                    OriginNode="Required" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="ShipNode" Name="DestinationNode">
                                <Description>The final node where inventory will be arriving.</Description>
                            </Attribute>
                            <Attribute DataType="ShipNode" Name="OriginNode">
                                <Description>The initial node where inventory is being shipped from.</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <Stops>
                        <yfc:doc>
                            <Element>
                                <Description>The stops desired to take. The <code>Stop</code>s should be in desired order.</Description>
                            </Element>
                        </yfc:doc>
                        <Stop Node="Required" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute DataType="ShipNode" Name="Node">
                                    <Description>The node for which the stop will be made.</Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                        </Stop>
                    </Stops>
                </MultiStopTransfer>
            </MultiStopTransfers>
        </Line>
    </Lines>
</Order>
