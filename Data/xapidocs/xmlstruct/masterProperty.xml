<property name="" path="" type="" value="" xmlns="http://www.sterlingcommerce.com/documentation/masterProperty">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					This element holds information about properties associated to item.  
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Text-480" Name="name">
                <Description>
                    <div>
						The name of the property.							
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="path">
                <Description>
                    <div>
						The path to the item that contains the property.									
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="type">
                <Description>
                    <div>
						The datatype of the property.
						<br/>Valid Values: <b>
                            <code>bigdecimal</code>
                        </b> or <b>
                            <code>int</code>
                        </b> or <b>
                            <code>List</code>
                        </b> or <b>
                            <code>long</code>
                        </b> or <b>
                            <code>double</code>
                        </b> or <b>
                            <code>String</code>
                        </b> or <b>
                            <code>date</code>
                        </b> or <b>
                            <code>unknown</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-480" Name="value">
                <Description>
                    <div>
						The value in string form for the property. List values are returned with each value separated by a carriage return.		
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</property>
