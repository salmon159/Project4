<?xml version="1.0" encoding="UTF-8"?>
<WaveSummary xmlns="http://www.sterlingcommerce.com/documentation/WMS/getWaveSummary/output">
    <TaskSummaryList>
        <TaskSummary Node="" TaskStatus="" TaskType="" TotalQuantity="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Key" EntityName="Wave"
                        Name="Node" NameInEntity="Node_Key">
                        <Description>Indicates the node or warehouse.</Description>
                    </Attribute>
                    <Attribute EntityName="Task" Name="TaskStatus" NameInEntity="Task_Status">
                        <Description>Status of the Task.</Description>
                    </Attribute>
                    <Attribute EntityName="Task_Type" Name="TaskType" NameInEntity="Task_Type"/>
                    <Attribute DataType="Number" Name="TotalQuantity">
                        <Description> is the total quantity obtained by grouping on TaskStatus and TaskType.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <TaskType TaskType="" TaskTypeName="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Task_Type"
                            Name="TaskType" NameInEntity="Task_Type"/>
                        <Attribute EntityName="Task_Type"
                            Name="TaskTypeName" NameInEntity="Task_Type_Name"/>
                    </Attributes>
                </yfc:doc>
            </TaskType>
        </TaskSummary>
    </TaskSummaryList>
    <ShipmentSummaryList>
        <ShipmentSummary Node="" SCAC="" Status="" TotalShipments=""
            WaveNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Key" EntityName="Wave"
                        Name="Node" NameInEntity="Node_Key">
                        <Description>Indicates the node or warehouse.</Description>
                    </Attribute>
                    <Attribute DataType="OrgCode" EntityName="Shipment"
                        Name="SCAC" NameInEntity="SCAC"/>
                    <Attribute DataType="ShortStatus"
                        EntityName="Shipment" Name="Status" NameInEntity="Status"/>
                    <Attribute DataType="Number" Name="TotalShipments">
                        <Description>is the total number of shipments obtained by grouping on SCAC,  Status and WaveNo.</Description>
                    </Attribute>
                    <Attribute EntityName="Wave" Name="WaveNo" NameInEntity="Wave_No"/>
                </Attributes>
            </yfc:doc>
            <Wave TotalShipments="" WaveNo="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Wave"/>
                    <Attributes>
                        <Attribute DataType="Number" Name="TotalShipments">
                            <Description>is the total number of shipments obtained for this WaveNo.</Description>
                        </Attribute>
                        <Attribute EntityName="Wave" Name="WaveNo" NameInEntity="Wave_No"/>
                    </Attributes>
                </yfc:doc>
            </Wave>
            <Status Description="" Status="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Description"
                            EntityName="Status" Name="Description" NameInEntity="Description"/>
                        <Attribute DataType="Status" EntityName="Status"
                            Name="Status" NameInEntity="Status"/>
                    </Attributes>
                </yfc:doc>
            </Status>
        </ShipmentSummary>
    </ShipmentSummaryList>
    <ShipmentProfileSummaryList>
        <ShipmentProfileSummary Node="" ProfileId="" Status=""
            TotalShipments="" WaveNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Key" EntityName="Wave"
                        Name="Node" NameInEntity="Node_Key">
                        <Description>Indicates the node or warehouse.</Description>
                    </Attribute>
                    <Attribute EntityName="Shipment" Name="ProfileId" NameInEntity="ProfileID"/>
                    <Attribute DataType="ShortStatus"
                        EntityName="Shipment" Name="Status" NameInEntity="Status"/>
                    <Attribute DataType="Number" Name="TotalShipments">
                        <Description>is the total number of shipments obtained by grouping on ProfileID,  Status and WaveNo.</Description>
                    </Attribute>
                    <Attribute EntityName="Wave" Name="WaveNo" NameInEntity="Wave_No"/>
                </Attributes>
            </yfc:doc>
            <Wave TotalShipments="" WaveNo="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Wave"/>
                    <Attributes>
                        <Attribute DataType="Number" Name="TotalShipments">
                            <Description>is the total number of shipments obtained for this WaveNo.</Description>
                        </Attribute>
                        <Attribute EntityName="Wave" Name="WaveNo" NameInEntity="Wave_No"/>
                    </Attributes>
                </yfc:doc>
            </Wave>
            <Status Description="" Status="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Description"
                            EntityName="Status" Name="Description" NameInEntity="Description"/>
                        <Attribute DataType="Status" EntityName="Status"
                            Name="Status" NameInEntity="Status"/>
                    </Attributes>
                </yfc:doc>
            </Status>
        </ShipmentProfileSummary>
    </ShipmentProfileSummaryList>
    <ContainerSummaryList>
        <ContainerSummary Node="" NumberOfCases="" NumberOfPallets=""
            Status="" WaveNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Key" EntityName="Wave"
                        Name="Node" NameInEntity="Node_Key">
                        <Description>Indicates the node or warehouse.</Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="NumberOfCases">
                        <Description>is the total number of Case type containers obtained by grouping on Status, WaveNo.</Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="NumberOfPallets">
                        <Description>is the total number of Pallet type containers obtained by grouping on Status, WaveNo.</Description>
                    </Attribute>
                    <Attribute DataType="ShortStatus"
                        EntityName="Shipment" Name="Status" NameInEntity="Status"/>
                    <Attribute EntityName="Wave" Name="WaveNo" NameInEntity="Wave_No"/>
                </Attributes>
            </yfc:doc>
            <Status Description="" Status="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Description"
                            EntityName="Status" Name="Description" NameInEntity="Description"/>
                        <Attribute DataType="Status" EntityName="Status"
                            Name="Status" NameInEntity="Status"/>
                    </Attributes>
                </yfc:doc>
            </Status>
        </ContainerSummary>
    </ContainerSummaryList>
    <WorkOrderSummaryList>
        <WorkOrderSummary ActivityCode="" CompletedWorkOrders="" Node=""
            PendingWorkOrders="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Work_Order_Activity"
                        Name="ActivityCode" NameInEntity="Activity_Code"/>
                    <Attribute DataType="Number" Name="CompletedWorkOrders">
                        <Description>is the total number of completed work orders for this  AcivityCode.</Description>
                    </Attribute>
                    <Attribute DataType="Key" EntityName="Wave"
                        Name="Node" NameInEntity="Node_Key">
                        <Description>Indicates the node or warehouse.</Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="PendingWorkOrders">
                        <Description>is the total number of pending work orders for this  ActivityCode.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </WorkOrderSummary>
    </WorkOrderSummaryList>
    <PlannedTaskSummaryList>
        <PlannedTaskSummary EstimatedMinutes="" RemainingMinutes=""
            ResourcePoolDescription="" ResourcePoolId="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Wave" Name="EstimatedMinutes" NameInEntity="Estimated_Minutes"/>
                    <Attribute EntityName="Wave" Name="RemainingMinutes" NameInEntity="Remaining_Minutes"/>
                    <Attribute EntityName="Res_Pool"
                        Name="ResourcePoolDescription" NameInEntity="Resource_Pool_Description"/>
                    <Attribute EntityName="Res_Pool"
                        Name="ResourcePoolId" NameInEntity="Resource_Pool_Id"/>
                </Attributes>
            </yfc:doc>
        </PlannedTaskSummary>
    </PlannedTaskSummaryList>
    <OverallSummary TotalProfiles="" TotalSKU="" TotalShipmentLines=""
        TotalShipments="" TotalVolume="" TotalWorkOrders="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Number" Name="TotalProfiles">
                    <Description>is the total  Profiles that belong to the given list of waves.</Description>
                </Attribute>
                <Attribute DataType="Number" Name="TotalSKU">
                    <Description>is the total SKUs that belong to the given list of waves.</Description>
                </Attribute>
                <Attribute DataType="Number" Name="TotalShipmentLines">
                    <Description>is the total ShipmentLines that belong to the given list of waves.</Description>
                </Attribute>
                <Attribute DataType="Number" Name="TotalShipments">
                    <Description>is the total Shipments that belong to the given list of waves.</Description>
                </Attribute>
                <Attribute DataType="Number" Name="TotalVolume">
                    <Description>is the summed up volume of items in all shipment lines that belong to the given list of waves.</Description>
                </Attribute>
                <Attribute DataType="String" Name="TotalWorkOrders"/>
            </Attributes>
        </yfc:doc>
        <QuantitySummary FullCaseEstimated="" FullCaseQuantity=""
            LessThanCaseEstimated="" LessThanCaseQuantity=""
            NoOfFullCases="" NoOfLooseCases="" NumberOfFullCases=""
            NumberOfLooseCases="" TotalEstimated="" TotalNoOfCases=""
            TotalNumberOfCases="" TotalQuantity="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Number" Name="FullCaseEstimated">
                        <Description>is the total number of full cases estimated.</Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="FullCaseQuantity">
                        <Description>is the quantity of full cases estimated.</Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="LessThanCaseEstimated">
                        <Description>is the total number of less than cases estimated.</Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="LessThanCaseQuantity">
                        <Description>is the quantity of less than cases estimated.</Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="NoOfFullCases">
                        <Description>is the actual number of full cases.</Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="NoOfLooseCases">
                        <Description>is the actual number of loose cases.</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="NumberOfFullCases"/>
                    <Attribute DataType="String" Name="NumberOfLooseCases"/>
                    <Attribute DataType="Number" Name="TotalEstimated">
                        <Description>is the total number of cases estimated.</Description>
                    </Attribute>
                    <Attribute DataType="Number" Name="TotalNoOfCases">
                        <Description>is the total number of cases.</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="TotalNumberOfCases"/>
                    <Attribute DataType="Number" Name="TotalQuantity">
                        <Description>is the total quantity of estimated.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </QuantitySummary>
    </OverallSummary>
    <WaveList>
        <Wave Createts="" Createuserid="" Description="" DocumentType=""
            EstimatedMinutes="" Node="" OverrideConcurrentWave=""
            OverrideReleaseDate="" PipelineKey="" Priority=""
            ReasonCode="" ReasonText="" ReleaseNoEarlierThan=""
            ReleaseNoLaterThan="" RemainingMinutes="" SeqNo=""
            ShipmentGroupId="" WaveKey="" WaveNo="" WaveStatus="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Wave_master.xml" IncludeXMLPath="/Wave"/>
            <ShipmentGroup AcceptWave="" CalendarKey="" Description=""
                EffectiveDateFrom="" EffectiveDateTo="" Lockid=""
                MaximumRecords="" MinNoOfShipments="" MinVolume=""
                NoOfConcurrentWaves="" Node="" PickStrategyKey=""
                PipelineKey="" Priority="" SeqNo="" ShipmentGroupId=""
                ShipmentGroupKey="" SortStrategyKey="">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="YFS_Shipment_Group_master.xml" IncludeXMLPath="/ShipmentGroup"/>
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="MaximumRecords"/>
                    </Attributes>
                </yfc:doc>
            </ShipmentGroup>
        </Wave>
    </WaveList>
</WaveSummary>
