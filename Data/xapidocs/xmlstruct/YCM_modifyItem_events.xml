<Item Action="" CanUseAsServiceTool="" GlobalItemID="" IsSyncRequired=""
    ItemGroupCode="" ItemID="" ItemKey="" OrganizationCode=""
    UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YCM/modifyItem_events">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="YCMAction" Name="Action">
                <Description>Action to take</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IsSyncRequired">
                <Description>
                    <div>This flag indicates whether item synchronization needs to be performed.
				  </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <!-- Either ItemKey or ItemID, OrganizationCode, UnitOfMeasure is required. -->
    <PrimaryInformation AppointmentType="" BundleFulfillmentMode=""
        BundlePricingStrategy="PARENT|COMP|ALL" CapacityPerOrderedQty=""
        CapacityQuantityStrategy="" ConfiguredModelKey=""
        CostCurrency="" CountryOfOrigin="" DefaultProductClass=""
        Description="" ExtendedDescription=""
        ExtendedDisplayDescription="" FixedCapacityQtyPerLine=""
        FixedPricingQtyPerLine="" IsAirShippingAllowed=""
        IsConfigurable="" IsDeliveryAllowed=""
        IsEligibleForShippingDiscount="|N|Y" IsForwardingAllowed=""
        IsFreezerRequired="" IsHazmat="" IsModelItem=""
        IsParcelShippingAllowed="" IsPickupAllowed="" IsPreConfigured=""
        IsProcurementAllowed="" IsReturnService="" IsShippingAllowed=""
        IsSoldSeparately="" IsSubOnOrderAllowed="" ItemType=""
        Keywords="" KitCode="" ManufacturerItem=""
        ManufacturerItemDesc="" ManufacturerName="" MasterCatalogID=""
        MaxOrderQuantity="" MinOrderQuantity=""
        MinimumCapacityQuantity="" ModelItemUnitOfMeasure=""
        NumSecondarySerials="" PricingQuantityConvFactor=""
        PricingQuantityStrategy="" PricingUOM="" PricingUOMStrategy=""
        PrimarySupplier="" ProductLine="" RequiresProdAssociation=""
        SerializedFlag="" ShipmentConsolidationWindow=""
        ShortDescription="" Status="" TaxableFlag="" UnitCost=""
        UnitHeight="" UnitHeightUOM="" UnitLength="" UnitLengthUOM=""
        UnitWeight="" UnitWeightUOM="" UnitWidth="" UnitWidthUOM="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="AppointmentType"/>
            </Attributes>
        </yfc:doc>
    </PrimaryInformation>
    <InventoryParameters ATPRule="" AdvanceNotificationTime=""
        DefaultExpirationDays="" FutureSafetyFactorPercentage=""
        InventoryMonitorRule="" IsFifoTracked=""
        IsItemBasedAllocationAllowed="" IsSerialTracked="" LeadTime=""
        MaximumNotificationTime="" MinNotificationTime=""
        OnhandSafetyFactorPercentage="" OnhandSafetyFactorQuantity=""
        ProcessingTime="" SerializedFlag="" TagControlFlag=""
        TimeSensitive="" UseUnplannedInventory=""/>
    <SafetyFactorDefinitions>
        <SafetyFactorDefinition DeliveryMethod=""
            FutureSafetyFactorPercentage=""
            OnhandSafetyFactorPercentage=""
            OnhandSafetyFactorQuantity="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="CommonCode"
                        EntityName="Sku_Safety_Factor_Defn"
                        Name="DeliveryMethod" NameInEntity="DeliveryMethod"/>
                    <Attribute EntityName="Sku_Safety_Factor_Defn"
                        Name="FutureSafetyFactorPercentage" NameInEntity="Future_Safety_Factor_Pct"/>
                    <Attribute EntityName="Sku_Safety_Factor_Defn"
                        Name="OnhandSafetyFactorPercentage" NameInEntity="Onhand_Safety_Factor_Pct"/>
                    <Attribute DataType="Quantity"
                        EntityName="Sku_Safety_Factor_Defn"
                        Name="OnhandSafetyFactorQuantity" NameInEntity="Onhand_Safety_Factor_Qty"/>
                </Attributes>
            </yfc:doc>
        </SafetyFactorDefinition>
    </SafetyFactorDefinitions>
    <ClassificationCodes CommodityCode="" ECCNNo="" HarmonizedCode=""
        HazmatClass="" NMFCClass="" NMFCCode=""
        OperationalConfigurationComplete="" PickingType=""
        PostingClassification="" Schedule_B_Code="" StorageType=""
        TaxProductCode="" UNSPSC=""/>
    <ItemAliasList>
        <ItemAlias AliasName="Required" AliasValue=""/>
    </ItemAliasList>
    <ItemServiceSkillList>
        <ItemServiceSkill AliasName="Required" AliasValue="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="AliasName"/>
                    <Attribute DataType="String" Name="AliasValue"/>
                </Attributes>
            </yfc:doc>
        </ItemServiceSkill>
    </ItemServiceSkillList>
    <ItemExclusionList>
        <ItemExclusion ExclusionCode="Required"/>
        <Remove>
            <ItemExclusion ExclusionCode="Required"/>
        </Remove>
    </ItemExclusionList>
    <ItemServiceAssocList>
        <ItemServiceAssoc HoldSchedTillCompletion=""
            PricingProductQuantity="" PricingServiceQuantity=""
            ProductQuantity="" ServiceItemGroupCode="Required"
            ServiceItemId="Required" ServiceQuantity=""
            ServiceTimeOffset="" ServiceUOM="Required" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="ServiceTimeOffset"/>
                </Attributes>
            </yfc:doc>
            <ItemServiceAssocSkillList>
                <ItemServiceAssocSkill ServiceSkillId="Required" yfc:DTDOccurrence="ZeroOrMany"/>
            </ItemServiceAssocSkillList>
        </ItemServiceAssoc>
    </ItemServiceAssocList>
    <AdditionalAttributeList>
        <AdditionalAttribute Name="Required" Value=""/>
    </AdditionalAttributeList>
    <Components Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>
                        <div>If the value of <code>Reset</code> passed is <b>
                                <code>Y</code>
                            </b>, the component definitions are changed. If the value of <code>Reset</code> passed is <b>
                                <code>N</code>
                            </b>, only the deltas are applied.
				  </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Component ComponentItemID="" ComponentItemKey=""
            ComponentOrganizationCode="" ComponentUnitOfMeasure=""
            KitQuantity="" Operation="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Kit_Item">
                    <Description>
                        <div>This element defines a component of the Item, if it is a Kit Item.
				  </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Item" Name="ComponentItemID" NameInEntity="Item_Id">
                        <Description>Along with <code>ComponentOrganizationCode</code> and <code>ComponentUnitOfMeasure</code>, it identifies a component item.</Description>
                    </Attribute>
                    <Attribute EntityName="Item" Name="ComponentItemKey" NameInEntity="Item_Key">
                        <Description>Component item's primary key.</Description>
                    </Attribute>
                    <Attribute EntityName="Item"
                        Name="ComponentOrganizationCode" NameInEntity="Organization_Code">
                        <Description>Organization that maintains the catalog for the Component Item.</Description>
                    </Attribute>
                    <Attribute EntityName="Item"
                        Name="ComponentUnitOfMeasure" NameInEntity="Uom">
                        <Description>Unit of measure for the Component Item.</Description>
                    </Attribute>
                    <Attribute DataType="Action" Name="Operation">
                        <Description>
                            <div>
					   Valid value is <b>
                                    <code>Delete</code>
                                </b>. To add or modify a component there is no need to pass operation attribute. It is determined internally. Delete is the one which should be passed explicitly.
					  </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Component>
    </Components>
    <LanguageDescriptionList>
        <LanguageDescription Description="" ExtendedDescription=""
            ExtendedDisplayDescription="" LocaleCode=""
            ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
    </LanguageDescriptionList>
    <ServiceActivities>
        <ServiceActivity ActivityCode="Required" ActivitySeqNo="Required">
            <ServiceActivityInstructions>
                <ServiceActivityInstruction InstructionText=""
                    InstructionURL="" SequenceNo="Required" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute EntityName="Instruction_Detail"
                                Name="InstructionText" NameInEntity="Instruction_Text"/>
                            <Attribute EntityName="Instruction_Detail"
                                Name="InstructionURL" NameInEntity="Instruction_URL"/>
                            <Attribute EntityName="Instruction_Detail"
                                Name="SequenceNo" NameInEntity="Sequence_No"/>
                        </Attributes>
                    </yfc:doc>
                </ServiceActivityInstruction>
            </ServiceActivityInstructions>
        </ServiceActivity>
    </ServiceActivities>
    <WMSItemAttributes ConstrainedByHeight="" Deformable=""
        PackByVolume="" PackedHeight="" PackedLength="" PackedWidth=""
        StackHeightIncrease="" Stackable="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Stackable">
                    <Description>This flag indicates whether the item is stackable.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </WMSItemAttributes>
</Item>
