<Item CallingNodeKey="" CallingOrganizationCode=""
    InheritAttributesFromClassification="" ItemID="" OrganizationCode=""
    UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YCM/getItemUOMList/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					The table stores product information. By Default,
					UOM information is retrieved from the Organization
					Locale settings in the configurator if no record is
					found in the Item UOM table
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="String" Name="CallingNodeKey"/>
            <Attribute DataType="String" Name="CallingOrganizationCode"/>
            <Attribute Name="InheritAttributesFromClassification">
                <Description>
                    <div>
					Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>. If passed as <b>
                            <code>N</code>
                        </b>, performance will be better by suppressing unnecessary calls to YFS_CLASSIFICATION_PURPOSE table. Default is <b>
                            <code>Y</code>
                        </b>.
					</div>
                </Description>
            </Attribute>
            <Attribute Name="ItemID">
                <Description>
                    <div>
						A unique identifier for an item that belongs to
						a catalog organization and for which the
						applicable UOMs are being returned.
					</div>
                </Description>
            </Attribute>
            <Attribute Name="OrganizationCode">
                <Description>
                    <div>
						The organization whose catalog organization is
						being referred to.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Item>
