<NodeSCACAccounts SCAC="" ShipNode="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/getNodeSCACAccountList/output">
    <yfc:doc>
        <Element EntityName="SCAC_Ex"/>
        <Attributes>
            <Attribute DataType="OrgCode" EntityName="SCAC_Ex"
                Name="SCAC" NameInEntity="Scac_Key">
                <Description>
                    <div>
				Organization code of an organization that plays the role of <code>CARRIER</code>.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="ShipNode" EntityName="SCAC_Ex"
                Name="ShipNode" NameInEntity="Record_Key">
                <Description>
                    <div>
				The participating <code>ShipNode</code> of the <code>SCAC</code>.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <NodeSCACAccount BillingAccountNo="" Enterprise=""
        ShipperAccountNo="" ThirdPartyOrganization="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element EntityName="SCAC_Ex"/>
            <Attributes>
                <Attribute DataType="AccountNo" EntityName="SCAC_Ex"
                    Name="BillingAccountNo" NameInEntity="Account1">
                    <Description>
                        <div>
				Indicates the <code>BillingAccountNo</code> set up by the third party organization for 
				the Scac with the Node.
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Key" EntityName="SCAC_Ex"
                    Name="Enterprise" NameInEntity="Enterprise_Key">
                    <Description>
                        <div>
				Indicates the enterprise.
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="AccountNo" EntityName="SCAC_Ex"
                    Name="ShipperAccountNo" NameInEntity="Account2">
                    <Description>
                        <div>
				Indicates the <code>ShipperAccountNo</code> set up by the enterprise for the Scac with the Node.
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Key" EntityName="SCAC_Ex"
                    Name="ThirdPartyOrganization" NameInEntity="Third_Party_Organization_Key">
                    <Description>
                        <div>
				Indicates the third party organization.
				</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </NodeSCACAccount>
</NodeSCACAccounts>
