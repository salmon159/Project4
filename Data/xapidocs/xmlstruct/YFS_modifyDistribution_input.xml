<ItemShipNode ActiveFlag="" CostCurrency="" DistributionRuleId=""
    EffectiveEndDate="" EffectiveStartDate="" ExternalInvOrg=""
    Inventorytype="" ItemId="" ItemType="" ItemshipnodeKey="Conditional"
    OwnerKey="" Priority="" ShipNodeItem="" ShipnodeKey=""
    SourceFromOrganization="" UnitCost="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/modifyDistribution/input">
    <yfc:doc>
        <Element EntityName="Item_Ship_Node">
            <Description>
                <div>
					Stores distribution setup for an organization. This setup is used during any inventory operation.  
					<Validation>
                        <Condition>To identify unique item ship node record, <b>ItemshipnodeKey</b> OR the <b>ItemId</b> and <b>ShipnodeKey</b> and <b>OwnerKey</b> and <b>DistributionRuleId</b> attributes must be passed and must be valid.
						</Condition>
                        <Error
                            Description="Record does not exist in the database." ErrorCode="YFC0002"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute Name="Priority">
                <Description>
                    <div>
			Node priority in the distribution rule.  Priority is not supported if distribution rule is used for procurement.  
			</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</ItemShipNode>
