<GetInventorySnapShot GetFutureSupplies="" LastInventoryItemKey=""
    MaximumNumberOfItems="" OrganizationCode="" ShipNode="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getInventorySnapShot/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="GetFutureSupplies">
                <Description>
					Pass this flag as <code>Y</code> if future supply types is desired in the output. Also, <code>InventorySupplyType</code> filter will work only if this flag is passed as <code>Y</code>.
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="LastInventoryItemKey">
                <Description>
					This attribute indicates the starting inventory item key of required batch of inventory item for the ship node provided.
					<code>ShipNode</code> is required for this attribute to be honored.
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="MaximumNumberOfItems">
                <Description>
					This attribute indicates the maximum number of items to return in the result for the ship node provided.
					<code>ShipNode</code> is required for this attribute to be honored.
                </Description>
            </Attribute>
            <Attribute EntityName="Organization" Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
				Unique identifier for an organization. 
				<Validation>
                            <Condition>If <code>OrganizationCode</code> is passed and not valid.
					</Condition>
                            <Error
                                Description="Invalid Organization Code." ErrorCode="YCP0040"/>
                        </Validation>
                        <Validation>
                            <Condition>If both <code>ShipNode</code> and <code>OrganizationCode</code> are not passed.
					</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Ship_Node" Name="ShipNode" NameInEntity="Ship_Node">
                <Description>
                    <div>
				Business key or unique identifier for a ship node. 
				<Validation>
                            <Condition>If both <code>ShipNode</code> and <code>OrganizationCode</code> are not passed.
					</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing" ErrorCode="YCM0002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <InventorySupplyTypeList>
        <InventorySupplyType SupplyType="" yfc:DTDOccurrence="ZeroOrMany"/>
    </InventorySupplyTypeList>
</GetInventorySnapShot>
