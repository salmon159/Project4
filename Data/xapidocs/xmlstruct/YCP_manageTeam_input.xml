<Team CustomerAccessMode="" Description="" DocumentTypeAccessMode=""
    EnterpriseAccessMode="" Operation="" OrganizationCode="Conditional"
    ParentTeamKey="" ShipNodeAccessMode="" TeamId="Conditional"
    TeamKey="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageTeam/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Data_Security_Group"
                Name="CustomerAccessMode" NameInEntity="Customer_Access_Mode"/>
            <Attribute EntityName="Data_Security_Group"
                Name="Description" NameInEntity="DESCRIPTION">
                <Description>
                    <div>The description of the Team record.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>
					   This attribute specifies the kind of operation to be done.  
			<br/>Valid Values: 
				<br/>
                        <b>
                            <code>Create</code>
                        </b> - To create a <code>Team</code>
                        <br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
				<br/>
                        <b>
                            <code>Delete</code>
                        </b> - To delete a  <code>Team</code>
                        <br/>Default: If not passed then API will try to either create or modify depending on the existence of the data.
					</div>
                </Description>
            </Attribute>
            <Attribute Name="OrganizationCode">
                <Description>
                    <div>
					The Organization that created this team.
						<Validation>
                            <Condition>Mandatory if creating a Data Security Group.</Condition>
                            <Error
                                Description="Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>Must be a valid organization in the <code>YFS_ORGANIZATION</code> table.
					</Condition>
                            <Error
                                Description="Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Data_Security_Group"
                Name="ParentTeamKey" NameInEntity="PARENT_GROUP_KEY">
                <Description>
                    <div>
				Refers to the DataSecurityGroupKey of the parent group.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Data_Security_Group" Name="TeamId" NameInEntity="DATA_SECURITY_GROUP_ID">
                <Description>
                    <div>
					The unique identifier for a team.
						<Validation>
                            <Condition>Mandatory if creating a new Team.</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Data_Security_Group" Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY">
                <Description>
                    <div>
						The primary key attribute for the
						YFS_DATA_SECURITY_GROUP table.
					</div>
                </Description>
            </Attribute>
        </Attributes>
        <Element EntityName="Data_Security_Group">
            <Description>
                <div>
				Teams that are used to control end-user access to data elements are stored here.  Identify <code>Team</code> to be created, modified or deleted.  <code>TeamKey</code> or a combination of  <code>TeamId</code> and <code>OrganizationCode</code> attributes can be used to identify a <code>Team</code>.
				</div>
            </Description>
        </Element>
    </yfc:doc>
    <TeamEnterpriseList>
        <TeamEnterprise EnterpriseOrgCode="Conditional" Operation=""
            TeamEnterpriseKey="Conditional" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute Name="EnterpriseOrgCode">
                        <Description>
                            <div>
					Organization code of the enterprise to which data access has been granted.
						<Validation>
                                    <Condition>Mandatory if creating a Team Enterprise.</Condition>
                                    <Error
                                    Description="Invalid organization code." ErrorCode="YFS10395"/>
                                </Validation>
                                <Validation>
                                    <Condition>Must be a valid organization in the <code>YFS_ORGANIZATION</code> table.
					</Condition>
                                    <Error
                                    Description="Invalid organization code." ErrorCode="YFS10395"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Action" Name="Operation">
                        <Description>
                            <div>
					   This attribute specifies the kind of operation to be done.  
			<br/>Valid Values: 
				<br/>
                                <b>
                                    <code>Create</code>
                                </b> - To associate a transaction to <code>Team</code>
                                <br/>
                                <b>
                                    <code>Modify</code>
                                </b> - Change values in an existing record.
				<br/>
                                <b>
                                    <code>Delete</code>
                                </b> - To de-associate a transaction to <code>Team</code>
                                <br/>Default: If not passed then API will try to either create or modify depending on the existence of the data.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Data_Security_Enterprise"
                        Name="TeamEnterpriseKey" NameInEntity="DATA_SECURITY_ENTERPRISE_KEY">
                        <Description>
				Primary key attribute of
				YFS_DATA_SECURITY_ENTERPRISE.
			</Description>
                    </Attribute>
                </Attributes>
                <Element EntityName="Data_Security_Enterprise">
                    <Description>
                        <div>
				This tables stores the enterprises that are accessible to users associated to this team.  Identify <code>TeamEnterprise</code> to be created/modified/deleted for a team type.  <code>TeamEnterpriseKey</code> or  <code>EnterpriseOrgCode</code> can be used to identify a <code>TeamEnterprise</code>. <code>EnterpriseOrgCode</code> is mandatory when <code>Operation</code> is Create.
				</div>
                    </Description>
                </Element>
            </yfc:doc>
        </TeamEnterprise>
    </TeamEnterpriseList>
    <TeamDocTypeList>
        <TeamDocType ApplicationCode="" DocumentType="" ModuleCode=""
            Operation="" TeamDocTypeKey="Conditional" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Data_Security_Doc_Type"
                        Name="ModuleCode" NameInEntity="MODULE_CODE">
                        <Description>
                            <div>
								Module code for the document type that this team has access to.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Action" Name="Operation">
                        <Description>
                            <div>
					   This attribute specifies the kind of operation to be done.  
			<br/>Valid Values: 
				<br/>
                                <b>
                                    <code>Create</code>
                                </b> - To associate a modification type to <code>Team</code>
                                <br/>
                                <b>
                                    <code>Modify</code>
                                </b> - Change values in an existing record.
				<br/>
                                <b>
                                    <code>Delete</code>
                                </b> - To de-associate a modification type to <code>Team</code>
                                <br/>Default: If not passed then API will try to either create or modify depending on the existence of the data.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Data_Security_Doc_Type"
                        Name="TeamDocTypeKey" NameInEntity="DATA_SECURITY_DOC_TYPE_KEY">
                        <Description>
                            <div>
								The primary key attribute for the
								YFS_DATA_SECURITY_DOC_TYPE table.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
                <Element EntityName="Data_Security_Doc_Type">
                    <Description>
                        <div>
				This table stores the document types (by application) that are accessible by users associated to the team.  Identity TeamDocType type to be created/modified/deleted for data security group type.  <code>TeamDocTypeKey</code> is mandatory when <code>Operation</code> is Modify or Delete. 
				</div>
                    </Description>
                </Element>
            </yfc:doc>
        </TeamDocType>
    </TeamDocTypeList>
    <TeamNodesList>
        <TeamNodes Operation="" ShipnodeKey="Conditional"
            TeamNodeKey="Conditional" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Action" Name="Operation">
                        <Description>
                            <div>
					   This attribute specifies the kind of operation to be done.  
			<br/>Valid Values: 
				<br/>
                                <b>
                                    <code>Create</code>
                                </b> - To associate a transaction to <code>Team</code>
                                <br/>
                                <b>
                                    <code>Modify</code>
                                </b> - Change values in an existing record.
				<br/>
                                <b>
                                    <code>Delete</code>
                                </b> - To de-associate a transaction to <code>Team</code>
                                <br/>Default: If not passed then API will try to either create or modify depending on the existence of the data.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="ShipnodeKey">
                        <Description>
                            <div>
					The primary key for the YFS_SHIP_NODE table.
						<Validation>
                                    <Condition>Mandatory if creating a team node.</Condition>
                                    <Error
                                    Description="Invalid ship node." ErrorCode="YFS10048"/>
                                </Validation>
                                <Validation>
                                    <Condition>Must be a valid ship node in the <code>YFS_SHIP_NODE</code> table.
					</Condition>
                                    <Error
                                    Description="Invalid ship node.." ErrorCode="YFS10048"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Data_Security_Nodes"
                        Name="TeamNodeKey" NameInEntity="DATA_SECURITY_NODE_KEY">
                        <Description>
                            <div>
						The primary key for the YFS_DATA_SECURITY_NODES table.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
                <Element EntityName="Data_Security_Nodes">
                    <Description>
                        <div>
				Team nodes that are used to control end-user access to data elements are stored here.  Identify <code>TeamNodes</code> to be created/modified/deleted for a data security group type.  <code>TeamNodeKey</code> or  <code>ShipnodeKey</code> can be used to identify a <code>TeamNodes</code>. <code>ShipnodeKey</code> is mandatory when <code>Operation</code> is Create.
				</div>
                    </Description>
                </Element>
            </yfc:doc>
        </TeamNodes>
    </TeamNodesList>
    <SubTeamList>
        <SubTeam Description="" DocumentTypeAccessMode=""
            EnterpriseAccessMode="" OrganizationCode="" ParentTeamKey=""
            ShipNodeAccessMode="" TeamId="" TeamKey="">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Data_Security_Group"
                        Name="Description" NameInEntity="DESCRIPTION">
                        <Description>
                            <div>The description of the Team record.</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="OrganizationCode">
                        <Description>
                            <div>
					The Organization that created this team.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Data_Security_Group"
                        Name="ParentTeamKey" NameInEntity="PARENT_GROUP_KEY">
                        <Description>
                            <div>
				Refers to the DataSecurityGroupKey of the parent group.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Data_Security_Group"
                        Name="TeamId" NameInEntity="DATA_SECURITY_GROUP_ID">
                        <Description>
                            <div>
					The unique identifier for a team.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Data_Security_Group"
                        Name="TeamKey" NameInEntity="DATA_SECURITY_GROUP_KEY">
                        <Description>
                            <div>
						The primary key attribute for the
						YFS_DATA_SECURITY_GROUP table.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
                <Element EntityName="Data_Security_Group">
                    <Description>
                        <div>
				Teams that are used to control end-user access to data elements are stored here.  Identify <code>Team</code> to be created, modified or deleted.  <code>TeamKey</code> or a combination of  <code>TeamId</code> and <code>OrganizationCode</code> attributes can be used to identify a <code>Team</code>.
				</div>
                    </Description>
                </Element>
            </yfc:doc>
        </SubTeam>
    </SubTeamList>
</Team>
