<PaymentCardType DocumentType="" InheritValues="" OrganizationCode=""
    PaymentCardType="" PaymentCardTypeKey="" PaymentType="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getPaymentCardTypeDetails/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="DocumentType" Name="DocumentType">
                <Description>
                    <div>
                        <br/>If <code>InheritValues</code> is <code>Y</code>, this is used to look up inherited values that are document type specific.  If not passed, the default value is <code>0001</code>. 
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="InheritValues">
                <Description>
                    <div>
                        <br/>Valid Values:
					<br/>
                        <b>
                            <code>Y</code>
                        </b> - Inherits values of payment rules from the payment configuration hierarchy if not present in this payment card type.
					<br/>
                        <b>
                            <code>N</code>
                        </b> - Shows values of payment rules set on this payment card type.  <code>null</code> values will not be returned.
					<br/>
                        <b>If not passed, the default value is <code>N</code>.</b>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</PaymentCardType>
