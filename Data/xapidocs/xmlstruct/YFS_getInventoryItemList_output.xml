<!--getInventoryItemList Output XML-->
<InventoryList LastInventoryItemKey="" LastRecordSet=""
    OrganizationCode="" TotalInventoryItemList=""
    TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getInventoryItemList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Key" Name="LastInventoryItemKey">
                <Description>Last Inventory Item Key in the output.
			</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="LastRecordSet">
                <Description>This flag indicates whether the records obtained are a complete list.  For example, if there are more than 500 records matching the criteria passed in input xml, only 500 records are returned and this flag is set to N.
				<br/>Valid Values: Y or N
			</Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>Identifies the organization. Value is same as the passed in input.
			</Description>
            </Attribute>
            <Attribute DataType="Count" Name="TotalInventoryItemList">
                <Description>Total number of inventory items returned.
			</Description>
            </Attribute>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description> number of records that match the input criteria.
			</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <InventoryItem InventoryItemKey="" InventoryOrganizationCode=""
        ItemID="" ProductClass="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
        <Item ItemID="" ItemKey="" OrganizationCode="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
					Catalog item information is returned only if there is an entry in the system catalog with a matching ItemID and UnitOfMeasure for the InventoryItem.  If inventory is stored in an alternate UOM from that specified in the catalog, Item information may not be retrieved.
				</Description>
                </Element>
            </yfc:doc>
            <InventoryParameters ATPRule="" AdvanceNotificationTime=""
                DefaultExpirationDays="" FutureSafetyFactorPercentage=""
                InventoryMonitorRule="" IsFifoTracked=""
                IsItemBasedAllocationAllowed="" IsSerialTracked=""
                LeadTime="" MaximumNotificationTime=""
                MinNotificationTime="" NodeLevelInventoryMonitorRule=""
                OnhandSafetyFactorPercentage=""
                OnhandSafetyFactorQuantity="" ProcessingTime=""
                RequiresDGOverride="" TagControlFlag=""
                TagReqAtReceipt="" TagReqAtShipment="" TimeSensitive="" UseUnplannedInventory=""/>
            <ItemAttributeGroupTypeList>
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="MasterItemAttributeGroupTypesForDetailsOutput.xml" IncludeXMLPath="/ItemAttributeGroupTypeList"/>
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="MasterItemAttributeGroupTypesForItem.xml" IncludeXMLPath="/ItemAttributeGroupTypeList"/>
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="MasterItemAttributeGroupTypes.xml" IncludeXMLPath="/ItemAttributeGroupTypeList"/>
                <ItemAttributeGroupType ClassificationPurposeCode=""
                    Description="" IsForMaintenance="" IsForSelling=""
                    IsSingleGroup="" IsValidForCategory=""
                    IsValidForClassification=""
                    ItemAttributeGroupType=""
                    ItemAttributeGroupTypeKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <ItemAttributeGroupList>
                        <ItemAttributeGroup
                            ItemAttributeGroupDescription=""
                            ItemAttributeGroupID=""
                            ItemAttributeGroupKey=""
                            ItemAttributeGroupPath=""
                            ParentItemAttributeGroupKey="" SequenceNo="" yfc:DTDOccurrence="ZeroOrMany">
                            <ItemAttributeList>
                                <ItemAttribute AttributeDomainID=""
                                    AttributeGroupID="" CategoryKey=""
                                    IsInherited="" IsKeyAttribute=""
                                    ItemAttributeDescription=""
                                    ItemAttributeGroupKey=""
                                    ItemAttributeGroupType=""
                                    ItemAttributeKey=""
                                    ItemAttributeName="" SequenceNo=""
                                    Value="" yfc:DTDOccurrence="ZeroOrMany">
                                    <yfc:includeInParent
                                    IncludeXMLChildNodes="Y"
                                    IncludeXMLFileName="MasterItemAttributeHangOffsForDetails.xml" IncludeXMLPath="/ItemAttribute"/>
                                    <yfc:includeInParent
                                    IncludeXMLChildNodes="Y"
                                    IncludeXMLFileName="MasterItemAttributeHangOffs.xml" IncludeXMLPath="/ItemAttribute"/>
                                    <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="String" Name="Value">
                                    <Description>
                                    <div>
										The associated value of <code>ItemAttributeName</code> for this item.
									</div>
                                    </Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                    <Attribute AllowMultipleValues=""
                                    AllowedValueLength=""
                                    AttributeDomainID=""
                                    AttributeGroupID=""
                                    AttributeID="" AttributeKey=""
                                    AttributePostFix="" DataType=""
                                    DerivedFromAttributeKey=""
                                    IsAllowedValueDefined=""
                                    IsValueMandatory=""
                                    LongDescription=""
                                    OrganizationCode=""
                                    SequenceNo="" ShortDescription="">
                                    <DerivedFromAttribute
                                    AttributeDomainID=""
                                    AttributeGroupID=""
                                    AttributeID=""
                                    AttributeKey=""
                                    OrganizationCode="" ShortDescription=""/>
                                    <AttributeReference
                                    AttributeKey=""
                                    AttributeReferenceKey=""
                                    ReferenceName="" TableName=""/>
                                    <AttributeAllowedValueList TotalNumberOfRecords="">
                                    <yfc:doc>
                                    <Attributes>
                                    <Attribute
                                    DataType="Count" Name="TotalNumberOfRecords">
                                    <Description>Number of Records in the DB satisfying the passed criteria.</Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                    <AttributeAllowedValue
                                    AttributeAllowedValueKey=""
                                    AttributeKey=""
                                    IsDefaultValue=""
                                    LongDescription=""
                                    NumericStep=""
                                    SequenceNo=""
                                    ShortDescription=""
                                    Value="" yfc:DTDOccurrence="ZeroOrMany"/>
                                    </AttributeAllowedValueList>
                                    </Attribute>
                                    <AssignedValueList>
                                    <AssignedValue DoubleValue=""
                                    IntegerValue=""
                                    IsInherited=""
                                    LongDescription=""
                                    SequenceNo=""
                                    ShortDescription="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                                    <yfc:doc>
                                    <Element EntityName="Additional_Attribute">
                                    <Description>
                                    <div>
				The attributes in this element are the values stored as <code>AdditionalAttribute</code> records for the <code>ItemAttribute</code> record under which it appears. For an <code>Attribute</code> configured as a derived attribute, the <code>Value</code> is the configured value for the derived attribute. The value itself is not assigned to the category. <code>IsInherited</code> flag will not appear for a derived attribute.
				</div>
                                    </Description>
                                    </Element>
                                    <Attributes>
                                    <Attribute
                                    EntityName="Attr_Allowed_Value"
                                    Name="LongDescription" NameInEntity="Long_Description"/>
                                    <Attribute
                                    EntityName="Attr_Allowed_Value"
                                    Name="SequenceNo" NameInEntity="Seq_No"/>
                                    <Attribute
                                    EntityName="Attr_Allowed_Value"
                                    Name="ShortDescription" NameInEntity="Short_Description"/>
                                    </Attributes>
                                    </yfc:doc>
                                    </AssignedValue>
                                    </AssignedValueList>
                                </ItemAttribute>
                            </ItemAttributeList>
                            <ItemAttrGroupLocaleList TotalNumberOfRecords="">
                                <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="Count" Name="TotalNumberOfRecords">
                                    <Description>Number of Records in the DB satisfying the passed criteria.</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                                <ItemAttrGroupLocale Country=""
                                    ItemAttrGroupKey=""
                                    ItemAttrGroupLocaleKey=""
                                    ItemAttributeGroupDescription=""
                                    Language="" Variant="" yfc:DTDOccurrence="ZeroOrMany"/>
                            </ItemAttrGroupLocaleList>
                        </ItemAttributeGroup>
                    </ItemAttributeGroupList>
                    <ItemAttrGrpTypeLocaleList TotalNumberOfRecords="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                                    <Description>Number of Records in the DB satisfying the passed criteria.</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <ItemAttrGrpTypeLocale Country="" Description=""
                            ItemAttrGroupTypeKey=""
                            ItemAttrGrpTypeLocaleKey="" Language=""
                            Variant="" yfc:DTDOccurrence="ZeroOrMany"/>
                    </ItemAttrGrpTypeLocaleList>
                </ItemAttributeGroupType>
            </ItemAttributeGroupTypeList>
            <PrimaryInformation Description="" ShortDescription=""/>
            <LanguageDescriptionList>
                <LanguageDescription Description=""
                    ExtendedDescription="" LocaleCode=""
                    ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
            </LanguageDescriptionList>
        </Item>
    </InventoryItem>
</InventoryList>
