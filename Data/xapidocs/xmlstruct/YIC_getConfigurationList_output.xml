<getConfigurationList TotalNumberOfRecords="" limit="" maxCreateDate=""
    maxUpdateDate="" minCreateDate="" minUpdateDate="" sku="" type="" xmlns="http://www.sterlingcommerce.com/documentation/YIC/getConfigurationList/output">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					The root node of the XML document contains header information for querying configuration list.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Integer" Name="TotalNumberOfRecords">
                <Description>
                    <div>
						The total number of returned configurations.
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="limit">
                <Description>
                    <div>
						The limit for the maximum number of records to be returned.			
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="maxCreateDate">
                <Description>
                    <div>
						The maximum create date for the returned configurations.
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="maxUpdateDate">
                <Description>
                    <div>
						The maximum update date for the returned configurations.
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="minCreateDate">
                <Description>
                    <div>
						The minimum create date for the returned configurations.					
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="minUpdateDate">
                <Description>
                    <div>
						The minimum update date for the returned configurations.						
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" EntityName="BOM_Header"
                Name="sku" NameInEntity="SKU">
                <Description>
                    <div>
						The matching top level sku of the returned configurations.					
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" EntityName="BOM_Header"
                Name="type" NameInEntity="Type">
                <Description>
                    <div>
						The type of configurations returned.						
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Configuration ConfigId="" CreateDate="" Partial="" SKU="" Type=""
        UpdateDate="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
						The root node of the XML document contains header information.
					</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Key" EntityName="BOM_Header"
                    Name="ConfigId" NameInEntity="BOM_Id">
                    <Description>
                        <div>
							The ID of the saved configuration.			
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="TimeStamp" EntityName="BOM_Header"
                    Name="CreateDate" NameInEntity="Createts">
                    <Description>
                        <div>
							The date of creation of the record.			
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="Partial">
                    <Description>
                        <div>
							This flag indicates if the saved configuration has errors or not. This is a derived attribute. If there are error messages associated to the saved configuration this flag is true else false.			
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" EntityName="BOM_Header"
                    Name="SKU" NameInEntity="SKU">
                    <Description>
                        <div>
							The SKU from the root node of the configuration.				
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-120" EntityName="BOM_Header"
                    Name="Type" NameInEntity="Type">
                    <Description>
                        <div>
							A caller supplied identifier string used to group saved configurations.	
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="TimeStamp" EntityName="BOM_Header"
                    Name="UpdateDate" NameInEntity="Modifyts">
                    <Description>
                        <div>
							The date of subsequent update of the record.		
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Configuration>
</getConfigurationList>
