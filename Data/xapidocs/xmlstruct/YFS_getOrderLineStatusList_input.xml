<!--getOrderLineStatusList Input XML-->
<OrderLineStatus DocumentType="Conditional" EnterpriseCode="Conditional"
    IgnoreTransactionDependencies="Y|N" ItemGroupCode=""
    MaximumRecords="" OrderHeaderKey="Conditional"
    OrderLineKey="Conditional" OrderNo="Conditional"
    OrderReleaseKey="Conditional" PrimeLineNo="Conditional"
    ProcureFromNode="" ReleaseNo="Conditional" ShipNode=""
    SubLineNo="Conditional" TransactionId="Conditional"
    xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderLineStatusList/input" yfc:UsedForIndexing="Order">
    <yfc:doc>
        <Element EntityName="Order_Release_Status"/>
        <Attributes>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute EntityName="Order_Header" Name="EnterpriseCode" NameInEntity="Enterprise_Key"/>
            <Attribute DataType="String" Name="IgnoreTransactionDependencies">
                <Description>
                    <div>Flag to override any transaction dependencies that may be defined for the transaction this API invokes.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>. When passed as <b>
                            <code>Y</code>
                        </b>, transaction dependencies will not be evaluated, and thus will not prevent the API from processing.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Line" Name="ItemGroupCode" NameInEntity="Item_Group_Code"/>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>Specifies the Maximum Number of records to be returned. Default is 500.</Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No"/>
            <Attribute EntityName="Order_Release_Status"
                Name="OrderReleaseKey" NameInEntity="Order_Release_Key">
                <Description>
                    <div>
				Pass <code>ReleaseNo</code> or <code>OrderReleaseKey</code> to select order lines in that release.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Line" Name="PrimeLineNo" NameInEntity="Prime_Line_No">
                <Description>
                    <div>
				Part of a key to identify an order line. Pass either <code>OrderLineKey</code> or <code>OrderHeaderKey</code>, <code>PrimeLineNo</code> and <code>SubLineNumber</code> to 
				get the order line status for a specific order line.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Line_Schedule"
                Name="ProcureFromNode" NameInEntity="Procure_From_Node"/>
            <Attribute EntityName="Order_Release" Name="ReleaseNo" NameInEntity="Release_No">
                <Description>
                    <div>
				Pass <code>ReleaseNo</code> or <code>OrderReleaseKey</code> to select order lines in that release.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Line_Schedule" Name="ShipNode" NameInEntity="Ship_Node"/>
            <Attribute EntityName="Order_Line" Name="SubLineNo" NameInEntity="Sub_Line_No">
                <Description>
                    <div>
				Part of a key to identify an order line. Pass either <code>OrderLineKey</code> or <code>OrderHeaderKey</code>, <code>PrimeLineNo</code> and <code>SubLineNumber</code> to 
				get the order line status for a specific order line.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="TransactionID" Name="TransactionId">
                <Description>
                    <div>
				 Pass <code>TransactionID</code> to get a list of <code>DropStatus</code> records for that transaction.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</OrderLineStatus>
