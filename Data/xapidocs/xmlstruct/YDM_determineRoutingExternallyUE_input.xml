                    Name="VolumeUOM" NameInEntity="VolumeUOM"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="Weight" NameInEntity="Weight"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="WeightUOM" NameInEntity="WeightUOM"/>
            </Attributes>
        </yfc:doc>
    </Routable>
</Routables>
