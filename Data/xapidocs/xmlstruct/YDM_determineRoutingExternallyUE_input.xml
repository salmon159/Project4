<Routables xmlns="http://www.sterlingcommerce.com/documentation/YDM/determineRoutingExternallyUE/input">
    <Routable BuyerOrganizationCode="" Company="" DepartmentCode=""
        EnterpriseCode="" ExpectedDeliveryDate="" ExpectedShipDate=""
        FreightTerms="" HandlingUnits="" InputCarrierServiceCode=""
        IsCommercialAddress="" ItemClassification="" LoadNo=""
        RoutableReferenceId="" SellerOrganizationCode="" ShipFromCity=""
        ShipFromCountry="" ShipFromNode="" ShipFromState=""
        ShipFromZip="" ShipToCity="" ShipToConsolNode=""
        ShipToCountry="" ShipToNode="" ShipToState="" ShipToStore=""
        ShipToZip="" ShipmentNo="" ShipmentType="" TaxGeoCode=""
        Volume="" VolumeUOM="" Weight="" WeightUOM="">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Shipment"
                    Name="BuyerOrganizationCode" NameInEntity="BuyerOrganizationCode">
                    <Description>
                        <div>Buyer Organization Code on the Shipment/Load</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Person_Info" Name="Company" NameInEntity="Company">
                    <Description>
                        <div>
The company this address is associated with.
</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Routing_Guide"
                    Name="DepartmentCode" NameInEntity="DepartmentCode"/>
                <Attribute EntityName="Shipment" Name="EnterpriseCode" NameInEntity="EnterpriseCode">
                    <Description>
                        <div>Enterprise Code on the Shipment/Load </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Date" Name="ExpectedDeliveryDate">
                    <Description>
                        <div>
						Expected Delivery Date of the Shipment.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Date" Name="ExpectedShipDate">
                    <Description>
                        <div>
					This is the expected date of Shipment to be shipped.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Routing_Guide"
                    Name="FreightTerms" NameInEntity="FreightTerms"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="HandlingUnits" NameInEntity="HandlingUnits"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="InputCarrierServiceCode" NameInEntity="InputCarrierServiceCode"/>
                <Attribute DataType="Flag" EntityName="Person_Info"
                    Name="IsCommercialAddress" NameInEntity="Is_Commercial_Address">
                    <Description>
                        <div>
					Indicates whether the address is commercial, residential or none. Values of this field can be Y is Commercial address.N is Residential address. NULL is None. Default value is Null.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Routing_Guide"
                    Name="ItemClassification" NameInEntity="ItemClassification"/>
                <Attribute EntityName="Load" Name="LoadNo" NameInEntity="Load_No">
                    <Description>
                        <div>Indicates the Number of the Load, for which routing is being performed.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="RoutableReferenceId">
                    <Description>
                        <div>
					Identier for this routable criteria. Routable refers to either the shipment or the load.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Shipment"
                    Name="SellerOrganizationCode" NameInEntity="EnterpriseCode">
                    <Description>
                        <div>Seller Organization Code on the Shipment/Load </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipFromCity" NameInEntity="ShipFromCity"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipFromCountry" NameInEntity="ShipFromCountry"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipFromNode" NameInEntity="ShipFromNode"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipFromState" NameInEntity="ShipFromState"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipFromZip" NameInEntity="ShipFromZip"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipToCity" NameInEntity="ShipToCity"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipToConsolNode" NameInEntity="ShipToConsolNode"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipToCountry" NameInEntity="ShipToCountry"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipToNode" NameInEntity="ShipToNode"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipToState" NameInEntity="ShipToState"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipToStore" NameInEntity="ShipToStore"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipToZip" NameInEntity="ShipToZip"/>
                <Attribute EntityName="Shipment" Name="ShipmentNo" NameInEntity="Shipment_No">
                    <Description>
                        <div>Indicates the Number of the Shipment, for which routing is being performed.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="ShipmentType" NameInEntity="ShipmentType"/>
                <Attribute EntityName="Person_Info" Name="TaxGeoCode" NameInEntity="Tax_Geo_Code"/>
                <Attribute EntityName="Routing_Guide_Detail"
                    Name="Volume" NameInEntity="Volume"/>
