<GetShipmentReceiptDiscrepancy SellerOrganizationCode="" ShipNode=""
    ShipmentKey="" ShipmentNo="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/getShipmentReceiptDiscrepancy/input">
    <yfc:doc>
        <Element EntityName="Shipment">
            <Description>
                <div>
	   The shipment for which receiving discrepancy has to be fetched. Pass  <code>ShipmentKey</code>
                    <i>or</i> pass <code>ShipmentNo</code>, <code>ShipNode</code> and <code>SellerOrganizationCode</code>  to identify the shipment.
			<Validation>
                        <Condition>Error if <code>Shipment</code> not found.
				</Condition>
                        <Error Description="Shipment not found." ErrorCode="OMP10066"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Shipment"
                Name="SellerOrganizationCode" NameInEntity="Seller_Organization_Code">
                <Description>
                    <div>
				Seller organization to which this shipment belongs.
					<Validation>
                            <Condition>Either <code>ShipmentKey</code> attribute <i>or</i>
                                <code>ShipmentNo</code>, <code>SellerOrganizationCode</code> and <code>ShipNode</code> attributes must be passed and must be valid.
						</Condition>
                            <Error
                                Description="Key shipment attributes are missing. Either ShipmentKey or ShipmentNo/SellerOrganizationCode/ShipNode should be passed." ErrorCode="OMP10065"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" EntityName="Shipment"
                Name="ShipNode" NameInEntity="Ship_Node">
                <Description>
                    <div>
				Ship node from where shipment was made.
					<Validation>
                            <Condition>Either <code>ShipmentKey</code> attribute <i>or</i>
                                <code>ShipmentNo</code>, <code>SellerOrganizationCode</code> and <code>ShipNode</code> attributes must be passed and must be valid.
						</Condition>
                            <Error
                                Description="Key shipment attributes are missing. Either ShipmentKey or ShipmentNo/SellerOrganizationCode/ShipNode should be passed." ErrorCode="OMP10065"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="ShipmentKey" NameInEntity="Shipment_Key">
                <Description>
                    <div>
				Primary key attribute of <code>YFS_SHIPMENT</code> table.
					<Validation>
                            <Condition>Either <code>ShipmentKey</code> attribute <i>or</i>
                                <code>ShipmentNo</code>, <code>SellerOrganizationCode</code> and <code>ShipNode</code> attributes must be passed and must be valid.
						</Condition>
                            <Error
                                Description="Key shipment attributes are missing. Either ShipmentKey or ShipmentNo/SellerOrganizationCode/ShipNode should be passed." ErrorCode="OMP10065"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="ShipmentNo" NameInEntity="Shipment_No">
                <Description>
                    <div>
				Part of Unique key or business key.
					<Validation>
                            <Condition>Either <code>ShipmentKey</code> attribute <i>or</i>
                                <code>ShipmentNo</code>, <code>SellerOrganizationCode</code> and <code>ShipNode</code> attributes must be passed and must be valid.
						</Condition>
                            <Error
                                Description="Key shipment attributes are missing. Either ShipmentKey or ShipmentNo/SellerOrganizationCode/ShipNode should be passed." ErrorCode="OMP10065"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</GetShipmentReceiptDiscrepancy>
