<!--checkOrderPricematch Output XML-->
<Order BillToID="" BuyerOrganizationCode="" EnterpriseCode=""
    OrderDate="" OrderHeaderKey="Required" OrderNo=""
    SellerOrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/checkOrderPriceMatchUE/output">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
                <div>
			Contains information about order.
			</div>
            </Description>
        </Element>
    </yfc:doc>
    <PriceInfo Currency="">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Order_Header" Name="Currency">
                    <Description>This is the Currency of the Order. </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </PriceInfo>
    <PersonInfoShipTo AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" ErrorTxt=""
        EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
        IsAddressVerified="" JobTitle="" LastName="" Lockid=""
        MiddleName="" MobilePhone="" OtherPhone="" PersonID=""
        PersonInfoKey="" PreferredShipAddress="" State="" Suffix=""
        TaxGeoCode="" Title="" UseCount="" VerificationStatus="" ZipCode="">
        <yfc:doc>
            <Element EntityName="Person_Info">
                <Description>
                    <div>
					Contains information about Person.
					</div>
                </Description>
            </Element>
        </yfc:doc>
    </PersonInfoShipTo>
    <OverallTotals GrandCharges="" GrandDiscount="" GrandTax=""
        GrandTotal="" HdrCharges="" HdrDiscount="" HdrTax="" HdrTotal="" LineSubTotal="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="GrandCharges">
                    <Description>This is the Grand Charges of the Order. </Description>
                </Attribute>
                <Attribute DataType="String" Name="GrandDiscount">
                    <Description>This is the Grand Discount of the Order. </Description>
                </Attribute>
                <Attribute DataType="String" Name="GrandTax">
                    <Description>This is the Grand Tax of the Order. </Description>
                </Attribute>
                <Attribute DataType="String" Name="GrandTotal">
                    <Description>This is the Grand Total of the Order. </Description>
                </Attribute>
                <Attribute DataType="String" Name="HdrCharges">
                    <Description>This is the Header Charges of the Order. </Description>
                </Attribute>
                <Attribute DataType="String" Name="HdrDiscount">
                    <Description>This is the Header Discount of the Order. </Description>
                </Attribute>
                <Attribute DataType="String" Name="HdrTax">
                    <Description>This is the Header Tax of the Order. </Description>
                </Attribute>
                <Attribute DataType="String" Name="HdrTotal">
                    <Description>This is the Header Total of the Order. </Description>
                </Attribute>
                <Attribute DataType="String" Name="LineSubTotal">
                    <Description>This is the Line Sub Total of the Order. </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </OverallTotals>
    <OrderLines>
        <OrderLine EnterpriseCode="" OrderLineKey="Required"
            OrderedQty="" PersonalizeFlag=""
            PriceMatchCompetitor="Required"
            PriceMatchMinQuantity="Required" PriceMatchPrice="Required"
            PriceMatchShippingCharge="Required" PriceMatched=""
            RegionCity="" RegionState="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="EnterpriseCode"/>
                    <Attribute EntityName="Order_Line"
                        Name="OrderLineKey" NameInEntity="Order_Line_Key">
                        <Description>This is the Order Line key Of the OrderLine  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line" Name="OrderedQty" NameInEntity="Ordered_Qty">
                        <Description>This is the Ordered Quantity of the Item in the OrderLine. </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="PersonalizeFlag" NameInEntity="Personalize_Flag">
                        <Description>This is the Personalize Flag. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match"
                        Name="PriceMatchCompetitor" NameInEntity="Competitor">
                        <Description>This is the Competitor  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute EntityName="Price_Match"
                        Name="PriceMatchMinQuantity" NameInEntity="Minimum_Quantity">
                        <Description>This is the Minimum Quantity for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match" Name="PriceMatchPrice" NameInEntity="New_Unit_Price">
                        <Description>This is the Competitor's Price of the Item  for which price Match is to be performed. </Description>
                    </Attribute>
                    <Attribute DataType="String"
                        EntityName="Price_Match"
                        Name="PriceMatchShippingCharge" NameInEntity="Shipping_Charges">
                        <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="PriceMatched">
                        <Description>This is the the Flag which says whether Price was matched or not. </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="RegionCity"/>
                    <Attribute DataType="String" Name="RegionState"/>
                </Attributes>
            </yfc:doc>
            <Item ItemID="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Item"/>
                </yfc:doc>
            </Item>
            <LinePriceInfo IsPriceLocked="" UnitPrice="">
                <yfc:doc>
                    <Element EntityName="Order_Line">
                        <Description>
                            <div>
				Contains information about order line pricing and quantity details.
				</div>
                        </Description>
                    </Element>
                </yfc:doc>
            </LinePriceInfo>
            <LineOverallTotals Charges="" Discount="" ExtendedPrice=""
                LineTotal="" OptionPrice="" PricingQty="" Tax="" UnitPrice="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="Charges">
                            <Description>This is charges associated with the Line. </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="Discount">
                            <Description>This is Discount associated with the Line. </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="ExtendedPrice">
                            <Description>This is Extended Price Of the Item. </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="LineTotal">
                            <Description>This is Line Total. </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="OptionPrice">
                            <Description>This is Option Price. </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="PricingQty">
                            <Description>This is Pricing Quantity. </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="Tax">
                            <Description>This is Tax associated with the Line. </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="UnitPrice">
                            <Description>This is Unit Price Of the item. </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </LineOverallTotals>
            <PriceMatchResult PriceMatchStatus="Required" PriceMatchStatusMessage="Required">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="PriceMatchStatus">
                            <Description>This is the Status of the price Match.possible values for this field are ALLOWED, POSSIBLE,MANUAL and REJECTED. If the status is ALLOWED then AbsoluteMatch element should be passed with the required attributes. If the status is POSSIBLE then PossibleMatches element should be passed and under that all the PossibleMatch elements with the required attributes. </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="PriceMatchStatusMessage">
                            <Description>This is the Status Message of the price Match. </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <AbsoluteMatch Competitor="Required"
                    ExpirationDate="Required" MinimumQuantity="Required"
                    NewUnitPrice="Required" RegionCity="Required"
                    RegionState="Required" ShippingCharges="Required">
                    <yfc:doc>
                        <Element EntityName="Price_Match">
                            <Description>This element should be passed when the PriceMatchStaus is ALLOWED.
							</Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="String"
                                EntityName="Price_Match"
                                Name="Competitor" NameInEntity="Competitor">
                                <Description>This is the Competitor  for which price Match is to be performed. </Description>
                            </Attribute>
                            <Attribute EntityName="Price_Match"
                                Name="ExpirationDate" NameInEntity="Expiration_Date">
                                <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                            </Attribute>
                            <Attribute EntityName="Price_Match"
                                Name="MinimumQuantity" NameInEntity="Minimum_Quantity">
                                <Description>This is the Minimum Quantity for which price Match is to be performed. </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="NewUnitPrice"/>
                            <Attribute EntityName="Price_Match"
                                Name="RegionCity" NameInEntity="Region_City">
                                <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                            </Attribute>
                            <Attribute EntityName="Price_Match"
                                Name="RegionState" NameInEntity="Region_State">
                                <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                            </Attribute>
                            <Attribute DataType="String"
                                EntityName="Price_Match"
                                Name="ShippingCharges" NameInEntity="Shipping_Charges">
                                <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </AbsoluteMatch>
                <PossibleMatches>
                    <PossibleMatch Competitor="" ExpirationDate=""
                        MinimumQuantity="" NewUnitPrice="" RegionCity=""
                        RegionState="" ShippingCharges="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element EntityName="Price_Match">
                                <Description>
							This element should be passed when the PriceMatchStaus is POSSIBLE.
							</Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="String"
                                    EntityName="Price_Match"
                                    Name="Competitor" NameInEntity="Competitor">
                                    <Description>This is the Competitor  for which price Match is to be performed. </Description>
                                </Attribute>
                                <Attribute EntityName="Price_Match"
                                    Name="ExpirationDate" NameInEntity="Expiration_Date">
                                    <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                                </Attribute>
                                <Attribute EntityName="Price_Match"
                                    Name="MinimumQuantity" NameInEntity="Minimum_Quantity">
                                    <Description>This is the Minimum Quantity for which price Match is to be performed. </Description>
                                </Attribute>
                                <Attribute DataType="String" Name="NewUnitPrice"/>
                                <Attribute EntityName="Price_Match"
                                    Name="RegionCity" NameInEntity="Region_City">
                                    <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                                </Attribute>
                                <Attribute EntityName="Price_Match"
                                    Name="RegionState" NameInEntity="Region_State">
                                    <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                                </Attribute>
                                <Attribute DataType="String"
                                    EntityName="Price_Match"
                                    Name="ShippingCharges" NameInEntity="Shipping_Charges">
                                    <Description>This is the Shipping Charges difference of Retailer and Competitor. </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </PossibleMatch>
                </PossibleMatches>
            </PriceMatchResult>
        </OrderLine>
    </OrderLines>
</Order>
