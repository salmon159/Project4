<RoutingGuide Action="Conditional" Comments="" EffectiveDate=""
    ExpirationDate="" OrganizationCode="Conditional" PublishDate=""
    RoutingGuideKey="Conditional" RoutingGuideName=""
    RoutingGuideNo="Conditional" SupercedesRoutingGuideNo=""
    Type="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YDM/beforeManageRoutingGuideUE/output">
    <yfc:doc>
        <Element EntityName="Routing_Guide">
            <Description>
                <div>
                    <Validation>
                        <Condition>Root Node must be <code>RoutingGuide</code>
                        </Condition>
                        <Error
                            Description="Invalid root element has been passed" ErrorCode="YDM000146"/>
                    </Validation>
                    <Validation>
                        <Condition>When this API is invoked in Create Mode, the attributes <code>RoutingGuideNo</code>,<code>Type</code> and <code>OrganiztionCode</code> are mandatory </Condition>
                        <Error
                            Description="Routing Guide No is mandatory" ErrorCode="YDM000160"/>
                        <Error Description="Type is mandatory" ErrorCode="YDM000161"/>
                        <Error
                            Description="Organization Code is mandatory" ErrorCode="YDM000162"/>
                    </Validation>
                    <Validation>
                        <Condition>When this API is invoked in Modify or Delete Mode,either the attribute <code>RoutingGuideKey</code> or the attributes <code>RoutingGuideNo</code>,<code>Type</code> and <code>OrganiztionCode</code> are mandatory </Condition>
                        <Error
                            Description="Either Routing Guide Key or routing Guide no, Type and Organization Code are mandatory" ErrorCode="YDM000159"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="ID-40" Name="Action">
                <Description>
                    <div>
					The action that is to be performed during this invocation of this API.
					<br/> Default: <b>
                            <code>VALUE</code>
                        </b> - Modify
					<br/>Valid Values:  <b>
                            <code>Create</code>
                        </b>, <b>
                            <code>Modify</code>
                        </b>, or 	<b>
                            <code>Delete</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SelectionCriteria DepartmentCode="" FreightTerms="" ItemClassification=""/>
    <PersonInfoContact AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" EveningFaxNo=""
        EveningPhone="" FirstName="" HttpUrl="" IsCommercialAddress=""
        JobTitle="" LastName="" MiddleName="" MobilePhone=""
        OtherPhone="" PersonID="" PreferredShipAddress="" State=""
        Suffix="" TaxGeoCode="" Title="" ZipCode=""/>
    <RoutingGuideAttrs>
        <RoutingGuideAttr AttrName="Required" Include="Required" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Routing_Guide_Attr" Name="Include">
                        <Description>
                            <div>
							A flag that indicates if the specified value is part of an Inclusion or Exclusion list.
							<br/>Valid Values:  <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>
                            </div>
                            <Validation>
                                <Condition>If the <code>RoutingGuideAttr</code> element is passed in the input,an error will be thrown if a value is not provided for the <code>AttrName</code> Attribute.</Condition>
                                <Error
                                    Description="Attribute Name is mandatory" ErrorCode="YDM000148"/>
                            </Validation>
                            <Validation>
                                <Condition>If the <code>RoutingGuideAttr</code> element is passed in the input, an error will be thrown if a value is not provided for the <code>Include</code> Attribute.</Condition>
                                <Error
                                    Description="Include flag is mandatory" ErrorCode="YDM000149"/>
                            </Validation>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Value EndZip="Conditional" StartZip="Conditional"
                Value="Conditional" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Routing_Guide_Attr">
                        <Description>
                            <div>
								Specifies the value or zip code ranges that are part of the inclusion/Exclusion list for the specified attribute.
								<Validation>
                                    <Condition>If the attribute name is either <code>ShipFromZip</code> or <code>ShipToZip</code> ,then an error will be thrown if the <code>StartZip</code> and <code>EndZip</code> attribute values are passed as blanks.</Condition>
                                    <Error
                                    Description="Start Zipcode cannot be blanks" ErrorCode="YDM000152"/>
                                    <Error
                                    Description="End Zipcode cannot be blanks" ErrorCode="YDM000153"/>
                                </Validation>
                                <Validation>
                                    <Condition>If the attribute name is something other than <code>ShipFromZip</code> or <code>ShipToZip</code> ,then an error will be thrown if the <code>Value</code> attribute value is are passed as blanks.</Condition>
                                    <Error
                                    Description="Value attribute cannot be blanks" ErrorCode="YDM000154"/>
                                </Validation>
                                <Validation>
                                    <Condition>If the <code>StartZip</code> and <code>EndZip</code> attributes are passed in the input,validation is performed to ensure that they are of the same length. </Condition>
                                    <Error
                                    Description="Start and End zip must be of the same length" ErrorCode="YDM000150"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Text-100" Name="Value">
                            <Description>
                                <div>Value of the attribute.For example if the attribute is <code>FreightTerms</code>, then valid values are <code>COLLECT</code>, <code>PREPAID</code>,<code>TP-COLLECT</code> and <code>TP-PREPAID</code>.
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Value>
        </RoutingGuideAttr>
    </RoutingGuideAttrs>
    <RoutingGuideDetails>
        <RoutingGuideDetail Action="" FreightTermsOverride=""
            RevisedShipToConsolNode="" RevisedShipToNode=""
            RevisedShipToStore="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="Action">
                        <Description>
                            <div>
							The action that is to be performed during this invocation of this API.
							<br/> If header level action passed is <code>Delete</code> or <code>Create</code>. System will ignore <code>Action</code> attribute passed in this node.
							<br/>
                                <code>Delete</code> - Routing guide detail record will be deleted from system.
							<br/> Else; If system could determine correct routing guide detail record from database - it will be updated with new values passed, otherwise a new routing guide detail record will be created.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ShipFrom ShipFromCity="Conditional"
                ShipFromCountry="Conditional" ShipFromNode="Conditional"
                ShipFromState="Conditional" ShipFromZip="Conditional">
                <yfc:doc>
                    <Element EntityName="Routing_Guide_Detail">
                        <Description>
                            <div>
						Origin Address information for which we need to determine SCAC and Carrier Service Code.
						<Validation>
                                    <Condition> An exception will be thrown if all the attributes in the <code>ShipFrom</code> element are void. </Condition>
                                    <Error
                                    Description="at least one ShipFrom attributes must be passed." ErrorCode="YDM000140"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                </yfc:doc>
            </ShipFrom>
            <ShipTo ShipToCity="Conditional"
                ShipToConsolNode="Conditional"
                ShipToCountry="Conditional" ShipToNode="Conditional"
                ShipToState="Conditional" ShipToStore="Conditional" ShipToZip="Conditional">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
						Destination Address information for which we need to determine SCAC and Carrier Service Code.
						<Validation>
                                    <Condition> An exception will be thrown if all the attributes in the <code>ShipTo</code> element are void. </Condition>
                                    <Error
                                    Description="at least one ShipTo attributes must be passed." ErrorCode="YDM000141"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                </yfc:doc>
            </ShipTo>
            <ShippingParameters InputCarrierServiceCode=""
                MaxHandlingUnits="" MaxVolume="" MaxWeight=""
                MinHandlingUnits="" MinVolume="" MinWeight=""
                VolumeUOM="" WeightUOM="">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Routing_Guide_Detail"
                            Name="InputCarrierServiceCode" NameInEntity="Input_Carrier_Service_Code">
                            <Description>
                                <div>
						Specifies the Carrier Service Code that has been requested by the buyer.If this attribute is passed, the <code>SCAC</code> and <code>carrier service code</code> that matches this input criteria will be returned as output by this API.
						</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ShippingParameters>
            <RoutingGuideAttrs>
                <RoutingGuideAttr AttrName="Required" Include="Required" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute Name="AttrName">
                                <Description>
                                    <div> Attribute name of the field that has to be included or excluded.  <code>for /RoutingGuide/RoutingGuideAttrs/RoutingGuideAttr element:</code> AttrName should be one of the following. 
									<br/> ShipFromCountry, ShipFromState, ShipFromZip, ShipFromNode, ShipToCountry, ShipToState, ShipToZip, ShipToConsolNode, ShipToNode, ShipToStore, MinHandlingUnits, MaxHandlingUnits, MinWeight, MaxWeight, MinVolume, MaxVolume, ShipmentType, RevisedShipToConsolNode, RevisedShipToNode, RevisedShipToStore, Priority, Scac,  InputCarrierServiceCode, CarrierServiceCode, FreightTermsOverride, Weight, WeightUOM, Volume, VolumeUOM, HandlingUnits.
									<br/>
                                    <code>for /RoutingGuide/RoutingGuideDetails/RoutingGuideDetail/RoutingGuideAttrs/RoutingGuideAttr element:</code> AttrName should one of the following. 
									<br/> DepartmentCode, ItemClassification, FreightTerms.
									</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <Value EndZip="Conditional" StartZip="Conditional"
                        Value="Conditional" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="ZipCode" Name="EndZip"/>
                                <Attribute DataType="ZipCode" Name="StartZip"/>
                                <Attribute DataType="Text-100" Name="Value"/>
                            </Attributes>
                        </yfc:doc>
                    </Value>
                </RoutingGuideAttr>
            </RoutingGuideAttrs>
            <RoutingGuideDetailCarrierList>
                <RoutingGuideDetailCarrier CarrierServiceCode=""
                    ContactKey="" Priority="" Scac="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="CarrierServiceCode"/>
                            <Attribute DataType="String" Name="Scac"/>
                        </Attributes>
                    </yfc:doc>
                    <PersonInfoContact AddressLine1="" AddressLine2=""
                        AddressLine3="" AddressLine4="" AddressLine5=""
                        AddressLine6="" AlternateEmailID="" Beeper=""
                        City="" Company="" Country="" DayFaxNo=""
                        DayPhone="" Department="" EMailID=""
                        EveningFaxNo="" EveningPhone="" FirstName=""
                        HttpUrl="" IsCommercialAddress="" JobTitle=""
                        LastName="" MiddleName="" MobilePhone=""
                        OtherPhone="" PersonID=""
                        PreferredShipAddress="" State="" Suffix=""
                        TaxGeoCode="" Title="" ZipCode=""/>
                    <ScacAndService ScacAndServiceKey=""/>
                </RoutingGuideDetailCarrier>
            </RoutingGuideDetailCarrierList>
        </RoutingGuideDetail>
    </RoutingGuideDetails>
</RoutingGuide>
