<EntitlementRule CallingOrganizationCode="" Description=""
    EffectiveFromDate="" EffectiveToDate="" EntitlementRuleID=""
    OrganizationCode="" Purpose="Required" StartWithAllCatalogItems="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getEntitlementRuleList/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
                    <Validation>
                        <Condition>
    		Either <code>OrganizationCode</code> or <code>CallingOrganizationCode</code> must be passed. 
        </Condition>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Organization"
                Name="CallingOrganizationCode" NameInEntity="Organization_Code">
                <Description>Calling Organization used to determine enterprise that manages the entitlement rule.</Description>
            </Attribute>
            <Attribute Name="Purpose">
                <Description>
                    <div> This field indicates whether the entitlement rule is used for selling entitlement or buying entitlement. 
					<br/>Valid Values:
					<br/>SELLING - Selling Entitlement
					<br/>BUYING - Buying Entitlement
					<Validation>
                            <Condition>The value of <code>Purpose</code> does not match valid values.
						</Condition>
                            <Error
                                Description="Invalid Entitlement Purpose." ErrorCode="YCM83_006"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <EntitlementRuleDetailList>
        <EntitlementRuleDetail InclusionFlag="" SequenceNo=""
            SourceType="" yfc:DTDOccurrence="ZeroOrMany">
            <Category CategoryPath=""/>
            <AttributeValue AttributeDomainID="" AttributeGroupID=""
                AttributeID="" DoubleValue="" IntegerValue="" Value=""/>
            <EntitlementRuleDetailItemList>
                <EntitlementRuleDetailItem ItemID="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany"/>
            </EntitlementRuleDetailItemList>
        </EntitlementRuleDetail>
    </EntitlementRuleDetailList>
    <EntitlementRuleAssignmentList>
        <EntitlementRuleAssignment AssignedToOrganizationCode=""
            CustomerID="" CustomerLevel="" CustomerType=""
            OrganizationCode="" Purpose="" Region="" RelationshipType=""
            Vertical="" yfc:DTDOccurrence="ZeroOrMany"/>
    </EntitlementRuleAssignmentList>
</EntitlementRule>
