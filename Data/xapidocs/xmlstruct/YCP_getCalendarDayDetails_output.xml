<!--getCalendarDayDetails Output XML-->
<Calendar AllowShiftOverride="" CalendarDescription=""
    CalendarId="Required" CalendarKey="" OrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getCalendarDayDetails/output">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="MasterCalendar.xml" IncludeXMLPath="/Calendar"/>
    <InheritFromCalendar CalendarId="" InheritCalendarKey="" OrganizationCode="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="MasterCalendar.xml" IncludeXMLPath="/Calendar/InheritFromCalendar"/>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				The parent calendar for which this calendar inherites its definition from.
				</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Calendar" Name="CalendarId" NameInEntity="Calendar_Id">
                    <Description>
                        <div>
					The <code>CalendarId</code> of the parent calendar.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Calendar"
                    Name="InheritCalendarKey" NameInEntity="Calendar_Key">
                    <Description>
                        <div>
					The <code>CalendarKey</code> of the parent calendar.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Calendar" Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
					The <code>OrganizationCode</code> of the parent calendar.
					</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </InheritFromCalendar>
    <Dates>
        <Date Date="" DayOfMonth="" EarliestShiftStartTime=""
            IsOverridden="" LastShiftEndTime="" Type="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Date" Name="Date">
                        <Description>Date.</Description>
                    </Attribute>
                    <Attribute DataType="DayOfMonth" Name="DayOfMonth">
                        <Description>Indicates the day of the month.</Description>
                    </Attribute>
                    <Attribute DataType="Time" Name="EarliestShiftStartTime">
                        <Description>Indicates the earliest shift's start time.</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IsOverridden">
                        <Description>This flag indicates that the day details are overridden based on the exception days and exception shifts of this calendar.  If a calendar inherits its definition from another calendar, the child calendar's exception days and exception shifts will be applied on top of the parent calendar's components.  The <code>IsOverridden</code> will be set to <code>
                                <b>Y</b>
                            </code> for the date that is configured as an exception date.</Description>
                    </Attribute>
                    <Attribute DataType="Time" Name="LastShiftEndTime">
                        <Description>Indicates the last shift's end time.</Description>
                    </Attribute>
                    <Attribute DataType="Text-1" Name="Type">
                        <Description>Type of the day.  Valid values: 0 for Off Day; 1 for Working Day; 2 for Edited Working Day; 3 for Edited Off Day.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Shifts>
                <Shift ShiftEndTime="" ShiftKey="" ShiftName=""
                    ShiftStartTime="" ShiftType="" ValidForDay="" yfc:DTDOccurrence="ZeroOrMany"/>
            </Shifts>
        </Date>
    </Dates>
</Calendar>
