<Items OrderReference="" TransactionId="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getAvailabilityCorrectionsForItemListUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="ID-24" Name="OrderReference">
                <Description>
                    <div>
						The <code>OrderReference</code> is the Order Header Key.The supplies can be adjusted based on order information (needed to allow hiding some of the supplies during availability inquiry for specific orders).  					
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid"/>
        </Attributes>
    </yfc:doc>
    <Item DemandType="" EnterpriseCode="" FirstShipDate=""
        IgnorePromised="" IgnoreUnpromised="" ItemID="" LastShipDate=""
        OrganizationCode="" ProductClass="" TotalRequiredQty=""
        TransactionId="" UnitOfMeasure="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Inventory_Item"/>
            <Attributes>
                <Attribute EntityName="Inventory_Demand"
                    Name="DemandType" NameInEntity="Demand_Type"/>
                <Attribute EntityName="Item" Name="EnterpriseCode" NameInEntity="Organization_Code"/>
                <Attribute DataType="TimeStamp" Name="FirstShipDate">
                    <Description>
                        <div>
			The first ship date to look for availability of the Item.
			</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="IgnorePromised">
                    <Description>
                        <div>
			This flag indicates whether to ignore promised demands in the availability picture.
			</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="IgnoreUnpromised">
                    <Description>
                        <div>
			This flag indicates whether to ignore uppromised demands in the availability picture.
			</div>
                    </Description>
                </Attribute>
                <Attribute DataType="TimeStamp" Name="LastShipDate">
                    <Description>
                        <div>
			The last ship date to look for availability of the Item.
			</div>
                    </Description>
                </Attribute>
                <Attribute DataType="OrgCode" EntityName="Organization"
                    Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
			Inventory is being checked on behalf of the organization that is associated with order.
			</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Item"
                    Name="ProductClass" NameInEntity="Product_Class"/>
                <Attribute DataType="Quantity" Name="TotalRequiredQty">
                    <Description>
                        <div>
			Total Required Quantity.
			</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid"/>
            </Attributes>
        </yfc:doc>
        <Supplies yfc:DTDOccurrence="REQUIRED">
            <Supply FirstShipDate="" IsUnplannedSupply=""
                LastShipDate="" OrganizationCode="" Quantity=""
                Segment="" SegmentType="" ShipNode="" SoftAssignedQty=""
                Track="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply"/>
                    <Attributes>
                        <Attribute DataType="TimeStamp" Name="FirstShipDate">
                            <Description>
                                <div>
				The first ship date of the supply.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsUnplannedSupply">
                            <Description>
                                <div>
			This flag indicates whether this supply is from unplanned inventory or not.
			</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="TimeStamp" Name="LastShipDate">
                            <Description>
                                <div>
				The last ship date of the supply.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Supply"
                            Name="OrganizationCode" NameInEntity="Owner_Key">
                            <Description>
                                <div>
				Supply belongs to the organization.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="SoftAssignedQty">
                            <Description>
                                <div>
				The soft assigned demand quantity for this available supply.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="Track">
                            <Description>
                                <div>
							This attribute indicates if the availability is tracked or infinite. If the inventory is tracked the value is <b>
                                    <code>Y</code>
                                    </b> . If the inventory is infinite the value is <b>
                                    <code>N</code>
                                    </b>.							
							<br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or  <b>
                                    <code>N</code>
                                    </b>.
						</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <InventoryTag BatchNo="" InventoryItemKey=""
                    InventoryTagKey="" LotAttribute1="" LotAttribute2=""
                    LotAttribute3="" LotKeyReference="" LotNumber=""
                    ManufacturingDate="" RevisionNo="" TagNumber=""
                    TotalDemand="" TotalOnhandSupply="" TotalOtherSupply=""/>
            </Supply>
        </Supplies>
        <DeliveryMethodForSupplyList>
            <DeliveryMethodForSupply DeliveryMethod="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Text-40" Name="DeliveryMethod">
                            <Description>
                                <div>
						The <code>DeliveryMethod</code> requested for the promise line. Possible values include Delivery DEL, Ship SHP or Pick Up PICK
							</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </DeliveryMethodForSupply>
        </DeliveryMethodForSupplyList>
    </Item>
</Items>
