<UserTimeSheet ActivityCode="" ActivityGroupColor=""
    AllowableIdleMinutes="" EndTime="" MaxEndTime="" MinStartTime=""
    ProductivityType="" StartTime="" UserID="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getUserTimeSheet/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="ActivityCode">
                <Description>Activity code for a task done by a user</Description>
            </Attribute>
            <Attribute DataType="String" Name="ActivityGroupColor">
                <Description>ActivityGroupColor for the activity group associated with the task</Description>
            </Attribute>
            <Attribute DataType="String" Name="AllowableIdleMinutes">
                <Description>Maximum Idle time associated with this productivity type</Description>
            </Attribute>
            <Attribute DataType="String" Name="EndTime">
                <Description>Ending time for all the activities done by a particular user on any day</Description>
            </Attribute>
            <Attribute DataType="String" Name="MaxEndTime">
                <Description>This timestamp indiacates the end time  for a group of users for a particular day.</Description>
            </Attribute>
            <Attribute DataType="String" Name="MinStartTime">
                <Description>This timestamp indiacates the start time  for a group of users for a particular day.</Description>
            </Attribute>
            <Attribute DataType="String" Name="ProductivityType">
                <Description>Productivity type associated with the task type</Description>
            </Attribute>
            <Attribute DataType="String" Name="StartTime">
                <Description>Starting time for  activities done by a particular user on any day</Description>
            </Attribute>
            <Attribute DataType="String" Name="UserID">
                <Description>Id of the user for whom the time sheet is prepared.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Users MaxEndTime="" MinStartTime="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="MaxEndTime"/>
                <Attribute DataType="String" Name="MinStartTime"/>
            </Attributes>
        </yfc:doc>
        <User UserID="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="UserID"/>
                </Attributes>
            </yfc:doc>
            <Activities Date="" EndTime="" StartTime="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="Date"/>
                        <Attribute DataType="String" Name="EndTime"/>
                        <Attribute DataType="String" Name="StartTime"/>
                    </Attributes>
                </yfc:doc>
                <Activity ActivityCode="" ActivityGroupColor=""
                    AllowableIdleMinutes="" EndTime=""
                    ProductivityType="" StartTime="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="ActivityGroupColor"/>
                            <Attribute DataType="String" Name="AllowableIdleMinutes"/>
                            <Attribute DataType="String" Name="EndTime"/>
                            <Attribute DataType="String" Name="ProductivityType"/>
                            <Attribute DataType="String" Name="StartTime"/>
                        </Attributes>
                    </yfc:doc>
                </Activity>
            </Activities>
        </User>
    </Users>
</UserTimeSheet>
