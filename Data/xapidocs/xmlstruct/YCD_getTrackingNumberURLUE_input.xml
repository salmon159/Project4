<TrackingNumbers xmlns="http://www.sterlingcommerce.com/documentation/YCD/getTrackingNumberURLUE/input">
    <TrackingNumber CarrierServiceCode="" RequestNo="" SCAC=""
        ScacAndService="" TrackingNo="Required">
        <yfc:doc>
            <Element EntityName="Shipment"/>
            <Attributes>
                <Attribute Name="CarrierServiceCode">
                    <Description>
                        <div>
							The carrier service availed for transporting this shipment.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="RequestNo">
                    <Description>
                        <div>
							This is a field available in both input and output of this user exit. This field will be used to uniquely identify the proper URL for a tracking number.
						</div>
                    </Description>
                </Attribute>
                <Attribute Name="SCAC">
                    <Description>
                        <div>
							The carrier name to which the container got manifested.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ScacAndService">
                    <Description>
                        <div>
							Value of SCAC and Service code. 
						</div>
                    </Description>
                </Attribute>
                <Attribute Name="TrackingNo">
                    <Description>
                        <div>
							The tracking number. If carrier integration is availed for the carrier on the shipment, then this information is returned by the carrier. This is a unique number assigned to each container that can be used to track individual containers in a shipment. 
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </TrackingNumber>
</TrackingNumbers>
