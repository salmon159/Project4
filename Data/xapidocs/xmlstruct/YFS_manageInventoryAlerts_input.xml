<InventoryAlerts AlertAction="" AlertLevel="" AlertPeriodEnd=""
    AlertPeriodStart="" AlertQuantity="" AlertRaisedOn="" AlertType=""
    DistributionRuleId="Required" EstimatedCost=""
    FirstFutureAvailableDate="" FutureAvailableDate=""
    FutureAvailableQuantity="" InventoryAlertKey="" InventoryItemKey=""
    Latitude="" Longitude="" OnhandAvailableDate=""
    OnhandAvailableQuantity="" Operation="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/manageInventoryAlerts/input">
    <yfc:doc>
        <Element EntityName="Inventory_Alerts"/>
        <Attributes>
            <Attribute Name="DistributionRuleId">
                <Description>
                    <div>
						The Distribution group for which the alert is being created. This is a mandatory parameter for all operations.
						<Validation>
                            <Condition>
                                <code>DistributionRuleId</code> is required.</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>
						Operation to perform on this record.
						<br/>Default: If not passed then API will try to either create or modify depending on the existence of the data.
						<br/>Valid Values: 
						<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
						<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <AvailabilityInformation>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="MasterAvailabilityInformation.xml" IncludeXMLPath="/AvailabilityInformation"/>
        <AvailableInventory AvailableQuantity="" EffectiveStartDate=""
            EffectiveUntilDate="" IsOnhand="" MaxShipDate="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute Name="AvailableQuantity">
                        <Description>
                            <div>
							This indicates the available quantity. If inventory is not tracked, unplanned, or infinite,
							the value may be Java's Integer.MAX_VALUE, Java's Long.MAX_VALUE, or the 
							RealisticQuantityForInfiniteInventory property (if set).
						</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="EffectiveStartDate">
                        <Description>
							The timestamp for which the ShipDate is effective from. Calculated based on the last RTAM execution time or the last <code>EffectiveUntilDate</code>.
					</Description>
                    </Attribute>
                    <Attribute Name="EffectiveUntilDate">
                        <Description>
                            <div>
							The timestamp for which the ShipDate is effective until.This is calculated based on earliest timestamp of the computed date of each node.   The computed date of each node is computed based on the last cut-off time of the ship date and subtract back the minimum notification time based on the node's shipping calendar.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="IsOnhand">
                        <Description>
                            <div>
							This flag indicates whether the AvailableQuantity is based on on-hand supply.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="MaxShipDate">
                        <Description>
                            <div>
							The date when this inventory is available. 
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </AvailableInventory>
    </AvailabilityInformation>
</InventoryAlerts>
