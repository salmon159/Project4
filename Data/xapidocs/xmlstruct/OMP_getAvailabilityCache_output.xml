<Availability CallingOrganizationCode="" DistanceToConsider=""
    DistanceUOMToConsider="" NumberOfClosestNodesToConsider=""
    OnlyConsiderAvailableCache="" xmlns="http://www.sterlingcommerce.com/documentation/OMP/getAvailabilityCache/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="CallingOrganizationCode">
                <Description>
                    <div>The organization for which items and cached availability will be inquired.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Distance" Name="DistanceToConsider">
                <Description>
                    <div>Indicates radius within which nodes can be considered for cached availbility. 
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="UOM" Name="DistanceUOMToConsider">
                <Description>
                    <div>Indicates radius unit of measure.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="NumberOfClosestNodesToConsider">
                <Description>If passed, only the closest number of nodes with availability will be returned.</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="OnlyConsiderAvailableCache">
                <Description>Determines whether all cache (<code>YFS_INVENTORY_ALERTS</code>) data should be considered.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ShipToAddress AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6="" City=""
        Country="" IsCommercialAddress="" Latitude="" Longitude=""
        PersonID="" State="" TaxGeoCode="" ZipCode="">
        <yfc:doc>
            <Element EntityName="Person_Info"/>
        </yfc:doc>
    </ShipToAddress>
    <Lines>
        <Line DeliveryMethod="" ItemID="" LineID="" ProductClass=""
            UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Inventory_Alerts" Name="DeliveryMethod">
                        <Description>Delivery method for alerts.  Only alerts with specified delivery method are returned, SHP is used by default if none are specified.</Description>
                    </Attribute>
                    <Attribute EntityName="Inventory_Item" Name="ItemID"/>
                    <Attribute DataType="Identifier" Name="LineID">
                        <Description>The unique identifier for this line.</Description>
                    </Attribute>
                    <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
                    <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
                </Attributes>
            </yfc:doc>
            <NetworkAvailabilities>
                <NetworkAvailability AlertLevel="" AlertType=""
                    AvailableDate="" AvailableQuantity=""
                    DeliveryMethod="" DistributionRuleId=""
                    FirstFutureAvailableDate="" FutureAvailableDate=""
                    FutureAvailableQuantity="" OnhandAvailableDate=""
                    OnhandAvailableQuantity="" OrganizationCode="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>Network level availability information.</Description>
                        </Element>
                        <Attributes>
                            <Attribute EntityName="Inventory_Alerts" Name="AlertLevel"/>
                            <Attribute EntityName="Inventory_Alerts" Name="AlertType"/>
                            <Attribute DataType="Date" Name="AvailableDate">
                                <Description>The earliest date when the product is available.</Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="AvailableQuantity">
                                <Description>The quantity available for the product.</Description>
                            </Attribute>
                            <Attribute EntityName="Inventory_Alerts" Name="DeliveryMethod"/>
                            <Attribute EntityName="Distribution_Rule" Name="DistributionRuleId"/>
                            <Attribute EntityName="Inventory_Alerts" Name="FirstFutureAvailableDate"/>
                            <Attribute EntityName="Inventory_Alerts" Name="FutureAvailableDate"/>
                            <Attribute EntityName="Inventory_Alerts" Name="FutureAvailableQuantity"/>
                            <Attribute EntityName="Inventory_Alerts" Name="OnhandAvailableDate"/>
                            <Attribute EntityName="Inventory_Alerts" Name="OnhandAvailableQuantity"/>
                            <Attribute EntityName="Distribution_Rule"
                                Name="OrganizationCode" NameInEntity="Owner_Key"/>
                        </Attributes>
                    </yfc:doc>
                </NetworkAvailability>
            </NetworkAvailabilities>
            <ShipNodeAvailabilities>
                <ShipNodeAvailability AlertLevel="" AlertType=""
                    AvailableDate="" AvailableQuantity=""
                    DeliveryMethod="" Distance=""
                    FirstFutureAvailableDate="" FutureAvailableDate=""
                    FutureAvailableQuantity="" Node=""
                    OnhandAvailableDate="" OnhandAvailableQuantity="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>Ship node level availability information.</Description>
                        </Element>
                        <Attributes>
                            <Attribute EntityName="Inventory_Alerts" Name="AlertLevel"/>
                            <Attribute EntityName="Inventory_Alerts" Name="AlertType"/>
                            <Attribute DataType="Date" Name="AvailableDate">
                                <Description>The earliest date when the product is available.</Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="AvailableQuantity">
                                <Description>The quantity available for the product.</Description>
                            </Attribute>
                            <Attribute EntityName="Inventory_Alerts" Name="DeliveryMethod"/>
                            <Attribute DataType="Number" Name="Distance">
                                <Description>Distance between the ship to address and node's address measured in 
								<code>MILES</code>.</Description>
                            </Attribute>
                            <Attribute EntityName="Inventory_Alerts" Name="FirstFutureAvailableDate"/>
                            <Attribute EntityName="Inventory_Alerts" Name="FutureAvailableDate"/>
                            <Attribute EntityName="Inventory_Alerts" Name="FutureAvailableQuantity"/>
                            <Attribute EntityName="Inventory_Alerts"
                                Name="Node" NameInEntity="Ship_Node"/>
                            <Attribute EntityName="Inventory_Alerts" Name="OnhandAvailableDate"/>
                            <Attribute EntityName="Inventory_Alerts" Name="OnhandAvailableQuantity"/>
                        </Attributes>
                    </yfc:doc>
                </ShipNodeAvailability>
            </ShipNodeAvailabilities>
        </Line>
    </Lines>
</Availability>
