<TableTypes xmlns="http://www.sterlingcommerce.com/documentation/YCP/getSupportedTableTypeList/output">
    <TableType CanShare="" KeyIdentifiesColony="" Sharing=""
        TableType="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div>List of supported table types that are derived from the entity XMLs.</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Boolean" Name="CanShare">
                    <Options IsEnumeration="Y">
                        <Option
                            Description="Shards belonging to this table type may be shared with other colonies." Value="Y"/>
                        <Option
                            Description="Shards belonging to this table type should only be in a single colony." Value="N"/>
                    </Options>
                </Attribute>
                <Attribute DataType="Boolean" Name="KeyIdentifiesColony">
                    <Options IsEnumeration="Y">
                        <Option
                            Description="The primary key of a record may be used to uniquely identify a colony." Value="Y"/>
                        <Option
                            Description="The primary key should not be used to derive a colony." Value="N"/>
                    </Options>
                </Attribute>
                <Attribute DataType="String" Name="Sharing">
                    <Options IsEnumeration="Y">
                        <Option
                            Description="All colonies should share a single shard of this type." Value="FORCE"/>
                        <Option
                            Description="New colonies may share the default shard for this type by default." Value="DEFAULT"/>
                        <Option
                            Description="Each colony should define its own shard for this table type." Value="NONE"/>
                    </Options>
                </Attribute>
                <Attribute DataType="String" Name="TableType">
                    <Description>
                        <div>Identifier of the table type.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </TableType>
</TableTypes>
