            <Attributes>
                <Attribute DataType="String" Name="ExceptionTypeKey"/>
            </Attributes>
        </yfc:doc>
        <InboxReferencesList>
            <InboxReferences FromInboxKey="" FromInboxReferenceKey=""
                FromName="" FromReferenceType="" FromValue=""
