        ItemIdQryType="" LastExceedAlert="" LastExceedAlertQryType=""
        LastUnassignAlert="" LastUnassignAlertQryType=""
        LastUnresolveAlert="" LastUnresolveAlertQryType=""
        LockedByUserKey="" LockedByUserKeyQryType="" LockedFlag=""
        LockedFlagQryType="" LockedOn="" LockedOnQryType=""
        OrderHeaderKey="" OrderHeaderKeyQryType="" OrderLineKey=""
        OrderLineKeyQryType="" OrderNo="" OrderNoQryType="" OwnerKey=""
        OwnerKeyQryType="" OwnerType="" OwnerTypeQryType=""
        ParentInboxKey="" ParentInboxKeyQryType="" Priority=""
        PriorityQryType="" QueueKey="" QueueKeyQryType=""
        ResolutionDate="" ResolutionDateQryType="" ResolveBy=""
        ResolveByQryType="" ShipnodeKey="" ShipnodeKeyQryType=""
        Status="" StatusQryType="" SupplierKey="" SupplierKeyQryType=""
        ToActiveFlag="" ToAssignedToUserKey="" ToAutoResolvedFlag=""
        ToClosedOn="" ToDescription="" ToDetailDescription=""
        ToEnterpriseKey="" ToExceptionEscalatedFlag=""
        ToExceptionType="" ToFollowupDate="" ToGeneratedOn=""
