<WorkOrderMonitor NextAlertTs="" RaiseAlert="" xmlns="http://www.sterlingcommerce.com/documentation/VAS/shouldWorkOrderMonitorAlertBeRaisedUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="TimeStamp" Name="NextAlertTs">
                <Description>This determines the earliest time when the work order will be picked up for monitoring again.</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="RaiseAlert">
                <Description>Determines whether to raise an alert or not based on this flag.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <MonitorConsolidation AutoResolve="" Description="" Field10IsKey=""
        Field1IsKey="" Field2IsKey="" Field3IsKey="" Field4IsKey=""
        Field5IsKey="" Field6IsKey="" Field7IsKey="" Field8IsKey=""
        Field9IsKey="" FlowKey="" GroupByField1="" GroupByField10=""
        GroupByField2="" GroupByField3="" GroupByField4=""
        GroupByField5="" GroupByField6="" GroupByField7=""
        GroupByField8="" GroupByField9="" InventoryOrganizationCode=""
        MonitorConsolidationId="" MonitorConsolidationKey=""
        MonitorTypeKey="" ProcessTypeKey="" RealertInterval="" RealertRequired="">
        <yfc:doc>
            <Element EntityName="Monitor_Consolidation"/>
        </yfc:doc>
        <WorkOrder AdditionalRequestedQuantity=""
            ApptStatusDescription="" CaseId=""
            ComputedRequestedQuantity="" CurrentApptSeq=""
            DelayAppointmentTaking="" DocumentType="" EnterpriseCode=""
            FinishNoLaterThan="" HoldFlag="" InvUpdateActivityCode=""
            IsMultiApptRequired="" IsPlanningComplete=""
            IsQuantityRequestedOverridden="" ItemID="" LevelOfService=""
            NextAlertTs="" NodeKey="" OpenWorkOrderFlag=""
            OrderHeaderKey="" OrderLineKey="" OrderNo="" PalletId=""
            PipeLineKey="" PreCallStatus="" PreCallStatusDescription=""
            PreferredResourcePoolId="" PreferredServiceResourceId=""
            Priority="" ProductAmount="" ProductClass=""
            PromisedApptEndDate="" PromisedApptStartDate=""
            ProviderOrganizationCode="" Purpose="" QuantityAllocated=""
            QuantityCompleted="" QuantityReleased="" QuantityRemoved=""
            QuantityRequested="" ReasonCode="" ReasonCodeDescription=""
            ReasonText="" Segment="" SegmentType=""
            SellerOrganizationCode="" SerialNo="" ServiceAmount=""
            ServiceComplexityLevel="" ServiceItemDescription=""
            ServiceItemGroupCode="" ServiceItemID=""
            ServiceSupervisorId="" ServiceUom="" ShipByDate=""
            ShipmentKey="" ShipmentNo="" StartNoEarlierThan="" Status=""
            StatusDate="" StatusDescription="" Timezone=""
            TotalAmount="" TotalVolume="" TotalWeight="" Uom=""
            VolumeUOM="" WeightUOM="" WorkOrderKey="" WorkOrderNo="" isHistory="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Work_Order_master.xml" IncludeXMLPath="/WorkOrder"/>
            <yfc:doc>
                <Element EntityName="Work_Order"/>
                <Attributes>
                    <Attribute DataType="Text-100" Name="ApptStatusDescription">
                        <Description>
                            <div>Description of appointment status.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="CaseId">
                        <Description>
                            <div>Case for which the work order is processed.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="LineNo" Name="CurrentApptSeq">
                        <Description>
                            <div>For work orders where multiple appointments are not required, this field denotes the current appointment sequence.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="PalletId">
                        <Description>
                            <div>Pallet for which the work order is processed.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="PreCallStatusDescription">
                        <Description>
                            <div>Description of Pre-call status.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="ReasonCodeDescription">
                        <Description>
                            <div>Description of Reason Code.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="ServiceComplexityLevel">
                        <Description>
                            <div>This field denotes the highest service level across all service items included in the work order.  For PS work orders only PS service items are considered.  Applicable only for PS or DS work orders.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="ServiceItemDescription">
                        <Description>
                            <div>Description of Service Item.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="StatusDescription">
                        <Description>
                            <div>Description of WorkOrder status.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="TotalAmount">
                        <Description>
                            <div>The total value of the work order.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="TotalVolume">
                        <Description>
                            <div>The quantity requested in the volume unit of measure.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="TotalWeight">
                        <Description>
                            <div>The quantity requested in the weight unit of measure.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-24" Name="VolumeUOM">
                        <Description>
                            <div>The volume unit of measure in which quantity needs to be requested.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-24" Name="WeightUOM">
                        <Description>
                            <div>The weight unit of measure in which quantity needs to be requested.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>
                            <div>Indicates if data should be retrieved from the history table rather than the regular table. 
				<br/>Default: <b>
                                    <code>N</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <MonitorRule Age="" ComparisonDateSubType=""
                ComparisonDateType="" ComparisonStatus=""
                ConditionKey="" DateSubTypeToMonitor=""
                DateTypeToMonitor="" FlowKey=""
                MonitorConsolidationKey="" MonitorRuleKey=""
                MonitorType="" PipelineKey="" Priority=""
                ProcessTypeKey="" StatusToMonitor="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="FlowKey"/>
                    </Attributes>
                </yfc:doc>
            </MonitorRule>
        </WorkOrder>
    </MonitorConsolidation>
</WorkOrderMonitor>
