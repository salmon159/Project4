<!-- receiveCustomerChanges Input XML -->
<Customer BuyerOrganizationCode="" CustomerLevel="" CustomerType=""
    Operation="Required" OrganizationCode="" RelationshipType=""
    RemoteModifyTS="" Status="" Vertical="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/receiveCustomerChanges/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Customer" Name="CustomerType">
                <Description>
                    <div>
				Type of customer.  Valid values are: 01-Business, 02-Consumer. 
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>Operation to perform
					<br/>Valid Values: 
					<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
					<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
					<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
					</div>
                </Description>
            </Attribute>
            <Attribute Name="RemoteModifyTS">
                <Description>
                    <div>
				The modification date from the external system. If this value is less than the modification date on the fulfillment side, this message will be ignored.		
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <CustomerContactList>
