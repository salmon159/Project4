    <CustomerCurrencyList Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>Indicates if all existing child node definition in the database need to be deleted and replace with the provided child nodes.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <CustomerCurrency Currency="" CustomerCurrencyKey=""
            IsDefaultCurrency="" Operation="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Customer_Currency"/>
                <Attributes>
                    <Attribute EntityName="Customer_Currency"
                        Name="IsDefaultCurrency" NameInEntity="Is_Default_Currency">
                        <Description>
                            <div>Indicates if a currency is the default customer currency. 
								<Validation>
                                    <Condition>
                                    <code>IsDefaultCurrency</code> not passed.
									</Condition>
                                    <Error
                                    Description="A customer cannot have multiple default currencies." ErrorCode="YSC83_0007"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Action" Name="Operation">
                        <Description>
                            <div>
								Valid value is <b>
                                    <code>Delete</code>
