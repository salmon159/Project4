<!-- multiApi input XML -->
<MultiApi xmlns="http://www.sterlingcommerce.com/documentation/YCP/multiApi/input">
    <!-- There can be one or more api elements -->
    <!-- Version attribute is optional, Optional values are "ver45", "ver40" -->
    <API FlowName="" IsExtendedDbApi="" Name="" Version="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div> Provide either the <code>Name</code> of the API or <code>FlowName</code> of the service.</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-100" Name="FlowName">
                    <Description>
                        <div>
		Name of the flow or service to call.
		</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="IsExtendedDbApi">
                    <Description>
                        <div>
		Set value to Y only if calling an extended database api.
		</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-100" Name="Name">
                    <Description>
                        <div>
		Name of the API to call.
		</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-10" Name="Version">
                    <Description>
                        <div>
		Version number of the API. Only provide a value if a specific version of the API is needed.
		</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <!-- The Input element must contain a child element that corresponds to
 the document element of the api input -->
        <Input yfc:AllowAnyElement="skip" yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
	 The Input element must contain a child element that corresponds to
 the document element of the API input
	</div>
                    </Description>
                </Element>
            </yfc:doc>
        </Input>
        <!-- The Template element (if given) may contain a child element that corresponds to
 the document element of the template to use for the API call -->
        <Template yfc:AllowAnyElement="skip">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
	 The Template element (if given) may contain a child element that corresponds to
 the document element of the template to use for the API call
	</div>
                    </Description>
                </Element>
            </yfc:doc>
        </Template>
    </API>
</MultiApi>
