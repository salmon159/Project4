<GenerateBom BuyerUserId="" ConfigurationDate="" Country="Required"
    Currency="Required" CustomerID="" EnterpriseCode="" FiringPhase=""
    IgnoreCustomerEntitlements="" Language="Required" ModelDepth=""
    OrganizationCode="Required" Path="Required" Variant="" xmlns="http://www.sterlingcommerce.com/documentation/YIC/generateConfigurationBOM/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
					The root xml element of the header.  
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="ContactUserId" Name="BuyerUserId">
                <Description>
                    <div>An identifier of the customer. This is a pass through field available to user exits.</div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="ConfigurationDate">
                <Description>
                    <div>Date on which the configuration is generated. 
						This date is used for all time sensitive information associated with the configuration, like pricing, entitlements, rule and constraint evaluations etc. 
						If void it is defaulted to current date and time.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Country">
                <Description>
                    <div>
						The country identifier for the models.
						<Validation>
                            <Condition>
                                <code>Country</code> is not passed. </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YIC10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Country</code> is not valid.</Condition>
                            <Error
                                Description="Invalid locale attributes passed to the API." ErrorCode="YIC10010"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Currency" Name="Currency">
                <Description>
                    <div>Currency used for pricing data selection.
		  				<Validation>
                            <Condition>
                                <code>Currency</code> is not passed. </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YIC10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Currency</code> is not valid.</Condition>
                            <Error Description="YFS: Invalid currency." ErrorCode="YFS10325"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Pricelist_Assignment"
                Name="CustomerID" NameInEntity="Customer_ID">
                <Description>
                    <div>The identifier for the customer.
						<Validation>
                            <Condition>If not void then <code>CustomerID</code> must be of one of the valid customer.</Condition>
                            <Error
                                Description="Invalid customer Id passed in the input." ErrorCode="YIC10015"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="EnterpriseCode">
                <Description>
                    <div>
					The <code>EnterpriseCode</code> on which behalf pricing inquiry is being made. If not passed <code>OrganizationCode</code> primary enterprise will be used.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-480" Name="FiringPhase">
                <Description>
                    <div>Firing Phase</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreCustomerEntitlements">
                <Description>
                    <div>
			  			If <code>IgnoreCustomerEntitlements</code> is passed as <b>
                            <code>Y</code>
                        </b> then the customer entitlements are not applied. Default value is <b>
                            <code>N</code>
                        </b>.
				  	</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Language">
                <Description>
                    <div>
						The language identifier for the models to search for.
						<Validation>
                            <Condition>
                                <code>Language</code> is not passed. </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YIC10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Language</code> is not valid.</Condition>
                            <Error
                                Description="Invalid locale attributes passed to the API." ErrorCode="YIC10010"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="ModelDepth">
                <Description>
                    <div>
						The depth of the current model with respect to the root model. 
						The <code>ModelDepth</code> attribute must be set for child models. Default value is <b>
                            <code>0</code>
                        </b>.
						<Validation>
                            <Condition>
                                <code>ModelDepth</code> is invalid. </Condition>
                            <Error Description="Invalid Integer" ErrorCode="PLT0001"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>Pricing data is read on behalf of the organization specified by <code>OrganizationCode</code>.
						<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not passed. </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YIC10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not valid. </Condition>
                            <Error
                                Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-480" Name="Path">
                <Description>
                    <div>
						The path to the model that should be retrieved.
						<Validation>
                            <Condition>
                                <code>Path</code> is not passed. </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YIC10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Path</code> is not valid.</Condition>
                            <Error
                                Description="Invalid model path has been passed in the input." ErrorCode="YIC10013"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-120" Name="Variant">
                <Description>
                    <div>
						The variant attribute denotes the variant (dialect) of the <code>Language</code> input
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Picks>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="masterPicks.xml" IncludeXMLPath="/"/>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					The <code>Picks</code> element from the output of previous API 
					call should be included in the input for subsequent API call.
				</div>
                </Description>
            </Element>
        </yfc:doc>
        <Pick Item="" ItemId="" Quantity="" Type="" Value="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
						A pick node contains the internal information about a pick. 
					</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Text-480" Name="Item">
                        <Description>
                            <div>
							The configurator item name for a pick.							
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Integer" Name="ItemId">
                        <Description>
                            <div>
							The <code>id</code> of the configurator item. This is applicable only
							for option items created dynamically through <code>instantiateSubModel</code> API. 
							This value is initially captured while creating child model instances using 
							<code>instantiateSubModel</code> API.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Integer" Name="Quantity">
                        <Description>
                            <div>
							The quantity of the pick.							
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-480" Name="Type">
                        <Description>
                            <div>
							The type of action to be performed on the pick. Default value is <code>add</code>. 
							For option items created dynamically through <code>instantiateSubModel</code> API,
							the type should be specified as <code>create</code>. This value is initially captured
							while creating child model instances using <code>instantiateSubModel</code> API.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-480" Name="Value">
                        <Description>
                            <div>
							The user entered value associated with the item.							
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <NestedConfiguration>
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
							The configuration of the entire sub-model tree associated with the option item. The nested 
							configuration includes all the configuration information from the descendants. This information 
							is initially captured while returning from the sub-model.
						</div>
                        </Description>
                    </Element>
                </yfc:doc>
                <ConfiguratorBOM ModelName="Required" Version=""
                    containerOnly="" errors="">
                    <yfc:includeInParent IncludeXMLChildNodes="Y"
                        IncludeXMLFileName="masterConfiguratorBOM.xml" IncludeXMLPath="/"/>
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
					This element has the header information of configurator BOM.  
				</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Text-254" Name="ModelName">
                                <Description>
                                    <div>
						The full configurator name for the model.
						<Validation>
                                    <Condition>
                                    <code>ModelName</code> is not passed. </Condition>
                                    <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YIC10000"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="PositiveInteger" Name="Version">
                                <Description>
                                    <div>
						The current BOM version.					
                    </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Flag" Name="containerOnly">
                                <Description>
                                    <div>
						This attribute is true if the model root has the container only flag set otherwise false.
						<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Flag" Name="errors">
                                <Description>
                                    <div>
						This attribute is true if there are errors in the BOM, otherwise false.
						<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <message item="" type="" value="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>
							BOMs can contain messages about the status of the configuration and are produced by rules or constraint tables.  
						</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Text-480" Name="item">
                                    <Description>
                                    <div>
								Name of the message.							
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-120" Name="type">
                                    <Description>
                                    <div>
								The type of the message in the configurator BOM.	
								<br/>Valid Values: <b>
                                    <code>error</code>
                                    </b> or <b>
                                    <code>warning</code>
                                    </b> or <b>
                                    <code>suggest</code>
                                    </b>
                                    </div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-240" Name="value">
                                    <Description>
                                    <div>
								The displayable value for the message.
							</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </message>
                    <LineItem CommerceItemKey="" Description=""
                        ItemID="Required" ItemKey="" Name="" Price=""
                        Quantity="Required" RulePicked="Required" SKU=""
                        UOM="" Visible="Required" lockprice="" uevValue="">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>
							LineItems form a tree structure in that they can contain other line items.  The top most LineItem is the root of the model.  
						</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Key" Name="CommerceItemKey">
                                    <Description>
                                    <div>
								An optional key for the item from an order.  These keys are preserved if it is passed in the input. It will be returned in the output.							
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-480" Name="Description">
                                    <Description>
                                    <div>
								The description of the <code>LineItem</code>.
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Integer" Name="ItemID">
                                    <Description>
                                    <div>
								The item configurator ID for the item.							
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Integer" Name="ItemKey">
                                    <Description>
                                    <div>
								The visual modeler key for the item.  This attribute is not considered in the input.
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-480" Name="Name">
                                    <Description>
                                    <div>
								The unique name for the <code>LineItem</code> in a particular depth of the hierarchy.							
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="UnitPrice" Name="Price">
                                    <Description>
                                    <div>
								The price of the <code>LineItem</code>.
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Integer" Name="Quantity">
                                    <Description>
                                    <div>
								The quantity of the item ordered for the <code>LineItem</code>.								
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="RulePicked">
                                    <Description>
                                    <div>
								This attribute is true if the item was picked by a rule otherwise false.
								<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-120" Name="SKU">
                                    <Description>
                                    <div>
								The sku associated to the <code>LineItem</code>.								
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="UOM" Name="UOM">
                                    <Description>
                                    <div>
								The UOM associated to the <code>LineItem</code>.								
							</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="Visible">
                                    <Description>
                                    <div>
								This attribute is true if the item should be visible to the user in the order, otherwise false.  Very often models contain information needed for reconfiguration that should not be visible in the order.
								<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="lockprice">
                                    <Description>
                                    <div>
								This attribute is true if the price in the BOM should used in the order, otherwise false. 
								<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-240" Name="uevValue">
                                    <Description>
                                    <div>
								The user entered value associated to the <code>LineItem</code>.								
							</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <property external="" name="" type="" value="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Element>
                                    <Description>
                                    <div>
								The holder of properties associated with the <code>LineItem</code>. It contains:
								<ol>
                                    <li>The Configurator Properties that are attached to the items in the model and 
									flagged as "output" properties.</li>
                                    <li>The external input properties injected into the model to influence its
									runtime behavior.</li>
                                    </ol>
                                    </div>
                                    </Description>
                                </Element>
                                <Attributes>
                                    <Attribute DataType="Flag" Name="external">
                                    <Description>
                                    <div>
									The flag to differentiate externally injected input properties from output properties.
									The attribute value is <b>
                                    <code>Y</code>
                                    </b>, if the property is an externally injected
									input property. Default value is <b>
                                    <code>N</code>
                                    </b>.
								</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Text-480" Name="name">
                                    <Description>
                                    <div>
									Name of the property.							
								</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Integer" Name="type">
                                    <Description>
                                    <div>
									The type of the property in the configurator BOM.	
									<br/>Valid Values: <b>
                                    <code>bigdecimal</code>
                                    </b> or <b>
                                    <code>int</code>
                                    </b> or <b>
                                    <code>List</code>
                                    </b> or <b>
                                    <code>long</code>
                                    </b> or <b>
                                    <code>double</code>
                                    </b> or <b>
                                    <code>String</code>
                                    </b> or <b>
                                    <code>date</code>
                                    </b> or <b>
                                    <code>unknown</code>
                                    </b>
                                    </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Text-480" Name="value">
                                    <Description>
                                    <div>
									The value of the property in string form.
								</div>
                                    </Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                        </property>
                        <LineItem CommerceItemKey="" Description=""
                            ItemID="Required" ItemKey="" Name=""
                            Price="" Quantity="Required"
                            RulePicked="Required" SKU="" UOM=""
                            Visible="Required" lockprice="" uevValue="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Element>
                                    <Description>
                                    <div>
											LineItems form a tree structure in that they can contain other line items.  The top most LineItem is the root of the model.  
										</div>
                                    </Description>
                                </Element>
                                <Attributes>
                                    <Attribute DataType="Key" Name="CommerceItemKey">
                                    <Description>
                                    <div>
												An optional key for the item from an order.  These keys are preserved if it is passed in the input. It will be returned in the output.							
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Text-480" Name="Description">
                                    <Description>
                                    <div>
												The description of the <code>LineItem</code>.
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Integer" Name="ItemID">
                                    <Description>
                                    <div>
												The item configurator ID for the item.							
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Integer" Name="ItemKey">
                                    <Description>
                                    <div>
												The visual modeler key for the item. This attribute is not considered in the input.
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Text-480" Name="Name">
                                    <Description>
                                    <div>
												The unique name for the <code>LineItem</code> in a particular depth of the hierarchy.							
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="UnitPrice" Name="Price">
                                    <Description>
                                    <div>
												The price of the <code>LineItem</code>.
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Integer" Name="Quantity">
                                    <Description>
                                    <div>
												The quantity of the item ordered for the <code>LineItem</code>.								
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Flag" Name="RulePicked">
                                    <Description>
                                    <div>
												This attribute is true if the item was picked by a rule otherwise false.
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Text-120" Name="SKU">
                                    <Description>
                                    <div>
												The sku associated to the <code>LineItem</code>.								
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="UOM" Name="UOM">
                                    <Description>
                                    <div>
												The UOM associated to the <code>LineItem</code>.								
											</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Flag" Name="Visible">
                                    <Description>
                                    <div>
												This attribute is true if the item should be visible to the user in the order, otherwise false.  Very often models contain information needed for reconfiguration that should not be visible in the order.
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Flag" Name="lockprice">
                                    <Description>
                                    <div>
												This attribute is true if the price in the BOM should used in the order, otherwise false. 
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Text-240" Name="uevValue">
                                    <Description>
                                    <div>
												The user entered value associated to the <code>LineItem</code>.								
											</div>
                                    </Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                        </LineItem>
                        <AdditionalLinePrices>
                            <yfc:doc>
                                <Element>
                                    <Description>
                                    <div>
									The holder of additional line prices associated with the <code>LineItem</code>. It contains one or more <code>AdditionalLinePrice</code> elements.
								</div>
                                    </Description>
                                </Element>
                            </yfc:doc>
                            <AdditionalLinePrice IsPriceLocked=""
                                Price="" PriceTypeName="" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:doc>
                                    <Element>
                                    <Description>
                                    <div>
										Contains the details of an additional line price type that is associated to the <code>LineItem</code>.
									</div>
                                    </Description>
                                    </Element>
                                    <Attributes>
                                    <Attribute DataType="Flag" Name="IsPriceLocked">
                                    <Description>
                                    <div>
											This attribute is true if the price type value in the BOM should used in the order, otherwise false. 
											<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="UnitPrice" Name="Price">
                                    <Description>
                                    <div>
											The price value of the price type.
										</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="Text-240" Name="PriceTypeName">
                                    <Description>
                                    <div>
											The name of the price type						
										</div>
                                    </Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                            </AdditionalLinePrice>
                        </AdditionalLinePrices>
                    </LineItem>
                </ConfiguratorBOM>
            </NestedConfiguration>
            <NestedProperties>
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
							The output properties from the sub-model and its descendants associated with the option item. This information 
							is initially captured while returning from the sub-model.
						</div>
                        </Description>
                    </Element>
                </yfc:doc>
                <properties>
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
								The holder of output properties.
							</div>
                            </Description>
                        </Element>
                    </yfc:doc>
                    <property name="" path="" type="" value="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>
									This element holds information about properties associated to item.  
								</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Text-480" Name="name">
                                    <Description>
                                    <div>
										The name of the property.							
									</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-120" Name="path">
                                    <Description>
                                    <div>
										The path to the item that contains the property.									
									</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-120" Name="type">
                                    <Description>
                                    <div>
										The datatype of the property.
										<br/>Valid Values: <b>
                                    <code>bigdecimal</code>
                                    </b> or <b>
                                    <code>int</code>
                                    </b> or <b>
                                    <code>List</code>
                                    </b> or <b>
                                    <code>long</code>
                                    </b> or <b>
                                    <code>double</code>
                                    </b> or <b>
                                    <code>String</code>
                                    </b> or <b>
                                    <code>date</code>
                                    </b> or <b>
                                    <code>unknown</code>
                                    </b>
                                    </div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Text-480" Name="value">
                                    <Description>
                                    <div>
										The value in string form for the property. List values are returned with each value separated by a carriage return.		
									</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </property>
                </properties>
            </NestedProperties>
        </Pick>
    </Picks>
    <InputProperties>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="masterInputProperties_input.xml" IncludeXMLPath="/"/>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					A holder of input properties applicable for the current model. It contains:
					<ol>
                            <li>External properties to be injected into the current model.</li>
                            <li>Properties derived from parent state, based on the input property list defined in the current model. 
						This information is initially captured while punching into the current model from its parent.</li>
                        </ol>
					These input property values are considered while evaluating the rules in the current model. The <code>InputProperties</code>
					element from the output of previous API call should be included in the input for subsequent API call.
				</div>
                </Description>
            </Element>
        </yfc:doc>
        <Property External="" Name="" Path="" Type="" Value="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
						This element holds information about input properties for the current model. 
					</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Flag" Name="External">
                        <Description>
                            <div>
				  			The flag to indicate whether the property is an external input property or not. If <code>External</code> 
				  			is passed as <b>
                                    <code>Y</code>
                                </b>, the property is injected into the model and also captured in the BOM 
				  			under the corresponding <code>LineItem</code>. Default value is <b>
                                    <code>N</code>
                                </b>.
					  	</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-120" Name="Name">
                        <Description>
                            <div>
							The name of the property in the property pool.
							<code>Name</code> is mandatory, if the property is an external input property.		
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-480" Name="Path">
                        <Description>
                            <div>
							The path to a property from the property pool.
							<code>Path</code> is mandatory, if the property is an external input property.				
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-120" Name="Type">
                        <Description>
                            <div>
							The datatype of the property. Valid values: 
							<code>String</code> or <code>Double</code> or <code>Integer</code> or <code>List</code> or <code>Date</code>.
							<code>Type</code> is mandatory, if the property is an external input property.
							<Validation>
                                    <Condition>
                                    <code>Type</code> is not valid.</Condition>
                                    <Error
                                    Description="Invalid property type was passed in input." ErrorCode="YIC10020"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-480" Name="Value">
                        <Description>
                            <div>
							The value of the property.				
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Property>
    </InputProperties>
</GenerateBom>
