<Task AssignedToUserId="" AssignmentNo="" AssignmentStatus=""
    BatchHold="" ConsolidatedTaskId="" DependencyHold="" EndTimeStamp=""
    EnterpriseKey="" EquipmentId="" EquipmentLocationId=""
    ExceptionHold="" FinishNoLaterThan="" HoldReasonCode=""
    HoldReasonText="" IsSummaryTask="" MaximumRecords="" Node=""
    OnlyUnassigned="" OrganizationCode="" ParentTaskId=""
    PredecessorTaskId="" ProductivityKey="" ReasonCode="" ReasonText=""
    RootTaskId="" ShowHierarchy="" SourceAisle="" SourceLocationId=""
    SourceSortSequence="" SourceZoneId="" StartNoEarlierThan=""
    StartTimeStamp="" TargetAisle="" TargetCaseId="" TargetLocationId=""
    TargetPalletId="" TargetSortSequence="" TargetZoneId="" TaskId=""
    TaskKey="" TaskPriority="" TaskStatus="" TaskType="" isHistory="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getTaskList/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="MasterTask.xml" IncludeXMLPath="/Task"/>
    <yfc:doc>
        <Attributes>
            <Attribute Name="ConsolidatedTaskId">
                <Description>
                    <div>
				Reference to indicate the task that was included in the list of tasks that got consolidated and suggested to the user as one consolidated task.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="MaximumRecords">
                <Description>
                    <div>
				Maximum number of records this api can return.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="OnlyUnassigned">
                <Description>
                    <div>
				Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: 
				<br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_TASK table.
				<br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns tasks which are not assigned to any user. If <code>ShowHierarchy</code> is passed as <b>
                            <code>Y</code>
                        </b>, some of the parent tasks returned may be already assigned.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ShowHierarchy">
                <Description>
                    <div>
				Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: 
				<br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria
				<br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria and any parent tasks. All parent tasks,  to the root task, are returned.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="TargetCaseId" NameInEntity="Target_LPN_No">
                <Description>
                    <div>Target Case Id of the task</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="TargetPalletId" NameInEntity="Target_LPN_No">
                <Description>
                    <div>Target Pallet Id of the task</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="isHistory">
                <Description>
                    <div>
				Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
				<br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_TASK table.
				<br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_TASK_H table (the history table).
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Inventory CountryOfOrigin="" FifoNo="" InventoryStatus="" ItemId=""
        ProductClass="" Quantity="" ReceiptHeaderKey="" Segment=""
        SegmentType="" SerialNo="" ShipByDate="" SourceCaseId=""
        SourcePalletId="" UnitOfMeasure="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="MasterTaskForInput.xml" IncludeXMLPath="/Task/Inventory"/>
        <yfc:doc>
            <Attributes>
                <Attribute DataType="LPNNo" Name="SourceCaseId" NameInEntity="Source_LPN_No">
                    <Description>
                        <div>Source Case Id of the task</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="SourcePalletId" NameInEntity="Source_LPN_No">
                    <Description>
                        <div>Source Pallet Id of the task</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <TagAttributes BatchNo="" LotAttribute1="" LotAttribute2=""
            LotAttribute3="" LotKeyReference="" LotNumber=""
            ManufacturingDate="" RevisionNo="">
            <yfc:doc>
                <Element EntityName="Inventory_Tag"/>
            </yfc:doc>
        </TagAttributes>
    </Inventory>
    <TaskReferences BatchNo="" BolNo="" ContainerNo="" DocumentType=""
        LoadNo="" ManifestNo="" OrderHeaderKey="" OrderLineKey=""
        OrderNo="" OrderReleaseKey="" PrimeLineNo="" ReleaseNo=""
        Scac="" ShipmentContainerKey="" ShipmentKey="" ShipmentNo=""
        SubLineNo="" TrailerNo="" WaveNo=""/>
    <TaskType ActivityGroupId="" TaskType=""/>
    <DataAccessFilter ApplyDataSecurityFilters="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Text-1" Name="ApplyDataSecurityFilters">
                    <Description>
                        <div>
						Default: <b>
                                <code>N</code>
                            </b>
                            <br/>Valid Values:
						<br/>
                            <b>
                                <code>Y</code>
                            </b> - Returns records by honoring the Data Security Groups.
						Note: Data Security Groups will not be honored for the Summary Tasks.
						<br/>
                            <b>
                                <code>N</code>
                            </b> - Returns records by ignoring the Data Security Groups.
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </DataAccessFilter>
</Task>
