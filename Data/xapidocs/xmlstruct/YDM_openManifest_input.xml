<Manifest ManifestClosedFlag="" ManifestDate="Required" ManifestKey=""
    ManifestNo="" ManifestStatus="" PrintCount="" Scac="Required"
    ShipNode="Required" ShipperAccountNo="Required" TrailerNo="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/openManifest/input">
    <yfc:includeInParent
        IncludeXMLFileName="YFS_Manifest_master_input.xml"
        IncludeXMLPath="/Manifest" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Manifest"/>
        <Attributes>
            <Attribute DataType="TimeStamp" Name="ManifestDate">
                <Description>
                    <div> Manifest creation date.
				<Validation>
                            <Condition> Cannot be blank.
				</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                        <Validation>
                            <Condition> Should not be before system date.
				</Condition>
                            <Error
                                Description="Manifest date should not be before system date." ErrorCode="YDM00164"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="Scac">
                <Description>
                    <div> One of the elements of the unique key or business key, that identifies a manifest.  
				<Validation>
                            <Condition> Cannot be blank.
				</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                        <Validation>
                            <Condition> Must be a valid Scac.
				</Condition>
                            <Error
                                Description="SCAC does not exist in the system." ErrorCode="YDM00087"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="ShipNode">
                <Description>
                    <div>One of the elements of the unique key or business key, that identifies a manifest. Manifest_No, Shipnode_Key and Scac in unison uniquely identify a manifest. 
				<Validation>
                            <Condition> Cannot be blank.
				</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                        <Validation>
                            <Condition> Must be a valid ShipNode.
				</Condition>
                            <Error
                                Description="ShipNode does not exist in the system." ErrorCode="YDM00085"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" Name="ShipperAccountNo">
                <Description>
                    <div>Gives the shippers account number. 
					<Validation>
                            <Condition> Cannot be blank.
					</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Manifest>
