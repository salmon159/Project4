<ReprintPackage Carrier="" IsContainerDomestic="" LabelFormatValue=""
    ShipperAccountNumber="" ThermalLabelPrinterID="" TrackingNumber=""
    UPSServiceType="" xmlns="http://www.sterlingcommerce.com/documentation/YCS/reprintCarrierLabelUserExit/input">
    <yfc:doc>
        <Element>
            <Description>
				The connection parameters used to connect to the Carrier Server.
			</Description>
        </Element>
        <Attributes>
            <Attribute DataType="String" Name="Carrier">
                <Description>
					The carrier.
				</Description>
            </Attribute>
            <Attribute DataType="String" Name="IsContainerDomestic">
                <Description>
					Is container domestic?
				</Description>
            </Attribute>
            <Attribute DataType="String" Name="LabelFormatValue">
                <Description>
					The label format value.
				</Description>
            </Attribute>
            <Attribute DataType="String" Name="ShipperAccountNumber">
                <Description>
					The shipper account number.
				</Description>
            </Attribute>
            <Attribute DataType="String" Name="ThermalLabelPrinterID">
                <Description>
					The thermal label printer ID.
				</Description>
            </Attribute>
            <Attribute DataType="String" Name="TrackingNumber">
                <Description>
					The tracking number of the shipment.
				</Description>
            </Attribute>
            <Attribute DataType="String" Name="UPSServiceType">
                <Description>
					The USP service type.
				</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ConnectionParameters SellerOrganizationCode="" ShipNode="">
        <yfc:doc>
            <Element>
                <Description>
					The connection parameters used to connect to the Carrier Server.
				</Description>
            </Element>
            <Attributes>
                <Attribute DataType="String" Name="SellerOrganizationCode">
                    <Description>
						The seller organization code of the shipment.
					</Description>
                </Attribute>
                <Attribute DataType="String" Name="ShipNode">
                    <Description>
						The Ship Node of the shipment.
					</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <PierbridgeParams PBDBPassword="" PBDBURL="" PBDBUserName=""
            PBDefaultRateRequestUser="" PBServerURL="">
            <yfc:doc>
                <Element>
                    <Description>
						The connection parameters used to connect to the Pierbridge Shipment server.
					</Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="PBDBPassword">
                        <Description>
							The password to connect to Pierbridge Shipment server's database.
						</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="PBDBURL">
                        <Description>
							The jdbc URL of Pierbridge Shipment server's database.
						</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="PBDBUserName">
                        <Description>
							The user name to connect to Pierbridge Shipment server's database.
						</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="PBDefaultRateRequestUser">
                        <Description>
							The user name used to get freight charges from Pierbridge Shipment Server.
						</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="PBServerURL">
                        <Description>
							The URL to connect to Pierbridge Shipment server.
						</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </PierbridgeParams>
        <FedExParams FedExIPAddress="" FedExPortNo="">
            <yfc:doc>
                <Element>
                    <Description>
						The connection parameters used to connect to the FedEx Shipment server.
					</Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="FedExIPAddress">
                        <Description>
							The  IP Address to FedEx Shipment server.
						</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="FedExPortNo">
                        <Description>
							The port number to Fedex shipment server.
						</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </FedExParams>
        <ConnectShipParams ConnectShipURL="">
            <yfc:doc>
                <Element>
                    <Description>
						The connection parameters used to connect to the Shipment server.
					</Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="ConnectShipURL">
                        <Description>
							The URL to connect to the Shipment server.
						</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ConnectShipParams>
    </ConnectionParameters>
</ReprintPackage>
