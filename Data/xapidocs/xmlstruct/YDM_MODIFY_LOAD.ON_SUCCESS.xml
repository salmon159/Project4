<Load ActualArrivalDate="" ActualDepartureDate="" ActualFreightCharge=""
    AppointmentNo="" BolNo="" BuyerOrganizationCode=""
    CarrierServiceCode="" Currency="" DeliveryPlanKey=""
    DestinationAddressKey="" DestinationNode="" DocumentType=""
    DropAtShipmentDestination="" EnterpriseCode=""
    ExpectedArrivalDate="" ExpectedDepartureDate="" HazardousMaterial=""
    ITNNo="" IsBreakBulkLoad="" IsExpInclInManifest="" LoadClosedFlag=""
    LoadKey="" LoadNo="" LoadType="" ManifestKey="" NumberOfCases=""
    NumberOfPallets="" OriginAddressKey="" OriginNode=""
    OwnerOrganizationCode="" PickupFromShipmentOrigin="" PipelineKey=""
    PodNo="" ProNo="" Scac="" ScacAndService="" ScacAndServiceKey=""
    SealNo="" ShipMode="" ShipVia="" Status="" StatusDate=""
    TotalActualCharge="" TotalEstimatedCharge="" TotalVolume=""
    TotalVolumeUOM="" TotalWeight="" TotalWeightUOM="" TrailerNo=""
    VesselName="" VoyageNo="" isHistory="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/MODIFY_LOAD/ON_SUCCESS">
    <yfc:includeInParent IncludeXMLFileName="YFS_Load_master.xml" IncludeXMLPath="/Load"/>
    <yfc:doc>
        <Element EntityName="Load"/>
        <Attributes>
            <Attribute DataType="PositiveInteger" Name="NumberOfCases">
                <Description>Indicates total number of outermost cases in the load.
				</Description>
            </Attribute>
            <Attribute DataType="PositiveInteger" Name="NumberOfPallets">
                <Description>Indicates total number of outermost pallets in the load.
				</Description>
            </Attribute>
            <Attribute EntityName="SCAC_And_Service"
                Name="ScacAndService" NameInEntity="Scac_And_Service"/>
            <Attribute EntityName="SCAC_And_Service"
                Name="ScacAndServiceKey" NameInEntity="Scac_And_Service_Key"/>
            <Attribute DataType="Flag" Name="isHistory">
                <Description>Indicates if data is fetched from the regular table or the history table. Valid values are Y and N.
			     </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OriginAddress AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" ErrorTxt=""
        EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
        IsCommercialAddress="" JobTitle="" LastName="" MiddleName=""
        MobilePhone="" OtherPhone="" PersonID="" PersonInfoKey=""
        PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
        Title="" UseCount="" VerificationStatus="" ZipCode="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Person_Info_master.xml" IncludeXMLPath="/Address"/>
        <yfc:doc>
            <Element EntityName="Person_Info"/>
        </yfc:doc>
    </OriginAddress>
    <DestinationAddress AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" ErrorTxt=""
        EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
        IsCommercialAddress="" JobTitle="" LastName="" MiddleName=""
        MobilePhone="" OtherPhone="" PersonID="" PersonInfoKey=""
        PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
        Title="" UseCount="" VerificationStatus="" ZipCode="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Person_Info_master.xml" IncludeXMLPath="/Address"/>
        <yfc:doc>
            <Element EntityName="Person_Info"/>
        </yfc:doc>
    </DestinationAddress>
    <LoadShipments>
        <LoadShipment BolNo="" DropoffStopKey=""
            DropoffUpdateReceived="" LoadKey="" LoadShipmentKey=""
            PickupStopKey="" PickupUpdateReceived="" ProNo=""
            SellerOrganizationCode="" ShipNode="" ShipmentKey=""
            ShipmentNo="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Load_Shipment_master.xml" IncludeXMLPath="/LoadShipment"/>
            <yfc:doc>
                <Element EntityName="Load_Shipment"/>
                <Attributes>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>Indicates if data is fetched from the regular table or the history table. Valid values are Y and N.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Shipment ShipmentNo=""/>
        </LoadShipment>
    </LoadShipments>
    <Manifest ExternalManifestNo="" IsHazmat="" IsParcel=""
        ManifestClosedFlag="" ManifestDate="" ManifestKey=""
        ManifestNo="" ManifestStatus="" NumberOfContainersManifested=""
        NumberOfLoadsManifestedCompletely=""
        NumberOfShipmentsManifestedCompletely="" PrintCount="" Scac=""
        ShipNode="" ShipperAccountNo="" TotalNumberOfContainers="" TrailerNo="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Manifest_master.xml" IncludeXMLPath="/Manifest"/>
        <yfc:doc>
            <Element EntityName="Manifest"/>
            <Attributes>
                <Attribute DataType="Flag" Name="IsParcel">
                    <Description>
                        <div>Indicates whether all shipments included in the manifest are parcels.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Count" Name="NumberOfContainersManifested">
                    <Description>
                        <div>Indicates the number of containers that are manifested across shipments and loads.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Count" Name="NumberOfLoadsManifestedCompletely">
                    <Description>
                        <div>Indicates the number of loads that are manifested completely. This attribute is not applicable when manifesting is at container level.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Count" Name="NumberOfShipmentsManifestedCompletely">
                    <Description>
                        <div>Indicates the number of shipments that are manifested completely. This attribute is not applicable when manifesting is at container level.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Count" Name="TotalNumberOfContainers">
                    <Description>
                        <div>Indicates the total number of containers across shipments and loads. This attribute is not applicable when manifesting is at container level.
					</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Manifest>
    <LoadStops>
        <LoadStop ActualArrivalDate="" ActualDepartureDate=""
            DockLocationId="" ExpectedArrivalDate=""
            ExpectedDepartureDate="" LoadKey="" LoadStopKey=""
            StopAddressKey="" StopNode="" StopReached=""
            StopSequenceNo="" StopType="" TotalVolumeIn=""
            TotalVolumeOut="" TotalWeightIn="" TotalWeightOut=""
            VolumeUOM="" WeightUOM="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Load_Stop_master.xml" IncludeXMLPath="/LoadStop"/>
            <yfc:doc>
                <Element EntityName="Load_Stop"/>
                <Attributes>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>Indicates if data is fetched from the regular table or the history table. Valid values are Y and N.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <StopAddress AddressLine1="" AddressLine2="" AddressLine3=""
                AddressLine4="" AddressLine5="" AddressLine6=""
                AlternateEmailID="" Beeper="" City="" Company=""
                Country="" DayFaxNo="" DayPhone="" Department=""
                EMailID="" ErrorTxt="" EveningFaxNo="" EveningPhone=""
                FirstName="" HttpUrl="" IsCommercialAddress=""
                JobTitle="" LastName="" MiddleName="" MobilePhone=""
                OtherPhone="" PersonID="" PersonInfoKey=""
                PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
                Title="" UseCount="" VerificationStatus="" ZipCode="">
                <yfc:includeInParent
                    IncludeXMLFileName="YFS_Person_Info_master.xml" IncludeXMLPath="/Address"/>
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                </yfc:doc>
            </StopAddress>
        </LoadStop>
    </LoadStops>
    <LoadCharges>
        <LoadCharge ActualCharge="" ChargeCategory="" ChargeName=""
            ChargeNameKey="" EstimatedCharge="" InvoiceNo=""
            KeyReference1="" KeyReference2="" LoadKey=""
            LoadShipmentChargeKey="" PayeeOrganizationCode=""
            ShipmentKey="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Load_Shipment_Charge_master.xml" IncludeXMLPath="/LoadShipmentCharge"/>
            <yfc:doc>
                <Element EntityName="Load_Shipment_Charge"/>
                <Attributes>
                    <Attribute DataType="Key" EntityName="Charge_Name"
                        Name="ChargeNameKey" NameInEntity="Charge_Name_Key">
                        <Description>Primary key for a charge name.</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>Indicates if data is fetched from the regular table or the history table. Valid values are Y and N.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </LoadCharge>
    </LoadCharges>
    <Instructions>
        <Instruction InstructionDetailKey="" InstructionText=""
            InstructionType="" InstructionURL="" InstructionUsage=""
            ReferenceKey="" SequenceNo="" TableName="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Instruction_Detail_master.xml" IncludeXMLPath="/Instruction"/>
            <yfc:doc>
                <Element EntityName="Instruction_Detail"/>
            </yfc:doc>
        </Instruction>
    </Instructions>
    <AdditionalDates>
        <AdditionalDate ActualDate="" AdditionalDateKey="" DateTypeId=""
            ExpectedDate="" FromActualDate="" FromExpectedDate=""
            FromRequestedDate="" ReferenceKey="" ReferenceType=""
            RequestedDate="" ToActualDate="" ToExpectedDate=""
            ToRequestedDate="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Additional_Date_master.xml" IncludeXMLPath="/AdditionalDate"/>
            <yfc:doc>
                <Element EntityName="Additional_Date"/>
                <Attributes>
                    <Attribute DataType="Date" Name="FromActualDate">
                        <Description>From Actual date criteria for shipment list.
               </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="FromExpectedDate">
                        <Description>From Expected date criteria for shipment list.
               </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="FromRequestedDate">
                        <Description>From Requested date criteria for shipment list.
               </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="ToActualDate">
                        <Description>To Actual date criteria for shipment list.
               </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="ToExpectedDate">
                        <Description>To Expected date criteria for shipment list.
               </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="ToRequestedDate">
                        <Description>To Requested date criteria for shipment list.
               </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </AdditionalDate>
    </AdditionalDates>
</Load>
