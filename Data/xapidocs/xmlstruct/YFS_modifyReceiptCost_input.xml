<Receipt Currency="" DateReceived="" InventoryOrganizationCode=""
    ItemID="Required" ProductClass="" ShipNode="Required"
    SupplyReference="" SupplyReferenceType="" UnitCost=""
    UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/modifyReceiptCost/input">
    <yfc:doc>
        <Element EntityName="Inventory_Receipt"/>
        <Attributes>
            <Attribute EntityName="Inventory_Receipt"
                Name="DateReceived" NameInEntity="Date_Received">
                <Description>
                    <div>
					If passed, supplies received only on this date will be considered.
						<Validation>
                            <Condition> If <code>DateReceived</code> is not a valid date:</Condition>
                            <Error Description="Invalid date format" ErrorCode="YFC0004"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="InventoryOrganizationCode"/>
            <Attribute EntityName="Inventory_Item" Name="ItemID">
                <Description>
                    <div>
                        <Validation>
                            <Condition> If <code>ItemID</code> is not passed:</Condition>
                            <Error Description="No record found" ErrorCode="YFS10001"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="ProductClass">
                <Description>
                    <div>
                        <Validation>
                            <Condition> If <code>ProductClass</code> is not passed:</Condition>
                            <Error Description="No record found" ErrorCode="YFS10001"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Receipt" Name="ShipNode" NameInEntity="Shipnode_Key">
                <Description>
                    <div>
					If passed, all open adjustments for this ship node are considered. Supplies received only on this ship node are considered. Inventory rules are read for the ship node's owner organization's inventory consolidator.
						<Validation>
                            <Condition> If <code>ShipNode</code> is not passed:</Condition>
                            <Error Description="Invalid shipnode" ErrorCode="YFS10048"/>
                        </Validation>
                        <Validation>
                            <Condition> If <code>ShipNode</code> is not a valid ship node:</Condition>
                            <Error Description="Invalid shipnode" ErrorCode="YFS10048"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Receipt"
                Name="SupplyReference" NameInEntity="Supply_Reference">
                <Description>
                    <div>
					If passed, supplies received with this <code>SupplyReference</code> are considered.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Receipt"
                Name="SupplyReferenceType" NameInEntity="Supply_Reference_Type">
                <Description>
                    <div>
					If passed, supplies received with this <code>SupplyReferenceType</code> are considered.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure">
                <Description>
                    <div>
                        <Validation>
                            <Condition> If <code>UnitOfMeasure</code> is not passed:</Condition>
                            <Error Description="No record found" ErrorCode="YFS10001"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Receipt>
