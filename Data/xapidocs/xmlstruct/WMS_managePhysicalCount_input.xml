<?xml version="1.0" encoding="UTF-8"?>
<!-- Action,Duration,DurationUOM,Node,PlanName,StartDate are mandatory-->
<PhysicalCountPlan Action="Required" Duration="Required"
    DurationUOM="Required" Enterprise="" Node="Required"
    PlanName="Required" StartDate="Required" xmlns="http://www.sterlingcommerce.com/documentation/WMS/managePhysicalCount/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="Action">
                <Description>Specifies the Action to be performed.Valid Actions are 'CREATE' , 'MODIFY', 'CANCEL' and 'DELETE'.</Description>
            </Attribute>
            <Attribute DataType="String" Name="Enterprise"/>
        </Attributes>
    </yfc:doc>
    <PhysicalCountPlanActivities>
        <PhysicalCountPlanActivity Duration="" DurationUOM="" PCActivityCategoryName="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="Duration"/>
                    <Attribute DataType="String" Name="DurationUOM"/>
                    <Attribute DataType="String" Name="PCActivityCategoryName"/>
                </Attributes>
            </yfc:doc>
        </PhysicalCountPlanActivity>
    </PhysicalCountPlanActivities>
</PhysicalCountPlan>
