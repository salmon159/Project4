<!--getReturnMethod Output XML-->
<Order OrderHeaderKey="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/getReturnMethod/output">
    <OrderLines>
        <OrderLine IsCustomerKeepAllowed="" IsReceiptExpected=""
            IsReturnNodeFound="" IsReturnToStoreAllowed=""
            IsSchedulePickupAllowed="" OrderLineKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="IsCustomerKeepAllowed">
                        <Description>This attribute determines whether customer can keep the return line while creating returns or not. Default value returned by the API is Y.</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="IsReceiptExpected">
                        <Description>This attribute determines whether a receipt is expected for this return or not </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="IsReturnNodeFound">
                        <Description>This attribute determines whether a return node is found for this return or not. </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="IsReturnToStoreAllowed">
                        <Description>This attribute determines whether return to store is allowed or not. </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="IsSchedulePickupAllowed">
                        <Description>This attribute determines whether schedule pickup is allowed or not. </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ShipNode ShipNode="" ShipNodeKey="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="ShipNodeKey"/>
                    </Attributes>
                </yfc:doc>
                <ShipNodePersonInfo AddressLine1="" AddressLine2=""
                    AddressLine3="" AddressLine4="" AddressLine5=""
                    AddressLine6="" AlternateEmailID="" Beeper=""
                    City="" Company="" Country="" DayFaxNo=""
                    DayPhone="" Department="" EMailID="" EveningFaxNo=""
                    EveningPhone="" FirstName="" JobTitle="" LastName=""
                    MiddleName="" MobilePhone="" OtherPhone=""
                    PersonID="" PersonInfoKey="" State="" Suffix=""
                    Title="" ZipCode=""/>
            </ShipNode>
        </OrderLine>
    </OrderLines>
    <HeaderReturnMethod DeliveryMethod="">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
						The value of the DeliveryMethod attribute returned under the HeaderReturnMethod element specifies the return method that is common to all lines on the order.  
						If the delivery method is set to SHP, then the ShipNode element needs to be present under the HeaderReturnMethod element.
					</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-40" Name="DeliveryMethod">
                    <Description>
                        <div>
							The delivery method for the product line. Valid values are: <code>SHP</code>, <code>DEL</code> and <code>PICK</code>.
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ShipNode ShipNode="" ShipNodeKey="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="ShipNodeKey"/>
                </Attributes>
            </yfc:doc>
            <ShipNodePersonInfo AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" EveningFaxNo="" EveningPhone=""
                FirstName="" JobTitle="" LastName="" MiddleName=""
                MobilePhone="" OtherPhone="" PersonID=""
                PersonInfoKey="" State="" Suffix="" Title="" ZipCode=""/>
        </ShipNode>
    </HeaderReturnMethod>
</Order>
