<UndoPick xmlns="http://www.sterlingcommerce.com/documentation/YCD/undoBackRoomPick/input">
    <Shipment SellerOrganizationCode="" ShipNode="" ShipmentKey=""
        ShipmentNo="" TransactionId="Required">
        <yfc:doc>
            <Element EntityName="Shipment">
                <Description>
                    <div> The shipment to undo pick. Pass  <code>ShipmentKey</code>
                        <i>or</i> pass <code>ShipmentNo</code>, <code>ShipNode</code> and <code>SellerOrganizationCode</code>  to identify the shipment.
				</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid">
                    <Description>
                        <div>Identifies a transaction. The transaction identified must be derived from
					 the abstract transaction <code>CHANGE_SHIPMENT_STATUS</code>.
					 <Validation>
                                <Condition>Valid <code>TransactionId</code> must be passed.
								</Condition>
                                <Error
                                    Description="Invalid Transaction Id" ErrorCode="YCD00016"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ShipmentLines>
            <ShipmentLine ShipmentLineKey="" ShipmentLineNo="" ShipmentSubLineNo="">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div> The shipment line to undo pick. Pass  <code>ShipmentLineKey</code>
                                <i>or</i>
                                <code>ShipmentLineNo</code>and <code>ShipmentSubLineNo</code>  to identify a shipment line uniquely.
					 <Validation>
                                    <Condition>At least one <code>ShipmentLine</code> must be passed.
								</Condition>
                                    <Error
                                    Description="Invalid Input. At least one shipment line should be provided in the input" ErrorCode="YCD00019"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                </yfc:doc>
            </ShipmentLine>
        </ShipmentLines>
        <ShipmentStatusAudit ReasonCode="" ReasonText="" StatusDate="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="StatusDate"/>
                </Attributes>
            </yfc:doc>
        </ShipmentStatusAudit>
    </Shipment>
</UndoPick>
