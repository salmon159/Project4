<AttributeGroup AttributeDomainID="" AttributeGroupID=""
    AttributeGroupKey="" AttributeGroupPath=""
    CallingOrganizationCode="" DisplayLocalizedFieldInLocale=""
    LongDescription="" OrganizationCode="" ParentAttributeGroupKey=""
    Purpose="" SequenceNo="" ShortDescription="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getAttributeGroupList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="CallingOrganizationCode">
                <Description>
                    <div>
					If <code>CallingOrganizationCode</code> is passed, the <code>AttributeGroup</code> list for the organization that has configured <code>AttributeGroup</code>s for this <code>CallingOrganizationCode</code> is returned.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Locale" Name="DisplayLocalizedFieldInLocale">
                <Description>
                    <div>
						The locale for which output should be returned in. Locale should be passed in the standard format of "language_country_variant" (e.g., "fr_FR", "en_US_CA", etc.).
					</div>
                </Description>
            </Attribute>
            <Attribute Name="OrganizationCode">
                <Description>
                    <div>
					The organization that has defined the <code>AttributeGroup</code>s.
					If <code>CallingOrganizationCode</code> is not passed in the input, <code>OrganizationCode</code> is obtained from the logged in user. If <code>OrganizationCode</code> cannot be obtained from the logged in user, then this <code>OrganizationCode</code> passed in the input is used to fetch the <code>AttributeGroup</code> list.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Attribute_Domain" Name="Purpose" NameInEntity="Purpose">
                <Description>
                    <div>
					The <code>Purpose</code> of the <code>AttributeDomain</code> to which this <code>AttributeGroup</code> belongs. If not passed in the input, a default value of 'ITEM' is used to fetch the <code>AttributeGroup</code> list.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
