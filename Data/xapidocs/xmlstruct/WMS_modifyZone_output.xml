<?xml version="1.0" encoding="UTF-8"?>
<Zone AttemptSKUConsolidation="" BuildingNumber="" CheckDigit=""
    ConfirmLocnEmptyAfterPick="" Createprogid="" Createts=""
    Createuserid="" Description="" ForceLocnScanOnVisit=""
    IsActiveZone="" LocationsPerItem="" Lockid="" MixCountryOfOrigin=""
    MixEnterprise="" MixFifo="" MixInventoryStatus="" MixInventoryTag=""
    MixProductClass="" MixReceiptNo="" MixSKU="" MixSegment=""
    MixShipByDate="" Modifyprogid="" Modifyts="" Modifyuserid="" Node=""
    PrimaryBulkZoneId="" PutOnVarianceHold="" TrackCaseLPN=""
    TrackInventory="" TrackPalletLPN="" ZoneId="" ZoneKey="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/modifyZone/output">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="YFS_Zone_master.xml" IncludeXMLPath="/Zone"/>
    <SKUDedications>
        <SKUDedication Createprogid="" Createts="" Createuserid=""
            EnterpriseCode="" ItemId="" LocationId="" Lockid=""
            MaxQty="" MaxQuantity="" MinQty="" MinQuantity=""
            Modifyprogid="" Modifyts="" Modifyuserid="" ProductClass=""
            SKUDedicationKey="" Segment="" SegmentType=""
            UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Zone_master.xml" IncludeXMLPath="/Zone/ZoneLocations/ZoneLocation/SKUDedications/SKUDedication"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Quantity" Name="MaxQuantity">
                        <Description> Maximum quantity of SKU that should be stored in the location. This attribute is used by put away location selection algorithm to ensure that no more than the maximum quantity for that item at that location is put away, even if location capacity constraints allow for more quantity than the maximum quantity. It is computed as minimum of LocationSize and LocationCapacity.</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="MinQuantity">
                        <Description>Minimum quantity of SKU for replenishing. It is computed as   <Code> MaxQuantity * (TriggerPercent/100)</Code>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </SKUDedication>
    </SKUDedications>
    <InventoryStatusTransitions>
        <InventoryStatusTransition Createprogid="" Createts=""
            Createuserid="" InventoryStatus=""
            InventoryStatusTransitionKey="" Lockid="" Modifyprogid=""
            Modifyts="" Modifyuserid="" NewInventoryStatus="" ZoneId=""/>
    </InventoryStatusTransitions>
    <ZoneEnterprises>
        <ZoneEnterprise Createprogid="" Createts="" Createuserid=""
            EnterpriseCode="" Lockid="" Modifyprogid="" Modifyts=""
            Modifyuserid="" ZoneEnterpriseKey="" ZoneKey="">
            <StorageConstraints>
                <StorageConstraint Createprogid="" Createts=""
                    Createuserid="" ItemClassification1=""
                    ItemClassification2="" ItemClassification3=""
                    Lockid="" Modifyprogid="" Modifyts=""
                    Modifyuserid="" StorageConstraintKey="" ZoneEnterpriseKey=""/>
            </StorageConstraints>
        </ZoneEnterprise>
    </ZoneEnterprises>
    <ZoneLocations>
        <ZoneLocation AisleNumber="" AisleSuffix="" BayNumber=""
            Createprogid="" Createts="" Createuserid="" FreezeMoveIn=""
            FreezeMoveOut="" InStagingLocationId="" LevelNumber=""
            LocationBarCode="" LocationId="" LocationKey=""
            LocationSizeCode="" LocationType="" Lockid=""
            Modifyprogid="" Modifyts="" Modifyuserid="" MoveInSeqNo=""
            MoveOutSeqNo="" OutStagingLocationId="" StorageCode=""
            VelocityCode="" ZoneId="">
            <yfc:doc>
                <Element EntityName="Location"/>
            </yfc:doc>
            <LocationSize Createprogid="" Createts="" Createuserid=""
                Description="" DimensionUom="" Height=""
                IsInfiniteCapacity="" Length="" LocationSizeCode=""
                LocationSizeKey="" Lockid="" MaxWeight=""
                Modifyprogid="" Modifyts="" Modifyuserid=""
                TotalNumberOfRecords="" Volume="" VolumeUom=""
                WeightUom="" Width="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="UOM" Name="DimensionUom">
                            <Description>Unit of measure for Dimension. This unit of measure applies to Height, Length and Width attributes.</Description>
                        </Attribute>
                        <Attribute DataType="Count" Name="TotalNumberOfRecords">
                            <Description>Indicates total number of records fetched.</Description>
                        </Attribute>
                        <Attribute DataType="Volume" Name="Volume">
                            <Description>Volume that a location can hold. This field is computed using the Height, Length and Width attributes as defined above.</Description>
                        </Attribute>
                        <Attribute DataType="UOM" Name="VolumeUom">
                            <Description>Unit of measure for Volume.</Description>
                        </Attribute>
                        <Attribute DataType="UOM" Name="WeightUom">
                            <Description>Unit of measure for Weight.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <LocationCapacities>
                    <LocationCapacity Createprogid="" Createts=""
                        Createuserid="" EnterpriseCode=""
                        ItemClassificationCode="" LocationCapacityKey=""
                        LocationSizeCode="" Lockid="" Modifyprogid=""
                        Modifyts="" Modifyuserid="" NoOfPackages="" PackagingType=""/>
                </LocationCapacities>
            </LocationSize>
            <SKUDedications>
                <SKUDedication Createprogid="" Createts=""
                    Createuserid="" EnterpriseCode="" ItemId=""
                    LocationId="" Lockid="" MaxQty="" MaxQuantity=""
                    MinQty="" MinQuantity="" Modifyprogid="" Modifyts=""
                    Modifyuserid="" ProductClass="" SKUDedicationKey=""
                    Segment="" SegmentType="" UnitOfMeasure="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="MaxQuantity"/>
                            <Attribute DataType="Quantity" Name="MinQuantity"/>
                        </Attributes>
                    </yfc:doc>
                </SKUDedication>
            </SKUDedications>
        </ZoneLocation>
    </ZoneLocations>
</Zone>
