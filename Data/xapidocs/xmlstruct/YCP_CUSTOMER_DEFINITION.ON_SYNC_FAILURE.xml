                        <Attributes>
                            <Attribute DataType="Action" Name="Operation">
                                <Description>
                                    <div>
					 Valid value is <b>
                                    <code>Delete</code>
                                    </b> or blank.  
					</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </CustomerPaymentMethod>
            </CustomerPaymentMethodList>
        </CustomerContact>
    </CustomerContactList>
