<Customer BuyerOrganizationCode="" CustomerType="" Operation=""
    OrganizationCode="" RemoteModifyTS="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/CUSTOMER_DEFINITION/ON_SYNC_FAILURE">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Customer" Name="CustomerType">
                <Description>
                    <div>
				Type of customer.  Valid values are: 01-Business, 02-Consumer. 
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="Operation"/>
            <Attribute Name="RemoteModifyTS">
                <Description>
                    <div>
				The modification date from the external system. 
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <CustomerContactList>
        <CustomerContact DayFaxNo="" DayPhone="" EmailID=""
            EveningFaxNo="" EveningPhone="" FirstName="" LastName=""
            MobilePhone="" Title="" UserID="Required" yfc:DTDOccurrence="ZeroOrMany">
            <CustomerAdditionalAddressList Reset="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Reset">
                            <Description>Indicates if all existing child node definition in the database need to be deleted and replace with the provided child nodes.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <CustomerAdditionalAddress
                    CustomerAdditionalAddressID="" IsBillTo=""
                    IsDefaultBillTo="" IsDefaultShipTo=""
                    IsDefaultSoldTo="" IsShipTo="" IsSoldTo=""
                    Operation="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>Defines an additional address for a customer.
						</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Action" Name="Operation">
                                <Description>
                                    <div>
					 Valid value is <b>
                                    <code>Delete</code>
                                    </b> or blank. 
					</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <PersonInfo AddressLine1="" AddressLine2=""
