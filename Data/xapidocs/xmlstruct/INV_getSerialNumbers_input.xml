<!--getSerialNumbers Input XML-->
<Serial InventoryItemKey="" ItemID="Required" LocationId="Required"
    OrganizationCode="Required" ProductClass="Required"
    ShipNodeKey="Required" UnitOfMeasure="Required" xmlns="http://www.sterlingcommerce.com/documentation/INV/getSerialNumbers/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="ItemID" NameInEntity="ItemID">
                <Description>
                    <div>Specifies the item to which serial belongs</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="OrganizationCode" NameInEntity="InventoryOrganizationCode">
                <Description>
                    <div>Specifies the organization to which the item belongs</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ProductClass" NameInEntity="ProductClass">
                <Description>
                    <div>Specifies the product class of the item</div>
                </Description>
            </Attribute>
            <Attribute Name="ShipNodeKey" NameInEntity="ShipNode">
                <Description>
                    <div>Specifies the ship node key to which serial belongs</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="UnitOfMeasure" NameInEntity="UnitOfMeasure">
                <Description>
                    <div>Specifies the unit of measure of the item</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SerialElem FromSerialNo="Required" ToSerialNo="Required">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="FromSerialNo">
                    <Description>
                        <div>
				This is the starting serial number of a range.
				</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ToSerialNo">
                    <Description>
                        <div>
				This is the ending serial number of a range. 
				</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </SerialElem>
</Serial>
