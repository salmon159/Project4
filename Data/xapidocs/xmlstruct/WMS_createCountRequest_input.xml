<?xml version="1.0" encoding="UTF-8"?>
<CreateCountRequest AisleNumber="" BayNumber="" CaseId=""
    CountProgramName="" CountRequestKey="" CountRequestNo=""
    DocumentType="" EnterpriseCode="" FinishNoLaterThan=""
    FromLocation="" ItemClassification1="" ItemClassification2=""
    ItemClassification3="" ItemID="" LevelNumber="" LocationId=""
    Node="Required" PalletId="" Priority="" ProductClass=""
    ReceiptHeaderKey="" RequestType="" RequestingUserId=""
    StartNoEarlierThan="" ToLocation="" UnitOfMeasure="" ZoneId="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/createCountRequest/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="YFS_Count_Request_tran.xml"
        IncludeXMLPath="/CountRequest" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Count_Request"/>
        <Attributes>
            <Attribute EntityName="Count_Request" Name="AisleNumber" NameInEntity="Aisle_Number">
                <Description>
                    <div>
						This is the Aisle Number of the location that the user wants to count.
							<Validation>
                            <Condition>If passed <code>AisleNumber</code> should be valid. 
								</Condition>
                            <Error
                                Description="Location does not exist." ErrorCode="WMS00005"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count_Request" Name="BayNumber" NameInEntity="Bay_Number">
                <Description>
                    <div>
						This is the Bay Number of the location that the user wants to count.
							<Validation>
                            <Condition>If passed <code>BayNumber</code> should be valid. 
								</Condition>
                            <Error
                                Description="Location does not exist." ErrorCode="WMS00005"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>
                    <div>A CaseId
						<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                        <Validation>
                            <Condition>Along with <code>CaseId,</code>
                                <code>EnterpriseCode</code> should also be passed. Error if <code>EnterpriseCode</code> not passed.
							</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                        <Validation>
                            <Condition>The <code>CaseId</code> should exist and belong to the given <code>EnterpriseCode</code>. 
							</Condition>
                            <Error
                                Description="WMS:CaseId/PalletId does not exist." ErrorCode="WMS00004"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Count_Request"
                Name="CountRequestKey" NameInEntity="Count_Request_Key">
                <Description>
                    <div>
						Primary key attribute of count request entity. Generated by system if not passed by the user.
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" EntityName="Count_Request"
                Name="CountRequestNo" NameInEntity="Count_Request_No">
                <Description>
                    <div>
						Identifier of the count request within the context of document type and node. If not passed, it is generated by the system. If passed it should not be duplicate of already available CountReqestNo. This is to avoid unique key violation.
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-24" EntityName="Count_Request"
                Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>
						Indicates the purpose of the count request. If not passed, defaulted to count.
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" EntityName="Count_Request"
                Name="EnterpriseCode" NameInEntity="Enterprise_Code">
                <Description>
                    <div>
						Indicates enterprise for which the count request is being created.
							<Validation>
                            <Condition>If <code>EnterpriseCode</code> is not a valid enterprise.
								</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>EnterpriseCode</code> is passed, the <code>Node</code> and <code>EnterpriseCode</code> combination must exist.
								</Condition>
                            <Error
                                Description="Invalid Node and EnterpriseCode combination." ErrorCode="WMS00040"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" EntityName="Count_Request"
                Name="FinishNoLaterThan" NameInEntity="Finish_No_Later_Than">
                <Description>
                    <div>
						Indicates <code>FinishNoLaterThan</code> time stamp of the count request. It is a pass through field to be stamped on each count task generated for the count request. It indicates that the task has to be completed before this date. If not passed, it is defaulted to High date (25000101T00:00:00).
							<Validation>
                            <Condition>If passed, <code>FinishNoLaterThan</code> should not be before the System date. Error if passed before System date.
								</Condition>
                            <Error
                                Description="Count cannot be requested before the system date." ErrorCode="WMS00112"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>StartNoEarlierThan</code> should not be after <code>FinishNoLaterThan</code>. Validation if <code>StartNoEarlierThan</code> after <code>FinishNoLaterThan</code>.
								</Condition>
                            <Error
                                Description="Count request finish date cannot be earlier than start date." ErrorCode="WMS00113"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count_Request" Name="FromLocation" NameInEntity="From_Location">
                <Description>
                    <div>
						This is the starting location of the range of locations that the user wants to count.
							<Validation>
                            <Condition>If passed <code>LocationId</code> should be valid. 
								</Condition>
                            <Error
                                Description="Location does not exist." ErrorCode="WMS00005"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="CommonCode" EntityName="Count_Request"
                Name="ItemClassification1" NameInEntity="Item_Classification1">
                <Description>
                    <div>
						The first item classification for the count request.
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="CommonCode" EntityName="Count_Request"
                Name="ItemClassification2" NameInEntity="Item_Classification2">
                <Description>
                    <div>
						The second item classification for the count request.
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="CommonCode" EntityName="Count_Request"
                Name="ItemClassification3" NameInEntity="Item_Classification3">
                <Description>
                    <div>
						The third item classification for the count request.
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="ItemID" EntityName="Count_Request"
                Name="ItemID" NameInEntity="Item_ID">
                <Description>
                    <div>
						ItemID along with EnterpriseCode determines the SKU for which the count request is being created.
							<Validation>
                            <Condition>Along with <code>ItemID</code>, <code>EnterpriseCode</code> should also be passed.
								</Condition>
                            <Error
                                Description="Item details are not passed in input." ErrorCode="WMS00020"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count_Request" Name="LevelNumber" NameInEntity="Level_Number">
                <Description>
                    <div>
						This is the Level Number of the location that the user wants to count.
							<Validation>
                            <Condition>If passed <code>LevelNumber</code> should be valid. 
								</Condition>
                            <Error
                                Description="Location does not exist." ErrorCode="WMS00005"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LocationId" EntityName="Count_Request"
                Name="LocationId" NameInEntity="Location_Id">
                <Description>
                    <div>
						Indicates the location for which the count request is being created.
							<Validation>
                            <Condition>If passed <code>LocationId</code> should be valid. 
								</Condition>
                            <Error
                                Description="Location does not exist." ErrorCode="WMS00005"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Count_Request"
                Name="Node" NameInEntity="Node_Key">
                <Description>
                    <div>
						Indicates the node or warehouse where the count request was created.
							<Validation>
                            <Condition>
                                <code>Node</code> not passed.
								</Condition>
                            <Error Description="Node not passed." ErrorCode="WMS00054"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Node</code> passed but does not exist.
								</Condition>
                            <Error
                                Description="Node passed does not exist." ErrorCode="WMS00042"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>
                    <div>A PalletId 
						<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                        <Validation>
                            <Condition>Along with <code>PalletId</code>, <code>EnterpriseCode</code> should also be passed. Error if <code>EnterpriseCode</code> not passed.
							</Condition>
                            <Error
                                Description="Mandatory Parameter(s) not passed" ErrorCode="WMS00063"/>
                        </Validation>
                        <Validation>
                            <Condition>The <code>PalletId</code> should exist and belong to the given <code>EnterpriseCode</code>. 
							</Condition>
                            <Error
                                Description="WMS:CaseId/PalletId does not exist." ErrorCode="WMS00004"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Priority" EntityName="Count_Request"
                Name="Priority" NameInEntity="Priority">
                <Description>
                    <div>
						Determines the urgency of completing a count request. 
						System defined Common Code with CodeType <code>PRIORITY</code>.
						If priority is not passed, then it is defaulted to value <code>
                            <b>3</b>
                        </code> i.e. <code>Normal</code>
                        <br/>Valid values - <code>
                            <b>1</b>
                        </code>, <code>
                            <b>2</b>
                        </code>, <code>
                            <b>3</b>
                        </code>, <code>
                            <b>4</b>
                        </code>. These values correspond to <code>Urgent</code>, <code>High</code>, <code>Normal</code> and <code>Low</code> values respectively.
							<Validation>
                            <Condition>If passed, checks for valid <code>Priority</code>.
								</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ProductClass"
                EntityName="Count_Request" Name="ProductClass" NameInEntity="Product_Class">
                <Description>
                    <div>
						ProductClass is always associated to an Item.
							<Validation>
                            <Condition>Along with <code>ProductClass</code>, <code>ItemID and EnterpriseCode</code> should also be passed.
								</Condition>
                            <Error
                                Description="Item details are not passed in input." ErrorCode="WMS00020"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Count_Request"
                Name="ReceiptHeaderKey" NameInEntity="Receipt_Header_Key">
                <Description>
                    <div>
						Indicates the receipt for which the count request is being created.
							<Validation>
                            <Condition>If passed, checks for valid <code>ReceiptHeaderKey</code>.
								</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="RequestType" EntityName="Count_Request"
                Name="RequestType" NameInEntity="Request_Type">
                <Description>
                    <div>
                        <Validation>
                            <Condition>
                                <code>RequestType</code> passed but not valid.
								</Condition>
                            <Error Description="Invalid Request Type." ErrorCode="WMS00226"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="UserId" EntityName="Count_Request"
                Name="RequestingUserId" NameInEntity="Requesting_User_Id">
                <Description>
                    <div>
						User that makes a request for performing a count operation. Such a user may exist outside of the warehouse management system.
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" EntityName="Count_Request"
                Name="StartNoEarlierThan" NameInEntity="Start_No_Earlier_Than">
                <Description>
                    <div>
						Indicates <code>StartNoEarlierThan</code> time stamp of the count request. It is a pass through field to be stamped on each count task generated for the count request. It indicates that the task is not to be suggested to the user for execution before this date. If not passed, it is defaulted to the System date.
							<Validation>
                            <Condition>If passed, <code>StartNoEarlierThan</code> should not be before the System date. Error if passed before System date.
								</Condition>
                            <Error
                                Description="Count cannot be requested before the system date." ErrorCode="WMS00112"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count_Request" Name="ToLocation" NameInEntity="To_Location">
                <Description>
                    <div>
						This is the end location of the range of locations that the user wants to count.
							<Validation>
                            <Condition>If passed <code>LocationId</code> should be valid. 
								</Condition>
                            <Error
                                Description="Location does not exist." ErrorCode="WMS00005"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="UOM" EntityName="Count_Request"
                Name="UnitOfMeasure" NameInEntity="Uom">
                <Description>
                    <div>
						UnitOfMeasure is always associated to an Item.
							<Validation>
                            <Condition>Along with <code>UnitOfMeasure</code>, <code>ItemID and EnterpriseCode</code> should also be passed.
								</Condition>
                            <Error
                                Description="Item details are not passed in input." ErrorCode="WMS00020"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ZoneId" EntityName="Count_Request"
                Name="ZoneId" NameInEntity="Zone_Id">
                <Description>
                    <div>
						Indicates the zone for which the count request is being created.
							<Validation>
                            <Condition>If passed <code>ZoneId</code> should be valid.
								</Condition>
                            <Error
                                Description="Invalid Node and ZoneId combination." ErrorCode="WMS00035"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Receipt ReceiptNo="">
        <yfc:doc>
            <Element EntityName="Receipt_Header"/>
            <Attributes>
                <Attribute EntityName="Receipt_Header" Name="ReceiptNo" NameInEntity="Receipt_No">
                    <Description>
                        <div>Indicates the receipt to which inventory belongs.
								<Validation>
                                <Condition>If passed, should be a valid receipt number.
									</Condition>
                                <Error
                                    Description="No record found  for passed inputs." ErrorCode="WMS00002"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Receipt>
</CreateCountRequest>
