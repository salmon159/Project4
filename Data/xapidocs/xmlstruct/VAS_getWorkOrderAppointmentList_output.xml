<WorkOrderAppointments TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/VAS/getWorkOrderAppointmentList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>Number of Records fetched from the DB satisfying the passed criteria.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <WorkOrderAppointment AdditionalEquipmentUsed="" AlertSales=""
        AlertScheduler="" AllocatedQuantity="" ApptSeq="" ApptStatus=""
        AttentionRequired="" ConsiderCurrentSlot="" ExecutionBy=""
        ExecutionComments="" ExecutionDuration="" ExecutionEndTime=""
        ExecutionStartTime="" ExpectedApptEndDate=""
        ExpectedApptStartDate="" IsRoutePlanningComplete=""
        ItemsNotUsed="" NonInventoryItemsReturned=""
        PromisedApptEndDate="" PromisedApptStartDate=""
        RequestedQuantity="" RequestedVolume="" RequestedWeight=""
        ResourcePoolId="" ServiceResourceId="" StopNumber=""
        VehicleNumber="" VehicleOdometerReading=""
        VehicleServiceRequest="" WorkOrderApptKey="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YFS_Work_Order_Appointment_master.xml" IncludeXMLPath="/WorkOrderAppointment"/>
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="ConsiderCurrentSlot">
                    <Description>
                        <div>
					The <code>ConsiderCurrentSlot</code> flag indicates that system should consider the available capacity of the current slots, which the current timestamp falls under.
					<br/>Default: <b>
                                <code>N</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Quantity" Name="RequestedVolume">
                    <Description>
                        <div>The quantity requested in the work order's volume unit of measure, which is specified in the <code>VolumeUOM</code> field of the work order</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Quantity" Name="RequestedWeight">
                    <Description>
                        <div>The quantity requested in the work order's weight unit of measure, which is specified in the <code>WeightUOM</code> field of the work order</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <WorkOrder AllocatedLevel="" CaseId="" DocumentType=""
            EnterpriseCode="" FinishNoLaterThan="" HoldFlag=""
            InvUpdateActivityCode="" ItemID="" LevelOfService=""
            NextAlertTs="" NodeKey="" OpenWorkOrderFlag=""
            OrderHeaderKey="" OrderLineKey="" OrderNo="" PalletId=""
            PipeLineKey="" PreCallStatus="" Priority="" ProductClass=""
            PromisedApptEndDate="" PromisedApptStartDate="" Purpose=""
            QuantityAllocated="" QuantityCompleted=""
            QuantityReleased="" QuantityRemoved="" QuantityRequested=""
            ReasonCode="" ReasonText="" ReleasedLevel="" Segment=""
            SegmentType="" SerialNo="" ServiceComplexityLevel=""
            ServiceItemGroupCode="" ServiceItemID=""
            ServiceSupervisorId="" ServiceUom="" ShipByDate=""
            ShipmentKey="" ShipmentNo="" StartNoEarlierThan="" Status=""
            StatusDate="" Uom="" WorkOrderKey="" WorkOrderNo="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Work_Order_tran.xml" IncludeXMLPath="/WorkOrder"/>
            <yfc:doc>
                <Element EntityName="Work_Order"/>
                <Attributes>
                    <Attribute DataType="String" Name="AllocatedLevel">
                        <Description>
                            <div>Indicates the extent of allocation for the work order. The valid values are FULL, PARTIAL, NONE, and ANY. The respective values tells wether the work order is fully, partially, not allocated or is in any of the the previously mentioned states.
	        		</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="CaseId">
                        <Description>
                            <div>Case for which the work order is processed.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="PalletId">
                        <Description>
                            <div>Pallet for which the work order is processed.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ReleasedLevel">
                        <Description>
                            <div>Indicates the extent to which the work order is released. The valid values are FULL, PARTIAL, NONE, and ANY. The respective values tells wether the work order is fully, partially, not relased or is in any of the previously mentioned states.
	            	</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <WorkOrderServiceLines>
                <WorkOrderServiceLine CauseAppointmentChange=""
                    HasChangedByThisTransaction="" IsComplete=""
                    ItemDescription="" ItemGroupCode="" ItemID=""
                    ItemShortDescription="" LineNo=""
                    QuantityRequired="" Uom=""
                    WorkOrderServiceLineKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="YFS_Work_Order_Service_Line_master.xml" IncludeXMLPath="/WorkOrderServiceLine"/>
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Flag" Name="HasChangedByThisTransaction">
                                <Description>
                                    <div>Flag indicates if this record has been changed by this transaction </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="ItemDesc" Name="ItemDescription">
                                <Description>
                                    <div>Description of item. </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Text-100" Name="ItemShortDescription">
                                <Description>
                                    <div>Short Description of item. </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </WorkOrderServiceLine>
            </WorkOrderServiceLines>
        </WorkOrder>
        <ResourcePoolCapacityConsumptionDetails Capacity=""
            OverrideCapacityCheck="" ResourcePoolId="" ServiceResourceId="">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Res_Pool"
                        Name="ResourcePoolId" NameInEntity="Resource_Pool_Id"/>
                    <Attribute EntityName="Service_Resource"
                        Name="ServiceResourceId" NameInEntity="Service_Resource_ID"/>
                </Attributes>
            </yfc:doc>
        </ResourcePoolCapacityConsumptionDetails>
        <WorkOrderAppointmentUsers>
            <WorkOrderAppointmentUser OverriddenFlag="" UserId=""
                Username="" WoApptUserKey="" WorkOrderApptKey="" yfc:DTDOccurrence="OneOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="User" Name="Username" NameInEntity="Username"/>
                    </Attributes>
                </yfc:doc>
            </WorkOrderAppointmentUser>
        </WorkOrderAppointmentUsers>
    </WorkOrderAppointment>
</WorkOrderAppointments>
