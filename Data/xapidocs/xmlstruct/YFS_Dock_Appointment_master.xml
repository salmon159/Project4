<DockAppointment AppointmentType="" BolNo="" Carrier=""
    DockAppointmentKey="" LoadKey="" LocationId="" Node="Required"
    Notes="" OrderHeaderKey="" OrganizationCode="" ProNo=""
    ResPoolConsmptnDtlsKey="" ShipmentKey="" TrailerNo="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/Dock_Appointment_master">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="LocationId"/>
        </Attributes>
    </yfc:doc>
    <Order DocumentType="" EnterpriseCode="" OrderNo=""/>
    <Shipment ShipmentNo=""/>
    <Load LoadNo=""/>
    <CapacityConsumptionGet AppointmentNo="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="AppointmentNo"/>
            </Attributes>
        </yfc:doc>
    </CapacityConsumptionGet>
    <CapacityConsumptionManage ConsumptionDate="Required"
        EndTime="Required" StartTime="Required">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="ConsumptionDate"/>
                <Attribute DataType="String" Name="EndTime"/>
                <Attribute DataType="String" Name="StartTime"/>
            </Attributes>
        </yfc:doc>
    </CapacityConsumptionManage>
    <DockAppointmentManage AppointmentType="" BolNo="" Carrier=""
        DockAppointmentKey="" LoadKey="" LocationId="" Node=""
        OrderHeaderKey="" OrganizationCode="" ProNo=""
        ResPoolConsmptnDtlsKey="" ShipmentKey="" TrailerNo="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="AppointmentType"/>
                <Attribute DataType="String" Name="BolNo"/>
                <Attribute DataType="String" Name="Carrier"/>
                <Attribute DataType="String" Name="DockAppointmentKey"/>
                <Attribute DataType="String" Name="LoadKey"/>
                <Attribute DataType="String" Name="LocationId">
                    <Description>
                        <div> Specifies the Dock location for which appointment is taken.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="Node"/>
                <Attribute DataType="String" Name="OrderHeaderKey"/>
                <Attribute DataType="String" Name="OrganizationCode"/>
                <Attribute DataType="String" Name="ProNo"/>
                <Attribute DataType="String" Name="ResPoolConsmptnDtlsKey"/>
                <Attribute DataType="String" Name="ShipmentKey"/>
                <Attribute DataType="String" Name="TrailerNo"/>
            </Attributes>
        </yfc:doc>
        <CapacityReservation ReservationId="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="ReservationId"/>
                </Attributes>
            </yfc:doc>
            <CapacityConsumption ConsumptionDate="" EndTime="" StartTime="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="ConsumptionDate"/>
                        <Attribute DataType="String" Name="EndTime"/>
                        <Attribute DataType="String" Name="StartTime"/>
                    </Attributes>
                </yfc:doc>
            </CapacityConsumption>
        </CapacityReservation>
    </DockAppointmentManage>
    <DockAppointmentList MaxEndTime="" MinStartTime="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="MaxEndTime"/>
                <Attribute DataType="String" Name="MinStartTime"/>
            </Attributes>
        </yfc:doc>
        <Dock DockType="" LocationId="" Node="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="DockType">
                        <Description>
                            <div> Specifies the type of Dock for which appointment is taken.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="LocationId">
                        <Description>
                            <div> Specifies the Dock location for which appointment is taken.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Node">
                        <Description>
                            <div> Specifies the Dock's node for which appointment is taken.
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Dates>
                <Date Date="" EndTime="" StartTime=""
                    TotalAvailableHours="" TotalWorkingHours="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="Date">
                                <Description>
                                    <div> Specifies the date of Dock appointment.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="EndTime">
                                <Description>
                                    <div> End time beyond which appointments may not be taken for the dock.
                        </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="StartTime">
                                <Description>
                                    <div> Start time before which appointments may not be taken for the dock.
                        </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="TotalAvailableHours">
                                <Description>
                                    <div> Specifies the total available hours of Dock.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="TotalWorkingHours">
                                <Description>
                                    <div> Specifies the total working hours of Dock.
						</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <UnavailableSlots NoOfMatchingAppointments="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="String" Name="NoOfMatchingAppointments">
                                    <Description>
                                    <div> Specifies the number of appointments that matched the input criteria for the given dock location and date.
                                        </div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <UnavailableSlot DueToCalendar="" EndTime="" StartTime="">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute DataType="String" Name="DueToCalendar">
                                    <Description>
                                    <div> Specifies whether the appointment is due to calendar.
                                        </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="String" Name="EndTime">
                                    <Description>
                                    <div> End time of appointment.
                                        </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="String" Name="StartTime">
                                    <Description>
                                    <div> Start time of appointment.
                                        </div>
                                    </Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                            <DockAppointment AppointmentType="" BolNo=""
                                Carrier="" DockAppointmentKey=""
                                IsReservedForInputCriteria="" LoadKey=""
                                Node="" Notes="" OrderHeaderKey=""
                                ProNo="" ResPoolConsmptnDtlsKey=""
                                ShipmentKey="" TrailerNo="">
                                <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="Flag" Name="IsReservedForInputCriteria">
                                    <Description>
                                    <div>Indicates if the dock appointment was searched for. 
											<br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b>: if the dock appointment met the searched criteria or <b>
                                    <code>N</code>
                                    </b>: otherwise.
											</div>
                                    </Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                                <Order DocumentType="" EnterpriseCode="" OrderNo=""/>
                                <Shipment ShipmentNo=""/>
                                <Load LoadNo=""/>
                                <CapacityConsumption ReservationId="">
                                    <yfc:doc>
                                    <Attributes>
                                    <Attribute DataType="String" Name="ReservationId"/>
                                    </Attributes>
                                    </yfc:doc>
                                </CapacityConsumption>
                            </DockAppointment>
                        </UnavailableSlot>
                    </UnavailableSlots>
                </Date>
            </Dates>
        </Dock>
    </DockAppointmentList>
</DockAppointment>
