<!--changeRelease Input XML-->
<OrderRelease Action="MODIFY|CANCEL|BACKORDER" AuditTransactionId=""
    CarrierAccountNo="" CarrierServiceCode="" Currency=""
    CustomerPoNo="" DeliveryCode="" Division="" DocumentType=""
    EnterpriseCode="" FOB="" GiftFlag="" HoldReasonCode=""
    LevelOfService="" MergeNode="" ModificationReasonCode=""
    ModificationReasonText="" ModificationReference1=""
    ModificationReference2="" ModificationReference3=""
    ModificationReference4="" NotificationReference=""
    NotificationType="" NotifyAfterShipmentFlag="" OrderDate=""
    OrderHeaderKey="Conditional" OrderName="" OrderNo=""
    OrderReleaseKey="Conditional" OrderType="" Override="Y|N"
    PacklistType="" PersonalizeCode="" PriorityCode="" Purpose=""
    PutInventoryOnHoldOnBackorder="" ReleaseNo="Conditional"
    ReqCancelDate="" ReqDeliveryDate="" ReqShipDate="" SCAC=""
    ScacAndService="" ScacAndServiceKey="" SelectMethod="WAIT|NO_WAIT"
    ShipAdviceNo="" ShipCompleteFlag="" ShipLineComplete="" ShipNode=""
    ShipOrderComplete="" ShipToId="" TaxpayerId="" TransactionId="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/changeRelease/input">
    <yfc:doc>
        <Element EntityName="Order_Release">
            <Description>
                <div>
		 The <code>OrderRelease</code> optional attributes are validated only if they are passed with values that are different from the existing values. For validations on these attributes, refer to <code>createOrder</code>. 
		 <br/>The <code>OrderReleaseKey</code> attribute <i>or</i> the <code>OrderHeaderKey</code> and <code>ReleaseNo</code>
                    <i>or</i> the <code>ShipAdviceNo</code> and <code>ShipNode</code> attributes must be passed, to identify the <code>OrderRelease</code>.
		</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="YFSAction" Name="Action">
                <Description>
                    <div>
				Action to perform.
				<br/>Default: MODIFY
				<br/>Valid Values:
				<br/>
                        <b>
                            <code>CANCEL</code>
                        </b> - Cancels the entire order release. Only released quantities can be canceled from the release lines and order lines.
				<br/>
                        <b>
                            <code>BACKORDER</code>
                        </b> - Backorders the entire order release. Only released quantities can be backordered from the release lines; order lines are not affected.
				<br/>
                        <b>
                            <code>MODIFY</code>
                        </b> - Modify the release.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="TransactionID" Name="AuditTransactionId"/>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute DataType="CommonCode" Name="ModificationReasonCode"/>
            <Attribute DataType="Text-100" Name="ModificationReasonText"/>
            <Attribute DataType="Text-254" Name="ModificationReference1"/>
            <Attribute DataType="Text-254" Name="ModificationReference2"/>
            <Attribute DataType="Text-254" Name="ModificationReference3"/>
            <Attribute DataType="Text-254" Name="ModificationReference4"/>
            <Attribute EntityName="Order_Release" Name="OrderHeaderKey" NameInEntity="Order_Header_Key">
                <Description>
                    <div>
				The <code>OrderReleaseKey</code> attribute <i>or</i> the <code>OrderHeaderKey</code> and <code>ReleaseNo</code>
                        <i>or</i> the <code>ShipAdviceNo</code> and <code>ShipNode</code> attributes must be passed.
				<Validation>
                            <Condition>
					Either code <code>OrderReleaseKey</code> attribute  <i>or</i> the <code>OrderHeaderKey</code> and <code>ReleaseNo</code>
                                <i>or</i> the <code>ShipAdviceNo</code> and <code>ShipNode</code> attributes were not passed, or were not valid.
					</Condition>
                            <Error Description="YFS_INVALID_RELEASE_NO" ErrorCode="YFS10128"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" Name="OrderNo"/>
            <Attribute EntityName="Order_Release" Name="OrderReleaseKey" NameInEntity="Order_Release_Key">
                <Description>
                    <div>
				The <code>OrderReleaseKey</code> attribute <i>or</i> the <code>OrderHeaderKey</code> and <code>ReleaseNo</code>
                        <i>or</i> the <code>ShipAdviceNo</code> and <code>ShipNode</code> attributes must be passed.
				<Validation>
                            <Condition>
					Either code <code>OrderReleaseKey</code> attribute  <i>or</i> the <code>OrderHeaderKey</code> and <code>ReleaseNo</code>
                                <i>or</i> the <code>ShipAdviceNo</code> and <code>ShipNode</code> attributes were not passed, or were not valid.
					</Condition>
                            <Error Description="YFS_INVALID_RELEASE_NO" ErrorCode="YFS10128"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Release" Name="Override" NameInEntity="Override">
                <Description>
                    <div>
				If <b>
                            <code>Y</code>
                        </b>, the release can be modified as long as any quantity is within the system-allowed minimum and maximum statuses.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="PutInventoryOnHoldOnBackorder">
                <Description>
                    <div>
				If set to <b>
                            <code>Y</code>
                        </b>, inventory node control will place this item and node combination on hold until configured amount of time or <code>FirstAvailableDate</code>. If set to <b>
                            <code>N</code>
                        </b>, an inventory node control will not be placed. If not passed, then the <code>MARK_INV_INCORRECT_ON_BACKORDER_FROM_NODE</code> rule will be used to determine whether inventory node control should be placed.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" EntityName="Order_Release"
                Name="ReleaseNo" NameInEntity="Order_No">
                <Description>
                    <div>
				The <code>OrderReleaseKey</code> attribute <i>or</i> the <code>OrderHeaderKey</code> and <code>ReleaseNo</code>
                        <i>or</i> the <code>ShipAdviceNo</code> and <code>ShipNode</code> attributes must be passed.
				<Validation>
                            <Condition>
					Either code <code>OrderReleaseKey</code> attribute  <i>or</i> the <code>OrderHeaderKey</code> and <code>ReleaseNo</code>
                                <i>or</i> the <code>ShipAdviceNo</code> and <code>ShipNode</code> attributes were not passed, or were not valid.
					</Condition>
                            <Error Description="YFS_INVALID_RELEASE_NO" ErrorCode="YFS10128"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="ScacAndService"/>
            <Attribute DataType="Key" Name="ScacAndServiceKey"/>
            <Attribute DataType="SelectMethod" Name="SelectMethod">
                <Description>
                    <div>
				This attribute determines whether database select operations are carried out in the <b>
                            <code>WAIT</code>
                        </b> or <b>
                            <code>NO_WAIT mode</code>
                        </b>.
				<br/>Default: <b>
                            <code>NO_WAIT</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Release" Name="ShipAdviceNo" NameInEntity="Ship_Advice_No">
                <Description>
                    <div>Ship advice number of the order release.  <code>ShipAdviceNo</code> will only be used when order release identifiers such as <code>OrderReleaseKey</code>
                        <i>or</i>
                        <code>ReleaseNo</code> and <code>OrderHeaderKey</code> are not provided.  <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided) will be used to determine the order release in the system.  If more than one order releases are found in the system with the same <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided), the order release with a pickable status will be used. 
				  <Validation>
                            <Condition>More than one order releases in a pickable status are found with the same  <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided).
					</Condition>
                            <Error
                                Description="Multiple order releases found with ShipAdviceNo." ErrorCode="OMP75_10000"/>
                        </Validation>
                        <Validation>
                            <Condition>System cannot find an order release with the provided <code>ShipAdviceNo</code> and <code>ShipNode</code> (if provided).
					</Condition>
                            <Error Description="Invalid ShipAdviceNo." ErrorCode="YDM75_00001"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Release" Name="ShipNode" NameInEntity="Shipnode_Key">
                <Description>
                    <div>
				The ship node that is associated with this order release.  Pass  <code>ShipNode</code> along with <code>ShipAdviceNo</code> to help with uniquely identify the order release when there are more than one order releases with the same <code>ShipAdviceNo</code>.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="TransactionID" Name="TransactionId"/>
        </Attributes>
    </yfc:doc>
    <Notes>
        <Note AuditTransactionId="" ContactReference="" ContactTime=""
            ContactType="" ContactUser="" CustomerSatIndicator=""
            NoteText="Required" Priority="" ReasonCode=""
            VisibleToAll="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Notes"/>
            </yfc:doc>
        </Note>
    </Notes>
    <!-- Note can only be added. Note cannot be modified or deleted by this API.-->
    <OrderLines>
        <!-- If Action = CANCEL or BACKORDER, then OrderLineKey, PrimeLineNo, SubLineNo, StatusQuantity, ChangeInQuantity, FromStatus and ToStatus are the only relevant attributes and Schedules is the only relevant child element.-->
        <!-- AvailableReference and FirstAvailableDate are relevant attributes of OrderLine only if Action=BACKORDER at OrderLine. -->
        <!-- If Action = CREATE, then OrderLineKey, StatusQuantity, ChangeInQuantity and FromStatus are irrelevant. OrderedQty is a REQUIRED attribute.-->
        <!-- OrderLine element : If the attribute StatusQuantity has a nonvoid value in the input XML, we will operate with that value and ignore ChangeInQuantity. However, if StatusQuantity is not present in the input XML or the input XML contains StatusQuantity="" and the attribute ChangeInQuantity is present, we will operate with ChangeInQuantity, regardless of whether it has void or nonvoid value. If ChangeInQuantity is absent in the input xml and StatusQuantity="" in the input xml or StatusQuantity is absent from the xml, we assume StatusQuantity="0" and operate accordingly.  ChangeInQuantity must have positive numerical value if you want to add quantity, and negative numerical value if you want to cancel or backorder quantity. If ChangeInQuantity="0" or ChangeInQuantity="", no action will happen.-->
        <OrderLine Action="CANCEL|BACKORDER|CREATE" AllocationDate=""
            AvailableReference="" ChangeInQuantity=""
            CustomerLinePONo="" CustomerPONo="" DepartmentCode=""
            DependencyShippingRule="" DependentOnLineKey=""
            FillQuantity="" FirstAvailableDate="" FromStatus=""
            HoldFlag="" HoldReasonCode="" ImportLicenseExpDate=""
            ImportLicenseNo="" KitCode="" LevelOfService="" LineType=""
            MinShipByDate="" OrderLineKey="Conditional" OrderedQty=""
            PersonalizeCode="" PickableFlag="" PrimeLineNo="Conditional"
            Purpose="" ReqCancelDate="" SerialNo="" ShipTogetherNo=""
            StatusQuantity="" SubLineNo="Conditional" ToStatus="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Order_Line">
                    <Description>
                        <div>
				For existing lines, <code>changeRelease</code> can only modify the value of <code>StatusQuantity</code>. Other modifications are ignored by the API.
				</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute
                        DataType="YFSActionWithCreateWithoutModify" Name="Action">
                        <Description>
                            <div>
					 Specifies the action to be taken.  
					 <br/>Valid values: 
					 <br/>
                                <b>
                                    <code>CREATE</code>
                                </b> - Line is added to the release. The release attributes cannot be specified for this line. The release attributes (see list below) are defaulted from the release. Refer to <code>createOrder</code> for other validations. Bundle cannot be added using this API. It is an error condition to add bundle to a release using changeRelease API.  Use <code>changeOrder</code> API to add a new order line for bundle item.
					 <Validation>
                                    <Condition>Order line with KitCode as <code>BUNDLE</code> is passed 
					</Condition>
                                    <Error
                                    Description="Bundle cannot be added to a release" ErrorCode="OMP79_0021"/>
                                </Validation>
                                <Validation>
                                    <Condition>Item in the catalog is defined as <code>BUNDLE</code>
                                    </Condition>
                                    <Error
                                    Description="Bundle cannot be added to a release" ErrorCode="OMP79_0021"/>
                                </Validation>
                                <br/>
                                <b>
                                    <code>CANCEL</code>
                                </b> - Cancels released quantity from the release line and the corresponding order line. If <code>StatusQuantity</code> and <code>ChangeInQuantity</code> are not provided, the total released quantity is canceled. If <code>StatusQuantity</code> is passed, then the remaining quantity is canceled. 
					 <br/>
                                <b>
                                    <code>BACKORDER</code>
                                </b> - Backorders released quantity from the release line. The quantity is not changed on the corresponding order line. If neither <code>StatusQuantity</code> nor <code>ChangeInQuantity</code> are provided, the total released quantity is backordered. Partial backordering of Product lines is also allowed.
					 <br/>
                                <br/>If: <code>StatusQuantity</code> is passed, and is less than existing <code>StatusQuantity</code>
                                <br/>
                                <i>or</i>
                                <code>ChangeInQuantity</code> is a negative number
					 <br/>the quantity can be either canceled or backordered, depending on the value which is passed for <code>Action</code>. If <code>Action</code> is not specified with either those conditions, the default action is <b>
                                    <code>BACKORDER</code>
                                </b>.
					 <br/>
                                <br/>If:<code>StatusQuantity</code> is passed and is greater than the <code>StatusQuantity</code> for the <code>OrderReleaseLine</code>
                                <br/>
                                <i>or</i>
                                <code>ChangeInQuantity</code> is positive, 
					 <br/>the difference in quantity is added to the release line and to the order line, regardless of the value passed for <code>Action</code>. 
					 <br/>
                                <br/>Release attributes that are defaulted during <b>
                                    <code>CREATE</code>
                                </b>: <code>ShipNode</code>, <code>ReceivingNode</code>, <code>SCAC</code>, <code>MergeNode</code>, <code>CarrierServiceCode</code>, <code>CarrierAccountNo</code>, <code>DeliveryCode</code>, <code>FreightTerms</code>, <code>PackListType</code>, <code>PersonInfoShipTo</code>, <code>CostCurrency</code>, <code>ReqShipDate</code>, <code>ReqDeliveryDate</code>, and <code>References</code>.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="AllocationDate"/>
                    <Attribute DataType="Text-100" Name="AvailableReference">
                        <Description>
                            <div>
					Used if the <code>Action</code> value is <b>
                                    <code>BACKORDER</code>
                                </b>.  If the <code>FirstAvailableDate</code> is passed and the <code>Action</code> value is <b>
                                    <code>BACKORDER</code>
                                </b>, an <code>INFO</code> record is created in the <code>YFS_INVENTORY_SUPPLY</code> table with the <code>ETA</code> set to the <code>FirstAvailableDate</code> value passed and the <code>SupplyReference</code> value set to the <code>AvailableReference</code> passed.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="QuantityPositiveAndNegative" Name="ChangeInQuantity">
                        <Description>
                            <div>
					If a value is passed for  <code>StatusQuantity</code>, that value is used, and <code>ChangeInQuantity</code> is ignored. If <code>StatusQuantity</code> is not passed, and  <code>ChangeInQuantity</code> is passed, the value of  <code>ChangeInQuantity</code> is used, regardless of whether it has void value or nonvoid value. If <code>Action</code>=<b>CANCEL</b> is passed, then the StatusQuantity shows the quantity which will remain after cancellation and the remaining quantity is cancelled. If a value for  <code>ChangeInQuantity</code> is not passed and a value for <code>StatusQuantity</code> is not passed, or is Null, <code>StatusQuantity</code> is defaulted to <b>
                                    <code>0</code>
                                </b>. 
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="TimeStamp" Name="FirstAvailableDate">
                        <Description>
                            <div>
					Used if the <code>Action</code> value is <b>
                                    <code>BACKORDER</code>
                                </b>.  If the <code>FirstAvailableDate</code> is passed and the <code>Action</code> value is <b>
                                    <code>BACKORDER</code>
                                </b>, an <code>INFO</code> record is created in the <code>YFS_INVENTORY_SUPPLY</code> table with the <code>ETA</code> set to the <code>FirstAvailableDate</code> value passed and the <code>SupplyReference</code> value set to the <code>AvailableReference</code> passed.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Status" Name="FromStatus">
                        <Description>
                            <div>
					 Indicates a status to search for, when a quantity should be canceled or backordered from a specific status. <code>StatusQuantity</code> is passed to indicate the amount to move to the new status, and <code>ToStatus</code> is passed to indicate the new status to use.
					 <Validation>
                                    <Condition>There is not enough quantity in the status passed in <code>FromStatus</code>.
						</Condition>
                                    <Error
                                    Description="YFS: Not Enough Quantity" ErrorCode="YFS10131"/>
                                </Validation>
                                <Validation>
                                    <Condition>Must be a valid status for the order's process type. Status equivalent to Cancel - <b>
                                    <code>9000</code>
                                    </b> cannot be specified.
							</Condition>
                                    <Error
                                    Description="YFS:Invalid Status" ErrorCode="YFS10235"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="OrderLineKey" NameInEntity="Order_Line_Key">
                        <Description>
                            <div>Unique identifier for <code>OrderLine</code>.
					<Validation>
                                    <Condition>The <code>OrderLineKey</code> attribute <i>or</i> the <code>PrimeLineNo</code> and <code>SubLineNo</code> attributes must be passed and must be valid.     
						</Condition>
                                    <Error
                                    Description="YFS_LINE_NOT_IN_RELEASE" ErrorCode="YFS10424"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="PrimeLineNo" NameInEntity="Prime_Line_No">
                        <Description>
                            <div>Part of the identification for an <code>OrderLine</code>.
					<Validation>
                                    <Condition>The <code>OrderLineKey</code> attribute <i>or</i> the <code>PrimeLineNo</code> and <code>SubLineNo</code> attributes must be passed and must be valid.     
						</Condition>
                                    <Error
                                    Description="YFS_LINE_NOT_IN_RELEASE" ErrorCode="YFS10424"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="StatusQuantity">
                        <Description>
                            <div>
					 Used if a quantity should be canceled or backordered from a specific status. <code>FromStatus</code> is passed to indicate the status to be found, <code>StatusQuantity</code> is passed to indicate the amount that should be moved to the new status, and <code>ToStatus</code> should be supplied to indicate the new status to be used.
					<br/>If the attribute <code>StatusQuantity</code> has a value passed, that value is used, and <code>ChangeInQuantity</code> is ignored. 
					<br/> If <code>ChangeInQuantity</code> is not passed and <code>StatusQuantity</code> is not passed, or is Null, <code>StatusQuantity</code> is defaulted to <b>
                                    <code>0</code>
                                </b>. 
					<br/>Default: <b>
                                    <code>0</code>
                                </b> , if no value is passed in <code>ChangeInQuantity</code>. 
					<Validation>
                                    <Condition>If passed, must be zero or a positive number.
						</Condition>
                                    <Error Description="" ErrorCode=""/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line" Name="SubLineNo" NameInEntity="Sub_Line_No">
                        <Description>
                            <div>Part of the identification for an <code>OrderLine</code>.
					<Validation>
                                    <Condition>The <code>OrderLineKey</code> attribute <i>or</i> the <code>PrimeLineNo</code> and <code>SubLineNo</code> attributes must be passed and must be valid.     
						</Condition>
                                    <Error
                                    Description="YFS_LINE_NOT_IN_RELEASE" ErrorCode="YFS10424"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Status" Name="ToStatus">
                        <Description>
                            <div>
					Indicates a new status. Used with <code>FromStatus</code> and <code>StatusQuantity</code>. When a quantity should be canceled or backordered from a specific status. <code>StatusQuantity</code> is passed to indicate the amount to move to the new status, and <code>ToStatus</code> is passed to indicate the new status to use.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <OrderLineTranQuantity ChangeInQuantity="" FillQuantity=""
                OrderedQty="" StatusQuantity="" TransactionalUOM="">
                <yfc:doc>
                    <Attributes>
                        <Attribute
                            DataType="QuantityPositiveAndNegative" Name="ChangeInQuantity"/>
                        <Attribute DataType="Quantity" Name="FillQuantity">
                            <Description>The minimum order line quantity that must be fulfilled in an order release before it is shipped.
					  </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="OrderedQty">
                            <Description>The quantity of the line item that has been ordered. If part of line is canceled, this number is reduced appropriately. If quantity on the order line has a status of returned, this number is not reduced.
					  </Description>
                        </Attribute>
                        <Attribute EntityName="Order_Release_Status"
                            Name="StatusQuantity" NameInEntity="Status_Quantity">
                            <Description>Quantity on the order line in the current status.
					  </Description>
                        </Attribute>
                        <Attribute DataType="UOM" Name="TransactionalUOM">
                            <Description>UOM on which all the transactional quantities are specified.
					  </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </OrderLineTranQuantity>
            <Schedules>
                <Schedule BatchNo="" ChangeInQuantity="Required"
                    LotNumber="" OverrideItemID="" OverrideUOM=""
                    RevisionNo="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute
                                DataType="QuantityPositiveAndNegative" Name="ChangeInQuantity"/>
                            <Attribute DataType="String" Name="OverrideUOM"/>
                        </Attributes>
                    </yfc:doc>
                    <ScheduleTranQuantity ChangeInQuantity="" TransactionalUOM="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute
                                    DataType="QuantityPositiveAndNegative" Name="ChangeInQuantity">
                                    <Description>
                                    <div>
							Any nonzero positive number if quantity is getting added to the line and any nonzero negative number if quantity is getting canceled from the line. It can be specified only for the quantity that is being added or getting canceled. 
							<Validation>
                                    <Condition>
									If total scheduled quantity is greater than the change in OrderedQty
								</Condition>
                                    <Error
                                    Description="Invalid Schedule Quantity" ErrorCode="YFS10377 "/>
                                    </Validation>
                                    </div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="UOM" Name="TransactionalUOM">
                                    <Description>UOM on which all the transactional quantities are specified.
					  </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </ScheduleTranQuantity>
                </Schedule>
            </Schedules>
            <OrderHoldTypes>
                <OrderHoldType HoldType="Required" ReasonText=""
                    Status="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Order_Hold_Type">
                            <Description>
                                <div>
			Optional. Stores hold type information. Multiple <code>HoldType</code>s can be passed for a single order. These hold types will be applied during change release. This should be passed only for the new line.
			</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute Name="HoldType">
                                <Description>
                                    <div>
					The code identifying the hold type to be applied on Order.
					<Validation>
                                    <Condition>
                                    <code>HoldType</code> must be defined in the configuration
						</Condition>
                                    <Error
                                    Description="Hold Type is not setup." ErrorCode="OMP11035"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </OrderHoldType>
            </OrderHoldTypes>
            <Item AlternateItemID="" CountryOfOrigin="" CustomerItem=""
                CustomerItemDesc="" ECCNNo="" HarmonizedCode="" ISBN=""
                ItemDesc="" ItemID="Conditional" ItemShortDesc=""
                ItemWeight="" ItemWeightUOM="" ManufacturerItem=""
                ManufacturerItemDesc="" ManufacturerName="" NMFCClass=""
                NMFCCode="" NMFCDescription="" ProductClass=""
                ProductLine="" ScheduleBCode="" SupplierItem=""
                SupplierItemDesc="" TaxProductCode="" UPCCode=""
                UnitCost="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Order_Line">
                        <Description>
                            <div>
					Either the <code>ItemID</code> or a valid <code>AlternateItemID</code> must be passed. If the <code>ItemID</code> is not passed, the catalog will be searched for an item which has the same alias as the <code>AlternateItemID</code>. If an item is still not found in the catalog, the <code>AlternateItemID</code> will be used as the <code>ItemID</code>.
				</div>
                        </Description>
                    </Element>
                </yfc:doc>
            </Item>
            <!--Optional-->
            <PersonInfoMarkFor AddressID="" AddressLine1=""
                AddressLine2="" AddressLine3="" AddressLine4=""
                AddressLine5="" AddressLine6="" AlternateEmailID=""
                Beeper="" City="" Company="" Country="" DayFaxNo=""
                DayPhone="" Department="" EMailID="" EveningFaxNo=""
                EveningPhone="" FirstName="" IsCommercialAddress=""
                JobTitle="" LastName="" MiddleName="" MobilePhone=""
                OtherPhone="" PersonID="" State="" Suffix=""
                TaxGeoCode="" Title="" ZipCode="">
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                </yfc:doc>
