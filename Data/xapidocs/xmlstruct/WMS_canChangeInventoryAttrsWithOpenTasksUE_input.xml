<?xml version="1.0" encoding="UTF-8"?>
<CanChangeInventoryAttributesWithOpenTasks EnterpriseCode="Required"
    ItemID="" LocationId="Required" Node="Required" ProductClass=""
    UOM="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/canChangeInventoryAttrsWithOpenTasksUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Inventory_Item" Name="EnterpriseCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
					The Enterprise whose product information is being stored. 
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="ItemID" NameInEntity="Item_ID">
                <Description>
                    <div>
					Item identity for the inventory.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Location" Name="LocationId" NameInEntity="Location_Id">
                <Description>
                    <div>
					Location Identifier. Forms unique key of this table in conjunction with <code>NODE_KEY</code>. Indicates the location where the inventory is being adjusted.  
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Ship_Node" Name="Node" NameInEntity="Ship_Node">
                <Description>
                    <div>
					Indicates the node or warehouse where the attributes of inventory are being changed.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="ProductClass" NameInEntity="Product_Class">
                <Description>
                    <div>
					Gives the ProductClass for the item.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="UOM"/>
            <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure" NameInEntity="Uom">
                <Description>
                    <div>
					Unit of measure for the item.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</CanChangeInventoryAttributesWithOpenTasks>
