<!-- copyHubModificationRules Input XML -->
<StatusModification CallingOrganizationCode="Required" ProcessTypeKey="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/copyHubModificationRules/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Organization"
                Name="CallingOrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>Target Organization Code to which the hub modification rules have to be copied. 
				<Validation>
                            <Condition>CallingOrganizationCode Should not be Void or DEFAULT.
				</Condition>
                            <Error
                                Description="Invalid Organization Code." ErrorCode="YCP0040"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Process_Type" Name="ProcessTypeKey" NameInEntity="Process_Type_Key">
                <Description>
                    <div>If the ProcessTypeKey is passed it will copy Status Modification only for specific ProcessType.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</StatusModification>
