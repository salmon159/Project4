<!-- ON_ATTRIBUTE_CHANGE Event XML -->
<CategoryList xmlns="http://www.sterlingcommerce.com/documentation/YCM/CATEGORY_DEFINITION/ON_ATTRIBUTE_CHANGE">
    <Category CategoryID="" CategoryKey="" CategoryPath=""
        OrganizationCode="" ParentCategoryKey="">
        <ItemAttributeList>
            <ItemAttribute AttributeDomainID="" AttributeGroupID=""
                IsInherited="" IsKeyAttribute=""
                ItemAttributeDescription="" ItemAttributeGroupKey=""
                ItemAttributeKey="" ItemAttributeName="" Operation="" SequenceNo="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Action" Name="Operation">
                            <Description>
                                <div>Operation to perform
							<br/>Valid Values: 
							<br/>
                                    <b>
                                    <code>Manage</code>
                                    </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
							<br/>
                                    <b>
                                    <code>Create</code>
                                    </b> - Creates a new record. 
							<br/>
                                    <b>
                                    <code>Modify</code>
                                    </b> - Change values in an existing record.
							<br/>
                                    <b>
                                    <code>Delete</code>
                                    </b> - Deletes an existing record.
							<br/>The default behavior is <b>
                                    <code>Manage</code>
                                    </b>.
							</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ItemAttribute>
        </ItemAttributeList>
        <AdditionalAttributeList>
            <AdditionalAttribute AttributeDomainID=""
                AttributeGroupID="" DoubleValue="" IntegerValue=""
                IsInherited="" Name="" Operation="" Value="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Action" Name="Operation">
                            <Description>
                                <div>Operation to perform
							<br/>Valid Values: 
							<br/>
                                    <b>
                                    <code>Manage</code>
                                    </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
							<br/>
                                    <b>
                                    <code>Create</code>
                                    </b> - Creates a new record. 
							<br/>
                                    <b>
                                    <code>Modify</code>
                                    </b> - Change values in an existing record.
							<br/>
                                    <b>
                                    <code>Delete</code>
                                    </b> - Deletes an existing record.
							<br/>The default behavior is <b>
                                    <code>Manage</code>
                                    </b>.
							</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </AdditionalAttribute>
        </AdditionalAttributeList>
    </Category>
</CategoryList>
