<?xml version="1.0" encoding="UTF-8"?>
<DemandPlan OrganizationCode="Required" PlannedForDate="Required"
    PlannedOnTimestamp="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/confirmDemandPlanningCompletion/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="OrganizationCode">
                <Description>
                    <div>
				Organization for which this planning is done.
				<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not passed.
					</Condition>
                            <Error
                                Description="OrganizationCode is mandatory." ErrorCode="WMS00077"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="PlannedForDate">
                <Description>
                    <div>
				Date for which planning is done.
				<Validation>
                            <Condition>
                                <code>PlannedForDate</code> is not passed.
					</Condition>
                            <Error
                                Description="PlannedForDate is mandatory." ErrorCode="WMSRP80_001"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="PlannedOnTimestamp">
                <Description>
                    <div>
				Time stamp when planning was marked complete for a date. If this is not
				passed then current time stamp is taken as the input.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</DemandPlan>
