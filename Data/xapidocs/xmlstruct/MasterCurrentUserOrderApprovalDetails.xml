<CurrentUserOrderApprovalDetails xmlns="http://www.sterlingcommerce.com/documentation/MasterCurrentUserOrderApprovalDetails">
    <yfc:doc>
        <Element>
            <Description>
                <div> The records shown below are based on the <code>ApproverUserID</code> passed in the input.
				</div>
            </Description>
        </Element>
    </yfc:doc>
    <CurrentOrderApprovalDetails TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="TotalNumberOfRecords">
                    <Description>
                        <div>The total number of order approval detail records.                   
						</div>
                    </Description>
                </Attribute>
            </Attributes>
            <Element>
                <Description>
                    <div> This element shows the order approval detail record of those order approvals which
	                	got created for the current user's user group.	                			
						</div>
                </Description>
            </Element>
        </yfc:doc>
        <OrderApprovalDetail ApprovalRuleID="" Message="" MessageCode=""
            OrderApprovalDetailKey="" OrderApprovalKey=""
            OrderHeaderKey="" OrderLineKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="MasterOrderApproval.xml" IncludeXMLPath="/OrderApprovals/OrderApproval/OrderApprovalDetails/OrderApprovalDetail"/>
            <OrderLine OrderLineKey="">
                <BundleParentLine OrderLineKey="" PrimeLineNo="" SubLineNo=""/>
            </OrderLine>
        </OrderApprovalDetail>
    </CurrentOrderApprovalDetails>
    <PredecessorOrderApprovalDetails TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="TotalNumberOfRecords">
                    <Description>
                        <div>The total number of predecessor order approval detail records.                   
						</div>
                    </Description>
                </Attribute>
            </Attributes>
            <Element>
                <Description>
                    <div> This element shows the order approval detail records of those order approvals
	                	which the current user can approve.
						</div>
                </Description>
            </Element>
        </yfc:doc>
        <OrderApprovalDetail ApprovalRuleID="" Message="" MessageCode=""
            OrderApprovalDetailKey="" OrderApprovalKey=""
            OrderHeaderKey="" OrderLineKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="MasterOrderApproval.xml" IncludeXMLPath="/OrderApprovals/OrderApproval/OrderApprovalDetails/OrderApprovalDetail"/>
            <OrderLine OrderLineKey="">
                <BundleParentLine OrderLineKey="" PrimeLineNo="" SubLineNo=""/>
            </OrderLine>
        </OrderApprovalDetail>
    </PredecessorOrderApprovalDetails>
</CurrentUserOrderApprovalDetails>
