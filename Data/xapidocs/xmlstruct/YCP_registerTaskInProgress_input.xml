<Task AssignedToUserId="" EquipmentId="" EquipmentLocationId=""
    HoldReasonCode="" IsConsolidatedTask="" OrganizationCode=""
    SourceLocationId="" TargetCaseId="" TargetLocationId=""
    TargetPalletId="" TaskId="" TaskKey="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/registerTaskInProgress/input">
    <yfc:doc>
        <Attributes>
            <Attribute Name="IsConsolidatedTask">
                <Description>
                    <div>
				Flag to indicate that the task being picked is a consolidated task.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="TargetCaseId" NameInEntity="Target_LPN_No">
                <Description>
                    <div>Target Case Id of the task</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="TargetPalletId" NameInEntity="Target_LPN_No">
                <Description>
                    <div>Target Pallet Id of the task</div>
                </Description>
            </Attribute>
            <Attribute Name="TaskId">
                <Description>
                    <div>
				Pass either <code>TaskKey</code> or <code>TaskId</code> to identify the task.
				<Validation>
                            <Condition>
                                <code>TaskId</code> passed does not exist in the YFS_TASK table.
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="TaskKey">
                <Description>
                    <div>
				Pass either <code>TaskKey</code> or <code>TaskId</code> to identify the task.
				<Validation>
                            <Condition>
                                <code>TaskKey</code> passed does not exist in the YFS_TASK table.
					</Condition>
                            <Error Description="No record found" ErrorCode="YCP0045"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Inventory InventoryStatus="" Quantity="" SourceCaseId="" SourcePalletId="">
        <yfc:doc>
            <Attributes>
                <Attribute Name="Quantity">
                    <Description>
                        <div>
					 If quantity is less than task quantity then the passed quantity moves to completed status with updated information. The rest of the quantity moves to open status.  Adding quantity is not allowed- if the quantity is more than task quantity, an error is thrown. 
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="SourceCaseId" NameInEntity="Source_LPN_No">
                    <Description>
                        <div>Source Case Id of the task</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="SourcePalletId" NameInEntity="Source_LPN_No">
                    <Description>
                        <div>Source Pallet Id of the task</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <TagAttributes BatchNo="" LotAttribute1="" LotAttribute2=""
            LotAttribute3="" LotKeyReference="" LotNumber=""
            ManufacturingDate="" RevisionNo="">
            <yfc:doc>
                <Element EntityName="Inventory_Tag"/>
            </yfc:doc>
        </TagAttributes>
        <SerialNumberDetails>
            <SerialRangeList>
                <SerialRange FromSerialNo="" ToSerialNo="" yfc:DTDOccurrence="OneOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="SerialNo" Name="FromSerialNo">
                                <Description>
                                    <div>Staring serial number of a range for item. 
										<Validation>
                                    <Condition> From Serial Number is blank.
											</Condition>
                                    <Error
                                    Description="From Serial Number is blank." ErrorCode="INV80_043"/>
                                    </Validation>
                                    <Validation>
                                    <Condition> To Serial Number is blank.
											</Condition>
                                    <Error
                                    Description="To Serial Number is blank." ErrorCode="INV80_044"/>
                                    </Validation>
                                    <Validation>
                                    <Condition> Both From and To Serial numbers are blank.
											</Condition>
                                    <Error
                                    Description="Both From and To Serial numbers are blank." ErrorCode="INV80_039"/>
                                    </Validation>
                                    <Validation>
                                    <Condition> Serial range entry is not supported when From and To Serial numbers are of different length.
											</Condition>
                                    <Error
                                    Description="From and To Serial numbers are of different length, Manual Scanning Required." ErrorCode="INV80_045"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="SerialNo" Name="ToSerialNo">
                                <Description>
                                    <div>Ending serial number of a range for item. 
										<Validation>
                                    <Condition> From Serial Number is blank.
											</Condition>
                                    <Error
                                    Description="From Serial Number is blank." ErrorCode="INV80_043"/>
                                    </Validation>
                                    <Validation>
                                    <Condition> To Serial Number is blank.
											</Condition>
                                    <Error
                                    Description="To Serial Number is blank." ErrorCode="INV80_044"/>
                                    </Validation>
                                    <Validation>
                                    <Condition> Both From and To Serial numbers are blank.
											</Condition>
                                    <Error
                                    Description="Both From and To Serial numbers are blank." ErrorCode="INV80_039"/>
                                    </Validation>
                                    <Validation>
                                    <Condition> Serial range entry is not supported when From and To Serial numbers are of different length.
											</Condition>
                                    <Error
                                    Description="From and To Serial numbers are of different length, Manual Scanning Required." ErrorCode="INV80_045"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </SerialRange>
            </SerialRangeList>
            <SerialNumberDetail SerialNo="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="SerialNo">
                            <Description>
                                <div>Serial Number of Item </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </SerialNumberDetail>
        </SerialNumberDetails>
    </Inventory>
</Task>
