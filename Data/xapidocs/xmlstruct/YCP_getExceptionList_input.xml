    <yfc:doc>
        <Attributes>
            <Attribute DataType="Count" Name="GeneratedAfterInHours">
                <Description>If this Attribute is set, only show the exceptions that were generated after a specific time. The time is calculated as the current time less the specified number of hours.</Description>
            </Attribute>
            <Attribute DataType="Count" Name="GeneratedBeforeInHours">
                <Description>If this Attribute is set, only show the exceptions that were generated before a specific time. The time is calculated as the current time less the specified number of hours.</Description>
            </Attribute>
            <Attribute DataType="ListMaxRecordCount" Name="MaximumRecords">
                <Description>Maximum number of records to be listed in the output.  </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="SubscribedQueues">
                <Description>Pass the attribute <code>SubscribedQueues</code> as 'Y' to only show the exceptions belonging to the queues subscribed by the user  </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <InboxReferencesList ConsiderAllChildCriteria="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="ConsiderAllChildCriteria">
                    <Description>
                        <code>ConsiderAllChildCriteria</code> attribute has to be passed as 'ture' or 'Y' for the API to filter Exceptions based on all <code>InboxReferences</code> passed.</Description>
                </Attribute>
