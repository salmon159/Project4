<!-- YCP_getModificationTypeAttributes_input.xml -->
<ModificationTypeAttributes BaseProcessType="" DocumentType=""
    ModificationLevel="" ModificationType="" ProcessType="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getModificationTypeAttributes/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
				This API will not throw an error if any attribute value is invalid or if the combination of attribute values is invalid.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Process_Type" Name="BaseProcessType" NameInEntity="Base_Process_Type"/>
            <Attribute EntityName="Process_Type" Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute EntityName="Status_Modification_Type"
                Name="ModificationLevel" NameInEntity="Modification_Level">
                <Description>
                    <div>
					The level at which a modification is defined. For example, Header/Line/Release. This API does not support <code>ModificationLevel</code>=<b>
                            <code>ORDER_RELEASE_LINE</code>
                        </b>. If the input XML contains <code>ModificationLevel</code>=<b>
                            <code>ORDER_RELEASE_LINE</code>
                        </b>, the list of modification types and the associated database attributes will not be returned.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Status_Modification_Type"
                Name="ModificationType" NameInEntity="Modification_Type"/>
            <Attribute EntityName="Process_Type" Name="ProcessType" NameInEntity="Process_Type"/>
        </Attributes>
    </yfc:doc>
</ModificationTypeAttributes>
