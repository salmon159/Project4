<ItemList xmlns="http://www.sterlingcommerce.com/documentation/YFS/getPromotionsForItemListUE/output">
    <Item ItemID="" ItemKey="" OrganizationCode="" UnitOfMeasure="" yfc:DTDOccurrence="OneOrMany">
        <PromotionList yfc:DTDOccurrence="REQUIRED">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="YFS_getItemListForOrdering_output.xml" IncludeXMLPath="/ItemList/Item/PromotionList"/>
            <yfc:doc>
                <Element>
                    <Description>
						The currently active promotions for the item, as determined by the user exit.
					</Description>
                </Element>
            </yfc:doc>
            <Promotion LongDescription="" ShortDescription="" URL="">
                <yfc:doc>
                    <Element>
                        <Description>
						Information on a currently available promotion for the item.  This data is not persisted, it is retrieved from the <code>YFSGetPromotionsForItemListUE</code> user exit.  The attributes provided are sample attributes; as there is no logic around these promotions in the output, the <code>YFSGetPromotionsForItemListUE</code> user exit may return any attributes.
					</Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="CLOB" Name="LongDescription">
                            <Description>
							A long description of the promotion.
						</Description>
                        </Attribute>
                        <Attribute DataType="CLOB" Name="ShortDescription">
                            <Description>
							A short description of the promotion.
						</Description>
                        </Attribute>
                        <Attribute DataType="CLOB" Name="URL">
                            <Description>
							A URL associated with the promotion.
						</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Promotion>
        </PromotionList>
    </Item>
</ItemList>
