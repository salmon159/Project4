<!-- getPage output XML -->
<Page GeneratedOn="" IsFirstPage="" IsLastPage="" IsValidPage=""
    PageNumber="" PageSetToken="" PageSize="" StartRowNumber=""
    TotalNumberOfPages="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getPage/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="TimeStamp" Name="GeneratedOn">
                <Description>
                    <div>Time the page set was generated on.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Boolean" Name="IsFirstPage">
                <Options IsEnumeration="Y">
                    <Option
                        Description="This is the first page in the set." Value="Y"/>
                    <Option Description="Any other page." Value="N"/>
                </Options>
            </Attribute>
            <Attribute DataType="Boolean" Name="IsLastPage">
                <Options IsEnumeration="Y">
                    <Option
                        Description="This is the last page in the set." Value="Y"/>
                    <Option
                        Description="This is either not the last page, or the underlying pagination strategy doesn't know whether it is the last page." Value="N"/>
                </Options>
            </Attribute>
            <Attribute DataType="Boolean" Name="IsValidPage">
                <Options IsEnumeration="Y">
                    <Option
                        Description="This is the first page or there is more than one record in the API output." Value="Y"/>
                    <Option
                        Description="This is not the first page and no records are available. This may happen if an invalid page is requested, for example." Value="N"/>
                </Options>
            </Attribute>
            <Attribute DataType="Integer" Name="PageNumber">
                <Description>
                    <div>The page number of this page. Set to -1 if the page number is unknown.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="PageSetToken">
                <Description>
                    <div>Reference to the current set of pages. This should be used in future getPage invocations to get more data, and passed to the deletePageCache API when the data is no longer needed.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="PageSize">
                <Description>
                    <div>Number of records per page</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="StartRowNumber">
                <Description>
                    <div>Row number of the first record in the page. Set to -1 if the row number is unknown.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="TotalNumberOfPages">
                <Description>
                    <div>Total number of pages in the page set. If this is unknown and the root element of the API output contains a TotalNumberOfRecords attribute, then this will be derived from that number and the page size. Set to -1 if the total number of pages is unknown.</div>
                </Description>
            </Attribute>
        </Attributes>
        <Element>
            <Description>
                <div>Contains metadata for the returned page</div>
            </Description>
        </Element>
    </yfc:doc>
    <Output yfc:AllowAnyElement="skip">
        <yfc:doc>
            <Element>
                <Description>
                    <div>The output of the invoked API is returned as a child of this element.</div>
                </Description>
            </Element>
        </yfc:doc>
    </Output>
</Page>
