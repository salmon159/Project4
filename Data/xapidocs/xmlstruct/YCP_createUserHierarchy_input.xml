<User Activateflag="" BillingaddressKey="" BusinessKey=""
    ChangePassNext="" ContactaddressKey="" CreatorOrganizationKey=""
    DataSecurityGroupId="" DepartmentCode="" DisplayUserID=""
    EnterpriseCode="" GeneratePassword="" Imagefile="" IsCustomer=""
    IsSupervisor="" Localecode="Required" Loginid="Required" Longdesc=""
    MaxCustomerAssignments="" MenuId="" NoteKey="" OrganizationKey=""
    ParentUserKey="" Password="Required" PasswordPolicyId=""
    PasswordPolicyKey="" PreferenceKey="" Pwdlastchangedon=""
    SessionTimeout="" SuperUser="" Theme="" UserKey=""
    Username="Required" Usertype="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/createUserHierarchy/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Text-20" Name="DepartmentCode">
                <Description>
                    <div>
                        <Validation>
                            <Condition>
								The department code is invalid or the organization does not contain the department
							</Condition>
                            <Error
                                Description="The Department code does not exist for the organization." ErrorCode="YCP0256"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-150" EntityName="User"
                Name="DisplayUserID" NameInEntity="Display_User_ID">
                <Description>
                    <div>
					Indicates Display User's ID which is used for login in to the system. Ensure that OrganizationSuffix is populated for the Organization this User belongs to	if this ID is being used across multiple Enterprises. Note: For global users the maximum supported character length is 40 characters.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="GeneratePassword">
                <Description>
                    <div>
					    Flag to generate a default 8 character password. Also, YCPGeneratePassword UE has been exposed  to plug in logic for default password. A new event RESET_PASSWORD.ON_SUCCESS also gets raised when a new password is created. Valid values are "true" or "false".						
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="User" Name="IsCustomer" NameInEntity="Is_Customer">
                <Description>
                    <div>
					This flag indicates if the user is a valid customer or not. This flag must be set to <code>Y</code> if the user is a customer contact user. The only valid value for this attribute is <code>Y</code>. This flag will be set to <code>Y</code> only if a valid customer contact exist in the system.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="User" Name="IsSupervisor" NameInEntity="Is_Supervisor">
                <Description>
                    <div>
					This flag indicates if the user is a supervisor or
					not.
				</div>
                </Description>
            </Attribute>
            <Attribute Name="Localecode">
                <Description>
                    <div>The locale of a user.
						<Validation>
                            <Condition>
								Record does not exist in database
							</Condition>
                            <Error Description="Invalid Localecode" ErrorCode="YFS10145"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="User" Name="MaxCustomerAssignments" NameInEntity="Max_Cust_Assign">
                <Description>
                    <div>
					This indicates maximum number of customers that can
					be assigned to this user.
				</div>
                </Description>
            </Attribute>
        </Attributes>
        <Element EntityName="User">
            <Description>
                <div>
				All users in the system are maintained here.
				<br/>Only one of either the BillingaddressKey attribute or the BillingPersonInfo element must be passed, although
				this is not mandatory.
				<br/>Only one of either the ContactaddressKey attribute or	the ContactPersonInfo element must be passed, although
				this is not mandatory.
				<p/>Either the <code>Loginid</code> or <code>DisplayUserID</code>	must be passed based on the <code>IsGlobalUser</code> 
				rule flag. If <code>IsGlobalUser</code> is set to <code>N</code> then only <code>Loginid</code>	should be passed. 
				Otherwise only <code>DisplayUserID</code> should be passed.
			</div>
            </Description>
        </Element>
    </yfc:doc>
    <ContactPersonInfo AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" ErrorTxt=""
        EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
        IsCommercialAddress="" JobTitle="" LastName="" MiddleName=""
        MobilePhone="" OtherPhone="" PersonID="" PersonInfoKey=""
        PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
        Title="" UseCount="" VerificationStatus="" ZipCode=""/>
    <BillingPersonInfo AddressLine1="" AddressLine2="" AddressLine3=""
        AddressLine4="" AddressLine5="" AddressLine6=""
        AlternateEmailID="" Beeper="" City="" Company="" Country=""
        DayFaxNo="" DayPhone="" Department="" EMailID="" ErrorTxt=""
        EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
        IsCommercialAddress="" JobTitle="" LastName="" MiddleName=""
        MobilePhone="" OtherPhone="" PersonID="" PersonInfoKey=""
        PreferredShipAddress="" State="" Suffix="" TaxGeoCode=""
        Title="" UseCount="" VerificationStatus="" ZipCode=""/>
    <QueueSubscriptionList>
        <QueueSubscription QueueKey="Required" QueueSubscriptionKey="" yfc:DTDOccurrence="ZeroOrMany"/>
    </QueueSubscriptionList>
    <ResourcePermissionList>
        <ResourcePermission ResourceKey="" Rights="" UserKey=""/>
    </ResourcePermissionList>
    <UserGroupLists>
        <UserGroupList IsPrimary="" UserGroupListKey="" UsergroupId=""
            UsergroupKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="User_Group"
                        Name="UsergroupId" NameInEntity="Usergroup_Id">
                        <Description>
                            <div>  A unique identifier to the user group.
                                                        <Validation>
                                    <Condition>Invalid <code>UsergroupId</code> passed.
                                                                </Condition>
                                    <Error
                                    Description="Invalid Usergroup." ErrorCode="YCP0152"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
                <Element>
                    <Description>
                        <div>
                                        Either <code>UsergroupKey</code> or <code>UsergroupId</code> must be passed to uniquely identify the UserGroup to which the user needs to belong.						
                                        </div>
                    </Description>
                </Element>
            </yfc:doc>
        </UserGroupList>
    </UserGroupLists>
    <SubUser>
        <User Activateflag="" BillingaddressKey="" BusinessKey=""
            ChangePassNext="" ContactaddressKey=""
            CreatorOrganizationKey="" DataSecurityGroupId=""
            DepartmentCode="" Imagefile="" Localecode="Required"
            Loginid="Required" Longdesc="" MenuId="" NoteKey=""
            OrganizationKey="" Password="Required" PreferenceKey=""
            Pwdlastchangedon="" SessionTimeout="" SuperUser="" Theme=""
            UserKey="" Username="Required" Usertype="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-20" Name="DepartmentCode"/>
                </Attributes>
            </yfc:doc>
            <ContactPersonInfo AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" ErrorTxt="" EveningFaxNo=""
                EveningPhone="" FirstName="" HttpUrl=""
                IsCommercialAddress="" JobTitle="" LastName=""
                MiddleName="" MobilePhone="" OtherPhone="" PersonID=""
                PersonInfoKey="" PreferredShipAddress="" State=""
                Suffix="" TaxGeoCode="" Title="" UseCount=""
                VerificationStatus="" ZipCode=""/>
            <BillingPersonInfo AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" ErrorTxt="" EveningFaxNo=""
                EveningPhone="" FirstName="" HttpUrl=""
                IsCommercialAddress="" JobTitle="" LastName=""
                MiddleName="" MobilePhone="" OtherPhone="" PersonID=""
                PersonInfoKey="" PreferredShipAddress="" State=""
                Suffix="" TaxGeoCode="" Title="" UseCount=""
                VerificationStatus="" ZipCode=""/>
            <QueueSubscriptionList>
                <QueueSubscription QueueKey="Required"
                    QueueSubscriptionKey="" yfc:DTDOccurrence="ZeroOrMany"/>
            </QueueSubscriptionList>
            <UserGroupLists>
                <UserGroupList UserGroupListKey="" UsergroupId=""
                    UsergroupKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute EntityName="User_Group"
                                Name="UsergroupId" NameInEntity="Usergroup_Id">
                                <Description>
                                    <div>
                                    <Validation>
                                    <Condition>Invalid <code>UsergroupId</code> passed.
                                                                        </Condition>
                                    <Error
                                    Description="Invalid Usergroup." ErrorCode="YCP0152"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                        <Element>
                            <Description>
                                <div>
                                                Either <code>UsergroupKey</code> or <code>UsergroupId</code> must be passed to uniquely identify the UserGroup to which the user needs to belong.						
                                                </div>
                            </Description>
                        </Element>
                    </yfc:doc>
                </UserGroupList>
            </UserGroupLists>
            <UserDelegateList>
                <UserDelegate ActiveFlag="" DelegateUserID=""
                    DelegationPurpose="" EffectiveEndDate=""
                    EffectiveStartDate="" IsPrimaryDelegate="" yfc:DTDOccurrence="ZeroOrMany"/>
            </UserDelegateList>
        </User>
    </SubUser>
    <UserDelegateList>
        <UserDelegate ActiveFlag="" DelegateUserID=""
            DelegationPurpose="" EffectiveEndDate=""
            EffectiveStartDate="" IsPrimaryDelegate="" yfc:DTDOccurrence="ZeroOrMany"/>
    </UserDelegateList>
</User>
