<DemandSummary FromDemandShipDate="" ItemID="Required"
    OrganizationCode="Required" ProductClass="" ShipNode=""
    ToDemandShipDate="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getDemandSummary/input">
    <yfc:doc>
        <Element EntityName="Inventory_Demand"/>
        <Attributes>
            <Attribute DataType="String" Name="FromDemandShipDate">
                <Description>
                    <div>
				The start date of the period for which the summary is requested.
				<br/>Default: As early as possible. The system restricts this to  no earlier than the year 1900.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="ItemID"/>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>
					Indicates the Organization.
				<Validation>
                            <Condition>Must be an existing <code>Organization</code>.
					</Condition>
                            <Error Description="Invalid organization." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
            <Attribute DataType="ShipNode" Name="ShipNode">
                <Description>
                    <div>
			 Specifies that this <code>ShipNode</code> is used in determining <code>Options</code>.
			<br/>If not passed, the list of ship nodes passed will be considered.  
			<br/>By Default, a summary of unassigned demands is returned, if both the <code>list</code> and the <code>ShipNode</code> is not passed.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="Date" Name="ToDemandShipDate">
                <Description>
                    <div>
				The end date of the period for which the summary is requested.
				<br/>Default: As far into the future as possible. The system restricts this to no later than the year 2500.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
        </Attributes>
    </yfc:doc>
    <ShipNodes>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					List of ShipNodes 
					</div>
                </Description>
            </Element>
        </yfc:doc>
        <ShipNode ShipNode="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Inventory_Demand">
                    <Description>
                        <div>
					ShipNode 
					</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="ShipNode" Name="ShipNode">
                        <Description>
                            <div>
						This <code>ShipNode</code> is used in determining <code>Options</code>.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ShipNode>
    </ShipNodes>
</DemandSummary>
