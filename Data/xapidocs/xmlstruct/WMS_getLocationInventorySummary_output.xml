<?xml version="1.0" encoding="UTF-8"?>
<LocationInventorySummary EnterpriseCode="" LocationId="Required"
    Node="Required" PendInPendOut="" TotalNoOfItems=""
    TotalValueOfInventory="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getLocationInventorySummary/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Key" EntityName="Inventory_Item"
                Name="EnterpriseCode" NameInEntity="Organization_Code">
                <Description>
                    <div> 
					Inventory Organization Code. The Enterprise whose product information is being stored.
						
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Location_Inventory"
                Name="LocationId" NameInEntity="Location_Id">
                <Description>
                    <div> 
					Specifies the location in which the inventory is stored.
						
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40"
                EntityName="Location_Inventory" Name="Node" NameInEntity="Node">
                <Description>
                    <div> Business key or unique identifier for a ship node.					
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="PendInPendOut">
                <Description>
                    <div> 
					Indicates whether the location has pend in or pend out.
						
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="TotalNoOfItems">
                <Description>
                    <div> 
					Gives the total number of inventory items at a location.
						
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="TotalValueOfInventory">
                <Description>
                    <div> 
					Gives the total value of inventory at a location.
						
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</LocationInventorySummary>
