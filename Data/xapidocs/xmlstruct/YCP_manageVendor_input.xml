<!-- manageVendor Input XML -->
<Vendor CommitmentTime="" FuncAckTime="" Operation=""
    OrganizationCode="Required" SellerOrganizationCode="" SendsAsn=""
    SendsCommitment="" SendsFuncAck="" VendorClassificationCode=""
    VendorID="" VendorKey="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageVendor/input">
    <yfc:doc>
        <Element EntityName="Vendor">
            <Description>
                <div>
				Specify the vendor information to be modified.  <code>VendorKey</code> or a combination of <code>OrganizationCode</code> and <code>VendorID</code> can be specified to identify a vendor.
				<Validation>
                        <Condition>If no vendor matches the criteria.
					</Condition>
                        <Error Description="No Record Found." ErrorCode="YFS10001"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>Operation to perform
						<br/>Valid Values: 
						<br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
						<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
						<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record. Please note that this action only deletes the Vendor record and not the parent Organizaton or Ship Nodes associated to the Vendor. 
						<br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
						</div>
                </Description>
            </Attribute>
            <Attribute Name="SellerOrganizationCode">
                <Description>
                    <div>The organization code of the seller.  This is required when modifying an existing vendor.  When creating a new vendor, the <code>SellerOrganization</code> element may be passed instead.
						</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="SendsAsn">
                <Description>
					Indicates,whether a supplier sends advance shipment notice.
					</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="SendsCommitment">
                <Description>
					Indicates,whether a supplier sends Commitment for a purchase order.
					</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="SendsFuncAck">
                <Description>
					Indicates,whether a supplier sends functional acknowledgment for a purchase order.
					</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SellerOrganization AccountWithHub="" AuthorityType=""
        BillingAddressKey="" CatalogOrganizationCode=""
        ContactAddressKey="" CorporateAddressKey=""
        CreatorOrganizationKey="" DunsNumber=""
        InventoryOrganizationCode="" InventoryPublished=""
        IsHubOrganization="" IsLegalEntity="" IsSourcingKept=""
        IssuingAuthority="" ItemXrefRule="" LocaleCode=""
        OrganizationCode="Required" OrganizationKey=""
        OrganizationName="" ParentOrganizationCode=""
        PaymentProcessingReqd="" PrimaryEnterpriseKey="Required"
        PrimarySicCode="" PrimaryUrl="" TaxExemptFlag=""
        TaxExemptionCertificate="" TaxJurisdiction="" TaxpayerId=""
        XrefAliasType="" XrefOrganizationCode="">
        <yfc:doc>
            <Element EntityName="Organization">
                <Description>
                    <div>
				Identify the organization or node which vendor represents.  <code>SellerOrganization</code> is required if <code>SellerOrganizationCode</code> attribute of an existing organization is not provided.  When creating a new vendor, the <code>SellerOrganization</code> element may be passed to create a new seller organization.
				<Validation>
                            <Condition>If <code>SellerOrganization</code> is not provided.
					</Condition>
                            <Error Description="Missing element in XML." ErrorCode="YFS10507"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Organization"
                    Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
					OrganizationCode which identifies an organization.
					<Validation>
                                <Condition>If <code>OrganizationCode</code> is not passed.
						</Condition>
                                <Error
                                    Description="Missing attribute in input." ErrorCode="YFS10460"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Organization"
                    Name="PrimaryEnterpriseKey" NameInEntity="Primary_Enterprise_Key">
                    <Description>
                        <div>
					Organizations may participate with multiple enterprises one of which is primary. This indicates the primary enterprise.
					<Validation>
                                <Condition>If <code>PrimaryEnterpriseKey</code> is not passed.
						</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Node AcceptanceRequired="" ActivateFlag=""
            AdvanceNotificationTime="" AgentCriteriaGroup=""
            CurrentBolNumber="" DcmIntegrationRealTime=""
            DefaultDeclaredValue="" ExportLicenseExpDate=""
            ExportLicenseNo="" GLN="" GenerateBolNumber=""
            IdentifiedByParentAs="" InheritConfigFromShipNode=""
            InterfaceSubType="" InterfaceSubTypeCode="" InterfaceType=""
            InventoryTracked="" Inventorytype="" IsFulfillmentNode=""
            IsItemBasedAllocationAllowed="" IsYard="" Latitude=""
            Longitude="" MaintainInventoryCost=""
            MaxDaysToScheduleBefore="" MinNotificationTime=""
            OwnerType="" PicklistType="" ProcureToShipAllowed=""
            ReceiptProcessingTime="" ReceivingCalendarKey=""
            ReceivingNode="" RequiresChangeRequest=""
            ReturnCenterFlag="" ReturnsNode="" SerialTracking=""
            ShipNode="" ShipNodeClass="" ShipnodeType=""
            ShippingCalendarKey="" ShippingNode="" StoreInvConfig=""
            SupplierKey="" ThreePlNode="" TimeDiff=""
            UseEndOfShiftTime="" XDocNode="">
            <yfc:doc>
                <Element EntityName="Ship_Node"/>
                <Attributes>
                    <Attribute DataType="NumberOfDays" Name="AdvanceNotificationTime">
                        <Description>
                            <div>
                                <b>Deprecated.</b> See <code>manageNodeNotification</code> API. The number of 
					days before which the ship node needs notification of an impending receipt or shipment.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="InterfaceSubTypeCode"/>
                    <Attribute DataType="Flag" Name="IsFulfillmentNode">
                        <Description>
                            <div>Indicates if this node is a fulfillment node.
						<br/>Valid Values: <code>Y</code>, <code>N</code>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IsYard">
                        <Description>
                            <div>Indicate if this node is a yard.
						<br/>Valid Values: <code>Y</code>, <code>N</code>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="NumberOfDays" Name="MinNotificationTime">
                        <Description>
                            <div>
                                <b>Deprecated.</b> See <code>manageNodeNotification</code> API. The number of 
					days before which the communication to ship the order to the shipping node or supplier should be 
					made. This field is used to determine the earliest ship date on the line.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Node>
        <CorporatePersonInfo AddressID="" AddressLine1=""
            AddressLine2="" AddressLine3="" AddressLine4=""
            AddressLine5="" AddressLine6="" AlternateEmailID=""
            Beeper="" City="" Company="" Country="" DayFaxNo=""
            DayPhone="" Department="" EMailID="" ErrorTxt=""
            EveningFaxNo="" EveningPhone="" FirstName="" HttpUrl=""
            JobTitle="" LastName="" MiddleName="" MobilePhone=""
            OtherPhone="" PersonID="" PreferredShipAddress="" State=""
            Suffix="" Title="" UseCount="" VerificationStatus="" ZipCode="">
            <yfc:doc>
                <Element EntityName="Person_Info"/>
            </yfc:doc>
        </CorporatePersonInfo>
        <ContactPersonInfo AddressID="" AddressLine1="" AddressLine2=""
            AddressLine3="" AddressLine4="" AddressLine5=""
            AddressLine6="" AlternateEmailID="" Beeper="" City=""
            Company="" Country="" DayFaxNo="" DayPhone="" Department=""
            EMailID="" ErrorTxt="" EveningFaxNo="" EveningPhone=""
            FirstName="" HttpUrl="" JobTitle="" LastName=""
            MiddleName="" MobilePhone="" OtherPhone="" PersonID=""
            PreferredShipAddress="" State="" Suffix="" Title=""
            UseCount="" VerificationStatus="" ZipCode="">
            <yfc:doc>
                <Element EntityName="Person_Info"/>
            </yfc:doc>
        </ContactPersonInfo>
        <BillingPersonInfo AddressID="" AddressLine1="" AddressLine2=""
            AddressLine3="" AddressLine4="" AddressLine5=""
            AddressLine6="" AlternateEmailID="" Beeper="" City=""
            Company="" Country="" DayFaxNo="" DayPhone="" Department=""
            EMailID="" ErrorTxt="" EveningFaxNo="" EveningPhone=""
            FirstName="" HttpUrl="" JobTitle="" LastName=""
            MiddleName="" MobilePhone="" OtherPhone="" PersonID=""
            PreferredShipAddress="" State="" Suffix="" Title=""
            UseCount="" VerificationStatus="" ZipCode="">
            <yfc:doc>
                <Element EntityName="Person_Info"/>
            </yfc:doc>
        </BillingPersonInfo>
    </SellerOrganization>
</Vendor>
