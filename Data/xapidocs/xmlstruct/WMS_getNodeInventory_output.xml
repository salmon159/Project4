<?xml version="1.0" encoding="UTF-8"?>
<NodeInventory EnterpriseCode="" InventoryOrganizationCode=""
    Node="Required" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getNodeInventory/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Organization" Name="EnterpriseCode" NameInEntity="Organization_Code"/>
            <Attribute EntityName="Organization"
                Name="InventoryOrganizationCode" NameInEntity="Inventory_Organization_Code"/>
            <Attribute EntityName="Location_Inventory" Name="Node" NameInEntity="Node_Key"/>
        </Attributes>
    </yfc:doc>
    <LocationInventoryList TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                    <Description>Indicates total number of records fetched.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <LocationInventory InventoryItemKey="" LocationId="Required"
            MultipleSummaryAttributes="" PendInQty="" PendOutQty=""
            Quantity="Required" ZoneId="Required" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Location_Inventory"/>
                <Attributes>
                    <Attribute DataType="Flag" Name="MultipleSummaryAttributes">
                        <Description>Indicates if multiple records exist for the summary attributes </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="PendInQty">
                        <Description>Indicates the total Quantity of the Item Pending In to this Location in the Node</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="PendOutQty">
                        <Description>Indicates the total Quantity of the Item Pending Out Of this Location in the Node</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="Quantity">
                        <Description>Indicates the total Quantity of the Item in this Location in the Node</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <InventoryItem ItemID="" OrganizationCode="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Inventory_Item"/>
                    <Attributes>
                        <Attribute DataType="String"
                            EntityName="Organization"
                            Name="OrganizationCode" NameInEntity="Inventory_Org_Code"/>
                    </Attributes>
                </yfc:doc>
                <Item ItemID="" ItemKey="" OrganizationCode="" UnitOfMeasure="">
                    <yfc:doc>
                        <Element EntityName="Item"/>
                    </yfc:doc>
                    <PrimaryInformation Description="" ShortDescription=""/>
                </Item>
            </InventoryItem>
            <SummaryAttributes CaseId="" CountryOfOrigin="" FifoNo=""
                InventoryItemKey="" InventoryStatus=""
                InventoryTagKey="" PalletId="" ReceiptHeaderKey=""
                Segment="" SegmentType="" ShipByDate="">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory"/>
                <yfc:doc>
                    <Element EntityName="Location_Inventory"/>
                    <Attributes>
                        <Attribute DataType="LPNNo" Name="CaseId">
                            <Description>Indicates the Id of the Case which contains the Item</Description>
                        </Attribute>
                        <Attribute DataType="LPNNo" Name="PalletId">
                            <Description>Indicates the Id of the Pallet which contains the Item or contains the Case containing the Item</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Receipt ReceiptNo="">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/Receipt"/>
                    <yfc:doc>
                        <Element EntityName="Receipt_Header"/>
                    </yfc:doc>
                </Receipt>
                <TagSummary BatchNo="" LotAttribute1="" LotAttribute2=""
                    LotAttribute3="" LotKeyReference="" LotNumber=""
                    ManufacturingDate="" RevisionNo="" TagNumber="">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/TagDetail"/>
                    <yfc:doc>
                        <Element EntityName="Inventory_Tag"/>
                        <Attributes>
                            <Attribute EntityName="Inventory_Tag" Name="TagNumber"/>
                        </Attributes>
                    </yfc:doc>
                </TagSummary>
            </SummaryAttributes>
            <ItemInventoryDetailList TotalNumberOfRecords="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Count" Name="TotalNumberOfRecords">
                            <Description>Indicates total number of records fetched.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <ItemInventoryDetail CaseId="" CountryOfOrigin=""
                    FifoNo="" InventoryItemKey="" InventoryStatus=""
                    InventoryTagKey="" PalletId="" PendInQty=""
                    PendOutQty="" Quantity="" ReceiptHeaderKey=""
                    Segment="" SegmentType="" ShipByDate="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory"/>
                    <yfc:doc>
                        <Element EntityName="Location_Inventory"/>
                        <Attributes>
                            <Attribute DataType="LPNNo" Name="CaseId">
                                <Description>Indicates the Id of the Case which contains the Item</Description>
                            </Attribute>
                            <Attribute DataType="LPNNo" Name="PalletId">
                                <Description>Indicates the Id of the Pallet which contains the Item or contains the Case containing the Item</Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="PendInQty">
                                <Description>Indicates the Quantity of the Item Pending In to this Location in the Node</Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="PendOutQty">
                                <Description>Indicates the Quantity of the Item Pending Out Of this Location in the Node</Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="Quantity">
                                <Description>Indicates the Quantity of the inventory </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <Receipt ReceiptNo=""/>
                    <TagDetail BatchNo="" LotAttribute1=""
                        LotAttribute2="" LotAttribute3=""
                        LotKeyReference="" LotNumber=""
                        ManufacturingDate="" RevisionNo="" TagNumber="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:includeInParent IncludeXMLChildNodes="N"
                            IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/TagDetail"/>
                        <yfc:doc>
                            <Element EntityName="Inventory_Tag"/>
                            <Attributes>
                                <Attribute EntityName="Inventory_Tag" Name="TagNumber"/>
                            </Attributes>
                        </yfc:doc>
                    </TagDetail>
                    <SerialList>
                        <SerialDetail SecondarySerial1=""
                            SecondarySerial2="" SecondarySerial3=""
                            SecondarySerial4="" SecondarySerial5=""
                            SecondarySerial6="" SecondarySerial7=""
                            SecondarySerial8="" SecondarySerial9=""
                            SerialNo="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:includeInParent
                                IncludeXMLChildNodes="N"
                                IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/SerialList/SerialDetail"/>
                            <yfc:doc>
                                <Element EntityName="Global_Serial_Num"/>
                            </yfc:doc>
                        </SerialDetail>
                    </SerialList>
                </ItemInventoryDetail>
            </ItemInventoryDetailList>
        </LocationInventory>
    </LocationInventoryList>
    <LPNList TotalNumberOfRecords="">
        <yfc:doc>
            <Element>
                <Description>Displays a list of all LPNs lying directly on the Set of Locations selected (i.e only OuterMostLPNs). If The search is based on a LPN, list of LPNs lying directly on the LPN are displayed.</Description>
            </Element>
            <Attributes>
                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                    <Description>Indicates total number of records fetched.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <LPN CaseId="" CaseQuantity="" DimensionUOM="" Height=""
            Length="" OrganizationCode="" PalletId=""
            ReceiptHeaderKey="" Weight="" WeightUOM="" Width="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="LPN_Hdr"/>
                <Attributes>
                    <Attribute DataType="LPNNo" Name="CaseId">
                        <Description>A CaseId </Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="PalletId">
                        <Description>A PalletId </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Receipt ReceiptNo=""/>
            <LPNLocation LocationId="" ZoneId="">
                <yfc:doc>
                    <Element EntityName="LPN_Location"/>
                </yfc:doc>
            </LPNLocation>
            <ParentLPN CaseId="" PalletId="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="LPNNo" Name="CaseId">
                            <Description>A CaseId </Description>
                        </Attribute>
                        <Attribute DataType="LPNNo" Name="PalletId">
                            <Description>A PalletId </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ParentLPN>
            <OuterMostLPN CaseId="" PalletId="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="LPNNo" Name="CaseId">
                            <Description>A CaseId </Description>
                        </Attribute>
                        <Attribute DataType="LPNNo" Name="PalletId">
                            <Description>A PalletId </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </OuterMostLPN>
        </LPN>
    </LPNList>
</NodeInventory>
