<MasterCatalogList xmlns="http://www.sterlingcommerce.com/documentation/YCM/modifyMasterCatalog/input">
    <MasterCatalog Action="" Description="" MasterCatalogID=""
        MasterCatalogKey="" OrganizationCode="" ShortDescription="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
                    Every organization can have multiple master catalogs. Each item defined by the organization belongs to one of these master catalogs.
                        <Validation>
                            <Condition>
                            Either the <code>MasterCatalogKey</code> attribute or both the <code>OrganizationCode</code> and <code>MasterCatalogID</code> attributes must be passed and must be valid. If not passed or not valid:
                            </Condition>
                            <Error
                                Description="Master Catalog Not Found" ErrorCode="YCM0037"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="YCMAction" Name="Action">
                    <Description>Action to take</Description>
                </Attribute>
                <Attribute Name="MasterCatalogID">
                    <Description>
                        <div>
                        A unique identifier for an organization's master catalog.
                            <Validation>
                                <Condition>
                                The <code>MasterCatalogID</code> attribute cannot be modified. If modification is attempted:
                                </Condition>
                                <Error
                                    Description="Primary Key or Unique Keys cannot be modified" ErrorCode="YCM0040"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute Name="OrganizationCode">
                    <Description>
                        <div>
                        The organization that owns a master catalog.
                            <Validation>
                                <Condition>
                                The <code>OrganizationCode</code> attribute cannot be modified. If modification is attempted:
                                </Condition>
                                <Error
                                    Description="Primary Key or Unique Keys cannot be modified" ErrorCode="YCM0040"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </MasterCatalog>
</MasterCatalogList>
