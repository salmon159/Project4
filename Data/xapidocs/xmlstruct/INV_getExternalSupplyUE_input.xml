<!--getExternalSupply Input XML-->
<Items xmlns="http://www.sterlingcommerce.com/documentation/INV/getExternalSupplyUE/input">
    <Item FromETA="" ItemID="Required" OrganizationCode="Required"
        ProductClass="" ToETA="" UnitOfMeasure="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Element EntityName="Inventory_Item"/>
            <Attributes>
                <Attribute EntityName="Inventory_Supply" Name="FromETA" NameInEntity="Eta">
                    <Description>
                        <div>
				The from date for the date range of the first available dates of inventory supplies.  It is expected that the user exit would return back the correct supply picture with <code>ETA</code> between the <code>FromETA</code> and <code>ToETA</code>, including onhand supplies with a low date of <code>19000101</code> as the <code>ETA</code>.
				</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inventory_Item"
                    Name="OrganizationCode" NameInEntity="Organization_Code"/>
                <Attribute EntityName="Inventory_Supply" Name="ToETA" NameInEntity="Eta">
                    <Description>
                        <div>
				The to date for the date range of the first available dates of inventory supplies.  It is expected that the user exit would return back the correct supply picture with <code>ETA</code> between the <code>FromETA</code> and <code>ToETA</code>, including onhand supplies with a low date of <code>19000101</code> as the <code>ETA</code>.
				</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ShipNodes>
            <ShipNode ShipNode="Required" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply"/>
                </yfc:doc>
            </ShipNode>
        </ShipNodes>
        <Tags>
            <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotKeyReference="" LotNumber=""
                ManufacturingDate="" RevisionNo="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Tag"/>
                </yfc:doc>
            </Tag>
        </Tags>
        <Segments>
            <Segment Segment="" SegmentType="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply"/>
                </yfc:doc>
            </Segment>
        </Segments>
    </Item>
</Items>
