<RoutingGuide BuyerOrganizationCode="Required" EnterpriseCode="Required"
    ExpectedDeliveryDate="" ExpectedShipDate="Required" IgnoreCosting=""
    LoadNo="" SellerOrganizationCode="Required" ShipmentNo="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/determineRouting/input">
    <yfc:doc>
        <Attributes>
            <Attribute Name="BuyerOrganizationCode">
                <Description>
							Buyer Organization Code on the Shipment/Load
						</Description>
            </Attribute>
            <Attribute Name="EnterpriseCode">
                <Description>
							Enterprise Code on the Shipment/Load
						</Description>
            </Attribute>
            <Attribute DataType="Date" Name="ExpectedDeliveryDate">
                <Description>
                    <div>
      Expected Delivery Date of the Shipment.
      </div>
                </Description>
            </Attribute>
            <Attribute DataType="Date" Name="ExpectedShipDate">
                <Description>
                    <div>
				This value should fall between <code>EffectiveDate</code> and <code>ExpirationDate</code> of the routing guide for a routing guide to get picked up.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreCosting">
                <Description>
                    <div>
				If the user wants to skip calling getTransportationCostUE this value can be set to 'Y'
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Load" Name="LoadNo" NameInEntity="Load_No">
                <Description>
                    <div>Indicates the Number of the Load, for which routing is being performed.</div>
                </Description>
            </Attribute>
            <Attribute Name="SellerOrganizationCode">
                <Description>
							Seller Organization Code on the Shipment/Load
						</Description>
            </Attribute>
            <Attribute EntityName="Shipment" Name="ShipmentNo" NameInEntity="Shipment_No">
                <Description>
                    <div>Indicates the Number of the Shipment, for which routing is being performed.</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SelectionCriteria DepartmentCode="" FreightTerms="" ItemClassification=""/>
    <RoutingGuideDetails>
        <RoutingGuideDetail>
            <ShipFrom ShipFromCity="Conditional"
                ShipFromCountry="Conditional" ShipFromNode="Conditional"
                ShipFromState="Conditional" ShipFromZip="Conditional" ShipToConsolNode="Conditional">
                <yfc:doc>
                    <Element EntityName="Routing_Guide_Detail">
                        <Description>
                            <div>
						Origin Address information for which we need to determine SCAC and Carrier Service Code.
						<Validation>
                                    <Condition> An exception will be thrown if all the attributes in the <code>ShipFrom</code> element are void. </Condition>
                                    <Error
                                    Description="at least one ShipFrom attributes must be passed." ErrorCode="YDM000140"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                </yfc:doc>
            </ShipFrom>
            <ShipTo AddressLine6="" Company="" IsCommercialAddress=""
                ShipToCity="Conditional" ShipToCountry="Conditional"
                ShipToNode="Conditional" ShipToState="Conditional"
                ShipToStore="Conditional" ShipToZip="Conditional" TaxGeoCode="">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
						Destination Address information for which we need to determine SCAC and Carrier Service Code.
						<Validation>
                                    <Condition> An exception will be thrown if all the attributes in the <code>ShipTo</code> element are void. </Condition>
                                    <Error
                                    Description="at least one ShipTo attributes must be passed." ErrorCode="YDM000141"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Address"
                            EntityName="Person_Info" Name="AddressLine6" NameInEntity="Address_Line6"/>
                        <Attribute EntityName="Person_Info"
                            Name="Company" NameInEntity="Company">
                            <Description>
                                <div>
The company this address is associated with.
</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag"
                            EntityName="Person_Info"
                            Name="IsCommercialAddress" NameInEntity="Is_Commercial_Address">
                            <Description>
                                <div>Indicates whether the address is commercial, residential or none.
<br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b> or <code>blank</code>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Person_Info"
                            Name="TaxGeoCode" NameInEntity="Tax_Geo_Code"/>
                    </Attributes>
                </yfc:doc>
            </ShipTo>
            <ShippingParameters HandlingUnits=""
                InputCarrierServiceCode="" ShipmentType="" Volume=""
                VolumeUOM="" Weight="" WeightUOM=""/>
        </RoutingGuideDetail>
    </RoutingGuideDetails>
</RoutingGuide>
