<!-- getExternalOrderLineArchiveInputUE Output XML -->
<ExternalOrderLineArchiveData xmlns:yfc="http://www.yantra.com/documentation">
    <yfc:doc>
        <Element EntityName="ExternalOrderLineArchiveData">
            <Description>
                <div>
  					ExternalOrderLineArchiveData holds the list of custom entities related to Order Line for which entity relationship is not defined in IBM Sterling Order Management System.
  					<Validation>
                        <Condition>If UE returns null or invalid output document.
						</Condition>
                        <Error
                            Description="User Exit output xml is null or invalid." ErrorCode="OSI10_UE01"/>
                    </Validation>
                </div>
            </Description>
        </Element>
    </yfc:doc>
    <Entity>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
	           Each Entity element should provide the data related to the same custom table. To provide the data from multiple custom tables, Entity element should be repeated with related custom table details and data.
	         </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="String" Name="TableName">
                    <Description>
                        <div>
                      Table Name of the custom entity related to Order Line for which entity relationship is not defined in IBM Sterling Order Management System.
                   </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Entity>
</ExternalOrderLineArchiveData>
