<Query Action="" OrganizationCode="" QueryDefinition="" QueryKey=""
    QueryName="" QueryType="" TableType="!TableType!" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageQuery/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Action" Name="Action">
                <Description>
                    <div>Action to take
					<br/>Valid Values: 
					<br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist or modifies an existing record if it does.
					<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
					<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Changes the value of an existing record.
					<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
					<br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" EntityName="Query"
                Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
					The organization that owns this query.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="TableType"/>
        </Attributes>
        <Element EntityName="Query">
            <Description>
                <div>Master table for Queries
				<Validation>
                        <Condition>If <code>Action</code> is <b>
                                <code>Modify</code>
                            </b> or <b>
                                <code>Delete</code>
                            </b> and no record matches the criteria:
					</Condition>
                        <Error
                            Description="Record does not exist in the database." ErrorCode="YFC0002"/>
                    </Validation>
                </div>
            </Description>
        </Element>
    </yfc:doc>
</Query>
