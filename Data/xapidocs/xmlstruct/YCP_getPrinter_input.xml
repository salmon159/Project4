<GetPrinter PrintDocumentId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getPrinter/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Print_Document"
                Name="PrintDocumentId" NameInEntity="Print_Document_Id">
                <Description>
                    <div>
					The Document to print.
						<Validation>
                            <Condition>
                                <code>PrintDocumentId</code> should be a valid document.
							</Condition>
                            <Error
                                Description="Invalid Print Document Identifier." ErrorCode="YCP0185"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PrinterPreference OrganizationCode="Required" PrinterId=""
        UserId="" UsergroupId="" WorkStationId="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="User_Printer_Preference"/>
            <Attributes>
                <Attribute EntityName="Device" Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
						The Organization for which the printer has to be determined.
						<Validation>
                                <Condition>One of <code>PrinterId</code> or <code>OrganizationCode</code> needs to be passed.<code>OrganizationCode</code> should be a valid Organization.
							</Condition>
                                <Error
                                    Description="Invalid Organization Code." ErrorCode="YCP0040"/>
                                <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="PrinterId">
                    <Description>
                        <div>
						Unique identifier of Printer.
							<Validation>
                                <Condition>One of <code>PrinterId</code> or <code>OrganizationCode</code> needs to be passed. If specified <code>PrinterId</code> should be an existing printer that is capable of printing the specified document.
								</Condition>
                                <Error
                                    Description="Invalid device ID for the printer." ErrorCode="YCP0176"/>
                                <Error
                                    Description="The printer is not configured to print the specified document." ErrorCode="YCP0177"/>
                                <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Equipment" Name="WorkStationId" NameInEntity="Equipment_Id">
                    <Description> The Work Station with which the printer is associated. </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </PrinterPreference>
</GetPrinter>
