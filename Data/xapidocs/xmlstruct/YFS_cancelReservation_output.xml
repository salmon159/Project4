<!--cancelReservation Output XML-->
<CancelReservation CancelledQty="" DeliveryMethod="" InventoryTagKey=""
    ItemID="" OrganizationCode="" ProductClass="" QtyToBeCancelled=""
    ReservationID="" Segment="" ShipNode="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/cancelReservation/output">
    <yfc:doc>
        <Element EntityName="Inventory_Reservation"/>
        <Attributes>
            <Attribute DataType="Quantity" Name="CancelledQty">
                <Description>
                    <div>
						Quantity of cancelled items.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="ItemID">
                <Description>
                    <div>
						The item ID associated with this reservation.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item"
                Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
						Inventory Organization code.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="ProductClass">
                <Description>
                    <div>
						The product class of the item.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="QtyToBeCancelled">
                <Description>
                    <div>
						Quantity to be cancelled.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Reservation"
                Name="ReservationID" NameInEntity="Reservation_Id">
                <Description>
                    <div>
						Identification of a reservation.  
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Reservation"
                Name="ShipNode" NameInEntity="Shipnode_Key">
                <Description>
                    <div>
						The ship node for the inventory items.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure">
                <Description>
                    <div>
						Item's Unit of measure.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Tag BatchNo="" LotNumber="" RevisionNo="">
        <yfc:doc>
            <Element EntityName="Inventory_Tag"/>
            <Attributes>
                <Attribute EntityName="Inventory_Tag" Name="BatchNo" NameInEntity="batch_no"/>
                <Attribute EntityName="Inventory_Tag" Name="LotNumber" NameInEntity="lot_number"/>
                <Attribute EntityName="Inventory_Tag" Name="RevisionNo" NameInEntity="revision_no"/>
            </Attributes>
        </yfc:doc>
    </Tag>
</CancelReservation>
