<Coupon CouponID="Required" OrganizationCode="Required" xmlns="http://www.sterlingcommerce.com/documentation/YPM/assignCouponCode/input">
    <yfc:doc>
        <Element EntityName="Coupon"/>
        <Attributes>
            <Attribute DataType="CouponId" Name="CouponID">
                <Description>
                    <div>Unique identifier of a coupon.
						<Validation>
                            <Condition>
                                <code>CouponID</code> must be passed.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                if <code>CouponID</code> does not matching any existing coupons.
							</Condition>
                            <Error Description="Coupon not found" ErrorCode="YPM10169"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>Pricing organization the <code>CouponID</code> belongs to.
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> must be passed.
                            </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not valid.
                            </Condition>
                            <Error
                                Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <CouponCodeList yfc:DTDOccurrence="REQUIRED">
        <CouponCode CouponCode="Required" CustomerContactID="" CustomerID="">
            <yfc:doc>
                <Element EntityName="Coupon_Code"/>
                <Attributes>
                    <Attribute EntityName="Coupon_Code"
                        Name="CouponCode" NameInEntity="Coupon_Code">
                        <Description>
                            <div>Unique coupon code.
						<Validation>
                                    <Condition>
                                    <code>CouponCode</code> must be passed.
							</Condition>
                                    <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </CouponCode>
    </CouponCodeList>
</Coupon>
