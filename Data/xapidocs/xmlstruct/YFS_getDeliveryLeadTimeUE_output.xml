<GetDeliveryLeadTime DeliveryLeadTime="" OverrideTransferSchedule=""
    TransitUnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getDeliveryLeadTimeUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Number" Name="DeliveryLeadTime">
                <Description>
                    <div>
				Transit time between from address and to address.  UOM for time is specified by TransitUOM
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="OverrideTransferSchedule">
                <Description>
                    <div>
				If passed Y, if Transfer Schedule exist for the Address, It will be overridden and Transit days from UE will be used. 
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-24" Name="TransitUnitOfMeasure">
                <Description>
                    <div>
				UOM for delivery lead time.  If passed as "HR", delivery lead time will be considered in hours. Otherwise, it will be considered in days.  The UOM will be applicable to all lead time fields, including those for any carrier services specified. 
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PossibleCarrierServices>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
						Carrier services available.
					</div>
                </Description>
            </Element>
        </yfc:doc>
        <CarrierService CarrierServiceCode="" DeliveryLeadTime=""
            RejectCarrierService="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Carrier_Service"/>
                <Attributes>
                    <Attribute DataType="Number" Name="DeliveryLeadTime">
                        <Description>
                            <div>
							Transit time between from address and to address for the carrier service. UOM for time is specified by TransitUnitOfMeasure
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="RejectCarrierService"/>
                </Attributes>
            </yfc:doc>
        </CarrierService>
    </PossibleCarrierServices>
</GetDeliveryLeadTime>
