<!-- preReceiveLPN Input XML -->
<!-- CaseId or PalledId is required -->
<Receipt CaseId="" DocumentType="Required" LocationId="" PalletId=""
    ReceiptHeaderKey="" ReceiptNo="" ReceivingDock=""
    ReceivingNode="Required" ShipmentKey="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/preReceiveLPN/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description> Built container or an expected container. </Description>
            </Attribute>
            <Attribute EntityName="Document_Params" Name="DocumentType"/>
            <Attribute DataType="LocationId" Name="LocationId">
                <Description> Receiving location for the license plate container. If not specified, then the receiving location will be the receipt's receiving dock.</Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description> Built container or an expected container. </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ParentLPN CaseId="" PalletId="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description> Parent CaseId. </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description> Parent PalletId. </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </ParentLPN>
    <Shipment CarrierServiceCode="Conditional" DocumentType=""
        EnterpriseCode="" ExpectedDeliveryDate="" OrderHeaderKey=""
        OrderNo="" OrderReleaseKey="" ReleaseNo="" ShipNode="" ShipmentNo=""/>
</Receipt>
