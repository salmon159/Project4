<Order xmlns="http://www.sterlingcommerce.com/documentation/OMP/determineBestMultistopTransferUE/input">
    <Lines TotalNumberOfRecords="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                    <Description>The total number of lines.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Line ItemID="" LineId="Required" ProductClass=""
            UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Order_Line"/>
                <Attributes>
                    <Attribute DataType="Text-40" Name="LineId">
                        <Description>
                            <div>Unique identifier for the line. It is mandatory to pass this attribute.
	                        </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <MultiStopTransfers TotalNumberOfRecords="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Count" Name="TotalNumberOfRecords">
                            <Description>Total number of routes.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <MultiStopTransfer DestinationNode="Required"
                    OriginNode="Required" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="ShipNode" Name="DestinationNode">
                                <Description>The final node where inventory will be arriving.</Description>
                            </Attribute>
                            <Attribute DataType="ShipNode" Name="OriginNode">
                                <Description>The initial node where inventory is being shipped from.</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <MultiStopTransferOptions TotalNumberOfRecords="">
                        <yfc:doc>
                            <Element>
                                <Description>The list of <code>MultiStopTransferOption</code>s in order from best
								option to worst option.</Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                                    <Description>The total number of multistop transfer options.</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <MultiStopTransferOption yfc:DTDOccurrence="ZeroOrMany">
                            <Stops TotalNumberOfRecords="">
                                <yfc:doc>
                                    <Element>
                                    <Description>The <code>Stop</code>s that this <code>MultiStopTransferOption</code> will take.
										The stops are garunteed to be in the correct order.</Description>
                                    </Element>
                                    <Attributes>
                                    <Attribute DataType="Count" Name="TotalNumberOfRecords">
                                    <Description>The total number of stops made for the multistop transfer option.</Description>
                                    </Attribute>
                                    </Attributes>
                                </yfc:doc>
                                <Stop Node="" yfc:DTDOccurrence="ZeroOrMany">
                                    <yfc:doc>
                                    <Attributes>
                                    <Attribute
                                    DataType="ShipNode" Name="Node">
                                    <Description>The node for which the stop will be made.</Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                </Stop>
                            </Stops>
                        </MultiStopTransferOption>
                    </MultiStopTransferOptions>
                </MultiStopTransfer>
            </MultiStopTransfers>
        </Line>
    </Lines>
</Order>
