<EntitlementRuleAssignmentList TotalEntitlementAssignmentList=""
    TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getEntitlementAssignmentList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Count" Name="TotalEntitlementAssignmentList">
                <Description>Number of records returned in the output</Description>
            </Attribute>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>Number of Records in the DB satisfying the passed criteria. It must be provided in the output template if <code>NEXTPAGE</code> pagination strategy is used.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <EntitlementRuleAssignment AssignedToOrganizationCode=""
        CustomerID="" CustomerLevel="" CustomerType=""
        OrganizationCode="" Purpose="" Region="" RelationshipType=""
        Vertical="" yfc:DTDOccurrence="ZeroOrMany">
        <EntitlementRule Description="" EffectiveFromDate=""
            EffectiveToDate="" EntitlementRuleID=""
            EntitlementRuleKey="" OrganizationCode=""
            StartWithAllCatalogItems="" yfc:DTDOccurrence="ZeroOrMany">
            <EntitlementRuleDetailList TotalNumberOfRecords="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Count" Name="TotalNumberOfRecords">
                            <Description>Number of Records in the DB satisfying the passed criteria.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <EntitlementRuleDetail EntitlementRuleDetailKey=""
                    EntitlementRuleKey="" InclusionFlag="" SequenceNo=""
                    SourceType="" yfc:DTDOccurrence="ZeroOrMany">
                    <Category CategoryKey="" CategoryPath=""/>
                    <AttributeValue AttributeDomainID=""
                        AttributeGroupID="" AttributeID=""
                        DoubleValue="" IntegerValue="" Value=""/>
                    <EntitlementRuleDetailItemList TotalNumberOfRecords="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                                    <Description>Number of Records in the DB satisfying the passed criteria.</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <EntitlementRuleDetailItem ItemID=""
                            UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany"/>
                    </EntitlementRuleDetailItemList>
                </EntitlementRuleDetail>
            </EntitlementRuleDetailList>
        </EntitlementRule>
        <Customer AggregateStatus="" AssignedReps=""
            BuyerOrganizationCode="" CanConsumeSupplementalCapacity=""
            CommitmentTime="" CustomerAssignedVendorID=""
            CustomerClassificationCode="" CustomerID="" CustomerKey=""
            CustomerLevel="" CustomerRewardsNo="" CustomerType=""
            ExternalCustomerID="" FuncAckTime="" MaxAssignRepsOrTeams=""
            OrganizationCode="" RegisteredDate="" RelationshipType=""
            SendsAsn="" SendsCommitment="" SendsFuncAck=""
            SlotPreferenceType="" Status="" Vertical="" yfc:DTDOccurrence="REQUIRED">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterCustomerOutput.xml" IncludeXMLPath="/Customer"/>
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="AggregateStatus">
                        <Description>
                            <div>
                                <code>AggregateStatus</code> is the resulting <code>Status</code> for the customer or contact after taking into consideration the <code>Status</code> of its customer hierarchy.  It will only go up the hierarchy to compare statuses.  The highest <code>Status</code> value (10,20,30) will always take priority. 
						Examples: 
						If the customer's <code>Status</code> is 10 (Active) and parent's <code>Status</code> is 30 (Inactive), then the current customer's <code>AggregateStatus</code> will be 30.  
						If the customer's <code>Status</code> is 20 (On Hold), parent's <code>Status</code> is 10 (Active), and child's <code>Status</code> is 30 (Inactive), then the customer's <code>AggregateStatus</code> will be 20.  </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <BuyerOrganization AccountWithHub="" AuthorityType=""
                BillingAddressKey="" CatalogOrganizationCode=""
                ContactAddressKey="" CorporateAddressKey=""
                CreatorOrganizationKey="" DunsNumber=""
                InventoryOrganizationCode="" InventoryPublished=""
                IsBuyer="" IsHubOrganization="" IsLegalEntity=""
                IsNode="" IsSourcingKept="" IssuingAuthority=""
                ItemXrefRule="" LocaleCode="" OrganizationCode=""
                OrganizationKey="" OrganizationName=""
                ParentOrganizationCode="" PaymentProcessingReqd=""
                PrimaryEnterpriseKey="" PrimarySicCode="" PrimaryUrl=""
                TaxExemptFlag="" TaxExemptionCertificate=""
                TaxJurisdiction="" TaxpayerId="" XrefAliasType="" XrefOrganizationCode="">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="MasterCustomerOutput.xml" IncludeXMLPath="/Customer/BuyerOrganization"/>
            </BuyerOrganization>
        </Customer>
        <Region CanAggregate="" ParentRegionKey="" RegionDescription=""
            RegionKey="" RegionLevelName="" RegionName="" yfc:DTDOccurrence="REQUIRED">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="YCP_getRegionDetails_output.xml" IncludeXMLPath="/Region"/>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
				This table contains information about the Region definition. A region schema key must be given to indicate the schema with which the Region is associated.
				</div>
                    </Description>
                </Element>
            </yfc:doc>
            <RegionSchema Country="" OrganizationCode=""
                RegionSchemaKey="" RegionSchemaName=""/>
            <ParentRegion CanAggregate="" ParentRegionKey=""
                RegionDescription="" RegionKey="" RegionLevelName="" RegionName="">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
					This table contains information about the Region definition. A region schema key must be given to indicate the schema with which the Region is associated.
					</div>
                        </Description>
                    </Element>
                </yfc:doc>
            </ParentRegion>
            <ZipCodeRanges>
                <ZipCodeRange Country="" FromZip="" ToZip="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Region_Detail"/>
                    </yfc:doc>
                </ZipCodeRange>
            </ZipCodeRanges>
        </Region>
        <AssignedToOrganization AccountWithHub="" ActivateFlag=""
            AssignedColonyId="" AuthorityType="" BillingAddressKey=""
            CapacityOrganizationCode="" CatalogOrganizationCode=""
            ContactAddressKey="" CorporateAddressKey=""
            CreatorOrganizationKey="" CustomerMaintainedExternally=""
            CustomerMasterOrganizationCode="" DunsNumber=""
            InheritOrgConfig="" InventoryKeptExternally=""
            InventoryOrganizationCode="" InventoryPublished=""
            IsHubOrganization="" IsLegalEntity="" IsSourcingKept=""
            IsTemplate="" IssuingAuthority="" ItemXrefRule=""
            LocaleCode="" OrganizationCode="" OrganizationKey=""
            OrganizationName="" OrganizationSuffix=""
            ParentOrganizationCode="" PasswordPolicyKey=""
            PaymentProcessingReqd="" PricingOrganizationCode=""
            PrimaryEnterpriseKey="" PrimarySicCode="" PrimaryUrl=""
            ResourceIdentifier="" StoreConfigOrganizationCode=""
            SupplyDemandKeptExternally="" SupplyKeptExternally=""
            TaxExemptFlag="" TaxExemptionCertificate=""
            TaxJurisdiction="" TaxpayerId="" XrefAliasType="" XrefOrganizationCode="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YCP_getOrganizationHierarchy_output.xml" IncludeXMLPath="/Organization"/>
            <yfc:doc>
                <Element EntityName="Organization">
                    <Description>
                        <div>
							Details of the organization to which the selling entitlement rule is assigned.
						</div>
                    </Description>
                </Element>
            </yfc:doc>
        </AssignedToOrganization>
    </EntitlementRuleAssignment>
</EntitlementRuleAssignmentList>
