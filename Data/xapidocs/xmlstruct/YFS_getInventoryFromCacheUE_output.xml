<!--getInventoryCacheUE output XML-->
<InventoryItemList InventoryOrganizationCode="Required"
    OrganizationCode="" TotalInventoryItemList="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getInventoryFromCacheUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="InventoryOrganizationCode">
                <Description>
                Inventory organization.
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="OrganizationCode"/>
            <Attribute DataType="Count" Name="TotalInventoryItemList">
                <Description>Specifies the Maximum Number of inventory items records returned
			</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <InventoryItem InventoryItemKey="" ItemID="" ProductClass=""
        UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Inventory_Item"
                    Name="InventoryItemKey" NameInEntity="Inventory_Item_Key"/>
                <Attribute EntityName="Inventory_Item" Name="ItemID" NameInEntity="Item_Id"/>
                <Attribute EntityName="Inventory_Item"
                    Name="ProductClass" NameInEntity="Product_Class"/>
                <Attribute EntityName="Inventory_Item"
                    Name="UnitOfMeasure" NameInEntity="Uom"/>
            </Attributes>
        </yfc:doc>
        <InventoryAlertsList LastInventoryAlertKey="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Key" Name="LastInventoryAlertKey">
                        <Description>Last inventory alert Key in the output.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <InventoryAlerts AlertLevel="" AlertPeriodEnd=""
                AlertPeriodStart="" AlertQuantity="" AlertRaisedOn=""
                AlertType="" DistributionRuleId=""
                FirstFutureAvailableDate="" FutureAvailableDate=""
                FutureAvailableQuantity="" InventoryAlertKey=""
                InventoryItemKey="" IsDefaultDistributionGroup=""
                Node="" OnhandAvailableDate=""
                OnhandAvailableQuantity="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Element EntityName="Inventory_Alerts"/>
                </yfc:doc>
                <AvailabilityInformation>
                    <yfc:includeInParent IncludeXMLChildNodes="Y"
                        IncludeXMLFileName="MasterAvailabilityInformation.xml" IncludeXMLPath="/AvailabilityInformation"/>
                    <AvailableInventory AvailableQuantity=""
                        EffectiveStartDate="" EffectiveUntilDate=""
                        IsOnhand="" MaxShipDate="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute Name="AvailableQuantity">
                                    <Description>
                                    <div>
							This indicates the available quantity. If inventory is not tracked, unplanned, or infinite,
							the value may be Java's Integer.MAX_VALUE, Java's Long.MAX_VALUE, or the 
							RealisticQuantityForInfiniteInventory property (if set).
						</div>
                                    </Description>
                                </Attribute>
                                <Attribute Name="EffectiveStartDate">
                                    <Description>
							The timestamp for which the ShipDate is effective from. Calculated based on the last RTAM execution time or the last <code>EffectiveUntilDate</code>.
					</Description>
                                </Attribute>
                                <Attribute Name="EffectiveUntilDate">
                                    <Description>
                                    <div>
							The timestamp for which the ShipDate is effective until.This is calculated based on earliest timestamp of the computed date of each node.   The computed date of each node is computed based on the last cut-off time of the ship date and subtract back the minimum notification time based on the node's shipping calendar.
						</div>
                                    </Description>
                                </Attribute>
                                <Attribute Name="IsOnhand">
                                    <Description>
                                    <div>
							This flag indicates whether the AvailableQuantity is based on on-hand supply.
						</div>
                                    </Description>
                                </Attribute>
                                <Attribute Name="MaxShipDate">
                                    <Description>
                                    <div>
							The date when this inventory is available. 
						</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </AvailableInventory>
                </AvailabilityInformation>
            </InventoryAlerts>
        </InventoryAlertsList>
    </InventoryItem>
</InventoryItemList>
