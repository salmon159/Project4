            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/OrderLines/OrderLine"/>
            <yfc:doc>
                <Element EntityName="Order_Line"/>
                <Attributes>
                    <Attribute DataType="Action" Name="Action">
                        <Description>
                            <div>
								Only applicable for published data of Delete Order event, which
								can be raised by the deleteOrder API.
								<code>Action</code>
								=
								<b>
                                    <code>DELETE</code>
                                </b>
								will be published to indicate that the order line will be
								deleted.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="AllocationDate">
                        <Description>
                            The date when this line will be allocated. This is calculated at order creation time, based on the node processing time. This field is not used from version 50 onwards. The <code>ALLOCATION_LEAD_TIME</code> field is used instead.
                        </Description>
                    </Attribute>
                    <Attribute DataType="NumberOfDays" Name="AllocationLeadTime">
                        <Description>
                            The number of days it will take any node to ship the item after a ship advice is sent. It is defaulted from the Rule Set Field Name (<code>YFS_RULES.LEAD_TIME</code>). The release process will this value, or the scheduled node's lead time (setup at ship node level), whichever is greater.
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ApptWarning">
                        <Description>
							Displays a warning message if
							<code>CannotScheduleBeforeDate</code>
							or
							<code>CannotScheduleAfterDate</code>
							fields have a Non-Null value.
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="AvailableQtyForStopDelivery">
                        <Description>The quantity available through modification rules for
							making a stop delivery request for this line.</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="AwaitingDeliveryRequest">
                        <Description>A flag to indicate if the order line is awaiting for
							a Delivery service association.
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasChainedLines">
                        <Description>
                            <div>
								Indicates if this order has chained or related lines.
								<br/>
								Default:
								<b>
                                    <code>N</code>
                                </b>
                                <br/>
								Valid Values:
								<b>
                                    <code>Y</code>
                                </b>
								or
								<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasDeliveryLines">
                        <Description>Indicates if the line has associated delivery lines.
							Valid only for product lines.
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasDerivedChild">
                        <Description>
                            <div>
								Indicates if order line has lines derived from it.
								<br/>
								Default:
								<b>
                                    <code>N</code>
                                </b>
                                <br/>
								Valid Values:
								<b>
                                    <code>Y</code>
                                </b>
								or
								<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasServiceLines">
                        <Description>Indicates if the line has associated service lines.
							Valid only for product lines.
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IntentionalBackorder">
                        <Description>A flag to indicate if the order line should be
							backordered upon creation. If passed as 'Y', the line will be
							backordered even if inventory is available. This flag is ignored
							if reservation is mandatory.
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="InvoicedQty">
                        <Description>
                            <b>Deprecated as Sterling Selling And Fulfillment Suite 9.2.1</b>.
						Invoiced quantity on the order line.
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="InvoicedQuantity">
                        <Description>Invoiced quantity on the order line. 
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IsBundleParent">
                        <Description>
							Indicates if Order line is bundle parent line or not.
							<div>
                                <br/>
								Valid Values:
								<b>
                                    <code>Y</code>
                                </b>
								or
								<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Status" Name="MaxLineStatus">
                        <Description>Maximum order line status among all records that
							belong to an order.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="MaxLineStatusDesc">
                        <Description>Description of maximum order line status among all
							records that belong to an order.
						</Description>
                    </Attribute>
                    <Attribute DataType="Status" Name="MinLineStatus">
                        <Description>Minimum order line status among all records that
							belong to an order.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="MinLineStatusDesc">
                        <Description>Description of minimum order line status among all
							records that belong to an order.
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="MultipleStatusesExist">
                        <Description>If order line is in more than one status then this
							flag will be returned as "Y".
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="OpenQty">
                        <Description>Total quantity that can be cancelled from the line.
						</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="OverallStatus">
                        <Description>
                            A description of the order line's overall status determined by the <code>YFSGetOverallStatusUE</code>.
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ProductAvailDate"/>
                    <Attribute DataType="String" Name="ProductShipDate"/>
                    <Attribute DataType="Quantity" Name="RemainingQty">
                        <Description>Remaining quantity.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ReturnReasonLongDesc">
                        <Description>Long description of the return reason code.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ReturnReasonShortDesc">
                        <Description>Short description of the return reason code.
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="ReturnableQty">
                        <Description>Quantity of item that could be returned from this line at its current status.
						</Description>
                    </Attribute>
                    <Attribute EntityName="SCAC_And_Service"
                        Name="ScacAndService" NameInEntity="Scac_And_Service">
                        <Description>Value of SCAC and Service code.
						</Description>
                    </Attribute>
                    <Attribute EntityName="SCAC_And_Service"
                        Name="ScacAndServiceKey" NameInEntity="Scac_And_Service_Key">
                        <Description>Link to YFS_SCAC_AND_SERVICE table.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="Status">
                        <Description>Status description of the order line.
						</Description>
                    </Attribute>
                    <Attribute EntityName="Order_Release_Status"
                        Name="StatusQuantity" NameInEntity="Status_Quantity">
                        <Description>Quantity on the order line in the current status.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="SubstituteItemID">
                        <Description>Substituted ItemID. This field is only populated if
							the item on the Order line is substituted. This field is
							deprecated, see OverrideItemID on the OrderStatus/Details
							element.</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="SubstituteItemUOM">
                        <Description>Substituted Item UOM. This field is only populated if
							the item on the Order line is substituted. This field is
							deprecated, see OverrideItemID on the OrderStatus/Details
							element.
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>
                            <div>
								Indicates if data is fetched from the history table rather than
								the regular table.
								<br/>
								Default:
								<b>
                                    <code>N</code>
                                </b>
                                <br/>
								Valid Values:
								<b>
                                    <code>Y</code>
                                </b>
								or
								<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
