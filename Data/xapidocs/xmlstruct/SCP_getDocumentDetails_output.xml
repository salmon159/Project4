<!-- SCP_getDocumentDetails_output.xml -->
<Document BodyName="" CharEncoding="" ContentSubType="" ContentType=""
    CreateTime="" DocumentId="" DocumentName="" DocumentSize=""
    PreviousDocumentId="" StorageType="" Subject="" WorkFlowId="" xmlns="http://www.sterlingcommerce.com/documentation/SCP/getDocumentDetails/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="BodyName">
                <Description>
                    <div>The title or name of the body.  Often this matches the filename from the DocumentName or is null.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="CharEncoding">
                <Description>
                    <div>UTF-8, SJIS, etc.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ContentSubType">
                <Description>
                    <div>This is simply a refinement of the ContentType</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ContentType">
                <Description>
                    <div>This could anything really.  text/html, binary, Application, null, etc.</div>
                </Description>
            </Attribute>
            <Attribute DataType="TimeStamp" Name="CreateTime">
                <Description>
                    <div>The system time when this Document was created.  This is not based on the time a file on disk was created that may become the body of the Document.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="DocumentId">
                <Description>
                    <div>The unique identifier of a given Document.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="DocumentName">
                <Description>
                    <div>The full path to the document from which this Document object was originally created.  Null is also seen sometimes for internal docs.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="DocumentSize">
                <Description>
                    <div>The size of this document, in bytes.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="PreviousDocumentId">
                <Description>
                    <div>The previous documentId if a Document has been altered.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="StorageType">
                <Description>
                    <div>FS|DB: File System or Database</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="Subject">
                <Description>
                    <div>Anything describing the content of the Document.</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="WorkFlowId">
                <Description>
                    <div>The WorkFlowId this Document is tied to.</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Body OutOfBand="">
        <yfc:doc>
            <Element>
                <Description>
                    <div>This is expected as CDATA content.  This is a required element.</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="String" Name="OutOfBand">
                    <Description>
                        <div>true|false.  This indicates if the nested CDATA content of the Body element will have the actual body of the Document or a token to be used with OutOfBand access functionality to retrieve the content.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Body>
</Document>
