<ResourcePool AllRegionsServiced="" CalendarKey=""
    CapacityInfoAvailable="" CapacityMaintainAtResourceLevel=""
    CapacityOrganizationCode="" CapacityUnitOfMeasure=""
    ItemGroupCode="" Node="" ProviderContactAddressKey=""
    ProviderOrganizationCode="" RegionSchemaKey=""
    ResourcePoolDescription="" ResourcePoolId="" ResourcePoolKey="" xmlns="http://www.sterlingcommerce.com/documentation/INV/CALCULATE_CAPACITY_AVAILABILITY/ON_CAPACITY_AVAILABILITY_CALCULATION">
    <yfc:doc>
        <Attributes>
            <Attribute Name="CapacityOrganizationCode">
                <Description>
                    <div>
							A Resource Pool is always created wihin the context of a Capacity Organization. An Organization can source services from any Resource Pools within the same Capacity Organization.
							</div>
                </Description>
            </Attribute>
            <Attribute Name="Node">
                <Description>
                    <div>
							The node associated with the resource pool.
							</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ResourcePoolAttributeList>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="MasterResourcePool.xml" IncludeXMLPath="/ResourcePool/ResourcePoolAttributeList"/>
        <ResourcePoolAttribute DeliveryMethod=""
            ResourcePoolAttributeKey="" ResourcePoolKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Res_Pool_Attr">
                    <Description>
                        <div>
						 	This element defines attributes and their values for the resource pool.
						</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="DeliveryMethod">
                        <Description>
                            <div>
								The delivery method for which the resource pool is defined. <br/>
								Valid values: <code>SHP</code> - Shipment, <code>PICK</code> - Pickup by user
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </ResourcePoolAttribute>
    </ResourcePoolAttributeList>
    <Slots>
        <Slot EndTime="" StartTime="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="EndTime"/>
                    <Attribute DataType="String" Name="StartTime"/>
                </Attributes>
            </yfc:doc>
            <Dates>
                <Date AdditionalStandardCapacity1=""
                    AdditionalStandardCapacity2=""
                    AdditionalSupplementCapacity1=""
                    AdditionalSupplementCapacity2=""
                    AvailableCapacity="" AvailableDate=""
                    TotalStandardCapacity="" TotalSupplementCapacity="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>This element shows the available capacity picture on a particular date.</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Capacity" Name="AdditionalStandardCapacity1">
                                <Description>
                                    <div>
											Additional Standard Capacity.
											</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Capacity" Name="AdditionalStandardCapacity2">
                                <Description>
                                    <div>
											Additional Standard Capacity.
											</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Capacity" Name="AdditionalSupplementCapacity1">
                                <Description>
                                    <div>
											Additional Supplement Capacity.
											</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Capacity" Name="AdditionalSupplementCapacity2">
                                <Description>
                                    <div>
											Additional Supplement Capacity.
											</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Capacity" Name="AvailableCapacity">
                                <Description>
                                    <div>
											Remaining capacity.
											</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Date" Name="AvailableDate">
                                <Description>
                                    <div>
											The date that this capacity consumption is specified.
											</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Capacity" Name="TotalStandardCapacity">
                                <Description>
                                    <div>
											Total Standard Capacity.
											</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Capacity" Name="TotalSupplementCapacity">
                                <Description>
                                    <div>
											Total Supplement Capacity.
											</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Date>
            </Dates>
        </Slot>
    </Slots>
</ResourcePool>
