    <SearchByAddress BillToKey="" MarkForKey="" ShipToKey="">
        <yfc:doc>
            <Element EntityName="Order_Header"/>
            <Attributes>
                <Attribute Name="BillToKey" NameInEntity="Bill_To_Key">
                    <Description>
                        <div>
					Link to YFS_PERSON_INFO identifying the bill to address.  If this attribute
					is passed, orders with order headers matching the <code>BillToKey</code>
					will be returned.
					If all three <code>MarkForKey</code>, <code>BillToKey</code> and <code>ShipToKey</code>
					are passed, then orders satisifying ANY match condition will be returned.
					</div>
                    </Description>
                </Attribute>
                <Attribute Name="MarkForKey" NameInEntity="Mark_For_Key">
                    <Description>
                        <div>
					Link to YFS_PERSON_INFO identifying the mark for address.  If this attribute
					is passed, orders with order headers or order lines matching the <code>MarkForKey</code>
					will be returned.
					If all three <code>MarkForKey</code>, <code>BillToKey</code> and <code>ShipToKey</code>
					are passed, then orders satisifying ANY match condition will be returned.
					</div>
                    </Description>
                </Attribute>
                <Attribute Name="ShipToKey" NameInEntity="Ship_To_Key">
                    <Description>
                        <div>
					Link to YFS_PERSON_INFO identifying the ship to address.  If this attribute
					is passed, orders with order headers or order lines matching the <code>ShipToKey</code>
					will be returned.
					If all three <code>MarkForKey</code>, <code>BillToKey</code> and <code>ShipToKey</code>
					are passed, then orders satisifying ANY match condition will be returned.
					</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </SearchByAddress>
