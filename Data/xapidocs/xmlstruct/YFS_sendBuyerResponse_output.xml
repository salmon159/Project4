<!-- Order is not a template-driven element. It is returned same as what was passed to the api. -->
<Order AcceptOrder="Y|N" AuditTransactionId="" BaseDropStatus=""
    DocumentType="" EnterpriseCode="" ModificationReasonCode=""
    ModificationReasonText="" ModificationReference1=""
    ModificationReference2="" ModificationReference3=""
    ModificationReference4="" OrderHeaderKey="" OrderNo=""
    TransactionId="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/sendBuyerResponse/output">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
                <div>
				This table stores the quote document in the system.
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="String" Name="AcceptOrder">
                <Description>
                    <div>
If the value of <code>AcceptOrder</code> is <b>
                            <code>Y</code>
                        </b>, then the order is moved to Accepted status. If it is <b>
                            <code>N</code>
                        </b>, the order is moved to Rejected status.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>Y</b> or <b>N</b>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Audit"
                Name="AuditTransactionId" NameInEntity="Audit_Transaction_Id"/>
            <Attribute DataType="String" Name="BaseDropStatus">
                <Description>
                    <div>
				The base drop status for the transaction ID.
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute EntityName="Order_Header" Name="EnterpriseCode" NameInEntity="Enterprise_Key"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonCode" NameInEntity="Reason_Code"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonText" NameInEntity="Reason_Text"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference1" NameInEntity="Reference_1"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference2" NameInEntity="Reference_2"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference3" NameInEntity="Reference_3"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference4" NameInEntity="Reference_4"/>
            <Attribute EntityName="Order_Header" Name="OrderHeaderKey" NameInEntity="Order_Header_Key"/>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No"/>
            <Attribute DataType="TransactionID" Name="TransactionId">
                <Description>
                    <div>
				Identifies a transaction that changes the status of the order.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Notes>
        <Note AuditTransactionId="" ContactReference="" ContactTime=""
            ContactType="" ContactUser="" CustomerSatIndicator=""
            NoteText="" Priority="" ReasonCode="" SequenceNo=""
            TableKey="" TableName="" Tranid="" Tranname=""
            VisibleToAll="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Notes"/>
                <Attributes>
                    <Attribute DataType="Text-50" Name="Tranname">
                        <Description>
							Specifies the name of the Transaction
						</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Note>
    </Notes>
    <OrderLines>
        <!-- OrderLine is not a template-driven element. It is returned same as what was passed to the api. -->
        <OrderLine OrderLineKey="" PrimeLineNo="" SubLineNo="" yfc:DTDOccurrence="ZeroOrMany">
            <Notes>
                <Note AuditTransactionId="" ContactReference=""
                    ContactTime="" ContactType="" ContactUser=""
                    CustomerSatIndicator="" NoteText="" Priority=""
                    ReasonCode="" SequenceNo="" TableKey="" TableName=""
                    Tranid="" Tranname="" VisibleToAll="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Notes"/>
                        <Attributes>
                            <Attribute DataType="Text-50" Name="Tranname">
                                <Description>
							Specifies the name of the Transaction
						</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Note>
            </Notes>
        </OrderLine>
    </OrderLines>
</Order>
