<Order ValidatePromotionAward="Y|N" xmlns="http://www.sterlingcommerce.com/documentation/YFS/orderRepricingUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="ValidatePromotionAward">
                <Description>Indicates if promotions and awards need to be validated and matched.
					<br/>Valid Values:<b>
                        <code>Y</code>
                    </b> or <b>
                        <code>N</code>
                    </b>
                    <br/>
					Default: <b>
                        <code>Y</code>
                    </b>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderLines>
        <OrderLine OrderLineKey="" PrimeLineNo="" SubLineNo="">
            <LinePriceInfo AdditionalLinePriceTotal=""
                TaxExemptionCertificate="" TaxableFlag="" UnitPrice="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Money" Name="AdditionalLinePriceTotal">
                            <Description>Total Price for all non-informational Additional
							Line Prices for the OrderLine. List Price times ActualPricingQuantity.
						</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </LinePriceInfo>
            <LineCharges Reset="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Reset">
                            <Description>If reset is passed as Y,all the existing charges that are not passed as a child node will be set to zero.  These charges will still appear on the order, however their charge amount will be set to zero.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <LineCharge ChargeCategory="Required" ChargeName=""
                    ChargePerLine="" ChargePerUnit="" IsBillable=""
                    IsDiscount="" Reference="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Flag" Name="IsBillable">
                                <Description>Describes if the charge is billable. Default: <code>N</code>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Flag" Name="IsDiscount">
                                <Description>Describes if the charge is discountable. Default: <code>N</code>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </LineCharge>
            </LineCharges>
            <LineTaxes>
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
						Pass this element to replace the tax information for an order line. To update the tax information for an order line, all applicable taxes (not just the modified or added tax) should be passed to the API. Tax which is not passed will be removed from the order line.
						</div>
                        </Description>
                    </Element>
                </yfc:doc>
                <LineTax ChargeCategory="" ChargeName="" Reference1=""
                    Reference2="" Reference3="" Tax=""
                    TaxName="Required" TaxPercentage="" TaxableFlag="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Tax_Breakup">
                            <Description>
                                <div>
						Contains tax related information for various entities in the system.  If <code>ChargeCategory</code> and <code>ChargeName</code> are not provided, tax will be considered as a billable and positive charge.
						</div>
                            </Description>
                        </Element>
                    </yfc:doc>
                </LineTax>
            </LineTaxes>
            <Awards>
                <Award AwardAmount="" AwardApplied="" AwardId=""
                    AwardKey="" AwardType="" ChargeCategory=""
                    ChargeName="" DenialReason="" Description=""
                    PosReasonCode="" PromotionId="" PromotionKey="">
                    <yfc:doc>
                        <Element EntityName="Promotion_Award"/>
                    </yfc:doc>
                </Award>
            </Awards>
            <AdditionalLinePrices>
                <AdditionalLinePrice AdditionalLinePriceKey=""
                    ExtendedPrice="" IsPriceLocked=""
                    IsPriceTypeForInformationOnly="" OrderLineKey=""
                    Price="" PriceTypeName="" RecurringType="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Money" Name="ExtendedPrice">
                                <Description>
									ExtendedPrice of the AdditionalLinePrice for this OrderLine. Equal to the Price of the additional line times ActualPricingQuantity.
								</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <Promotions>
                        <Promotion AdditionalLinePriceKey=""
                            DenialReason="" Description=""
                            PromotionApplied="" PromotionId=""
                            PromotionKey="" PromotionType="" yfc:DTDOccurrence="ZeroOrMany"/>
                    </Promotions>
                    <Awards>
                        <Award AdditionalLinePriceKey="" AwardAmount=""
                            AwardApplied="" AwardId="" AwardKey=""
                            AwardQuantity="" AwardType=""
                            ChargeCategory="" ChargeName=""
                            DenialReason="" Description="" IsManual=""
                            IsPromotionOnOrder=""
                            IsPromotionOnOrderLine="" PosReasonCode=""
                            PromotionId="" PromotionKey=""
                            yfc:DTDOccurrence="ZeroOrMany" yfc:GroupName="AdditionalLinePrice:Award">
                            <yfc:doc>
                                <Element EntityName="Promotion_Award"/>
                                <Attributes>
                                    <Attribute DataType="Flag" Name="IsPromotionOnOrder">
                                    <Description>Determines whether the Award was created for a
											Promotion
											on the order.</Description>
                                    </Attribute>
                                    <Attribute DataType="Flag" Name="IsPromotionOnOrderLine">
                                    <Description>Determines whether the Award was created for a
											Promotion
											on the order line.</Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                        </Award>
                    </Awards>
                </AdditionalLinePrice>
            </AdditionalLinePrices>
        </OrderLine>
    </OrderLines>
    <HeaderCharges Reset="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Reset">
                    <Description>If reset is passed as Y, all the existing charges that are not passed as a child node will be set to zero.  These charges will still appear on the order, however their charge amount will be set to zero.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <HeaderCharge ChargeAmount="" ChargeCategory="Required"
            ChargeName="" IsBillable="" IsDiscount="" Reference="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="IsBillable">
                        <Description>Describes if the charge is billable. Default: <code>N</code>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IsDiscount">
                        <Description>Describes if the charge is discountable. Default: <code>N</code>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </HeaderCharge>
    </HeaderCharges>
    <HeaderTaxes>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
						Pass this element to replace the tax information. To update the tax information for an order, all applicable taxes (not just the modified or added tax) should be passed to the API. Tax which is not passed will be removed from the order.
						</div>
                </Description>
            </Element>
        </yfc:doc>
        <HeaderTax ChargeCategory="Required" ChargeName="" Reference1=""
            Reference2="" Reference3="" Tax="" TaxName="Required"
            TaxPercentage="" TaxableFlag="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Tax_Breakup">
                    <Description>
                        <div>
						Contains tax related information for various entities in the system.  If <code>ChargeCategory</code> and <code>ChargeName</code> are not provided, tax will be considered as a billable and positive charge.
						</div>
                    </Description>
                </Element>
            </yfc:doc>
        </HeaderTax>
    </HeaderTaxes>
    <Promotions>
        <Promotion DenialReason="" Description="" PromotionApplied=""
            PromotionId="" PromotionKey="" PromotionType=""/>
    </Promotions>
    <Awards>
        <Award AwardAmount="" AwardApplied="" AwardId="" AwardKey=""
            AwardType="" ChargeCategory="" ChargeName="" DenialReason=""
            Description="" PosReasonCode="" PromotionId="" PromotionKey="">
            <yfc:doc>
                <Element EntityName="Promotion_Award"/>
            </yfc:doc>
        </Award>
    </Awards>
</Order>
