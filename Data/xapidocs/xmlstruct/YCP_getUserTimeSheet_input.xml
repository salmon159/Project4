<UserTimeSheet xmlns="http://www.sterlingcommerce.com/documentation/YCP/getUserTimeSheet/input">
    <Users EndDate="" OrganizationCode="" StartDate="" UserId="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Text-100" Name="EndDate">
                    <Description>The time sheet is prepared for any user for all tasks done by him ending at this date</Description>
                </Attribute>
                <Attribute EntityName="Organization"
                    Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>Organization Code for the user</Description>
                </Attribute>
                <Attribute DataType="Text-100" Name="StartDate">
                    <Description> The time sheet is prepared for any user for all tasks done by him starting from this date </Description>
                </Attribute>
                <Attribute EntityName="User" Name="UserId" NameInEntity="Loginid">
                    <Description>User Id for which we want to prepare the time sheet</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Users>
</UserTimeSheet>
