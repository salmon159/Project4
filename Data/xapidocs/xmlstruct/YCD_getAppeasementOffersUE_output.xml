<AppeasementOffers ChargeCategory="" ChargeName="" OrderHeaderKey=""
    ReasonCode="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/getAppeasementOffersUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="ChargeCategory" Name="ChargeCategory">
                <Description>
                    <div>
								Charge Category for Appeasement Offers.
							</div>
                </Description>
            </Attribute>
            <Attribute DataType="ChargeName" Name="ChargeName">
                <Description>
                    <div>
								Charge Name for Appeasement Offers.
							</div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="OrderHeaderKey">
                <Description>
                    <div>
								OrderHeaderKey of the order.
							</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="ReasonCode">
                <Description>
                    <div>
								Reason for Appeasement.
							</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <AppeasementOffer DiscountPercent="" OfferAmount="" OfferType="" Preferred="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Percentage" Name="DiscountPercent">
                    <Description>
                        <div>
								The discount percentage of the offer.
							</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="OfferAmount">
                    <Description>
                        <div>
								The total amount of the offer.
							</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="OfferType">
                    <Description>
                        <div>
								The type of the offer. Valid values are: <code>FLAT_AMOUNT_ORDER</code>, <code>PERCENT_ORDER</code>, <code>PERCENT_FUTURE_ORDER</code> and <code>VARIABLE_AMOUNT_ORDER</code>.
							</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="Preferred">
                    <Description>
                        <div>
								Flag to indicate if the offer is the preferred offer. Valid values are: <code>Y</code> and <code>N</code>.
							</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Order ChargeCategory="" ChargeName="" HeaderOfferAmount="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="ChargeCategory" Name="ChargeCategory">
                        <Description>
                            <div>
								Charge Category for Order Header Appeasement.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="ChargeName" Name="ChargeName">
                        <Description>
                            <div>
								Charge Name for Order Header Appeasement.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="HeaderOfferAmount">
                        <Description>
                            <div>
								Appeasement Amount for Order Header.
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Order>
        <OrderLines>
            <OrderLine ChargeCategory="" ChargeName=""
                LineOfferAmount="" OrderLineKey="" OrderedQty="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="ChargeCategory" Name="ChargeCategory">
                            <Description>
                                <div>
								Charge Category for Order Line Appeasement.
							</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="ChargeName" Name="ChargeName">
                            <Description>
                                <div>
								Charge Name for Order Line Appeasement.
							</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="LineOfferAmount">
                            <Description>
                                <div>
								Appeasement Amount for Order Line.
							</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </OrderLine>
        </OrderLines>
    </AppeasementOffer>
</AppeasementOffers>
