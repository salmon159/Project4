<!-- manageServiceSkill Input XML -->
<ServiceSkills xmlns="http://www.sterlingcommerce.com/documentation/YCM/manageServiceSkill/input">
    <ServiceSkill Description="" Operation="" OrganizationCode=""
        ServiceSkillId="" ServiceSkillKey="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Element EntityName="Service_Skill">
                <Description>
                    <div>Details of the Service Skill to be created/modified/deleted. To modify either ServiceSkillKey or ServiceSkillId/OrganizationCode combination should be passed.
				</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Action" Name="Operation">
                    <Description>
                        <div>
					   This attribute specifies the kind of operation to be done.  Valid values are <b>
                                <code>Delete</code>
                            </b> and  <b>
                                <code>Create</code>
                            </b>. Set to <b>
                                <code>Delete</code>
                            </b> to delete an existing Service Skill.  Set to <b>
                                <code>Create</code>
                            </b> to create a new Service Skill.  If Operation is not given or Operation has a blank value, a Service Skill will be created if it does not exists or be modified if it already exists.</div>
                    </Description>
                </Attribute>
                <Attribute Name="OrganizationCode">
                    <Description>
                        <div>
					   Organization code for which a <code>Service Skill</code> is defined.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </ServiceSkill>
</ServiceSkills>
