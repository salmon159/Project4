<Login DisplayUserID="" EnterpriseCode="" LoginID="Required"
    Password="Required" Timeout="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/login/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="User" Name="DisplayUserID" NameInEntity="Display_User_ID"/>
            <Attribute DataType="String" EntityName="Enterprise"
                Name="EnterpriseCode" NameInEntity="Enterprise_Code"/>
            <Attribute EntityName="User" Name="LoginID" NameInEntity="Loginid"/>
            <Attribute EntityName="User" Name="Password"/>
            <Attribute EntityName="User_Activity" Name="Timeout"/>
        </Attributes>
        <Element>
            <Description>
                <div>
                    <Validation>
                        <Condition>
						If the external authenticator class is not provided, the values for <code>LoginID</code> and <code>Password</code> must match the values stored in the database.
						</Condition>
                        <Error Description="Invalid Login ID" ErrorCode="YFS10192"/>
                    </Validation>
                </div>
            </Description>
        </Element>
    </yfc:doc>
</Login>
