<!-- getResourcePoolDetails Output XML -->
<ResourcePool AllRegionsServiced="" AssociateRegionBySlot=""
    CalendarKey="" CapacityInfoAvailable=""
    CapacityMaintainAtResourceLevel="" CapacityOrganizationCode=""
    CapacityUnitOfMeasure="" ItemGroupCode="" Node=""
