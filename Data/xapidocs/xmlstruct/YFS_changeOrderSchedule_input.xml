<!--  ChangeSchedule is not a template-driven element. It is returned same as what was passed to the api. -->
<ChangeSchedule AuditTransactionId="" DocumentType="Conditional"
    EnterpriseCode="Conditional" ModificationReasonCode=""
    ModificationReasonText="" ModificationReference1=""
    ModificationReference2="" ModificationReference3=""
    ModificationReference4="" OrderHeaderKey="Conditional"
    OrderNo="Conditional" Override="" SelectMethod="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/changeOrderSchedule/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
		Pass either <code>OrderHeaderKey</code> or <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> to identify the Order to be modified.
		</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Order_Audit"
                Name="AuditTransactionId" NameInEntity="Audit_Transaction_Id"/>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute DataType="String" EntityName="Order_Header"
                Name="EnterpriseCode" NameInEntity="Enterprise_Code"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonCode" NameInEntity="Reason_Code"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonText" NameInEntity="Reason_Text"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference1" NameInEntity="Reference_1"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference2" NameInEntity="Reference_2"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference3" NameInEntity="Reference_3"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference4" NameInEntity="Reference_4"/>
            <Attribute EntityName="Order_Header" Name="OrderHeaderKey" NameInEntity="Order_Header_Key"/>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No"/>
            <Attribute DataType="Flag" Name="Override">
                <Description>
                    <div>
				Pass as <b>
                            <code>Y</code>
                        </b> to override status change validation.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Action" Name="SelectMethod">
                <Description>
                    <div>
				This attribute determines whether database select operations are carried out in the <b>
                            <code>WAIT</code>
                        </b> or <b>
                            <code>NO_WAIT</code> mode</b>.
				<br/>Default: <b>
                            <code>NO_WAIT</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>WAIT</code>
                        </b> or  <b>
                            <code>NO_WAIT</code> mode</b>.
			</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <FromSchedules>
        <!--  FromSchedule is not a template-driven element. It is returned same as what was passed to the api. -->
        <FromSchedule FromBatchNo="" FromExpectedDeliveryDate=""
            FromExpectedShipDate="" FromLotNumber="" FromRevisionNo=""
            FromStatus="Required" OrderLineKey="Conditional"
            OrderLineScheduleKey="" OrderReleaseKey="Conditional"
            PrimeLineNo="Conditional" ReleaseNo="Conditional" SubLineNo="Conditional">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
				Contains the status and schedule information from which quantity needs to be moved. Pass the either the <code>OrderLineKey</code>
				or the <code>PrimeLineNo</code> and <code>SubLineNo</code> to identify the line that will have its schedule changed.
				</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Order_Line_Schedule"
                        Name="FromBatchNo" NameInEntity="Batch_No">
                        <Description>
                            <div>
					If <code>FromBatchNo</code> is passed, it is used to find the matching schedule information.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line_Schedule"
                        Name="FromExpectedDeliveryDate" NameInEntity="Expected_Delivery_Date">
                        <Description>
                            <div>
						If <code>FromExpectedDeliveryDate</code> is passed, it is used to find the matching schedule information.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line_Schedule"
                        Name="FromExpectedShipDate" NameInEntity="Expected_Shipment_Date">
                        <Description>
                            <div>
					If <code>FromExpectedShipDate</code> is passed, it is used to find the matching schedule information.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line_Schedule"
                        Name="FromLotNumber" NameInEntity="Lot_Number">
                        <Description>
                            <div>
					If <code>FromLotNumber</code> is passed, it is used to find the matching schedule information.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40"
                        EntityName="Order_Line_Schedule"
                        Name="FromRevisionNo" NameInEntity="Revision_No">
                        <Description>
                            <div>
					If <code>FromRevisionNo</code> is passed, it is used to find the matching schedule information.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Release_Status"
                        Name="FromStatus" NameInEntity="Status">
                        <Description>
                            <div>
                                <code>FromStatus</code> is used to find the matching schedule information.
                                <Validation>
                                    <Condition>Invalid Status</Condition>
                                    <Error
                                    Description="YFS:Invalid Status" ErrorCode="YFS10235"/>
                                </Validation>
                                <Validation>
                                    <Condition>Order release not found.</Condition>
                                    <Error
                                    Description="YFS: No Order Release Key" ErrorCode="YFS10130"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="OrderLineKey" NameInEntity="Order_Line_Key"/>
                    <Attribute EntityName="Order_Line_Schedule"
                        Name="OrderLineScheduleKey" NameInEntity="Order_Line_Schedule_Key">
                        <Description>
                            <div>
					If <code>OrderLineScheduleKey</code> is passed, it is used to find the matching schedule information and other schedule attributes, such as <code>FromExpectedShipDate</code> and <code>FromLotNumber</code>, are ignored. 
					</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Release"
                        Name="OrderReleaseKey" NameInEntity="Order_Release_Key">
                        <Description>
                            <div>
					Pass either the <code>OrderReleaseKey</code> or <code>ReleaseNo</code> to change the schedule for a released line.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line"
                        Name="PrimeLineNo" NameInEntity="Prime_Line_No"/>
                    <Attribute EntityName="Order_Release"
                        Name="ReleaseNo" NameInEntity="Release_No">
                        <Description>
                            <div>
					Pass either the <code>OrderReleaseKey</code> or <code>ReleaseNo</code> to change the schedule for a released line.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Order_Line" Name="SubLineNo" NameInEntity="Sub_Line_No"/>
                </Attributes>
            </yfc:doc>
            <ToSchedules>
                <!--  ToSchedule is not a template-driven element. It is returned same as what was passed to the api. -->
                <ToSchedule Quantity="Required" ToBatchNo=""
                    ToExpectedDeliveryDate="" ToExpectedShipDate=""
                    ToLotNumber="" ToRevisionNo="">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
				Contains the schedule information to which the quantity needs to be moved.
				</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="String" Name="Quantity">
                                <Description>
                                    <div>
						The quantity to move. Must be a numeric greater than zero.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Order_Line_Schedule"
                                Name="ToBatchNo" NameInEntity="Batch_No">
                                <Description>
                                    <div>
						If <code>ToBatchNo</code> is passed in the input, is is used for the new schedule. If not passed, the match is for a schedule record with blank batch number.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Order_Line_Schedule"
                                Name="ToExpectedDeliveryDate" NameInEntity="Expected_Delivery_Date">
                                <Description>
                                    <div>
						If <code>ToExpectedDeliveryDate</code> is passed in the input, is is used for the new schedule.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Order_Line_Schedule"
                                Name="ToExpectedShipDate" NameInEntity="Expected_Shipment_Date">
                                <Description>
                                    <div>
						If <code>ToExpectedShipDate</code> is passed in the input, is is used for the new schedule.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Order_Line_Schedule"
                                Name="ToLotNumber" NameInEntity="Lot_Number">
                                <Description>
                                    <div>
						If <code>ToLotNumber</code> is passed in the input, is is used for the new schedule. If not passed, the match is for a schedule record with blank lot number.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Order_Line_Schedule"
                                Name="ToRevisionNo" NameInEntity="Revision_No">
                                <Description>
                                    <div>
						If <code>ToRevisionNumber</code> is passed in the input, is is used for the new schedule. If not passed, the match is for a schedule record with blank revision number.
						</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <!--  ToScheduleTranQuantity is not a template-driven element. It is returned same as what was passed to the api. -->
                    <ToScheduleTranQuantity Quantity="" TransactionalUOM="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Quantity" Name="Quantity">
                                    <Description>
					  Quantity to move in TransactionalUOM 
					  </Description>
                                </Attribute>
                                <Attribute DataType="UOM" Name="TransactionalUOM">
                                    <Description>UOM on which all the transactional quantities are specified.
					  </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </ToScheduleTranQuantity>
                </ToSchedule>
            </ToSchedules>
        </FromSchedule>
    </FromSchedules>
</ChangeSchedule>
