<!--  Either primary key ManifestKey or all unique key attributes (ManifestNo, Scac, ShipNode) must be passed to uniquely identify the Manifest.  -->
<Manifest ManifestClosedFlag="" ManifestDate="" ManifestKey=""
    ManifestNo="" ManifestStatus="" PrintCount="" Scac="" ShipNode=""
    ShipperAccountNo="" TrailerNo="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/closeManifest/input">
    <yfc:includeInParent
        IncludeXMLFileName="YFS_Manifest_master_input.xml"
        IncludeXMLPath="/Manifest" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Manifest">
            <Description>
                <div>
				An existing <code>Manifest</code> is identified by using one of the following: 
				<code>ManifestKey </code> or combination of
				<code>ManifestNo</code>, <code>ShipNode</code> and <code>Scac </code>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Flag" Name="ManifestClosedFlag">
                <Description>
                    <div>
                        <Validation>
                            <Condition> Should not be 'Y'.</Condition>
                            <Error Description="Manifest is closed." ErrorCode="YDM00096"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="ManifestKey">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Either primary key (<code>ManifestKey</code>) or all 
								attributes of unique key (<code>ManifestNo</code>, <code>Scac</code> and <code>ShipNode</code>) must be passed. Error if either of them not passed. 
							</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                        <Validation>
                            <Condition> Should give a manifest record.</Condition>
                            <Error Description="Invalid Manifest." ErrorCode="YDM00097"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" Name="ManifestNo">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Either primary key (<code>ManifestKey</code>) or all 
								attributes of unique key (<code>ManifestNo</code>, <code>Scac</code> and <code>ShipNode</code>) must be passed. Error if either of them not passed. 
							</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                        <Validation>
                            <Condition> Should give unique record along with 
								<code>ShipNode</code> and <code>Scac </code>
                            </Condition>
                            <Error Description="Invalid Manifest." ErrorCode="YDM00097"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="Scac">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Either primary key (<code>ManifestKey</code>) or all 
								attributes of unique key (<code>ManifestNo</code>, <code>Scac</code> and <code>ShipNode</code>) must be passed. Error if either of them not passed. 
							</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                        <Validation>
                            <Condition> Must be a valid scac.</Condition>
                            <Error
                                Description="SCAC does not exist in the system." ErrorCode="YDM00087"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="ShipNode">
                <Description>
                    <div>
                        <Validation>
                            <Condition>Either primary key (<code>ManifestKey</code>) or all 
								attributes of unique key (<code>ManifestNo</code>, <code>Scac</code> and <code>ShipNode</code>) must be passed. Error if either of them not passed. 
							</Condition>
                            <Error
                                Description="Mandatory parameters are not passed." ErrorCode="YDM00163"/>
                        </Validation>
                        <Validation>
                            <Condition> Must be a valid ShipNode.</Condition>
                            <Error
                                Description="ShipNode does not exist in the system." ErrorCode="YDM00085"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Manifest>
