<!--getTransactionSummary Output XML-->
<getTransactionSummary StartMonth="" Year="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getTransactionSummary/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="StartMonth"/>
            <Attribute DataType="String" Name="Year"/>
        </Attributes>
    </yfc:doc>
    <TransactionSummaryList>
        <TransactionSummary Description="" DocumentType="Required"
            EnterpriseCode="Required" StartMonth="">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Document_Params" Name="Description"/>
                    <Attribute EntityName="Document_Params"
                        Name="DocumentType" NameInEntity="Document_Type"/>
                    <Attribute EntityName="Enterprise"
                        Name="EnterpriseCode" NameInEntity="Enterprisecode"/>
                    <Attribute DataType="String" Name="StartMonth">
                        <Description>In case of rolling count this attribute identifies the start month for first year</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <MonthlySummaryList>
                <MonthlySummary Month="" NoOfOrderLinesCreated=""
                    NoOfOrdersCreated="" NoOfShipmentsCreated="" Year="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="Month">
                                <Description>The number of the month. Like '1' for January, '12' for December</Description>
                            </Attribute>
                            <Attribute DataType="String" Name="NoOfOrderLinesCreated"/>
                            <Attribute DataType="String" Name="NoOfOrdersCreated"/>
                            <Attribute DataType="String" Name="NoOfShipmentsCreated"/>
                            <Attribute DataType="String" Name="Year">
                                <Description>In case of rolling count it will identify the Year. </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </MonthlySummary>
            </MonthlySummaryList>
            <YTDSummary YTDNoOfOrderLinesCreated=""
                YTDNoOfOrdersCreated="" YTDNoOfShipmentsCreated="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="YTDNoOfOrderLinesCreated"/>
                        <Attribute DataType="String" Name="YTDNoOfOrdersCreated"/>
                        <Attribute DataType="String" Name="YTDNoOfShipmentsCreated"/>
                    </Attributes>
                </yfc:doc>
            </YTDSummary>
        </TransactionSummary>
    </TransactionSummaryList>
</getTransactionSummary>
