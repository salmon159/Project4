<?xml version="1.0" encoding="UTF-8"?>
<!-- Either CountRequestKey or (CountRequestNo, DocumentType and Node) must be passed. -->
<CountRequest CountRequestKey="" CountRequestNo="" DocumentType=""
    Node="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getCountRequestDetails/input">
    <yfc:includeInParent IncludeXMLFileName="YFS_Count_Request_keys.xml"
        IncludeXMLPath="/CountRequest" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Count_Request"/>
        <Attributes>
            <Attribute DataType="Key" Name="CountRequestKey">
                <Description>
                    <div>
						The primary key for the YFS_COUNT_REQUEST table. 
							<Validation>
                            <Condition>Either primary key (<code>CountRequestKey</code>) or all attributes of unique key (<code>CountRequestNo</code>,<code>DocumentType </code> and <code>Node</code>) must be passed. If invalid <code>CountRequestKey</code> is passed. 
								</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" Name="CountRequestNo">
                <Description>
                    <div>
						The count request number associated with the document type and node. 
							<Validation>
                            <Condition>Either primary key (<code>CountRequestKey</code>) or all attributes of unique key (<code>CountRequestNo</code>,<code>DocumentType </code> and <code>Node</code>) must be passed. If invalid <code>CountRequestNo</code> is passed. 
								</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-24" Name="DocumentType">
                <Description>
                    <div>
						The document type associated with the count request.  
							<Validation>
                            <Condition>Either primary key (<code>CountRequestKey</code>) or all attributes of unique key (<code>CountRequestNo</code>,<code>DocumentType </code> and <code>Node</code>) must be passed. If invalid <code>DocumentType</code> is passed. 
								</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="Node">
                <Description>
                    <div>
						The node key that is associated with this count request. 
							<Validation>
                            <Condition>Either primary key (<code>CountRequestKey</code>) or all attributes of unique key (<code>CountRequestNo</code>,<code>DocumentType </code> and <code>Node</code>) must be passed. If invalid <code>Node</code> is passed. 
								</Condition>
                            <Error
                                Description="No record found for passed inputs." ErrorCode="WMS00002"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</CountRequest>
