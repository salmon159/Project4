<?xml version="1.0" encoding="UTF-8"?>
<WaveSummary Node="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getWaveSummary/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Key" EntityName="Wave" Name="Node" NameInEntity="Node_Key">
                <Description>
                    Waves will be summarised for a node. Node attribute can be specified at  WaveSummary or at Wave element level in the input.
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <WaveList yfc:DTDOccurrence="REQUIRED">
        <!-- Either WaveKey OR WaveNo and Node should be passed -->
        <Wave Createts="" Createuserid="" Description="" DocumentType=""
            EstimatedMinutes="" Node="" OverrideConcurrentWave=""
            OverrideReleaseDate="" PipelineKey="" Priority=""
            ReasonCode="" ReasonText="" ReleaseNoEarlierThan=""
            ReleaseNoLaterThan="" RemainingMinutes="" SeqNo=""
            ShipmentGroupId="" WaveKey="" WaveNo="" WaveStatus="" yfc:DTDOccurrence="REQUIRED">
            <yfc:includeInParent
                IncludeXMLFileName="YFS_Wave_master.xml" IncludeXMLPath="/Wave"/>
            <yfc:doc>
                <Element EntityName="Wave"/>
            </yfc:doc>
        </Wave>
    </WaveList>
</WaveSummary>
