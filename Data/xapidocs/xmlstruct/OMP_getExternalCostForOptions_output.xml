<Promise Overridden="Y|N" xmlns="http://www.sterlingcommerce.com/documentation/OMP/getExternalCostForOptions/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="Overridden">
                <Description>
                    <div>
					The <code>Overridden</code> flag is considered when the rule <code> ENABLE_SHADOW_SOLVER</code> is enabled for the enterprise. The flag should be set to Y if any of the possible assignments from the UE is modified.				
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PromiseLines>
        <PromiseLine LineId="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="LineId">
                        <Description>
                            <div>
								Unique identifier for the line. 
							</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <PossibleAssignments>
                <Assignment AssignmentKey="" Quantity="" RejectChoice=""
                    RejectionReason="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>Assignment node.</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Key" Name="AssignmentKey">
                                <Description>
                                    <div>Assignment key of the choice.</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="Quantity">
                                <Description>
                                    <div>
							Quantity that can be obtained for this assignemnt.
							</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Flag" Name="RejectChoice">
                                <Description>Indicates if the assignment is to be rejected.<div>
                                    <br/>Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Text-24" Name="RejectionReason">
                                <Description>
                                    <div>Reason for the assignment rejection</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
