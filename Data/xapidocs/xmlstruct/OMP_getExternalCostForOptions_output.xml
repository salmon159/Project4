                                    <Attributes>
                                    <Attribute
                                    DataType="Text-24" Name="Currency">
                                    <Description>
                                    <div>Currency in which cost is maintained. </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-24" Name="ItemInventoryCostPerUnit">
                                    <Description>
                                    <div>Per unit inventory cost of the item at the node. </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-24" Name="NodePriorityCostPerUnit">
                                    <Description>
                                    <div>Per unit priority cost of the item at the node. </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-24" Name="NodeShippingCapacityCostPerUnit">
                                    <Description>
                                    <div>Per unit shipping capacity cost of the item at the node. </div>
                                    </Description>
                                    </Attribute>
                                    <Attribute
                                    DataType="Text-24" Name="OutboundShipmentHandlingCostPerUnit">
                                    <Description>
                                    <div>Outbound shipment handling cost per unit of the item. </div>
                                    </Description>
                                    </Attribute>
                                    </Attributes>
                                    </yfc:doc>
                                    </Costs>
                                </Procurement>
                            </Procurements>
                        </SubstituteItem>
                    </SubstituteItems>
                    <Procurements>
