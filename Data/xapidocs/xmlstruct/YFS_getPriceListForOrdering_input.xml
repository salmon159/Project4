<!-- getPriceListForOrdering Input XML -->
<GetPriceListForOrdering EnterpriseCode="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getPriceListForOrdering/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="EnterpriseCode">
                <Description>The enterprise that is associated with this order.
	<div>
                        <Validation>
                            <Condition>If invalid <code>EnterpriseCode</code> is passed.
	</Condition>
                            <Error
                                Description="YFS:Invalid Enterprise Code." ErrorCode="YFS10113"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Items yfc:DTDOccurrence="REQUIRED">
        <Item AlternateItemID="" ItemID="Required" ModificationStatus=""
            PriceProgramName="" PricingDate="" ProductClass=""
            Quantity="" UnitOfMeasure="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element EntityName="Order_Line"/>
                <Attributes>
                    <Attribute Name="ItemID">
                        <Description>The identifier for the item. The ITEM_ID, UOM, and PRODUCT_CLASS form the unique inventory identifier for an Item. The ITEM_ID, UOM and ORGANIZATION_CODE form the unique item identifier for the Product Management module. 
	<div>
                                <Validation>
                                    <Condition>If <code>ItemID</code> is Null.
	</Condition>
                                    <Error
                                    Description="YFS:Blank Item ID" ErrorCode="YFS10047"/>
                                </Validation>
                                <Validation>
                                    <Condition>If no<code>Item</code> element is passed.
	</Condition>
                                    <Error
                                    Description="YFS:Blank Item ID" ErrorCode="OMP80_0027"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ModificationStatus">
                        <Description>Modification status of the Item. Valid values are <b>
                                <code>NEW</code>
                            </b>, <b>
                                <code>MODIFIED</code>
                            </b>, <b>
                                <code>DELETED</code>
                            </b>, <b>
                                <code>UNCHANGED</code>
                            </b>. </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="PriceProgramName">
                        <Description>The price program used for pricing the order
		</Description>
                    </Attribute>
                    <Attribute Name="Quantity" NameInEntity="Ordered_Qty"/>
                </Attributes>
            </yfc:doc>
            <!--There may be different price list depending on region. ShiptoAddress is to identify the region' -->
            <ShipToAddress AddressID="" AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" ErrorTxt="" EveningFaxNo=""
                EveningPhone="" FirstName="" HttpUrl=""
                IsCommercialAddress="" JobTitle="" LastName=""
                MiddleName="" MobilePhone="" OtherPhone="" PersonID=""
                PersonInfoKey="" PreferredShipAddress="" State=""
                Suffix="" TaxGeoCode="" Title="" UseCount=""
                VerificationStatus="" ZipCode="">
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                </yfc:doc>
            </ShipToAddress>
            <TranQuantity OrderedQty="" TransactionalUOM="">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
	  Contains information about the quantities for item in Transactional Unit Of Measure.
	 </div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="OrderedQty">
                            <Description>Quantity.
	 </Description>
                        </Attribute>
                        <Attribute DataType="UOM" Name="TransactionalUOM">
                            <Description>UOM on which all the transactional quantities are specified.
	 </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </TranQuantity>
            <PriceInfo IsPriceLocked="" ListPrice="" PricingUOM=""
                RetailPrice="" UnitPrice="">
                <yfc:doc>
                    <Element EntityName="Order_Line"/>
                    <Attributes>
                        <Attribute DataType="Flag" Name="IsPriceLocked">
                            <Description>This may be the negotiated price with the customer. Price engine should not recompute the price for this item. </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </PriceInfo>
            <Charges>
                <Charge ChargeCategory="Required" ChargeName=""
                    ChargePerLine="" ChargePerUnit="" Reference="" yfc:DTDOccurrence="OneOrMany">
                    <yfc:doc>
                        <Element EntityName="Line_Charges"/>
                    </yfc:doc>
                </Charge>
            </Charges>
            <Taxes>
                <Tax ChargeCategory="" ChargeName="" Reference1=""
                    Reference2="" Reference3="" Tax=""
                    TaxName="Required" TaxPercentage="" TaxableFlag="" yfc:DTDOccurrence="OneOrMany">
                    <yfc:doc>
                        <Element EntityName="Tax_Breakup"/>
                    </yfc:doc>
                </Tax>
            </Taxes>
            <Awards>
                <Award AwardAmount="" AwardApplied="" AwardId=""
                    AwardKey="" AwardType="" DenialReason=""
                    Description="" PosReasonCode="" PromotionId=""
                    PromotionKey="" yfc:DTDOccurrence="OneOrMany"/>
            </Awards>
        </Item>
        <!-- If you have fixed charges that apply to entire order no matter how many items in the order then pass those charges as HeaderCharges' -->
        <HeaderCharges>
            <HeaderCharge ChargeAmount="" ChargeCategory="Required"
                ChargeName="" Reference="" yfc:DTDOccurrence="OneOrMany"/>
        </HeaderCharges>
        <!-- Taxes for the HeaderCharges'-->
        <HeaderTaxes>
            <HeaderTax ChargeCategory="Required" ChargeName=""
                Reference1="" Reference2="" Reference3="" Tax=""
                TaxName="Required" TaxPercentage="" TaxableFlag="" yfc:DTDOccurrence="OneOrMany">
                <yfc:doc>
                    <Element EntityName="Tax_Breakup"/>
                </yfc:doc>
            </HeaderTax>
        </HeaderTaxes>
        <Promotions>
            <Promotion DenialReason="" Description=""
                PromotionApplied="" PromotionId="" PromotionKey=""
                PromotionType="" yfc:DTDOccurrence="OneOrMany"/>
        </Promotions>
        <Awards>
            <Award AwardAmount="" AwardApplied="" AwardId="" AwardKey=""
                AwardType="" DenialReason="" Description=""
                PosReasonCode="" PromotionId="" PromotionKey="" yfc:DTDOccurrence="OneOrMany"/>
        </Awards>
    </Items>
</GetPriceListForOrdering>
