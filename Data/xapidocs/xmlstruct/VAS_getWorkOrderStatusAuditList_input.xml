<WorkOrderStatusAudit AuditTransactionId="" AuditUserId="" Comments=""
    NewStatus="" NewStatusDate="" OldStatus="" OldStatusDate=""
    ReasonCode="" TransactionId="" WorkOrderKey=""
    WorkOrderStatusAuditKey=""
    xmlns="http://www.sterlingcommerce.com/documentation/VAS/getWorkOrderStatusAuditList/input" yfc:QryTypeSupported="Y">
    <WorkOrder AllocatedLevel="" CaseId="" DocumentType=""
        EnterpriseCode="" FinishNoLaterThan="" HoldFlag=""
        InvUpdateActivityCode="" ItemID="" LevelOfService=""
        NextAlertTs="" NodeKey="" OpenWorkOrderFlag="" OrderHeaderKey=""
        OrderLineKey="" OrderNo="" PalletId="" PipeLineKey=""
        PreCallStatus="" Priority="" ProductClass=""
        PromisedApptEndDate="" PromisedApptStartDate="" Purpose=""
        QuantityAllocated="" QuantityCompleted="" QuantityReleased=""
        QuantityRemoved="" QuantityRequested="" ReasonCode=""
        ReasonText="" ReleasedLevel="" Segment="" SegmentType=""
        SerialNo="" ServiceComplexityLevel="" ServiceItemGroupCode=""
        ServiceItemID="" ServiceSupervisorId="" ServiceUom=""
        ShipByDate="" ShipmentKey="" ShipmentNo="" StartNoEarlierThan=""
        Status="" StatusDate="" Uom="" WorkOrderKey="" WorkOrderNo="">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YFS_Work_Order_tran.xml" IncludeXMLPath="/WorkOrder"/>
        <yfc:doc>
            <Element EntityName="Work_Order"/>
            <Attributes>
                <Attribute DataType="String" Name="AllocatedLevel">
                    <Description>
                        <div>Indicates the extent of allocation for the work order. The valid values are FULL, PARTIAL, NONE, and ANY. The respective values tells wether the work order is fully, partially, not allocated or is in any of the the previously mentioned states.
	        		</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description>
                        <div>Case for which the work order is processed.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description>
                        <div>Pallet for which the work order is processed.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ReleasedLevel">
                    <Description>
                        <div>Indicates the extent to which the work order is released. The valid values are FULL, PARTIAL, NONE, and ANY. The respective values tells wether the work order is fully, partially, not relased or is in any of the previously mentioned states.
	            	</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <WorkOrderComponents>
            <WorkOrderComponent ComponentQuantity="" ItemID=""
                ProductClass="" Segment="" SegmentType="" SerialNo=""
                ShipByDate="" Uom="" WorkOrderComponentKey=""
                WorkOrderKey="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YFS_Work_Order_Component_master.xml" IncludeXMLPath="/WorkOrderComponent"/>
                <yfc:doc>
                    <Element EntityName="Work_Order_Component">
                        <Description>
                            <div>
			This table stores information about all the components required by the work order.<br/>Components are not mandatory for work orders created for the <code>KITTING</code> service item group code.
			</div>
                        </Description>
                    </Element>
                </yfc:doc>
                <WorkOrderComponentTag BatchNo="" LotAttribute1=""
                    LotAttribute2="" LotAttribute3="" LotKeyReference=""
                    LotNumber="" ManufacturingDate="" RevisionNo=""
                    WorkOrderComponentKey="" WorkOrderComponentTagKey="">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
					Stores details of the inventory tag attributes for work order components that are tag tracked. Tag information is stored only if the <code>Node</code> tracks tag information in inventory.	
					</div>
                            </Description>
                        </Element>
                    </yfc:doc>
                </WorkOrderComponentTag>
            </WorkOrderComponent>
        </WorkOrderComponents>
        <WorkOrderActivities>
            <WorkOrderActivity ActivityCode="" ActivitySeqNo=""
                IsComplete="" QuantityComplete=""
                WorkOrderActivityKey="" WorkOrderKey="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent
                    IncludeXMLFileName="YFS_Work_Order_Activity_master.xml" IncludeXMLPath="/WorkOrderActivity"/>
                <yfc:doc>
                    <Element EntityName="Work_Order_Activity"/>
                </yfc:doc>
                <WorkOrderActivityDtls>
                    <WorkOrderActivityDtl ActivityCode=""
                        ActivityLocationId="" CaseId="" PalletId=""
                        QuantityCompleted="" QuantityReleased=""
                        QuantityRequested="" SerialNo=""
                        WorkOrderActivityDtlKey="" WorkOrderKey="" yfc:DTDOccurrence="OneOrMany">
                        <yfc:includeInParent
                            IncludeXMLFileName="YFS_Work_Order_Activity_Dtl_master.xml" IncludeXMLPath="/WorkOrderActivityDtl"/>
                        <yfc:doc>
                            <Element EntityName="Work_Order_Activity_Dtl"/>
                            <Attributes>
                                <Attribute DataType="LPNNo" Name="CaseId">
                                    <Description>
                                    <div>Case for which the activity is performed.
					</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="LPNNo" Name="PalletId">
                                    <Description>
                                    <div>Pallet for which the activity is performed.
					</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </WorkOrderActivityDtl>
                </WorkOrderActivityDtls>
                <WorkOrderActivityInstructions>
                    <Instruction InstructionDetailKey=""
                        InstructionText="" InstructionType=""
                        InstructionURL="" InstructionUsage=""
                        SequenceNo="" yfc:DTDOccurrence="OneOrMany"/>
                </WorkOrderActivityInstructions>
            </WorkOrderActivity>
        </WorkOrderActivities>
        <WorkOrderTag BatchNo="" LotAttribute1="" LotAttribute2=""
            LotAttribute3="" LotKeyReference="" LotNumber=""
            ManufacturingDate="" RevisionNo="" WorkOrderKey="" WorkOrderTagKey=""/>
    </WorkOrder>
</WorkOrderStatusAudit>
