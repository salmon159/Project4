<CreateExportDataEx ApiName="" EnterpriseCode="Required" FlowName=""
    FromOrgCode="" Priority="" ReplyToId="" ShipString="" SubFlowName=""
    ToOrgCode="" UserReference="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/createExportDataEx/input">
    <yfc:doc>
        <Element EntityName="Export"/>
        <Attributes>
            <Attribute EntityName="Export" Name="EnterpriseCode" NameInEntity="EnterpriseKey"/>
            <Attribute EntityName="Export" Name="ReplyToId" NameInEntity="ReplyToId"/>
            <Attribute EntityName="Export" Name="ShipString" NameInEntity="ShipNode"/>
        </Attributes>
    </yfc:doc>
    <ExportSystemIds yfc:DTDOccurrence="REQUIRED">
        <ExportSystemId SystemId="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
                            <Validation>
                                <Condition>
								At least one <code>ExportSystemId</code> element must be passed. If this element is not passed:	 
								</Condition>
                                <Error
                                    Description="No Export System Ids exist in the XML" ErrorCode=""/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Export" Name="SystemId" NameInEntity="SystemName"/>
                </Attributes>
            </yfc:doc>
        </ExportSystemId>
    </ExportSystemIds>
    <XmlExportData yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
                        <Validation>
                            <Condition>
								Element <code>XmlExportData</code> must be passed. If this element is not passed:	 
								</Condition>
                            <Error
                                Description="XmlExportData Element is mandatory" ErrorCode=""/>
                        </Validation>
                        <Validation>
                            <Condition>
								If element <code>XmlExportData</code> is passed but data is not passed: 
								</Condition>
                            <Error
                                Description="Xml Export Data cannot be empty" ErrorCode=""/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
    </XmlExportData>
</CreateExportDataEx>
