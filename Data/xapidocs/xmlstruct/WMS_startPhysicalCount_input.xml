<?xml version="1.0" encoding="UTF-8"?>
<PhysicalCount Node="Required" PhysicalCountPlanName="" StartDate="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/startPhysicalCount/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Ship_Node" Name="Node" NameInEntity="Ship_Node">
                <Description>The node for which the plan is configured</Description>
            </Attribute>
            <Attribute DataType="String" Name="PhysicalCountPlanName">
                <Description>
                    <div>Name of the Physical Count plan
								<Validation>
                            <Condition>If <code>Physical Count Plan</code> is specified then <code>Zone Id</code> is a mandatory parameter.
									</Condition>
                            <Error Description="Node not passed" ErrorCode="WMS00054"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="StartDate">
                <Description>Proposed Start Date for the Physical Count</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <!-- Zone List is mandatory if PhysicalCountPlanName is Passed -->
    <Zones>
        <Zone ZoneId=""/>
    </Zones>
</PhysicalCount>
