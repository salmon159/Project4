<ColonyMetadata AvailablePrefix="" CurrentVersion="" IsEnabled="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getColonyMetadata/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="AvailablePrefix">
                <Description>
                    <div>A valid prefix that is not currently used by any existing colony</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="CurrentVersion">
                <Description>
                    <div>Current multischema version</div>
                </Description>
            </Attribute>
            <Attribute DataType="Boolean" Name="IsEnabled">
                <Description>
                    <div>Whether multischema support is enabled</div>
                </Description>
                <Options IsEnumeration="Y">
                    <Option Description="Multischema is enabled" Value="Y"/>
                    <Option Description="Multischema is not enabled" Value="N"/>
                </Options>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PoolList>
        <yfc:doc>
            <Element>
                <Description>
                    <div>List of all known <code>TableType</code> and <code>PoolId</code> combinations, optionally filtered by the <code>PoolTableTypes</code> attribute in the input. Note this is not applicable to a non-multischema environment.</div>
                </Description>
            </Element>
        </yfc:doc>
        <Pool Id="" TableType="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>Mapping from a <code>PoolId</code> to its associated <code>TableType</code>.</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="Id">
                        <Description>
                            <div>Name that uniquely identifies the shard to use for a database connection</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="TableType">
                        <Description>
                            <div>Tables of this type are stored in the shard</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Pool>
    </PoolList>
    <DefaultPoolParamList DBVendor="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div>List of known database connection parameters and their default values. Note this element will be repeated for each requested database vendor.</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="String" Name="DBVendor">
                    <Description>
                        <div>Associated database vendor.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Param DefaultValue="" IsSuppressed="" Name="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>A property for database shard configuration</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="DefaultValue">
                        <Description>
                            <div>Default value for the given configuration property. May be empty or missing.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="IsSuppressed">
                        <Description>
                            <div>Whether the given DefaultValue is suppressed (for example, if it is a password field).
                        This attribute is set to Y if the value is suppressed. If this attribute is not present or set to N, then the value
                        is not suppressed.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Name">
                        <Description>
                            <div>A database configuration property</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Param>
    </DefaultPoolParamList>
</ColonyMetadata>
