                    <Attribute EntityName="Inventory_Alerts"
                        Name="AlertType" NameInEntity="Alert_Type">
                        <Description>
                            <div>
				Values: REALTIME_ONHAND value represents alert raised for onhand inventory and REALTIME_FUTURE_MAX value represents alert raised for maximum future inventory within the horizon.  Alerts are only raised with <code>AlertType</code> REALTIME_FUTURE_MAX if the <code>OnhandAvailableQuantity</code> falls below the quantity configured for alert level 3.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Inventory_Alerts"
                        Name="DistributionRuleId" NameInEntity="Distribution_Rule_Id">
                        <Description>
				Distribution group for which the alert has been raised.
				</Description>
                    </Attribute>
                    <Attribute EntityName="Inventory_Alerts"
                        Name="IsDefaultDistributionGroup" NameInEntity="Is_Default_Distribution_Group">
                        <Description>
                            <div>
				Values: Y value represents alert was raised by RealtimeAvailabilityMonitor Agent.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Integer4AgentCriteria" Name="MonitorOption">
                        <Description>
                            <div>
				Monitor option of the agent which raised the event.
				Values: 1-ActivityBased  2-QuickSync  3-FullSync.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Inventory_Alerts" Name="Node" NameInEntity="Ship_Node">
                        <Description>
                            <div>
				Monitored node that the inventory information belongs to.  If <code>ShipNode</code> value is Null, it means that the inventory picture belongs to the default distribution group of the item.
				</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Inventory_Alerts"
                        Name="OrganizationCode" NameInEntity="Organization_Code">
                        <Description>
                            <div>
				The code of the organization that maintains the item.
				</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <AvailabilityInformation>
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="MasterAvailabilityInformation.xml" IncludeXMLPath="/AvailabilityInformation"/>
                <AvailableInventory AvailableQuantity=""
                    EffectiveStartDate="" EffectiveUntilDate=""
                    IsOnhand="" MaxShipDate="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute Name="AvailableQuantity">
                                <Description>
                                    <div>
							This indicates the available quantity. If inventory is not tracked, unplanned, or infinite,
							the value may be Java's Integer.MAX_VALUE, Java's Long.MAX_VALUE, or the 
							RealisticQuantityForInfiniteInventory property (if set).
						</div>
                                </Description>
                            </Attribute>
                            <Attribute Name="EffectiveStartDate">
                                <Description>
							The timestamp for which the ShipDate is effective from. Calculated based on the last RTAM execution time or the last <code>EffectiveUntilDate</code>.
					</Description>
                            </Attribute>
                            <Attribute Name="EffectiveUntilDate">
                                <Description>
                                    <div>
							The timestamp for which the ShipDate is effective until.This is calculated based on earliest timestamp of the computed date of each node.   The computed date of each node is computed based on the last cut-off time of the ship date and subtract back the minimum notification time based on the node's shipping calendar.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute Name="IsOnhand">
                                <Description>
                                    <div>
							This flag indicates whether the AvailableQuantity is based on on-hand supply.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute Name="MaxShipDate">
                                <Description>
                                    <div>
							The date when this inventory is available. 
						</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </AvailableInventory>
            </AvailabilityInformation>
            <ProcureFromAvailabilityInformation>
                <ProcureFromAvailability FutureAvailableQuantity=""
                    Node="" OnhandAvailableQuantity="">
                    <yfc:doc>
                        <Element EntityName="Inventory_Alerts"/>
                        <Attributes>
                            <Attribute Name="Node">
                                <Description>The procurement node that accounts for the procured quantity in this 
								availability change.</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </ProcureFromAvailability>
            </ProcureFromAvailabilityInformation>
        </AvailabilityChange>
    </AvailabilityChanges>
</InventoryItem>
