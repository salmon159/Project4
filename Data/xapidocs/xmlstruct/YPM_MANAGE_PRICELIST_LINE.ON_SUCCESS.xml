<PricelistLine AbsoluteAdjustment="" EndDateActive="" FromQuantity=""
    ItemID="" ItemShortDesc="" ListPrice="" MassAuditID="" Operation=""
    PercentAdjustment="" PriceTypeName="" PricelistHeaderKey=""
    PricelistLineKey="" PricingStatus="" StartDateActive=""
    ToQuantity="" UnitOfMeasure="" UnitPrice="" xmlns="http://www.sterlingcommerce.com/documentation/YPM/MANAGE_PRICELIST_LINE/ON_SUCCESS">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="YPM_Pricelist_Line_master.xml" IncludeXMLPath="/PricelistLine"/>
    <yfc:doc>
        <Element EntityName="Pricelist_Line"/>
        <Attributes>
            <Attribute EntityName="Item" Name="ItemShortDesc" NameInEntity="Short_Description">
                <Description>The short description of the item.</Description>
            </Attribute>
            <Attribute DataType="Action" Name="Operation">
                <Description>
                    <div>Operation performed
						<br/>Valid Values: 
						<br/>
                        <b>
                            <code>Create</code>
                        </b> - A new price list line is created. 
						<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Existing price list line is modified.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Price list line is deleted.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="UnitPrice" Name="UnitPrice">
                <Description>The price for one unit of an item.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PricelistLineQuantityTierList>
        <yfc:doc>
            <Element>
                <Description>
                    <div>Quantity tiers associated with this price list line.</div>
                </Description>
            </Element>
        </yfc:doc>
        <PricelistLineQuantityTier AbsoluteAdjustment=""
            EndDateActive="" FromQuantity="" ItemID="" ItemShortDesc=""
            ListPrice="" MassAuditID="" PercentAdjustment=""
            PriceTypeName="" PricelistHeaderKey="" PricelistLineKey=""
            PricingStatus="" StartDateActive="" ToQuantity=""
            UnitOfMeasure="" UnitPrice="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="YPM_Pricelist_Line_master.xml" IncludeXMLPath="/PricelistLine"/>
            <yfc:doc>
                <Element EntityName="Pricelist_Line">
                    <Description>
                        <div>Quantity tiers associated with this price list line.</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Item" Name="ItemShortDesc" NameInEntity="Short_Description">
                        <Description>The short description of the item.</Description>
                    </Attribute>
                    <Attribute DataType="UnitPrice" Name="UnitPrice">
                        <Description>The price for one unit of an item.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </PricelistLineQuantityTier>
    </PricelistLineQuantityTierList>
    <PricelistHeader AbsoluteAdjustmentOverride="" Currency=""
        Description="" EndDateActive="" InheritFromPricelistHeaderKey=""
        MarkedForListPrice="" OrganizationCode=""
        PercentAdjustmentOverride="" PricelistHeaderKey=""
        PricelistName="" PricingStatus="" SellerOrganizationCode=""
        StartDateActive="" yfc:DTDOccurrence="REQUIRED">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YPM_Pricelist_Header_master.xml" IncludeXMLPath="/PricelistHeader"/>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YPM_Pricelist_Header_master_noOrgCode.xml" IncludeXMLPath="/PricelistHeader"/>
        <yfc:doc>
            <Element EntityName="Pricelist_Header">
                <Description>
                    <div>The price list to which this line belongs.</div>
                </Description>
            </Element>
        </yfc:doc>
    </PricelistHeader>
</PricelistLine>
