<?xml version="1.0" encoding="UTF-8"?>
<LPN CaseId="" CaseQuantity="" DimensionUOM="" Height="" Length=""
    PalletId="" ReceiptHeaderKey="" Weight="" WeightUOM="" Width="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/suggestOpenContainer/output">
    <yfc:doc>
        <Element EntityName="LPN_Hdr"/>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>A CaseId </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>A PalletId </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Receipt ReceiptNo=""/>
    <ParentLPN CaseId="" PalletId="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description>A CaseId </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description>A PalletId </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </ParentLPN>
    <OuterMostLPN CaseId="" PalletId="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description>Indicates the Id of the Case which contains the Item</Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description>Indicates the Id of the Pallet which contains the Item or contains the Case containing the Item</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </OuterMostLPN>
    <LPNLocation LocationId="" Node="" ZoneId="">
        <yfc:doc>
            <Element EntityName="LPN_Location"/>
        </yfc:doc>
    </LPNLocation>
    <Allocations>
        <Allocation AllocType="" TargetLocationId="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="LPN_Pend_Move"/>
            </yfc:doc>
        </Allocation>
    </Allocations>
    <ItemInventoryDetailList>
        <ItemInventoryDetail CaseId="" CountryOfOrigin="" FifoNo=""
            InventoryStatus="" PalletId="" PendOutQty="" Quantity=""
            Segment="" SegmentType="" ShipByDate="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="LPN_Dtl"/>
                <Attributes>
                    <Attribute DataType="LPNNo" Name="CaseId">
                        <Description>Indicates the Id of the Case which contains the Item</Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="PalletId">
                        <Description>Indicates the Id of the Pallet which contains the Item or contains the Case containing the Item</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="PendOutQty">
                        <Description>Quantity pending out of the LPN
						</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <InventoryItem ItemID="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Inventory_Item"/>
                </yfc:doc>
                <Item ItemID="" ItemKey="" UnitOfMeasure="">
                    <yfc:doc>
                        <Element EntityName="Item"/>
                    </yfc:doc>
                    <PrimaryInformation Description="" ShortDescription=""/>
                </Item>
            </InventoryItem>
            <Receipt ReceiptNo=""/>
            <TagDetail BatchNo="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotKeyReference="" LotNumber=""
                ManufacturingDate="" RevisionNo="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:includeInParent IncludeXMLChildNodes="N"
                    IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/TagDetail"/>
                <yfc:doc>
                    <Element EntityName="Inventory_Tag"/>
                </yfc:doc>
            </TagDetail>
            <SerialList>
                <SerialDetail SecondarySerial1="" SecondarySerial2=""
                    SecondarySerial3="" SecondarySerial4=""
                    SecondarySerial5="" SecondarySerial6=""
                    SecondarySerial7="" SecondarySerial8=""
                    SecondarySerial9="" SerialNo="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent IncludeXMLChildNodes="N"
                        IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/SerialList/SerialDetail"/>
                    <yfc:doc>
                        <Element EntityName="Global_Serial_Num"/>
                    </yfc:doc>
                </SerialDetail>
            </SerialList>
        </ItemInventoryDetail>
    </ItemInventoryDetailList>
    <ChildLPNs>
        <ChildLPN CaseId="" CaseQuantity="" DimensionUOM="" Height=""
            Length="" PalletId="" ReceiptHeaderKey="" Weight=""
            WeightUOM="" Width="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="LPN_Hdr"/>
                <Attributes>
                    <Attribute DataType="LPNNo" Name="CaseId">
                        <Description>A CaseId </Description>
                    </Attribute>
                    <Attribute DataType="LPNNo" Name="PalletId">
                        <Description>A PalletId </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Receipt ReceiptNo=""/>
        </ChildLPN>
    </ChildLPNs>
</LPN>
