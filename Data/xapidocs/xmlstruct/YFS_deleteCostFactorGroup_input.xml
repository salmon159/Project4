<CostFactorGroup CostFactorGroupName="Required"
    EnterpriseCode="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/deleteCostFactorGroup/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="CostFactorGroupName">
                <Description>
                    <div>
          Name of the cost factor group.
          <Validation>
                            <Condition>
              A value should be passed.
            </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="EnterpriseCode">
                <Description>
                    <div>
          The enterprise code associated with the cost factor group.
          <Validation>
                            <Condition>
              A value should be passed.
            </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</CostFactorGroup>
