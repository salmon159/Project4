<InventorySnapShot xmlns="http://www.sterlingcommerce.com/documentation/YFS/getInventorySnapShot/output">
    <!-- If only OrganizationCode is passed in the input and the given OrganizationCode participates with multiple ShipNodes, then the output will have <ShipNode> element for each ShipNode that stores inventory of this Organization. -->
    <ShipNode CompleteInventoryFlag="" LastInventoryItemKey=""
        ShipNode="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="CompleteInventoryFlag"/>
                <Attribute DataType="String" Name="LastInventoryItemKey">
                    <Description>
					This attribute indicates the last inventory item key of the batch of inventory item for the ship node provided.					
                </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Item Description="" ExtendedDescription="" InventoryItemKey=""
            InventoryOrganizationCode="" ItemID="" ProductClass=""
            ShortDescription="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Inventory_Item"/>
                <Attributes>
                    <Attribute EntityName="Item" Name="Description" NameInEntity="Description"/>
                    <Attribute EntityName="Item"
                        Name="ExtendedDescription" NameInEntity="Extended_Description"/>
                    <Attribute DataType="OrgCode" Name="InventoryOrganizationCode"/>
                    <Attribute EntityName="Item" Name="ShortDescription" NameInEntity="Short_Description"/>
                </Attributes>
            </yfc:doc>
            <LanguageDescriptionList>
                <LanguageDescription Description=""
                    ExtendedDescription="" LocaleCode=""
                    ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
            </LanguageDescriptionList>
            <SupplyDetails ETA="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotNumber="" LotReference=""
                ManufacturingDate="" Quantity="" Segment=""
                SegmentType="" ShipByDate="" SupplyReference=""
                SupplyReferenceType="" SupplyType="" Track="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply"/>
                    <Attributes>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotAttribute1" NameInEntity="Lot_Attribute1"/>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotAttribute2" NameInEntity="Lot_Attribute2"/>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotAttribute3" NameInEntity="Lot_Attribute3"/>
                        <Attribute DataType="String" Name="LotNumber"/>
                        <Attribute EntityName="Inventory_Tag"
                            Name="LotReference" NameInEntity="Lot_Key_Reference"/>
                        <Attribute EntityName="Inventory_Tag"
                            Name="ManufacturingDate" NameInEntity="Lot_Manufacture_Date"/>
                        <Attribute EntityName="Inventory_Supply"
                            Name="Quantity" NameInEntity="Quantity">
                            <Description>Quantity of the supply.</Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="Track"/>
                    </Attributes>
                </yfc:doc>
                <Tag BatchNo="" InventoryItemKey="" InventoryTagKey=""
                    LotAttribute1="" LotAttribute2="" LotAttribute3=""
                    LotKeyReference="" LotNumber="" ManufacturingDate=""
                    RevisionNo="" TagNumber="" TotalDemand=""
                    TotalOnhandSupply="" TotalOtherSupply="">
                    <yfc:doc>
                        <Element EntityName="Inventory_Tag"/>
                    </yfc:doc>
                </Tag>
            </SupplyDetails>
            <DemandDetails DemandShipDate="" DemandType=""
                MinShipByDate="" Quantity="" Segment="" SegmentType=""
                ShipNode="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Demand"/>
                </yfc:doc>
                <Tag BatchNo="" InventoryItemKey="" InventoryTagKey=""
                    LotAttribute1="" LotAttribute2="" LotAttribute3=""
                    LotKeyReference="" LotNumber="" ManufacturingDate=""
                    RevisionNo="" TagNumber="" TotalDemand=""
                    TotalOnhandSupply="" TotalOtherSupply="">
                    <yfc:doc>
                        <Element EntityName="Inventory_Tag"/>
                    </yfc:doc>
                </Tag>
            </DemandDetails>
        </Item>
    </ShipNode>
</InventorySnapShot>
