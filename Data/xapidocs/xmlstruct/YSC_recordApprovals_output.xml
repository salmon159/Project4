<ApproverResponse ApproverUserID="Required" OrganizationCode="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/recordApprovals/output">
    <yfc:doc>
        <Element EntityName="Transaction_Approval_Status">
            <Description>
                <div>
					Specifies the user who is recording a response to a violation.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Transaction_Violation"
                Name="OrganizationCode" NameInEntity="ORGANIZATION_CODE"/>
        </Attributes>
    </yfc:doc>
    <ValidationList TotalNumberOfRecords="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                    <Description>
						The total number of
						<code>Validations</code>
						.
					</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Validation CanApprovalBePostponed="" Domain="" ErrorCode=""
            OrganizationCode="" ValidationID="" ValidationKey=""
            ViolationEntity="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Element EntityName="Validation">
                    <Description>
                        <div>
							Specifies the validation against which violations are to be
							recorded.
						</div>
                    </Description>
                </Element>
            </yfc:doc>
            <TransactionViolationList TotalNumberOfRecords="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Count" Name="TotalNumberOfRecords">
                            <Description>
								The total number of
								<code>Violations</code>
								.
							</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <TransactionViolation ApprovalRuleID="Required"
                    Domain="Required" MessageCode=""
                    OrganizationCode="Required" SequenceNumber=""
                    Status="Required" TransactionInfoID=""
                    TransactionViolationKey="" yfc:DTDOccurrence="OneOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
									An individual violation.
									<Validation>
                                    <Condition>The Approval Rule does not exist.
										</Condition>
                                    <Error
                                    Description="Approval Rule Not Found." ErrorCode="SomethingNew"/>
                                    </Validation>
                                    <Validation>
                                    <Condition>
                                    <code>ApprovalRuleID</code>
											is invalid for the specified Validation.
										</Condition>
                                    <Error
                                    Description="Approval Rule is not valid for Validaion" ErrorCode="Something New"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Element>
                    </yfc:doc>
                    <TransactionInfo OrderHeaderKey="">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>
										The element containing the unique fields for a given
										Concrete Transaction Info table.
									</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute
                                    EntityName="Order_Transaction_Info"
                                    Name="OrderHeaderKey" NameInEntity="ORDER_HEADER_KEY"/>
                            </Attributes>
                        </yfc:doc>
                    </TransactionInfo>
                    <TransactionApproverList TotalNumberOfRecords="" yfc:DTDOccurrence="REQUIRED">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                                    <Description>
										The total number of
										<code>Transaction Approvers</code>
										.
									</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <TransactionApprover ApproverUserGroupID=""
                            ApproverUserTeam="" DelegateUserID=""
                            TransactionApproverKey="" UserID="Required" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Element>
                                    <Description>
                                    <div>
											An individual approver.
										</div>
                                    </Description>
                                </Element>
                            </yfc:doc>
                        </TransactionApprover>
                    </TransactionApproverList>
                    <TransactionApprovalStatusList
                        TotalNumberOfRecords="" yfc:DTDOccurrence="REQUIRED">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                                    <Description>
										The total number of
										<code>Approval Statuses</code>
										.
									</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <TransactionApprovalStatus ApproverComments=""
                            ApproverSequence="" ApproverUserID=""
                            IsMandatory="" PreviousApproverSequence=""
                            Status="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Element>
                                    <Description>
                                    <div>
											The Status of a the approver.
										</div>
                                    </Description>
                                </Element>
                            </yfc:doc>
                        </TransactionApprovalStatus>
                    </TransactionApprovalStatusList>
                    <TransactionViolationReferenceList
                        TotalNumberOfRecords="" yfc:DTDOccurrence="REQUIRED">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="Count" Name="TotalNumberOfRecords">
                                    <Description>
										The total number of
										<code>Violation References</code>
										.
									</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <TransactionViolationReference
                            ReferenceName="Required"
                            ReferenceValue="Required" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Element>
                                    <Description>
                                    <div>
											A Violation Reference.
										</div>
                                    </Description>
                                </Element>
                            </yfc:doc>
                        </TransactionViolationReference>
                    </TransactionViolationReferenceList>
                </TransactionViolation>
            </TransactionViolationList>
        </Validation>
    </ValidationList>
</ApproverResponse>
