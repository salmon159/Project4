<WorkOrderAppointment AdditionalEquipmentUsed="" AlertSales=""
    AlertScheduler="" AllocatedQuantity="" ApptSeq="" ApptStatus=""
    AttentionRequired="" ConsiderCurrentSlot="" ExecutionBy=""
    ExecutionComments="" ExecutionDuration="" ExecutionEndTime=""
    ExecutionStartTime="" ExpectedApptEndDate=""
    ExpectedApptStartDate="" IsRoutePlanningComplete="" ItemsNotUsed=""
    NonInventoryItemsReturned="" PromisedApptEndDate=""
    PromisedApptStartDate="" RequestedQuantity="" RequestedVolume=""
    RequestedWeight="" ResourcePoolId="" ServiceResourceId=""
    StopNumber="" VehicleNumber="" VehicleOdometerReading=""
    VehicleServiceRequest="" WorkOrderApptKey="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/Work_Order_Appointment_master">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="ConsiderCurrentSlot">
                <Description>
                    <div>
					The <code>ConsiderCurrentSlot</code> flag indicates that system should consider the available capacity of the current slots, which the current timestamp falls under.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="RequestedVolume">
                <Description>
                    <div>The quantity requested in the work order's volume unit of measure, which is specified in the <code>VolumeUOM</code> field of the work order</div>
                </Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="RequestedWeight">
                <Description>
                    <div>The quantity requested in the work order's weight unit of measure, which is specified in the <code>WeightUOM</code> field of the work order</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ResourcePoolCapacityConsumptionDetails Capacity=""
        OverrideCapacityCheck="" ResourcePoolId="" ServiceResourceId="">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Res_Pool" Name="ResourcePoolId" NameInEntity="Resource_Pool_Id"/>
                <Attribute EntityName="Service_Resource"
                    Name="ServiceResourceId" NameInEntity="Service_Resource_ID"/>
            </Attributes>
        </yfc:doc>
    </ResourcePoolCapacityConsumptionDetails>
    <WorkOrderAppointmentUsers>
        <WorkOrderAppointmentUser OverriddenFlag="" UserId=""
            Username="" WoApptUserKey="" WorkOrderApptKey="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="User" Name="Username" NameInEntity="Username"/>
                </Attributes>
            </yfc:doc>
        </WorkOrderAppointmentUser>
    </WorkOrderAppointmentUsers>
</WorkOrderAppointment>
