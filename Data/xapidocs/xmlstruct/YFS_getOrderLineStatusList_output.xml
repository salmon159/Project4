<!--getOrderLineStatusList output XML-->
<OrderLineStatusList TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderLineStatusList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Count" Name="TotalNumberOfRecords">
                <Description>The total number of OrderLine Status returned by the API.
        </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderStatus MergeNode="" OrderHeaderKey="" OrderLineKey=""
        OrderLineScheduleKey="" OrderReleaseKey=""
        OrderReleaseStatusKey="" PipelineKey="" ProcureFromNode=""
        ReceivingNode="" ShipNode="" Status="" StatusDate=""
        StatusDescription="" StatusQuantity="" StatusReason=""
        TotalQuantity="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute EntityName="Order_Line_Schedule"
                    Name="MergeNode" NameInEntity="Merge_Node"/>
                <Attribute EntityName="Order_Line_Schedule"
                    Name="ProcureFromNode" NameInEntity="Procure_From_Node"/>
                <Attribute EntityName="Order_Line_Schedule"
                    Name="ReceivingNode" NameInEntity="Receiving_Node"/>
                <Attribute EntityName="Order_Line_Schedule"
                    Name="ShipNode" NameInEntity="Ship_Node"/>
                <Attribute DataType="Text-100" Name="StatusDescription">
                    <Description>Description of Order Line Status
        </Description>
                </Attribute>
                <Attribute DataType="String" Name="StatusQuantity"/>
                <Attribute DataType="String" Name="StatusReason"/>
            </Attributes>
        </yfc:doc>
        <Schedule BatchNo="" ExpectedShipmentDate="" LotNumber=""
            OrderLineScheduleKey="" RevisionNo="" ShipByDate=""
            ShipNode="" TagNumber=""/>
        <OrderRelease OrderHeaderKey="" OrderReleaseKey="" ReleaseNo="" ShipNode=""/>
        <OrderLine Action="" AllocationDate="" AllocationLeadTime=""
            ApptStatus="" ApptWarning="" AvailableQtyForStopDelivery=""
            AwaitingDeliveryRequest="" BuyerMarkForNodeId=""
            BuyerReceivingNodeId="" CanAddServiceLines=""
            CarrierAccountNo="" CarrierServiceCode=""
            ChainedFromOrderHeaderKey="" ChainedFromOrderLineKey=""
            CommittedQuantity="" ConditionVariable1=""
            ConditionVariable2="" ConfigurationKey=""
            CustomerLinePONo="" CustomerPONo="" DeliveryCode=""
            DeliveryMethod="" DepartmentCode=""
            DependencyShippingRule="" DependentOnLineKey=""
            DerivedFromOrderHeaderKey="" DerivedFromOrderLineKey=""
            DerivedFromOrderReleaseKey="" DistributionRuleId=""
            FillQuantity="" FirstIterationSeqNo="" FreightTerms=""
            FulfillmentType="" FutureAvailabilityDate="" GiftFlag=""
            GiftWrap="" GroupSequenceNumber="" HasChainedLines=""
            HasDeliveryLines="" HasDerivedChild="" HasServiceLines=""
            HoldFlag="" HoldReasonCode="" ImportLicenseExpDate=""
            ImportLicenseNo="" InStorePaymentRequired=""
            IntentionalBackorder="" InvoicedExtendedPrice=""
            InvoicedQty="" InvoicedQuantity="" IsBlindLine="N|Y"
            IsBundleParent="" IsFirmPredefinedNode="" ItemGroupCode=""
            ItemNotExist="" KitCode="" KitQty="" LastIterationSeqNo=""
            LevelOfService="" LineSeqNo="" LineType="" MaxLineStatus=""
            MaxLineStatusDesc="" MergeNode="" MinLineStatus=""
            MinLineStatusDesc="" MinShipByDate=""
            MultipleStatusesExist="" OpenQty="" OrderHeaderKey=""
            OrderLineKey="" OrderedQty="" OrigOrderLineKey=""
            OriginalOrderedQty="" OtherCharges="" OverallStatus=""
            PackListType="" ParentOfDependentGroup="" PersonalizeCode=""
            PersonalizeFlag="" PickableFlag="" PipelineKey=""
            PrimeLineNo="" ProcureFromNode="" PromisedApptEndDate=""
            PromisedApptStartDate="" Purpose="" ReceivingNode=""
            RemainingQty="" ReqCancelDate="" ReqDeliveryDate=""
            ReqShipDate="" ReservationID="" ReservationMandatory=""
            ReservationPool="" ReshipParentLineKey="" ReturnReason=""
            ReturnReasonLongDesc="" ReturnReasonShortDesc=""
            ReturnableQty="" SCAC="" ScacAndService=""
            ScacAndServiceKey="" SchedFailureReasonCode="" Segment=""
            SegmentType="" SerialNo="" ShipNode="" ShipToID=""
            ShipToKey="" ShipTogetherNo=""
            ShipmentConsolidationGroupId="" SourceFromOrganization=""
            SplitQty="" Status="" StatusQuantity="" SubLineNo=""
            SubstituteItemID="" SubstituteItemUOM="" Timezone=""
            TranDiscrepancyQty="" isHistory=""
            yfc:DTDOccurrence="ZeroOrMany" yfc:IgnoreOrdering="Y">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/OrderLines/OrderLine"/>
            <yfc:doc>
                <Element EntityName="Order_Line"/>
                <Attributes>
                    <Attribute DataType="Action" Name="Action">
                        <Description>
                            <div>
								Only applicable for published data of Delete Order event, which
								can be raised by the deleteOrder API.
								<code>Action</code>
								=
								<b>
                                    <code>DELETE</code>
                                </b>
								will be published to indicate that the order line will be
								deleted.
							</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="AllocationDate">
                        <Description>
                            The date when this line will be allocated. This is calculated at order creation time, based on the node processing time. This field is not used from version 50 onwards. The <code>ALLOCATION_LEAD_TIME</code> field is used instead.
                        </Description>
                    </Attribute>
                    <Attribute DataType="NumberOfDays" Name="AllocationLeadTime">
                        <Description>
                            The number of days it will take any node to ship the item after a ship advice is sent. It is defaulted from the Rule Set Field Name (<code>YFS_RULES.LEAD_TIME</code>). The release process will this value, or the scheduled node's lead time (setup at ship node level), whichever is greater.
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ApptWarning">
                        <Description>
							Displays a warning message if
							<code>CannotScheduleBeforeDate</code>
							or
							<code>CannotScheduleAfterDate</code>
							fields have a Non-Null value.
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="AvailableQtyForStopDelivery">
                        <Description>The quantity available through modification rules for
							making a stop delivery request for this line.</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="AwaitingDeliveryRequest">
                        <Description>A flag to indicate if the order line is awaiting for
							a Delivery service association.
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasChainedLines">
                        <Description>
                            <div>
								Indicates if this order has chained or related lines.
								<br/>
								Default:
								<b>
                                    <code>N</code>
                                </b>
                                <br/>
								Valid Values:
								<b>
                                    <code>Y</code>
                                </b>
								or
								<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasDeliveryLines">
                        <Description>Indicates if the line has associated delivery lines.
							Valid only for product lines.
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasDerivedChild">
                        <Description>
                            <div>
								Indicates if order line has lines derived from it.
								<br/>
								Default:
								<b>
                                    <code>N</code>
                                </b>
                                <br/>
								Valid Values:
								<b>
                                    <code>Y</code>
                                </b>
								or
								<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasServiceLines">
                        <Description>Indicates if the line has associated service lines.
							Valid only for product lines.
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IntentionalBackorder">
                        <Description>A flag to indicate if the order line should be
							backordered upon creation. If passed as 'Y', the line will be
							backordered even if inventory is available. This flag is ignored
							if reservation is mandatory.
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="InvoicedQty">
                        <Description>
                            <b>Deprecated as Sterling Selling And Fulfillment Suite 9.2.1</b>.
						Invoiced quantity on the order line.
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="InvoicedQuantity">
                        <Description>Invoiced quantity on the order line. 
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IsBundleParent">
                        <Description>
							Indicates if Order line is bundle parent line or not.
							<div>
                                <br/>
								Valid Values:
								<b>
                                    <code>Y</code>
                                </b>
								or
								<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Status" Name="MaxLineStatus">
                        <Description>Maximum order line status among all records that
							belong to an order.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="MaxLineStatusDesc">
                        <Description>Description of maximum order line status among all
							records that belong to an order.
						</Description>
                    </Attribute>
                    <Attribute DataType="Status" Name="MinLineStatus">
                        <Description>Minimum order line status among all records that
							belong to an order.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="MinLineStatusDesc">
                        <Description>Description of minimum order line status among all
							records that belong to an order.
						</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="MultipleStatusesExist">
                        <Description>If order line is in more than one status then this
							flag will be returned as "Y".
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="OpenQty">
                        <Description>Total quantity that can be cancelled from the line.
						</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="OverallStatus">
                        <Description>
                            A description of the order line's overall status determined by the <code>YFSGetOverallStatusUE</code>.
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="RemainingQty">
                        <Description>Remaining quantity.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ReturnReasonLongDesc">
                        <Description>Long description of the return reason code.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ReturnReasonShortDesc">
                        <Description>Short description of the return reason code.
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="ReturnableQty">
                        <Description>Quantity of item that could be returned from this line at its current status.
						</Description>
                    </Attribute>
                    <Attribute EntityName="SCAC_And_Service"
                        Name="ScacAndService" NameInEntity="Scac_And_Service">
                        <Description>Value of SCAC and Service code.
						</Description>
                    </Attribute>
                    <Attribute EntityName="SCAC_And_Service"
                        Name="ScacAndServiceKey" NameInEntity="Scac_And_Service_Key">
                        <Description>Link to YFS_SCAC_AND_SERVICE table.
						</Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="Status">
                        <Description>Status description of the order line.
						</Description>
                    </Attribute>
                    <Attribute DataType="Quantity"
                        EntityName="Order_Release_Status"
                        Name="StatusQuantity" NameInEntity="Status_Quantity">
                        <Description>Quantity on the order line in the current status.
					</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="SubstituteItemID">
                        <Description>Substituted ItemID. This field is only populated if the item on the Order line is substituted. 
					</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="SubstituteItemUOM">
                        <Description>Substituted Item UOM. This field is only populated if the item on the Order line is substituted.
					</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>
                            <div>
								Indicates if data is fetched from the history table rather than
								the regular table.
								<br/>
								Default:
								<b>
                                    <code>N</code>
                                </b>
                                <br/>
								Valid Values:
								<b>
                                    <code>Y</code>
                                </b>
								or
								<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <OrderLineTranQuantity OrderedQty="" OriginalOrderedQty=""
                StatusQuantity="" TransactionalUOM="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="OrderedQty">
                            <Description>The quantity of the the line item that has been ordered. If part of line is cancelled, this number is reduced appropriately. If an order line is returned, this number is not reduced.
						  </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="OriginalOrderedQty">
                            <Description>Quantity which was ordered during order creation. This is not modified after order confirmation.
						  </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="StatusQuantity">
                            <Description>Quantity on the order line in the current status.
						  </Description>
                        </Attribute>
                        <Attribute DataType="UOM" Name="TransactionalUOM">
                            <Description>UOM on which all the transactional quantities are specified.
						  </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </OrderLineTranQuantity>
            <Order BuyerOrganizationCode="" DocumentType=""
                DraftOrderFlag="" EnterpriseCode="" OrderHeaderKey=""
                OrderNo="" PaymentRuleId="" PaymentStatus="" SellerOrganizationCode=""/>
            <Item ItemDesc="" ItemID="" ItemShortDesc="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Order_Line"/>
                </yfc:doc>
            </Item>
            <Shipnode Description="" IdentifiedByParentAs="" OwnerKey="">
                <yfc:includeInParent IncludeXMLChildNodes="N" IncludeXMLFileName="YFS_Ship_Node_Element_master.xml"/>
                <yfc:doc>
                    <Element EntityName="Ship_Node"/>
                </yfc:doc>
            </Shipnode>
        </OrderLine>
        <OrderStatusTranQuantity StatusQty="" TotalQuantity="" TransactionalUOM="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Quantity" Name="StatusQty">
                        <Description>Quantity which has been moved into this status.
				  </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="TotalQuantity">
                        <Description>Total quantity which was ever moved into this status. If you have a cyclic status (e.g, backorder which can happen multiple times), this column will not provide useful information.
				  </Description>
                    </Attribute>
                    <Attribute DataType="UOM" Name="TransactionalUOM">
                        <Description>UOM on which all the transactional quantities are specified.
				  </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </OrderStatusTranQuantity>
        <ShipNode Description="" IdentifiedByParentAs="" OwnerKey="">
            <yfc:includeInParent IncludeXMLChildNodes="N" IncludeXMLFileName="YFS_Ship_Node_Element_master.xml"/>
            <yfc:doc>
                <Element EntityName="Ship_Node"/>
            </yfc:doc>
        </ShipNode>
    </OrderStatus>
    <FilteredStatusList>
        <yfc:doc>
            <Element>
                <Description>
                    <div>List of OrderReleaseStatus records that cannot be processed by the transaction passed in the input due to unmet transaction dependencies.</div>
                </Description>
            </Element>
        </yfc:doc>
        <OrderStatus MergeNode="" OrderHeaderKey="" OrderLineKey=""
            OrderLineScheduleKey="" OrderReleaseKey=""
            OrderReleaseStatusKey="" PipelineKey="" ProcureFromNode=""
            ReceivingNode="" ShipNode="" Status="" StatusDate=""
            StatusDescription="" StatusReason="" TotalQuantity="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Order_Line_Schedule"
                        Name="MergeNode" NameInEntity="Merge_Node"/>
                    <Attribute EntityName="Order_Line_Schedule"
                        Name="ProcureFromNode" NameInEntity="Procure_From_Node"/>
                    <Attribute EntityName="Order_Line_Schedule"
                        Name="ReceivingNode" NameInEntity="Receiving_Node"/>
                    <Attribute EntityName="Order_Line_Schedule"
                        Name="ShipNode" NameInEntity="Ship_Node"/>
                    <Attribute DataType="Text-100" Name="StatusDescription">
                        <Description>Description of Order Line Status
        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="StatusReason"/>
                </Attributes>
            </yfc:doc>
            <OrderStatusTranQuantity StatusQty="" TotalQuantity="" TransactionalUOM="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="StatusQty">
                            <Description>Quantity which has been moved into this status.
				  </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="TotalQuantity">
                            <Description>Total quantity which was ever moved into this status. If you have a cyclic status (e.g, backorder which can happen multiple times), this column will not provide useful information.
				  </Description>
                        </Attribute>
                        <Attribute DataType="UOM" Name="TransactionalUOM">
                            <Description>UOM on which all the transactional quantities are specified.
				  </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </OrderStatusTranQuantity>
            <Schedule BatchNo="" ExpectedShipmentDate="" LotNumber=""
                OrderLineScheduleKey="" RevisionNo="" ShipByDate=""
                ShipNode="" TagNumber=""/>
            <OrderRelease OrderHeaderKey="" OrderReleaseKey=""
                ReleaseNo="" ShipNode=""/>
            <OrderLine GiftFlag="" ItemGroupCode="" LevelOfService=""
                OrderHeaderKey="" OrderLineKey="" OrderedQty=""
                OriginalOrderedQty="" PrimeLineNo="" ShipNode=""
                ShipToKey="" StatusQuantity="" SubLineNo="">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Order_Release_Status"
                            Name="StatusQuantity" NameInEntity="Status_Quantity"/>
                    </Attributes>
                </yfc:doc>
                <OrderLineTranQuantity OrderedQty=""
                    OriginalOrderedQty="" StatusQuantity="" TransactionalUOM="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="OrderedQty">
                                <Description>The quantity of the the line item that has been ordered. If part of line is cancelled, this number is reduced appropriately. If an order line is returned, this number is not reduced.
						  </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="OriginalOrderedQty">
                                <Description>Quantity which was ordered during order creation. This is not modified after order confirmation.
						  </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="StatusQuantity">
                                <Description>Quantity on the order line in the current status.
						  </Description>
                            </Attribute>
                            <Attribute DataType="UOM" Name="TransactionalUOM">
                                <Description>UOM on which all the transactional quantities are specified.
						  </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </OrderLineTranQuantity>
                <Order BuyerOrganizationCode="" DocumentType=""
                    DraftOrderFlag="" EnterpriseCode=""
                    OrderHeaderKey="" OrderNo="" PaymentRuleId=""
                    PaymentStatus="" SellerOrganizationCode=""/>
                <Item ItemDesc="" ItemID="" ItemShortDesc=""
                    ProductClass="" UnitOfMeasure="">
                    <yfc:doc>
                        <Element EntityName="Order_Line"/>
                    </yfc:doc>
                </Item>
                <Shipnode Description="" IdentifiedByParentAs="" OwnerKey="">
                    <yfc:includeInParent IncludeXMLChildNodes="N" IncludeXMLFileName="YFS_Ship_Node_Element_master.xml"/>
                    <yfc:doc>
                        <Element EntityName="Ship_Node"/>
                    </yfc:doc>
                </Shipnode>
            </OrderLine>
            <ShipNode Description="" IdentifiedByParentAs="" OwnerKey="">
                <yfc:includeInParent IncludeXMLChildNodes="N" IncludeXMLFileName="YFS_Ship_Node_Element_master.xml"/>
                <yfc:doc>
                    <Element EntityName="Ship_Node"/>
                </yfc:doc>
            </ShipNode>
        </OrderStatus>
    </FilteredStatusList>
    <!-- DropStatuses is a template-driven element but its attributes are not. If TransactionId is passed to the api, it will come in the output xml if this element comes. -->
    <DropStatuses TransactionId="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="TransactionID" Name="TransactionId">
                    <Description>
                        <div>
				If TransactionId is passed, all statuses which will be picked up by this transaction will be returned.
				</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <DropStatus Description="" OwnerKey="" ProcessTypeKey=""
            Status="" StatusKey="" StatusName="" StatusType="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="Description">
                        <Description>
                            <div>
				Description of the status
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="OwnerKey">
                        <Description>
                            <div>
				The key of the Owner
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="ProcessTypeKey">
                        <Description>
                            <div>
				The Key of the Process Type
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Status" Name="Status">
                        <Description>
                            <div>
					Status
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="StatusKey">
                        <Description>
                            <div>
				Status Key
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-50" Name="StatusName">
                        <Description>
                            <div>
				The name of the status
				</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-15" Name="StatusType">
                        <Description>
                            <div>
				Depicts whether the status is a System or User Created Status. Possible values are <b>SYSTEM</b> or  <b>USER</b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </DropStatus>
    </DropStatuses>
</OrderLineStatusList>
