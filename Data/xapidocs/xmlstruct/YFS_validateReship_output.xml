                    IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/OrderLines/OrderLine/ComputedPrice"/>
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Money" Name="AdditionalLinePriceTotal">
                            <Description>Total Price for all non-informational Additional
								Line Prices for the Component Lines of a Bundle Parent. List Price times ActualPricingQuantity.
							</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="Charges">
                            <Description>
                                <div>The computed amount of the charges for this line.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="Discount">
                            <Description>
                                <div>The computed total of the discount amounts.
								</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="ExtendedPrice">
                            <Description>The computed price for this line before taxes,
								charges, and
								discounts are accounted for.</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="LineCost">
                            <Description>
								The total cost incurred on this order line. If this order line
								is part of a bundle,
								<code>LineCost</code>
								is evaluated based on the bundle's' pricing strategy.
							</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="LineTotal">
                            <Description>The total amount for this line.</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="LineTotalWithoutTax">
                            <Description>The computed total amount for this line before taxes
								are accounted
								for.</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="PercentProfitMargin">
                            <Description>The percentage of the profit margin for this line.
								Depending on the pricing strategy, all components or part of the
								components are considered for the profit margin calculation. 
								OrderLine Option OptionPrices and OrderLine AdditionalLinePriceTotals are
								not considered for the Profit Margin.
							</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="Tax">
                            <Description>The computed amount of taxes for this line.</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="UnitPrice">
                            <Description>The price for one unit of this line.</Description>
                        </Attribute>
                    </Attributes>
                    <Element>
                        <Description>
							Contains computed information about the price and total amount
							details
							for the order line. If the line is a bundle parent,
							components with
							<code>IsLinePriceForInformationOnly</code>
							flag set to
							<code>Y</code>
							are ignored in
							the total calculation. If the bundle parent is a
							recommended line,
							then the total
							will be based on the
							<code>BundlePricingStrategy</code>
							defined for the item.
						</Description>
                    </Element>
