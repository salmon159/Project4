                TranDiscrepancyQty="" TransactionalUOM="">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/OrderLines/OrderLine/OrderLineTranQuantity"/>
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="FillQuantity">
                            <Description>The minimum order line quantity that must be
								fulfilled in an order release before it is shipped.
							</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="InvoicedQuantity">
                            <Description>Invoiced quantity on the order line.
							</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="OpenQty">
                            <Description>Total quantity that can be cancelled from the line.
							</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="OrderedQty">
                            <Description>The quantity of the the line item that has been
								ordered. If part of line is cancelled, this number is reduced
								appropriately. If an order line is returned, this number is not
								reduced.
							</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="OriginalOrderedQty">
                            <Description>Quantity which was ordered during order creation.
								This is not modified after order confirmation.
							</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="ReceivedQty">
                            <Description>Received quantity on the order line.					  </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="RemainingQty">
                            <Description>Remaining quantity.
							</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="SettledQuantity">
                            <Description>Quantity which should be settled before it is
								actually invoiced.
							</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="ShippedQuantity">
                            <Description>Shippped quantity on the order line.					  </Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="SplitQty">
                            <Description>Quantity which has been split from this line due to
								substitutions, price negotiations etc.
							</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="StatusQuantity">
                            <Description>Quantity on the order line in the current status.
							</Description>
                        </Attribute>
                        <Attribute DataType="Quantity" Name="TranDiscrepancyQty">
                            <Description>Quantity which was over or under received.
							</Description>
                        </Attribute>
                        <Attribute DataType="UOM" Name="TransactionalUOM">
                            <Description>UOM on which all the transactional quantities are
								specified.
							</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </OrderLineTranQuantity>
            <BundleParentLine OrderLineKey="" PrimeLineNo="" SubLineNo="">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="MasterOrder.xml" IncludeXMLPath="/Order/OrderLines/OrderLine/BundleParentLine"/>
                <yfc:doc>
                    <Element EntityName="Order_Line">
                        <Description>Specifies the unique identifiers of the bundle parent
							order line for this order line.</Description>
                    </Element>
                </yfc:doc>
            </BundleParentLine>
        </OrderLine>
    </OrderLines>
</Order>
