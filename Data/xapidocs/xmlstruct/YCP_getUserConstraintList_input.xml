<User LoginId="" MaximumRecords="" OnlyLoggedInUsers=""
    OrganizationKey="" UserId="" UserKey="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getUserConstraintList/input">
    <yfc:doc>
        <Attributes>
            <Attribute Name="LoginId">
                <Description> Login id for whom constraints to be searched based on partial criteria such as "LIKE","CONTAINS".</Description>
            </Attribute>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description> Maximum records return by list api.</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="OnlyLoggedInUsers">
                <Description>
                    <div>				Pass as <b>
                            <code>Y</code>
                        </b> to return only logged-in users. 				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="UserId">
                <Description> User id for whom constraints to be searched.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <EligibleTaskTypeList>
        <EligibleTaskType TaskType="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Task"/>
            </yfc:doc>
        </EligibleTaskType>
    </EligibleTaskTypeList>
    <UserZoneList>
        <UserZone Aisle="" Node="" RecordType="" ZoneId=""/>
    </UserZoneList>
    <UserTranConstraintList>
        <UserTranConstraint ActiveFlag="" AssignedToUserId="" BatchNo=""
            ContainerNo="" CountRequestNo="" EnterpriseKey=""
            MoveRequestNo="" OrganizationCode="" ReceiptNo=""
            ShipmentNo="" TaskType="" UserTranConstraintsKey=""
            WaveNo="" WorkOrderNo="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="MasterUserTranConstraint.xml" IncludeXMLPath="/UserTranConstraint"/>
        </UserTranConstraint>
    </UserTranConstraintList>
</User>
