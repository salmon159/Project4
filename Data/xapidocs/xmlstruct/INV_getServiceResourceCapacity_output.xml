<!-- getServiceResourceCapacity output XML -->
<ServiceResources EndDate="" NumberOfDaysFromStartDate="" StartDate=""
    TransactInWorkDay="" xmlns="http://www.sterlingcommerce.com/documentation/INV/getServiceResourceCapacity/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Date" Name="EndDate">
                <Description>
                    <div>
					The end date of the date range.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="NumberOfDaysFromStartDate">
                <Description>
                    <div>
					The number of days (including start date) from the start date for the date range to the end date.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Date" Name="StartDate">
                <Description>
                    <div>
					The start date of the date range.  
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="TransactInWorkDay">
                <Description>
                    <div>
					The flag will be set to <code>
                            <b>Y</b>
                        </code> only if the flag is passed as  <code>
                            <b>Y</b>
                        </code> in the input.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ServiceResource ServiceResourceId="" ServiceResourceKey="" yfc:DTDOccurrence="ZeroOrMany">
        <ResourcePool ItemGroupCode="" Node=""
            ProviderOrganizationCode="" ResourcePoolDescription=""
            ResourcePoolId="" ResourcePoolKey=""/>
        <Dates>
            <Date Date="" EarliestShiftStartTime=""
                HasCalendarException="" LatestShiftEndTime=""
                TotalAvailableHours="" TotalWorkingHours="" yfc:DTDOccurrence="OneOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Date" Name="Date">
                            <Description>
                                <div>Date.</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Time" Name="EarliestShiftStartTime">
                            <Description>
                                <div>StartTime of the first shift from the resource calendar.</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="HasCalendarException">
                            <Description>
                                <div>Indicates whether the resource calendar has any calendar day exception for the date or not.</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Time" Name="LatestShiftEndTime">
                            <Description>
                                <div>EndTime of the last shift from the resource calendar.</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="NumberOfHours" Name="TotalAvailableHours">
                            <Description>
                                <div>The total available hours for the date.</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="NumberOfHours" Name="TotalWorkingHours">
                            <Description>
                                <div>The total working hours for the date according to the resource or the service resource's calendar.</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <AssignedToResourcePool ResourcePoolDescription=""
                    ResourcePoolId="" ResourcePoolKey="">
                    <yfc:doc>
                        <Element EntityName="Res_Pool">
                            <Description>
                                <div>The resource pool to which, the service resource is assigned to for the date.</div>
                            </Description>
                        </Element>
                    </yfc:doc>
                </AssignedToResourcePool>
                <UnavailableSlots>
                    <UnavailableSlot DueToCalendar=""
                        DueToConcurrentAppt="" EndTime="" StartTime="" yfc:DTDOccurrence="OneOrMany">
                        <yfc:doc>
                            <Element>
                                <Description>
                                    <div>Unavailable slot within the date.</div>
                                </Description>
                            </Element>
                            <Attributes>
                                <Attribute DataType="Flag" Name="DueToCalendar">
                                    <Description>
                                    <div>This flag indicates that the time slot is unavailable because it is not a working slot according to service resource's calendar.</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="DueToConcurrentAppt">
                                    <Description>
                                    <div>This flag indicates that the time slot is unavailable because the maximum number of concurrent appointments for the time slot of the resource pool has been reached.</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Time" Name="EndTime">
                                    <Description>
                                    <div>The end time of the unavailable slot.</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Time" Name="StartTime">
                                    <Description>
                                    <div>The start time of the unavailable slot.</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <CurrentConsumptions>
                            <CurrentConsumption ResPoolConsmptnDtlsKey="">
                                <yfc:doc>
                                    <Element>
                                    <Description>
                                    <div>Current non-slot based consumption of the unavailable slot.</div>
                                    </Description>
                                    </Element>
                                    <Attributes>
                                    <Attribute
                                    EntityName="Res_Pool_Consmptn_Dtls"
                                    Name="ResPoolConsmptnDtlsKey" NameInEntity="Res_Pool_Consmptn_Dtls_Key"/>
                                    </Attributes>
                                </yfc:doc>
                            </CurrentConsumption>
                        </CurrentConsumptions>
                    </UnavailableSlot>
                </UnavailableSlots>
            </Date>
        </Dates>
    </ServiceResource>
</ServiceResources>
