<GetLinesToReceive ArrivalDateTime="" DocumentType="Required"
    DriverName="" NumOfCartons="" NumOfPallets="" OpenReceiptFlag=""
    ReceiptDate="" ReceiptHeaderKey="" ReceiptNo="" ReceivingDock=""
    ReceivingNode="Required" ShipmentKey="" TrailerLPNNo="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getLinesToReceive/input">
    <yfc:doc>
        <Element EntityName="Receipt_Header">
            <Description>
                <div>
                    <Validation>
                        <Condition>Error if more than one Receipt is found for the given input.
							</Condition>
                        <Error
                            Description="Multiple receipts found for receiving." ErrorCode="YFS10482"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Document_Params" Name="DocumentType">
                <Description>
                    <div>
					The Document Type of the order being received.
						<Validation>
                            <Condition>
                                <code>DocumentType</code> should not be void.
							</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Receipt_Header" Name="ReceivingNode" NameInEntity="ReceivingNode_Key">
                <Description>
                    <div>
					The Node that is receiving the shipment.
						<Validation>
                            <Condition>
                                <code>ReceivingNode</code> should not be void.
							</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Shipment ActualDeliveryDate="" ActualFreightCharge=""
        ActualShipmentDate="" AppointmentNo="" AssociationAction=""
        BolNo="" BuyerOrganizationCode="" CarrierAccountNo=""
        CarrierServiceCode="" CarrierType="" Code="" CommercialValue=""
        Currency="" DeliveryPlanKey="" DeliveryTS="" DestinationZone=""
        DocumentType="" DownLoadCount="" EnterpriseCode=""
        ExpectedDeliveryDate="" ExpectedShipmentDate=""
        ExportTaxpayerID="" FreightTerms="" FromAddressKey=""
        FromAppointment="" HasOtherShipments="" HazardousMaterialFlag=""
        HeldForConfirmation="" ITDate="" ITNo="" InvoiceComplete=""
        IsSingleOrder="" LevelOfService="" LinesEntered=""
        ManifestKey="" ManifestNo="" ManuallyEntered="" MergeNode=""
        NumOfCartons="" NumOfPallets="" OrderAvailableOnSystem=""
        OrderHeaderKey="" OrderNo="" OrderReleaseKey="" OriginZone=""
        OverrideManualShipmentEntry="" PackListType=""
        ParentShipmentKey="" PickListNo="" PickticketNo=""
        PipelineKey="" PodNo="" ProNo="" ReceivingNode="" ReleaseNo=""
        RequestedDeliveryDate="" RequestedShipmentDate=""
        RequiresApptConfirmation="" SCAC="" ScacIntegrationRequired=""
        SealNo="" SellerOrganizationCode="" ShipDate="" ShipMode=""
        ShipNode="" ShipVia="" ShipmentClosedFlag=""
        ShipmentConfirmUpdatesDone="" ShipmentContainerizedFlag=""
        ShipmentDeliverUpdatesDone="" ShipmentKey="" ShipmentNo=""
        ShipmentPlannedFlag="" ShipmentType="" Status="" StatusDate=""
        ToAppointment="" TotalActualCharge="" TotalEstimatedCharge=""
        TotalVolume="" TotalVolumeUOM="" TotalWeight=""
        TotalWeightUOM="" TrackingNo="" TrailerNo="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Shipment_tran.xml" IncludeXMLPath="/Shipment"/>
        <yfc:doc>
            <Element EntityName="Shipment"/>
            <Attributes>
                <Attribute DataType="String" Name="HeldForConfirmation"/>
            </Attributes>
        </yfc:doc>
    </Shipment>
</GetLinesToReceive>
