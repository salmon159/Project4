<InvoiceDetail xmlns="http://www.sterlingcommerce.com/documentation/YFS/SEND_INVOICE/PUBLISH_INVOIVE_DETAIL">
    <!-- InvoiceHeader is a template-driven element but some of its attributes are not. Attributes that are not template driven: TotalCharges, TotalDiscount, TotalHeaderCharges, HeaderCharges, HeaderDiscount, HeaderTax and LineSubTotal.   -->
    <InvoiceHeader ActualFreightCharge="" AmountCollected="Required"
        ChargedActualFreight="" CollectedExternally=""
        CollectedThroughAR="" DateInvoiced=""
        DerivedFromOrderHeaderKey="" HeaderCharges="" HeaderDiscount=""
        HeaderTax="" InvoiceNo="" InvoiceType="" LineSubTotal=""
        MasterInvoiceNo="" OrderInvoiceKey="" Reference1=""
        TotalAmount="" TotalCharges="" TotalDiscount=""
        TotalHeaderCharges="" TotalTax="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="ActualFreightCharge"/>
                <Attribute DataType="String" Name="AmountCollected"/>
                <Attribute DataType="String" Name="ChargedActualFreight"/>
                <Attribute DataType="String" Name="CollectedExternally"/>
                <Attribute DataType="String" Name="CollectedThroughAR"/>
                <Attribute DataType="String" Name="DateInvoiced"/>
                <Attribute DataType="String" Name="DerivedFromOrderHeaderKey"/>
                <Attribute DataType="String" Name="HeaderCharges"/>
                <Attribute DataType="String" Name="HeaderDiscount"/>
                <Attribute DataType="String" Name="HeaderTax"/>
                <Attribute DataType="String" Name="InvoiceNo"/>
                <Attribute DataType="String" Name="InvoiceType"/>
                <Attribute DataType="String" Name="LineSubTotal"/>
                <Attribute DataType="String" Name="MasterInvoiceNo"/>
                <Attribute DataType="String" Name="OrderInvoiceKey"/>
                <Attribute DataType="String" Name="Reference1"/>
                <Attribute DataType="String" Name="TotalAmount"/>
                <Attribute DataType="String" Name="TotalCharges"/>
                <Attribute DataType="String" Name="TotalDiscount"/>
                <Attribute DataType="String" Name="TotalHeaderCharges"/>
                <Attribute DataType="String" Name="TotalTax"/>
            </Attributes>
        </yfc:doc>
        <Order BuyerOrganizationCode="" DeliveryCode="" DocumentType=""
            EnteredBy="" EnterpriseCode="" EntryType=""
            OrderHeaderKey="" OrderNo="" SellerOrganizationCode="" TermsCode="">
            <PriceInfo Currency=""/>
            <PersonInfoBillTo AddressID="" AddressLine1=""
                AddressLine2="" AddressLine3="" AddressLine4=""
                AddressLine5="" AddressLine6="" AlternateEmailID=""
                Beeper="" City="" Company="" Country="" DayFaxNo=""
                DayPhone="" Department="" EMailID="" EveningFaxNo=""
                EveningPhone="" FirstName="" IsCommercialAddress=""
                JobTitle="" LastName="" MiddleName="" MobilePhone=""
                OtherPhone="" PersonID="" PersonInfoKey="" State=""
                Suffix="" TaxGeoCode="" Title="" ZipCode=""/>
            <AdditionalAddresses>
                <AdditionalAddress AddressType="">
                    <PersonInfo AddressID="" AddressLine1=""
                        AddressLine2="" AddressLine3="" AddressLine4=""
                        AddressLine5="" AddressLine6="" AddressType=""
                        AlternateEmailID="" Beeper="" City="" Company=""
                        Country="" DayFaxNo="" DayPhone="" Department=""
                        EMailID="" EveningFaxNo="" EveningPhone=""
                        FirstName="" IsCommercialAddress="" JobTitle=""
                        LastName="" MiddleName="" MobilePhone=""
                        OtherPhone="" PersonID="" State="" Suffix=""
                        TaxGeoCode="" Title="" ZipCode="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="String" Name="AddressType"/>
                            </Attributes>
                        </yfc:doc>
                    </PersonInfo>
                </AdditionalAddress>
            </AdditionalAddresses>
            <References>
                <Reference Name="" Value="" yfc:DTDOccurrence="ZeroOrMany"/>
            </References>
        </Order>
        <Shipment ManifestNo="" ReceivingNode="" SCAC="" ShipDate=""
            ShipNode="" ShipmentKey="">
            <ToAddress AddressID="" AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" EveningFaxNo="" EveningPhone=""
                FirstName="" HttpUrl="" IsCommercialAddress=""
                JobTitle="" LastName="" MiddleName="" MobilePhone=""
                OtherPhone="" PersonID="" State="" Suffix=""
                TaxGeoCode="" Title="" ZipCode=""/>
            <ShipNode ShipnodeKey="">
                <ShipNodePersonInfo AddressID="" AddressLine1=""
                    AddressLine2="" AddressLine3="" AddressLine4=""
                    AddressLine5="" AddressLine6="" AlternateEmailID=""
                    Beeper="" City="" Company="" Country="" DayFaxNo=""
                    DayPhone="" Department="" EMailID="" EveningFaxNo=""
                    EveningPhone="" FirstName="" HttpUrl=""
                    IsCommercialAddress="" JobTitle="" LastName=""
                    MiddleName="" MobilePhone="" OtherPhone=""
                    PersonID="" State="" Suffix="" TaxGeoCode=""
                    Title="" ZipCode=""/>
            </ShipNode>
        </Shipment>
        <!-- TotalLines is not a template driven attribute. -->
        <LineDetails TotalLines="" yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="TotalLines"/>
                </Attributes>
            </yfc:doc>
            <LineDetail Charges="" ExtendedPrice="" LineTotal=""
                OrderInvoiceDetailKey="Required" OrderLineKey=""
                PrimeLineNo="" ShippedQty="" SubLineNo="" Tax=""
                UnitPrice="" yfc:DTDOccurrence="OneOrMany">
                <yfc:doc>
                    <Element EntityName="Order_Invoice_Detail"/>
                </yfc:doc>
                <LineDetailTranQuantity ShippedQty="" TransactionalUOM="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="ShippedQty"/>
                            <Attribute DataType="UOM" Name="TransactionalUOM">
                                <Description>UOM on which all the transactional quantities are specified.
						  </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </LineDetailTranQuantity>
                <OrderLine OrderLineKey="" PrimeLineNo="" SubLineNo="">
                    <Item ItemID="" ProductClass="" UnitOfMeasure="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="String" Name="ProductClass"/>
                            </Attributes>
                        </yfc:doc>
                        <LanguageDescriptionList>
                            <LanguageDescription Description=""
                                ExtendedDescription="" LocaleCode=""
                                ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
                        </LanguageDescriptionList>
                    </Item>
                    <LinePriceInfo DiscountPercentage=""
                        DiscountReference="" DiscountType=""
                        InvoicedPricingQty=""
                        IsLinePriceForInformationOnly="" ListPrice=""
                        PricingQty="" PricingQtyConversionFactor=""
                        PricingUOM="" RetailPrice=""
                        TaxExemptionCertificate="" TaxableFlag="" UnitPrice="">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="String" Name="PricingQty"/>
                            </Attributes>
                        </yfc:doc>
                    </LinePriceInfo>
                    <References>
                        <Reference Name="" Value="" yfc:DTDOccurrence="ZeroOrMany"/>
                    </References>
                </OrderLine>
                <!-- LineCharges is not a template driven element.  If a line has line charges, LineCharges will appear in the output alongwith all its child elements.-->
                <LineCharges>
                    <LineCharge ChargeAmount="" ChargeCategory=""
                        ChargeName="" ChargeNameKey="" ChargePerLine=""
                        ChargePerUnit="" IsBillable="" IsDiscount=""
                        IsShippingCharge="|N|Y" Reference="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="String" Name="ChargeNameKey"/>
                                <Attribute DataType="Flag" Name="IsBillable">
                                    <Description>Describes if the charge is billable. Default: <code>N</code>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="IsDiscount">
                                    <Description>Describes if the charge is discountable. Default: <code>N</code>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Flag" Name="IsShippingCharge">
                                    <Description>Determines whether the charge is a shipping charge. This attribute value is system defined and cannot be overridden. The value for this attribute is set as <code>Y</code> only for Charge Name configured as <code>CHARGE_NAME_FOR_SHIPPING</code> in financial rule. For all other Charges this attribute is set to <code>N</code>.
									</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </LineCharge>
                </LineCharges>
                <!-- LineTaxes is not a template driven element.  If a line has line taxes, LineTaxes will appear in the output alongwith all its child elements.-->
                <LineTaxes>
                    <LineTax ChargeCategory="" ChargeName=""
                        ChargeNameKey="" Reference1="" Reference2=""
                        Reference3="" Tax="" TaxName="" TaxPercentage="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Attributes>
                                <Attribute DataType="String" Name="ChargeCategory"/>
                                <Attribute DataType="String" Name="ChargeName"/>
                                <Attribute DataType="String" Name="ChargeNameKey"/>
                                <Attribute DataType="String" Name="Reference1"/>
                                <Attribute DataType="String" Name="Reference2"/>
                                <Attribute DataType="String" Name="Reference3"/>
                                <Attribute DataType="String" Name="Tax"/>
                                <Attribute DataType="String" Name="TaxName"/>
                                <Attribute DataType="String" Name="TaxPercentage"/>
                            </Attributes>
                        </yfc:doc>
                    </LineTax>
                    <TaxSummary>
                        <TaxSummaryDetail Tax="Required" TaxName="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute DataType="String" Name="Tax"/>
                                    <Attribute DataType="String" Name="TaxName"/>
                                </Attributes>
                            </yfc:doc>
                        </TaxSummaryDetail>
                    </TaxSummary>
                </LineTaxes>
                <References>
                    <Reference Name="" Value="" yfc:DTDOccurrence="ZeroOrMany"/>
                </References>
            </LineDetail>
        </LineDetails>
        <TotalSummary yfc:DTDOccurrence="REQUIRED">
            <ChargeSummary>
                <!-- ChargeSummaryDetail is not a template-driven element. -->
                <ChargeSummaryDetail ChargeAmount="" ChargeCategory=""
                    ChargeName="" ChargeNameKey="" Reference="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="ChargeAmount"/>
                            <Attribute DataType="String" Name="ChargeCategory"/>
                            <Attribute DataType="String" Name="ChargeName"/>
                            <Attribute DataType="String" Name="ChargeNameKey"/>
                            <Attribute DataType="String" Name="Reference"/>
                        </Attributes>
                    </yfc:doc>
                </ChargeSummaryDetail>
            </ChargeSummary>
            <TaxSummary>
                <!-- ChargeSummaryDetail is not a template-driven element. -->
                <TaxSummaryDetail Tax="Required" TaxName="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="Tax"/>
                            <Attribute DataType="String" Name="TaxName"/>
                        </Attributes>
                    </yfc:doc>
                </TaxSummaryDetail>
            </TaxSummary>
        </TotalSummary>
        <HeaderCharges yfc:DTDOccurrence="REQUIRED">
            <!-- HeaderCharge is not a template driven element. If the template contains HeaderCharges element, HeaderCharge will appear in the output, alongwith all its attributes. -->
            <HeaderCharge ChargeAmount="" ChargeCategory=""
                ChargeName="" ChargeNameKey="" IsBillable=""
                IsDiscount="" IsShippingCharge="" Reference="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="ChargeNameKey"/>
                        <Attribute DataType="Flag" Name="IsBillable">
                            <Description>Describes if the charge is billable. Default: <code>N</code>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsDiscount">
                            <Description>Describes if the charge is discountable. Default: <code>N</code>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsShippingCharge">
                            <Description>Determines whether the charge is a shipping charge. This attribute value is system defined and cannot be overridden. The value for this attribute is set as <code>Y</code> only for Charge Name configured as <code>CHARGE_NAME_FOR_SHIPPING</code> in financial rule. For all other Charges this attribute is set to <code>N</code>.
						</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </HeaderCharge>
        </HeaderCharges>
        <!-- HeaderTaxes is not a template driven element. If the template contains HeaderCharges element, HeaderTaxes will appear in the output, alongwith all its child elements -->
        <HeaderTaxes>
            <HeaderTax ChargeCategory="" ChargeName="" ChargeNameKey=""
                Reference1="" Reference2="" Reference3="" Tax=""
                TaxName="" TaxPercentage="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="ChargeCategory"/>
                        <Attribute DataType="String" Name="ChargeName"/>
                        <Attribute DataType="String" Name="ChargeNameKey"/>
                        <Attribute DataType="String" Name="Reference1"/>
                        <Attribute DataType="String" Name="Reference2"/>
                        <Attribute DataType="String" Name="Reference3"/>
                        <Attribute DataType="String" Name="Tax"/>
                        <Attribute DataType="String" Name="TaxName"/>
                        <Attribute DataType="String" Name="TaxPercentage"/>
                    </Attributes>
                </yfc:doc>
            </HeaderTax>
            <TaxSummary>
                <TaxSummaryDetail Tax="Required" TaxName="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="Tax"/>
                            <Attribute DataType="String" Name="TaxName"/>
                        </Attributes>
                    </yfc:doc>
                </TaxSummaryDetail>
            </TaxSummary>
        </HeaderTaxes>
        <CollectionDetails TotalLines="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="TotalLines"/>
                </Attributes>
            </yfc:doc>
            <CollectionDetail AmountCollected="Required"
                AuditTransactionID="" AuthorizationExpirationDate=""
                AuthorizationID="" BookAmount="" ChargeTransactionKey=""
                ChargeType="" CollectionDate="" CreditAmount=""
                DebitAmount="" DistributedAmount="" ExecutionDate=""
                HoldAgainstBook="" OpenAuthorizedAmount=""
                OrderHeaderKey="" OrderInvoiceKey="" PaymentKey=""
                RequestAmount="" SettledAmount="" Status=""
                StatusReason="" TransactionDate="" UserExitStatus="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="AmountCollected"/>
                        <Attribute DataType="String" Name="AuditTransactionID"/>
                        <Attribute DataType="String" Name="AuthorizationExpirationDate"/>
                        <Attribute DataType="String" Name="AuthorizationID"/>
                        <Attribute DataType="String" Name="BookAmount"/>
                        <Attribute DataType="String" Name="ChargeTransactionKey"/>
                        <Attribute DataType="String" Name="ChargeType"/>
                        <Attribute DataType="String" Name="CollectionDate"/>
                        <Attribute DataType="String" Name="CreditAmount"/>
                        <Attribute DataType="String" Name="DebitAmount"/>
                        <Attribute DataType="String" Name="DistributedAmount"/>
                        <Attribute DataType="String" Name="ExecutionDate"/>
                        <Attribute DataType="String" Name="HoldAgainstBook"/>
                        <Attribute DataType="String" Name="OpenAuthorizedAmount"/>
                        <Attribute DataType="String" Name="OrderHeaderKey"/>
                        <Attribute DataType="String" Name="OrderInvoiceKey"/>
                        <Attribute DataType="String" Name="PaymentKey"/>
                        <Attribute DataType="String" Name="RequestAmount"/>
                        <Attribute DataType="String" Name="SettledAmount"/>
                        <Attribute DataType="String" Name="Status"/>
                        <Attribute DataType="String" Name="StatusReason"/>
                        <Attribute DataType="String" Name="TransactionDate"/>
                        <Attribute DataType="String" Name="UserExitStatus"/>
                    </Attributes>
                </yfc:doc>
                <CreditCardTransactions>
                    <CreditCardTransaction AuthAmount="" AuthAvs=""
                        AuthCode="" AuthReturnCode="" AuthReturnFlag=""
                        AuthReturnMessage="" AuthTime="" CVVAuthCode=""
                        ChargeTransactionKey=""
                        CreditCardTransactionKey=""
                        InternalReturnCode="" InternalReturnFlag=""
                        InternalReturnMessage="" ParentKey=""
                        Reference1="" Reference2="" RequestId=""
                        TranAmount="" TranRequestTime=""
                        TranReturnCode="" TranReturnFlag=""
                        TranReturnMessage="" TranType=""/>
                </CreditCardTransactions>
                <PaymentMethod AwaitingAuthInterfaceAmount=""
                    AwaitingChargeInterfaceAmount="" BillToKey=""
                    CashBackAmount="" ChargeSequence="" CheckNo=""
                    CheckReference="" CreditCardExpDate=""
                    CreditCardName="" CreditCardNo="" CreditCardType=""
                    CustomerAccountNo="" CustomerPONo="" DebitCardNo=""
                    DisplayCreditCardNo="" DisplayCustomerAccountNo=""
                    DisplayDebitCardNo="" DisplayPaymentReference1=""
                    DisplayPrimaryAccountNo="" DisplaySvcNo=""
                    FirstName="" FundsAvailable=""
                    GetFundsAvailableUserExitInvoked=""
                    IncompletePaymentType="" LastName=""
                    MaxChargeLimit="" MiddleName="" PaymentKey=""
                    PaymentReference1="" PaymentReference2=""
                    PaymentReference3="" PaymentReference4=""
                    PaymentReference5="" PaymentReference6=""
                    PaymentReference7="" PaymentReference8=""
                    PaymentReference9="" PaymentTemplateType=""
                    PaymentType="" PaymentTypeGroup=""
                    PlannedRefundAmount="" PrimaryAccountNo=""
                    ReasonCode="" RequestedAuthAmount=""
                    RequestedChargeAmount="" RequestedRefundAmount=""
                    SuspendAnyMoreCharges=""
                    SuspendExpiredAuthReversal="" SvcNo=""
                    TotalAltRefundedAmount="" TotalAuthorized=""
                    TotalCharged="" TotalRefundedAmount=""
                    UnlimitedCharges="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:includeInParent IncludeXMLChildNodes="Y"
                        IncludeXMLFileName="MasterPaymentMethod.xml" IncludeXMLPath="/PaymentRelated/OutputForList/PaymentMethods/PaymentMethod"/>
                    <yfc:doc>
                        <Element EntityName="Payment"/>
                        <Attributes>
                            <Attribute DataType="Money" Name="AwaitingAuthInterfaceAmount">
                                <Description>
								The total authorization amount that has not been
								sent to the payment processing system for processing yet.
							</Description>
                            </Attribute>
                            <Attribute DataType="Money" Name="AwaitingChargeInterfaceAmount">
                                <Description>
								The total charge amount that has not been sent to
								the payment
								processing system for processing yet.
						</Description>
                            </Attribute>
                            <Attribute DataType="Text-4" Name="DisplayPrimaryAccountNo">
                                <Description>The appropriate display value of the primary
								identifier of this PaymentType, depending upon the
								PaymentTypeGroup.</Description>
                            </Attribute>
                            <Attribute DataType="Money" Name="FundsAvailable">
                                <Description>
                                    <div>
									The funds available to be charged on this payment method as
									determined by the
									<code>YFSGetFundsAvailableUE</code>
									user exit, if it was invoked. If the order is created for a
									customer and the payment method is a valid customer payment
									method, it indicates the amount available for the customer
									account. If customer account is not maintained internally or if
									the MaxChargeLimit is not set on the customer payment method,
									this value is returned as 0.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Flag" Name="GetFundsAvailableUserExitInvoked">
                                <Description>
                                    <div>
									Indicates if the
									<code>YFSGetFundsAvailableUE</code>
									user exit was invoked for this payment method.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Text-100"
                                EntityName="Payment_Type" Name="PaymentTemplateType"/>
                            <Attribute DataType="CodeValue"
                                EntityName="Payment_Type" Name="PaymentTypeGroup"/>
                            <Attribute DataType="Text-1000" Name="PrimaryAccountNo">
                                <Description>The appropriate primary identifier of this
								PaymentType, depending upon the PaymentTypeGroup.</Description>
                            </Attribute>
                            <Attribute Name="RequestedAuthAmount">
                                <Description>
								The total amount of authorizations sent to an
								external
								system and still
								awaiting response on this payment
								method. 
						</Description>
                            </Attribute>
                            <Attribute Name="RequestedChargeAmount">
                                <Description>
								The total amount of charges sent to an external
								system and still
								awaiting response on this payment method.
							</Description>
                            </Attribute>
                            <Attribute Name="RequestedRefundAmount">
                                <Description>
								The total amount of refunds sent to an external
								system and still
								awaiting response on this payment method. This
								number will be
								positive. This total will no longer be included in
								the
								RequestedChargeAmount, but if an asynchronous refund is
								submitted
								in excess of this amount, the RequestedChargeAmount
								will
								be
								reduced for backwards compatibility.
						</Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <PersonInfoBillTo AddressID="" AddressLine1=""
                        AddressLine2="" AddressLine3="" AddressLine4=""
                        AddressLine5="" AddressLine6=""
                        AlternateEmailID="" Beeper="" City="" Company=""
                        Country="" DayFaxNo="" DayPhone="" Department=""
                        EMailID="" EveningFaxNo="" EveningPhone=""
                        FirstName="" IsAddressVerified=""
                        IsCommercialAddress="" JobTitle="" LastName=""
                        Latitude="" Longitude="" MiddleName=""
                        MobilePhone="" OtherPhone="" PersonID=""
                        PersonInfoKey="" State="" Suffix=""
                        TaxGeoCode="" Title="" ZipCode="">
                        <yfc:includeInParent IncludeXMLChildNodes="Y"
                            IncludeXMLFileName="MasterPersonInfo.xml" IncludeXMLPath="/PersonInfo"/>
                    </PersonInfoBillTo>
                </PaymentMethod>
            </CollectionDetail>
        </CollectionDetails>
    </InvoiceHeader>
</InvoiceDetail>
