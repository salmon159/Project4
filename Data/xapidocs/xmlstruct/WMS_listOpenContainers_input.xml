<?xml version="1.0" encoding="UTF-8"?>
<OpenContainer CaseId="" IsShippingContainer="" LoadKey="" LocationId=""
    Node="Required" OpenContainerKey="" PalletId="" Scac="" ShipDate=""
    ShipmentKey="" SortStationId="" TargetLocationId="" ZoneId="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/listOpenContainers/input">
    <yfc:doc>
        <Element EntityName="Open_Container"/>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>
                    <div> A case identifier. 
					<Validation>
                            <Condition> Both<code>CaseId</code> and <code>PalletId</code> should not be passed.
						</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" Name="Node">
                <Description>
                    <div> Node identifier. 
				<Validation>
                            <Condition> Cannot be blank. </Condition>
                            <Error Description="Node not passed." ErrorCode="WMS00054"/>
                        </Validation>
                        <Validation>
                            <Condition> Must be a valid node. </Condition>
                            <Error
                                Description="Node passed does not exist." ErrorCode="WMS00042"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>
                    <div>A pallet identifier.
						<Validation>
                            <Condition> Both<code>CaseId</code> and <code>PalletId</code> should not be passed.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ID-40" Name="SortStationId">
                <Description>
                    <div> SortStation to which the Location belongs.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="LocationId" Name="TargetLocationId">
                <Description>
                    <div> Target Location of the Open Container.If this attribute is passed and there are no Open Containers with this TargetLocation then we list the open Containers with Target Location belonging to the DropOff chain in the Order.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</OpenContainer>
