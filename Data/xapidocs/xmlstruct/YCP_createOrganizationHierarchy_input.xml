    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Organization"
                Name="BillingAddressKey" NameInEntity="Billing_Address_Key">
                <Description>
                    <div>
					Billing address of the organization. 
					<Validation>
                            <Condition>Invalid value is passed.
						</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="CatalogOrganizationCode" NameInEntity="Catalog_Organization_Code">
                <Description>
                    <div>
				Organization that owns the catalog of this organization. 
				<Validation>
                            <Condition>Invalid value is passed.
					</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="ContactAddressKey" NameInEntity="Contact_Address_Key">
                <Description>
                    <div>
					Contact address of the organization. 
					<Validation>
                            <Condition>Invalid value is passed.
						</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="CorporateAddressKey" NameInEntity="Corporate_Address_Key">
                <Description>
                    <div>
					Corporate Address of the organization. 
					<Validation>
                            <Condition>Invalid value is passed.
						</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="CreatorOrganizationKey" NameInEntity="Creator_Organization_Key">
                <Description>
                    <div>
					Identifies the organization that created an organization. 
					<Validation>
                            <Condition>Invalid value is passed.
						</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="InventoryOrganizationCode" NameInEntity="Inventory_Organization_Code">
                <Description>
                    <div>
					Organization that owns the inventory of this organization. 
					<Validation>
                            <Condition>Invalid value is passed.
						</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization" Name="LocaleCode" NameInEntity="Locale_code">
                <Description>
                    <div>
					Locale of the organization. 
					<Validation>
                            <Condition>Invalid value is passed.
						</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization" Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
				Unique identifier of the organization. 
				<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not passed.
					</Condition>
                            <Error
                                Description="Missing mandatory parameters" ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="ParentOrganizationCode" NameInEntity="Parent_Organization_Code">
                <Description>
                    <div>
					Identifies the parent of an organization. Organization can have multiple child organizations but only one parent. 
					<Validation>
                            <Condition>Invalid value is passed.
						</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                        <Validation>
                            <Condition>If the organization is also a node, <b>
                                    <code>ParentOrganizationCode</code>
                                </b> is mandatory.
						</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="PricingOrganizationCode" NameInEntity="Pricing_Organization_Code">
                <Description>
                    <div>
				Organization that owns the pricing for this organization. 
				<Validation>
                            <Condition>Invalid value is passed.
					</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="PrimaryEnterpriseKey" NameInEntity="Primary_Enterprise_Key">
                <Description>
                    <div>
					Primary enterprise of an organization.
					<Validation>
                            <Condition>
                                <b>
                                    <code>PrimaryEnterpriseKey</code>
                                </b> is mandatory.
						</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="SupplyKeptExternally" NameInEntity="Supply_Kept_Externally">
                <Description>
                    <div>
				This flag indicates whether the organization tracks supply externally. If set to <code>
                            <b>Y</b>
                        </code>, the system gets the supply picture from an external system and computes the availability based on the external supplies, and the demands kept within Yantra. 
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                        <Validation>
                            <Condition>Child organization has <b>
                                    <code>SupplyKeptExternally</code>
                                </b> set to <b>
                                    <code>Y</code>
                                </b> and the parent organization has <b>
                                    <code>InventoryKeptExternally</code>
                                </b> set to <b>
                                    <code>Y</code>
                                </b>
                            </Condition>
                            <Error
                                Description="Child organization cannot maintain supply externally when the parent organization maintains inventory externally." ErrorCode="YCP75_00021"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
        <Element EntityName="Organization">
            <Description>
                <div>
		The organization to be created. Pass either a value for <code>CorporateAddressKey</code> or  <code>CorporatePersonInfo</code> element to 	provide address information.
		<br/>
                    <br/>If the <code>Organization</code> also has a <code>Node</code> element, then the <code>Organization</code> cannot have <code>SubOrganizations</code>. If the <code>Organization</code> has a parent <code>Organization</code>, and <code>SubOrganizations</code> are being passed, then the parent <code>Organization</code> cannot have a node. 
		<Validation>
                        <Condition>Neither <code>CorporateAddressKey</code> nor the <code>CorporatePersonInfo</code> element is passed.
			</Condition>
                        <Error
                            Description="Either CorporateAddressKey Attribute or CorporatePersonInfo Element is mandatory." ErrorCode="YCP0180 "/>
                    </Validation>
                    <Validation>
                        <Condition>This <code>Organization</code>, or a parent <code>Organization</code>, has a node, and <code>Suborganizations</code> are being passed.
				</Condition>
                        <Error
                            Description="Node Organization cannot have a childnode." ErrorCode="YCP0167"/>
                    </Validation>
                    <Validation>
                        <Condition>Tried to assign enterprise role and node role to organization.					</Condition>
                        <Error
                            Description="Organization can either a enterprise or a node." ErrorCode="YCP0231"/>
                    </Validation>
                </div>
            </Description>
        </Element>
