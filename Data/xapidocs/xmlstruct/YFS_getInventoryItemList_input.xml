<!--getInventoryItemList Input XML-->
<InventoryItem FromItemID="" FromProductClass="" FromUnitOfMeasure=""
    InventoryOrganizationCode="Conditional" ItemID="" ItemIDQryType=""
    MaximumRecords="" OrganizationCode="Conditional" ProductClass=""
    ProductClassQryType="" ShipNode="" ToItemID="" ToProductClass=""
    ToUnitOfMeasure="" UnitOfMeasure="" UnitOfMeasureQryType="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getInventoryItemList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="OrgCode" Name="InventoryOrganizationCode">
                <Description>
                    <div>
				Identifies the inventory organization. Either the <code>OrganizationCode</code> or the <code>InventoryOrganizationCode</code> must be passed in the input XML.
				<Validation>
                            <Condition>Must be a existing <code>OrganizationCode</code>.
					</Condition>
                            <Error
                                Description="Invalid Organization Code" ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>One of these must be passed: <code>OrganizationCode</code> or <code>InventoryOrganizationCode</code>.
					</Condition>
                            <Error
                                Description="OrganizationCode is mandatory" ErrorCode="YFS10435"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>Specifies the Maximum Number of records to be returned. Default is 500.
			</Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>
				Identifies the organization. Either the <code>OrganizationCode</code> or the <code>InventoryOrganizationCode</code> must be passed in the input XML.
				<Validation>
                            <Condition>Must be a existing <code>OrganizationCode</code>.
					</Condition>
                            <Error
                                Description="Invalid Organization Code" ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>One of these must be passed: <code>OrganizationCode</code> or <code>InventoryOrganizationCode</code>.
					</Condition>
                            <Error
                                Description="OrganizationCode is mandatory" ErrorCode="YFS10435"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Supply" Name="ShipNode" NameInEntity="Shipnode_Key"/>
        </Attributes>
    </yfc:doc>
</InventoryItem>
