<!-- releaseOrder Input XML -->
<ReleaseOrder AllocationRuleID="" CheckInventory=""
    DocumentType="Conditional" EnterpriseCode="Conditional"
    IgnoreReleaseDate="" IgnoreTransactionDependencies="Y|N"
    OrderHeaderKey="Conditional" OrderNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YFS/releaseOrder/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" EntityName="Order_Header"
                Name="AllocationRuleID" NameInEntity="AllocationRuleId">
                <Description>
                    <div>
				Identifies an Allocation Rule. An Enterprise can configure multiple allocation rule sets for each document types.
				If an <code>AllocationRuleID</code> is passed, the associated allocation rule is used for this order. 
				<Validation>
                            <Condition> The <code>AllocationRuleID</code> passed is invalid for the combination of document type of the order, and  Enterprise's ruleset. It may be invalid because there is no matching allocation rule for that ID.
					</Condition>
                            <Error
                                Description="Invalid Allocation Rule Id" ErrorCode="YFS10309"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="CheckInventory">
                <Description>
                    <div>
				Flag to indicates whether inventory availability should be checked during scheduling. 
				<br/>Default: <b>
                            <code>Y</code>
                        </b> - <i>Check Inventory</i>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>The document type used to describe the order. Part of the unique key for the order. Either <code>OrderHeaderKey</code>
                        <i>or</i>
                        <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> of the order must be passed.
					<Validation>
                            <Condition>Either <code>EnterpriseCode</code>, <code>DocumentType</code>  and <code>OrderNo</code>
                                <i>or</i>
                                <code>OrderHeaderKey</code> of the order is not passed.
					</Condition>
                            <Error Description="Invalid Order" ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="EnterpriseCode" NameInEntity="Enterprise_Key">
                <Description>
                    <div>Identifies the Enterprise.  Either <code>OrderHeaderKey</code>
                        <i>or</i>
                        <code>EnterpriseCode</code>, <code>DocumentType</code>  and <code>OrderNo</code> of the order must be passed.
					<Validation>
                            <Condition>Either <code>EnterpriseCode</code>, <code>DocumentType</code>  and <code>OrderNo</code>
                                <i>or</i>
                                <code>OrderHeaderKey</code> of the order is not passed.
					</Condition>
                            <Error Description="Invalid Order" ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreReleaseDate">
                <Description>
                    <div>
				Flag to notify the schedule process to ignore the line release dates. Setting this to <b>
                            <code>Y</code>
                        </b> results in line quantities being scheduled even if the date criteria for release is not met.
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="IgnoreTransactionDependencies">
                <Description>
                    <div>Flag to override any transaction dependencies that may be defined for the transaction this API invokes.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>. When passed as <b>
                            <code>Y</code>
                        </b>, transaction dependencies will not be evaluated, and thus will not prevent the API from processing.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="OrderHeaderKey" NameInEntity="Order_Header_Key">
                <Description>
                    <div>Unique identifier of the Order. Either <code>OrderHeaderKey</code>
                        <i>or</i>
                        <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> of the order must be passed.
					<Validation>
                            <Condition>Either <code>EnterpriseCode</code>, <code>DocumentType</code>  and <code>OrderNo</code>
                                <i>or</i>
                                <code>OrderHeaderKey</code> of the order is not passed.
					</Condition>
                            <Error Description="Invalid Order" ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No">
                <Description>
                    <div>A value that identifies an order within an enterprise. Either <code>OrderHeaderKey</code>
                        <i>or</i>
                        <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> of the order must be passed.
					<Validation>
                            <Condition>Either <code>EnterpriseCode</code>, <code>DocumentType</code>  and <code>OrderNo</code>
                                <i>or</i>
                                <code>OrderHeaderKey</code> of the order is not passed.
					</Condition>
                            <Error Description="Invalid Order" ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</ReleaseOrder>
