<Promise xmlns="http://www.sterlingcommerce.com/documentation/YCD/getItemAvailabilityForStore/output">
    <PromiseLines>
        <PromiseLine ItemID="" ProductClass="" Quantity=""
            RequiredQty="" UnitOfMeasure="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="ItemID" Name="ItemID">
                        <Description>
                            <div>
					The unique identifier for the item. 
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="ProductClass" Name="ProductClass">
                        <Description>
                            <div>The product class associated with the item.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Quantity"/>
                    <Attribute DataType="Quantity" Name="RequiredQty">
                        <Description>
                            <div> Requested quantity.</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="UOM" Name="UnitOfMeasure">
                        <Description>
                            <div>The unit of measure for item quantity. </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Availabilities HasAnyUnavailableQty="" IsAvailabilityRealTimeData="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="HasAnyUnavailableQty">
                            <Description>
                                <div>
										This flag indicates whether there are any unavailable quantities.
									</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsAvailabilityRealTimeData">
                            <Description>
                                <div>
										This flag indicates whether availability is real time or cached data.
									</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Availability AvailableQuantity="" DeliveryMethod=""
                    EarliestShipDate="" HasAnyUnavailableQty=""
                    ProductAvailDate="" ProductAvailableDate=""
                    ShipNode="" ShipNodeDescription="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="AvailableQuantity">
                                <Description>
                                    <div>
										Total available quantity in current store.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="DeliveryMethod" Name="DeliveryMethod">
                                <Description>
                                    <div>
										DeliveryMethod used for obtaining the promise line.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="TimeStamp" Name="EarliestShipDate">
                                <Description>
                                    <div>
										The earliest ship date available for the assignment.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Flag" Name="HasAnyUnavailableQty">
                                <Description>
                                    <div>
										This flag indicates whether there are any unavailable quantities.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Date" Name="ProductAvailDate">
                                <Description>
                                    <div>
										The latest product availability date of all the assignments.
									</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="ProductAvailableDate"/>
                            <Attribute DataType="ShipNode" Name="ShipNode">
                                <Description>
                                    <div>
										 The unique identifier for the ship node. 
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Description-100" Name="ShipNodeDescription">
                                <Description>
                                    <div>
										 The description of the ship node. 
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Availability>
            </Availabilities>
        </PromiseLine>
    </PromiseLines>
</Promise>
