<!--getOrderLineDetail Input XML-->
<OrderLineDetail DocumentType="Conditional" EnterpriseCode="Conditional"
    OrderHeaderKey="Conditional" OrderLineKey="Conditional"
    OrderNo="Conditional" PrimeLineNo="Conditional"
    SubLineNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderLineDetails/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>
						The document type associated with the Order.
						<Validation>
                            <Condition>If <code>OrderHeaderKey</code> or <code>OrderLineKey</code> is not known, then <code>DocumentType</code> must be passed.</Condition>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" EntityName="Order_Header"
                Name="EnterpriseCode" NameInEntity="Enterprise_Code">
                <Description>
                    <div>
						The enterprise code associated with the Order.
						<Validation>
                            <Condition>If <code>OrderHeaderKey</code> or <code>OrderLineKey</code> is not known, then <code>EnterpriseCode</code> must be passed.</Condition>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="OrderHeaderKey" NameInEntity="Order_Header_Key">
                <Description>
                    <div>
						Primary Key of the table. This is system generated.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Line" Name="OrderLineKey" NameInEntity="Order_Line_Key">
                <Description>
                    <div>
						Primary Key of the table. This is system generated.
					</div>
                    <div>
						If the line was added as part of pending changes that have not been applied yet, then <code>OrderHeaderKey</code> has to be passed in addition
						to the <code>OrderLineKey</code>.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No">
                <Description>
                    <div>
						Part of unique key for the order. Enterprise code, Document type and order number together uniquely identify an order. Order number can be passed during order creation or generated based on a system rule setting. 
						<Validation>
                            <Condition>If <code>OrderHeaderKey</code> or <code>OrderLineKey</code> is not known, then <code>OrderNo</code>  must be passed.</Condition>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Line" Name="PrimeLineNo" NameInEntity="Prime_Line_No">
                <Description>
                    <div>
						The order line number. Part of an order line's unique key. 
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Line" Name="SubLineNo" NameInEntity="Sub_Line_No">
                <Description>
                    <div>
						Part of unique key of line.
					</div>
                </Description>
            </Attribute>
        </Attributes>
        <Element EntityName="Order_Line">
            <Description>
                <div>
			One of the following must be passed to identify the <code>OrderLine</code>:
			<br/>- <code>OrderLineKey</code>
                    <br/>- <code>OrderHeaderKey</code>, <code>PrimeLineNo</code> and <code>SubLineNo</code>
                    <br/>- <code>EnterpriseCode</code>, <code>DocumentType</code>, <code>OrderNo</code>, <code>PrimeLineNo</code> and <code>SubLineNo</code>
                </div>
            </Description>
        </Element>
    </yfc:doc>
</OrderLineDetail>
