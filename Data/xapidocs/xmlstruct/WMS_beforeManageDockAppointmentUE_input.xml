<DockAppointment AppointmentType="" BolNo="" Carrier=""
    DockAppointmentKey="" LoadKey="" LocationId="" Node="Required"
    Notes="" Operation="" OrderHeaderKey="" OrganizationCode="" ProNo=""
    ResPoolConsmptnDtlsKey="" ShipmentKey="" TrailerNo="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/beforeManageDockAppointmentUE/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="YFS_Dock_Appointment_master.xml"
        IncludeXMLPath="/DockAppointment" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Dock_Appointment">
            <Description>
                <div>
				All appointments taken for dock location(s) are stored in this table. The appointment type can be either INBOUND or OUTBOUND. The table can also store the reference to the shipment, load, carrier etc. for which appointment is taken.
				<Validation>
                        <Condition>If the <code>Operation</code> is <code>Modify</code> or <code>Delete</code> either <code>DockAppointmentKey</code> or <code>ResPoolConsmptnDtlsKey</code> to be passed.
				</Condition>
                        <Error
                            Description="Mandatory Parameter(s) not passed." ErrorCode="WMS00063"/>
                    </Validation>
                    <Validation>
                        <Condition>For creating dock appointment, the <code>ConsumptionDate</code> should be prior to the maximum number of days in the future as configured in the Dock Scheduling Rule.
				</Condition>
                        <Error
                            Description="A dock appointment cannot be taken beyond the maximum number of days in the future, as configured in the Dock Scheduling Rule." ErrorCode="WMS00278"/>
                    </Validation>
                    <Validation>
                        <Condition>Must be a valid <code>Carrier</code>.
				</Condition>
                        <Error
                            Description="SCAC does not exist in the system." ErrorCode="YDM00087"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="String" Name="LocationId">
                <Description>Specifies the Dock location for which appointment is taken. 
				</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="Operation">
                <Description>This flag indicates whether to create, modify or delete operation to perform. 
				</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Order DocumentType="" EnterpriseCode="" OrderNo="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Dock_Appointment_master.xml"
            IncludeXMLPath="/DockAppointment/Order" RemoveDBHouseKeeping="Y"/>
        <yfc:doc>
            <Element EntityName="Order_Header">
                <Description>
                    <div>
				Identifies the Order for which dock appointment is taken.
				<Validation>
                            <Condition>Must be a valid <Code>Order</Code>.
                </Condition>
                            <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
    </Order>
    <Shipment ShipmentNo="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Dock_Appointment_master.xml"
            IncludeXMLPath="/DockAppointment/Shipment" RemoveDBHouseKeeping="Y"/>
        <yfc:doc>
            <Element EntityName="Shipment">
                <Description>
                    <div>
				Identifies the Shipment for which dock appointment is taken.
				<Validation>
                            <Condition>
                                <Code>ShipmentKey</Code> attribute OR the <Code>ShipmentNo</Code> attribute must be valid.
                </Condition>
                            <Error
                                Description="ShipmentKey or ShipmentNo points to an invalid shipment." ErrorCode="YDM00017"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
    </Shipment>
    <Load LoadNo="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Dock_Appointment_master.xml"
            IncludeXMLPath="/DockAppointment/Load" RemoveDBHouseKeeping="Y"/>
        <yfc:doc>
            <Element EntityName="Load">
                <Description>
                    <div>
				Identifies the Load for which dock appointment is taken.
				<Validation>
                            <Condition>
                                <Code>LoadKey</Code> attribute OR the <Code>LoadNo</Code> attribute must be valid.
                </Condition>
                            <Error
                                Description="Load does not exist in the system." ErrorCode="YDM00029"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
    </Load>
    <CapacityConsumption ConsumptionDate="Required" EndTime="Required" StartTime="Required">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Dock_Appointment_master.xml"
            IncludeXMLPath="/DockAppointment/CapacityConsumptionManage" RemoveDBHouseKeeping="Y"/>
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="ConsumptionDate">
                    <Description>
                        <div> Specifies the date of Dock appointment.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="EndTime">
                    <Description>
                        <div> End time of Dock appointment.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="StartTime">
                    <Description>
                        <div> Start time of Dock appointment.
						</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </CapacityConsumption>
</DockAppointment>
