<!-- YCP_getCachedDetails_input.xml -->
<CachedGroups xmlns="http://www.sterlingcommerce.com/documentation/YCP/getCachedDetails/input">
    <CachedGroup Name="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
						This element is optional. If it is specified, the <code>Name</code> attribute must be passed.
						</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Text-200" Name="Name">
                    <Description>
                        <div>
							The Name of the cached group.
								<Validation>
                                <Condition>
									 Must be passed if the <code>CachedGroup</code> element is specified.
									</Condition>
                                <Error Description="Invalid Input XML" ErrorCode="YCP0042"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <CachedObject Class="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-200" Name="Class">
                        <Description>Name of the java class that is cached</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </CachedObject>
    </CachedGroup>
</CachedGroups>
