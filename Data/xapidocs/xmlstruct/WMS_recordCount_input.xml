<?xml version="1.0" encoding="UTF-8"?>
<RecordCount CaseId="" CountKey="" CountQuantity="" CountRequestKey=""
    InventoryStatus="" InventoryTagKey="" ItemID=""
    LocationId="Required" Node="Required" OrganizationCode="Required"
    PalletId="" ProductClass="" Segment="" SegmentType="" SerialNo=""
    ShipByDate="" SummaryTaskId="" TagNumber="" TaskId="Required"
    TaskType="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/recordCount/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="YFS_Count_master.xml"
        IncludeXMLPath="/Count" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Count"/>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>
                    <div>
					A CaseId 
						<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>
                    <div>
					A PalletId 
						<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
							</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <SerialRangeList>
        <SerialRange FromSerialNo="" ToSerialNo="" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="SerialNo" Name="FromSerialNo">
                        <Description>
                            <div>Staring serial number of a range for item. 
 							<Validation>
                                    <Condition> If item has secondary serials, serial range entry is not supported.
								</Condition>
                                    <Error
                                    Description="Secondary serial exists for the item. Single serial scanning is required." ErrorCode="INV80_037"/>
                                </Validation>
                                <Validation>
                                    <Condition> From Serial Number is blank.
								</Condition>
                                    <Error
                                    Description="From Serial Number is blank." ErrorCode="INV80_043"/>
                                </Validation>
                                <Validation>
                                    <Condition> To Serial Number is blank.
								</Condition>
                                    <Error
                                    Description="To Serial Number is blank." ErrorCode="INV80_044"/>
                                </Validation>
                                <Validation>
                                    <Condition> Both From and To Serial numbers are blank.
								</Condition>
                                    <Error
                                    Description="Both From and To Serial numbers are blank." ErrorCode="INV80_039"/>
                                </Validation>
                                <Validation>
                                    <Condition> Serial range entry is not supported when From and To Serial numbers are of different length.
								</Condition>
                                    <Error
                                    Description="From and To Serial numbers are of different length, Manual Scanning Required." ErrorCode="INV80_045"/>
                                </Validation>
                                <Validation>
                                    <Condition> Default implementation is given only for numeric serial numbers.
								</Condition>
                                    <Error
                                    Description="Serial is neither Integer nor Long. User Exit needs to be implemented." ErrorCode="INV80_038"/>
                                </Validation>
                                <Validation>
                                    <Condition> To serial numbers needs to be greater than from serial number for default implementation.
								</Condition>
                                    <Error
                                    Description="From Serial number is greater than To Serial number." ErrorCode="INV80_040"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="SerialNo" Name="ToSerialNo">
                        <Description>
                            <div>Ending serial number of a range for item. 
 							<Validation>
                                    <Condition> If item has secondary serials, serial range entry is not supported.
								</Condition>
                                    <Error
                                    Description="Secondary serial exists for the item. Single serial scanning is required." ErrorCode="INV80_037"/>
                                </Validation>
                                <Validation>
                                    <Condition> From Serial Number is blank.
								</Condition>
                                    <Error
                                    Description="From Serial Number is blank." ErrorCode="INV80_043"/>
                                </Validation>
                                <Validation>
                                    <Condition> To Serial Number is blank.
								</Condition>
                                    <Error
                                    Description="To Serial Number is blank." ErrorCode="INV80_044"/>
                                </Validation>
                                <Validation>
                                    <Condition> Both From and To Serial numbers are blank.
								</Condition>
                                    <Error
                                    Description="Both From and To Serial numbers are blank." ErrorCode="INV80_039"/>
                                </Validation>
                                <Validation>
                                    <Condition> Serial range entry is not supported when From and To Serial numbers are of different length.
								</Condition>
                                    <Error
                                    Description="From and To Serial numbers are of different length, Manual Scanning Required." ErrorCode="INV80_045"/>
                                </Validation>
                                <Validation>
                                    <Condition> Default implementation is given only for numeric serial numbers.
								</Condition>
                                    <Error
                                    Description="Serial is neither Integer nor Long. User Exit needs to be implemented." ErrorCode="INV80_038"/>
                                </Validation>
                                <Validation>
                                    <Condition> To serial numbers needs to be greater than from serial number for default implementation.
								</Condition>
                                    <Error
                                    Description="From Serial number is greater than To Serial number." ErrorCode="INV80_040"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </SerialRange>
    </SerialRangeList>
    <CountTag BatchNo="" CountKey="" CountTagKey="" LotAttribute1=""
        LotAttribute2="" LotAttribute3="" LotKeyReference=""
        LotNumber="" ManufacturingDate="" RevisionNo="" TagNumber="">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="YFS_Count_master.xml"
            IncludeXMLPath="/Count/CountTag" RemoveDBHouseKeeping="Y"/>
        <yfc:doc>
            <Element EntityName="Count_Tag">
                <Description>Gives the information about the tag to which inventory belongs. Attributes passed in this element will be used to identify a tag for the item in inventory based on attributes marked as unique tag attributes in the item catalog. If a tag is present, it will be used and the existing tag will be updated with the other non-unique tag attributes passed in the <code>CountTag</code> element (if different from the existing tag record). 
				</Description>
            </Element>
        </yfc:doc>
    </CountTag>
</RecordCount>
