<!-- Either PriseSetKey OR OrganizationCode, PriceSetName mandatory attributes  -->
<ItemPriceSet ItemID="Required" ListPrice="" Operation=""
    OrganizationCode="" PriceSetKey="" PriceSetName="" ProductClass=""
    RetailPrice="" Uom="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/manageItemPriceSet/input">
    <yfc:doc>
        <Element>
            <Description>
                <div> The price list for items. </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute Name="ItemID">
                <Description>
                    <div> An Item for which a price set is maintained. <Validation>
                            <Condition> If <code>ItemPriceSet</code> is passed, <code>ItemID</code> is mandatory. If not passed: </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing" ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="Operation"/>
            <Attribute DataType="String" Name="OrganizationCode"/>
            <Attribute DataType="String" Name="PriceSetName"/>
        </Attributes>
    </yfc:doc>
    <ItemPriceSetDtlList>
        <ItemPriceSetDtl BreakQtyHigh="Required" BreakQtyLow="Required"
            ItemPriceSetDtlKey="" Operation="" RegionKey="" UnitPrice="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div> If an item requires step pricing, this table stores the quantity breaks and associated price. </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute Name="BreakQtyLow">
                        <Description>
                            <div> The Break Low quantity. <Validation>
                                    <Condition>
                                    <code>BreakQtyLow</code> cannot be greater than <code>BreakQtyHigh</code>. If invalid: </Condition>
                                    <Error
                                    Description="BreakQtyLow cannot be greater than BreakQtyHigh" ErrorCode="OMP10031"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Operation"/>
                </Attributes>
            </yfc:doc>
        </ItemPriceSetDtl>
    </ItemPriceSetDtlList>
</ItemPriceSet>
