<CostFactorGroup CostFactorGroupDescription=""
    CostFactorGroupName="Required" EnterpriseCode="Required" xmlns="http://www.sterlingcommerce.com/documentation/YFS/createCostFactorGroup/input">
    <CostFactorGroupDetailList>
        <CostFactorGroupDetail CostApplicationMethod="Required"
            CostCalculationMethod="Required" CostFactorDescription=""
            CostFactorName="Required" Uom="" Value="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
              This table will contain the information about the cost factor setup for each cost 
              factor groups. This is defined by the enterprise organization.
            </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="CostApplicationMethod">
                        <Description>
                            <div>
                Attribute on which the calculation has to be done. Valid only if the calculation method is AMOUNT. 
                Valid values are WEIGHT, VOLUME and QUANTITY
                <Validation>
                                    <Condition>
                    The value should be passed and valid values are WEIGHT, VOLUME and QUANTITY 
                  </Condition>
                                    <Error
                                    Description="Invalid Cost Application Method. Supported values are WEIGHT, VOLUME, QUANTITY " ErrorCode="YFS10530"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="CostCalculationMethod">
                        <Description>
                            <div>
                Calculation method for the cost factor. Valid values are PERCENTAGE(00) and AMOUNT(01). 
                <Validation>
                                    <Condition>
                    The value should be passed and valid values are 00 (PERCENTAGE) and 01 (AMOUNT)
                  </Condition>
                                    <Error
                                    Description="Invalid Cost Calculation Method. Supported values are 00(Percentage) and 01(Amount)" ErrorCode="YFS10529"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="CostFactorName">
                        <Description>
                            <div>
                Name of the cost factor.
              </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </CostFactorGroupDetail>
    </CostFactorGroupDetailList>
</CostFactorGroup>
