<?xml version="1.0" encoding="UTF-8"?>
<DoesItemMatchCountTask IsMatching="Y|N" ItemID="" OrganizationCode=""
    TaskId="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/doesItemMatchCountTask/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="IsMatching">
                <Description>
                    <div>
						Indicates whether the item for which count is being performed matched the count task's criteria.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count" Name="ItemID">
                <Description>
                    <div>
						Item that has been counted against the task.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count" Name="OrganizationCode">
                <Description>
                    <div>
						Indicates the enterprise of the item counted.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count" Name="TaskId">
                <Description>
                    <div>
						The task identifier for which count is being performed.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Count" Name="UnitOfMeasure">
                <Description>
                    <div>
						Indicate the unit of measure of the item counted.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</DoesItemMatchCountTask>
