<Items xmlns="http://www.sterlingcommerce.com/documentation/YFS/getAvailabilityCorrectionsForItemListUE/output">
    <Item ItemID="Required" OrganizationCode="Required" ProductClass=""
        UnitOfMeasure="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Element EntityName="Inventory_Item"/>
            <Attributes>
                <Attribute EntityName="Inventory_Item"
                    Name="OrganizationCode" NameInEntity="Organization_Code"/>
            </Attributes>
        </yfc:doc>
        <Supplies yfc:DTDOccurrence="REQUIRED">
            <Supply FirstShipDate="Required" IsUnplannedSupply=""
                LastShipDate="Required" OrganizationCode="" Quantity=""
                Segment="" SegmentType="" ShipNode="" SoftAssignedQty=""
                Track="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Inventory_Supply"/>
                    <Attributes>
                        <Attribute DataType="TimeStamp" Name="FirstShipDate">
                            <Description>
                                <div>
				The first ship date of the supply.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsUnplannedSupply">
                            <Description>
                                <div>
			This flag indicates whether this supply is from unplanned inventory or not.
			</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="TimeStamp" Name="LastShipDate">
                            <Description>
                                <div>
				The last ship date of the supply.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Inventory_Supply"
                            Name="OrganizationCode" NameInEntity="Owner_Key"/>
                        <Attribute DataType="Quantity" Name="SoftAssignedQty">
                            <Description>
                                <div>
				The soft assigned demand quantity for this available supply.
				</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="Track">
                            <Description>
                                <div>
							This attribute indicates if the availability is tracked or infinite. If the inventory is tracked the value is <b>
                                    <code>Y</code>
                                    </b> . If the inventory is infinite the value is <b>
                                    <code>N</code>
                                    </b>.							
							<br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or  <b>
                                    <code>N</code>
                                    </b>.
						</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <Tag BatchNo="" LotAttribute1="" LotAttribute2=""
                    LotAttribute3="" LotKeyReference="" LotNumber=""
                    ManufacturingDate="" RevisionNo="" TagNumber="">
                    <yfc:doc>
                        <Element EntityName="Inventory_Tag"/>
                    </yfc:doc>
                </Tag>
            </Supply>
        </Supplies>
    </Item>
</Items>
