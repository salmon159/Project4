<Item AvailableQty="" ConsiderAllNodes="" ConsiderAllSegments=""
    ConsiderFEFOMatching="" ConsiderInventoryNodeControl=""
    ConsiderUnassignedDemand="" Description="" DistributionRuleId=""
    EndDate="" ExtendedDescription="" ItemID="" ProductClass=""
    ShortDescription="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getSupplyDetails/output">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Inventory_Supply" Name="AvailableQty" NameInEntity="Quantity">
                <Description>
                    <div>
				Returned as <b>
                            <code>0</code>
                        </b> if <code>\getSupplyDetails\RequiredQty</code> was not passed. Otherwise <code>AvailableQty</code> is set to how much is available for the required quantity.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderAllNodes">
                <Description>
                    <div>
					Indicates whether all nodes were used in the inventory reporting.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderAllSegments">
                <Description>
                    <div>
				Indicates whether all segments were used in the inventory reporting.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderFEFOMatching">
                <Description>
                    <div>
				This flag indicates whether the demand should be allocated against the applicable supply in the First Expire First Out order.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderInventoryNodeControl">
                <Description>
                    <div>
                Indicates whether inventory node control data were used in the inventory reporting.
                </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ConsiderUnassignedDemand">
                <Description>
                    <div>
				Indicates whether unassigned demands were used in the inventory reporting.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Item_Ship_Node"
                Name="DistributionRuleId" NameInEntity="Distribution_Rule_Id"/>
            <Attribute DataType="Date" Name="EndDate">
                <Description>
                    <div>
                        <code>EndDate</code> specifies the lastest date to look for inventory.  
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item" Name="ItemID"/>
            <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
            <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
        </Attributes>
    </yfc:doc>
    <LanguageDescriptionList>
        <LanguageDescription Description="" ExtendedDescription=""
            LocaleCode="" ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
    </LanguageDescriptionList>
    <ShipNodes>
        <ShipNode AvailableQty="" Priority="" ShipNode="" Tracked="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Quantity" Name="AvailableQty">
                        <Description>
                            <div>
					Available quantity at the node.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute EntityName="Item_Ship_Node"
                        Name="Priority" NameInEntity="Priority"/>
                    <Attribute DataType="Flag" Name="Tracked">
                        <Description>
                            <div>
					Indicate that the inventory is tracked.
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Supplies>
                <Supply AvailableQty="" SupplyType="" TotalQuantity="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Inventory_Supply"/>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="AvailableQty">
                                <Description>
                                    <div>
				Available quantity for the supply.
				</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="TotalQuantity">
                                <Description>
                                    <div>
				Total quantity of the supply.
				</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <SupplyDetails ETA="" LotAttribute1=""
                        LotAttribute2="" LotAttribute3="" LotNumber=""
                        LotReference="" ManufacturingDate="" Quantity=""
                        Segment="" SegmentType="" ShipByDate=""
                        SupplyReference="" SupplyReferenceType=""
                        SupplyType="" TagNumber="" Track="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element EntityName="Inventory_Supply"/>
                            <Attributes>
                                <Attribute EntityName="Inventory_Tag"
                                    Name="LotAttribute1" NameInEntity="Lot_Attribute1"/>
                                <Attribute EntityName="Inventory_Tag"
                                    Name="LotAttribute2" NameInEntity="Lot_Attribute2"/>
                                <Attribute EntityName="Inventory_Tag"
                                    Name="LotAttribute3" NameInEntity="Lot_Attribute3"/>
                                <Attribute EntityName="Inventory_Tag"
                                    Name="LotNumber" NameInEntity="Lot_Number"/>
                                <Attribute EntityName="Inventory_Tag"
                                    Name="LotReference" NameInEntity="Lot_Key_Reference"/>
                                <Attribute EntityName="Inventory_Tag"
                                    Name="ManufacturingDate" NameInEntity="Lot_manufacture_date"/>
                                <Attribute EntityName="Inventory_Tag"
                                    Name="TagNumber" NameInEntity="Tag_Number"/>
                                <Attribute DataType="Flag" Name="Track">
                                    <Description>
                                    <div>
					Indicate that supply is tracked.
					</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <Tag BatchNo="" InventoryItemKey=""
                            InventoryTagKey="" LotAttribute1=""
                            LotAttribute2="" LotAttribute3=""
                            LotKeyReference="" LotNumber=""
                            ManufacturingDate="" RevisionNo=""
                            TagNumber="" TotalDemand=""
                            TotalOnhandSupply="" TotalOtherSupply="">
                            <yfc:doc>
                                <Element EntityName="Inventory_Tag"/>
                            </yfc:doc>
                        </Tag>
                        <ExactMatchedDemands TotalQuantity="">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute DataType="Quantity" Name="TotalQuantity">
                                    <Description>
                                    <div>
						Total quantity of the exact matched demands.
					</div>
                                    </Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                            <DemandDetails DemandType="" LotNumber=""
                                MatchedQuantity="" MinShipByDate=""
                                OrganizationCode="" Quantity=""
                                ShipDate="" ShipNode="" TagNumber="" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:doc>
                                    <Element EntityName="Inventory_Demand"/>
                                    <Attributes>
                                    <Attribute
                                    EntityName="Inventory_Demand" Name="DemandType">
                                    <Description>
                                    <div>
						Specifies a <code>DemandType</code> used in the inventory availability calculations.
						<br/>Only valid <code>SupplyType</code>s that can be consumed against the <code>DemandType</code> value are considered in the inventory availability calculations.
						<BR/>Default: OPEN_ORDER
						</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute
                                    EntityName="Inventory_Tag" Name="LotNumber"/>
                                    <Attribute DataType="Quantity" Name="MatchedQuantity">
                                    <Description>
                                    <div>
							The quantity of this demand that matches the supply. 
						</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute
                                    EntityName="Inventory_Demand"
                                    Name="MinShipByDate" NameInEntity="Min_Ship_By_Date"/>
                                    <Attribute
                                    EntityName="Inventory_Demand"
                                    Name="OrganizationCode" NameInEntity="Owner_Key"/>
                                    <Attribute
                                    EntityName="Inventory_Demand"
                                    Name="ShipDate" NameInEntity="Demand_Ship_Date"/>
                                    <Attribute
                                    EntityName="Inventory_Demand"
                                    Name="ShipNode" NameInEntity="Shipnode_Key"/>
                                    <Attribute
                                    EntityName="Inventory_Tag" Name="TagNumber"/>
                                    </Attributes>
                                </yfc:doc>
                                <InventoryItem ItemID="" UnitOfMeasure=""/>
                            </DemandDetails>
                        </ExactMatchedDemands>
                        <SoftMatchedDemands TotalQuantity="">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute DataType="Quantity" Name="TotalQuantity">
                                    <Description>
                                    <div>
					Total quantity of soft matched demands.
					</div>
                                    </Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                            <DemandDetails DemandType="" LotNumber=""
                                MatchedQuantity="" MinShipByDate=""
                                OrganizationCode="" Quantity=""
                                ShipDate="" ShipNode="" TagNumber="" yfc:DTDOccurrence="ZeroOrMany">
                                <yfc:doc>
                                    <Element EntityName="Inventory_Demand"/>
                                    <Attributes>
                                    <Attribute
                                    EntityName="Inventory_Demand" Name="DemandType">
                                    <Description>
                                    <div>
						Specifies a <code>DemandType</code> used in the inventory availability calculations.
						<br/>Only valid <code>SupplyType</code>s that can be consumed against the <code>DemandType</code> value are considered in the inventory availability calculations.
						<BR/>Default: OPEN_ORDER
						</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute
                                    EntityName="Inventory_Tag" Name="LotNumber"/>
                                    <Attribute DataType="Quantity" Name="MatchedQuantity">
                                    <Description>
                                    <div>
							The quantity of this demand that matches the supply. 
						</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute
                                    EntityName="Inventory_Demand"
                                    Name="MinShipByDate" NameInEntity="Min_Ship_By_Date"/>
                                    <Attribute DataType="Key" Name="OrganizationCode"/>
                                    <Attribute DataType="Date" Name="ShipDate"/>
                                    <Attribute
                                    EntityName="Inventory_Tag" Name="TagNumber"/>
                                    </Attributes>
                                </yfc:doc>
                                <InventoryItem ItemID="" UnitOfMeasure=""/>
                            </DemandDetails>
                        </SoftMatchedDemands>
                        <InventoryItem ItemID="" UnitOfMeasure=""/>
                    </SupplyDetails>
                </Supply>
            </Supplies>
            <UnmatchedDemands>
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
					Demands with no matching supply at the node.
					</div>
                        </Description>
                    </Element>
                </yfc:doc>
                <Demand DemandType="" TotalQuantity="" UnavailableQty="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Inventory_Demand"/>
                        <Attributes>
                            <Attribute EntityName="Inventory_Demand"
                                Name="DemandType" NameInEntity="Demand_Type">
                                <Description>
                                    <div>
					Specifies a <code>DemandType</code> used in the inventory availability calculations.
					<br/>Only valid <code>SupplyType</code>s that can be consumed against the <code>DemandType</code> value are considered in the inventory availability calculations.
					<BR/>Default: OPEN_ORDER
					</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="TotalQuantity">
                                <Description>
                                    <div>
					Total quantity of unmatched demands with the demand type.
					</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="UnavailableQty">
                                <Description>
                                    <div>
					Unavailable quantity of unmatched demands with the demand type.
					</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <DemandDetails DemandType="" MatchedQuantity=""
                        MinShipByDate="" OrganizationCode="" Quantity=""
                        ShipDate="" ShipNode="" TagNumber=""
                        UnavailableQty="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element EntityName="Inventory_Demand"/>
                            <Attributes>
                                <Attribute EntityName="Inventory_Demand" Name="DemandType">
                                    <Description>
                                    <div>
						Specifies a <code>DemandType</code> used in the inventory availability calculations.
						<br/>Only valid <code>SupplyType</code>s that can be consumed against the <code>DemandType</code> value are considered in the inventory availability calculations.
						<BR/>Default: OPEN_ORDER
						</div>
                                    </Description>
                                </Attribute>
                                <Attribute DataType="Quantity" Name="MatchedQuantity">
                                    <Description>
                                    <div>
							The quantity of this demand that matches the supply. 
						</div>
                                    </Description>
                                </Attribute>
                                <Attribute EntityName="Inventory_Demand"
                                    Name="MinShipByDate" NameInEntity="Min_Ship_By_Date"/>
                                <Attribute EntityName="Inventory_Demand"
                                    Name="OrganizationCode" NameInEntity="Owner_Key"/>
                                <Attribute EntityName="Inventory_Demand"
                                    Name="ShipDate" NameInEntity="Demand_Ship_Date"/>
                                <Attribute EntityName="Inventory_Demand"
                                    Name="ShipNode" NameInEntity="Shipnode_Key"/>
                                <Attribute EntityName="Inventory_Tag" Name="TagNumber"/>
                                <Attribute DataType="Quantity" Name="UnavailableQty">
                                    <Description>
                                    <div>
							The unavailable quantity of the unmatched demand. 
						</div>
                                    </Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                        <Tag BatchNo="" InventoryItemKey=""
                            InventoryTagKey="" LotAttribute1=""
                            LotAttribute2="" LotAttribute3=""
                            LotKeyReference="" LotNumber=""
                            ManufacturingDate="" RevisionNo=""
                            TagNumber="" TotalDemand=""
                            TotalOnhandSupply="" TotalOtherSupply="">
                            <yfc:doc>
                                <Element EntityName="Inventory_Tag"/>
                            </yfc:doc>
                        </Tag>
                        <InventoryItem ItemID="" UnitOfMeasure=""/>
                    </DemandDetails>
                </Demand>
            </UnmatchedDemands>
        </ShipNode>
    </ShipNodes>
    <ItemFilter GetUnpublishedItems="N|Y" IgnoreInvalidItems="N|Y">
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YFS_getSupplyDetails_input.xml" IncludeXMLPath="/getSupplyDetails/ItemFilter"/>
        <yfc:includeInParent IncludeXMLChildNodes="Y"
            IncludeXMLFileName="YFS_getATP_input.xml" IncludeXMLPath="/GetATP/ItemFilter"/>
        <yfc:doc>
            <Element EntityName="Item">
                <Description>Determines the filtering attributes when determining availability of a model item.
				This can be used to specify the correct style, size, or color of a model item and only include
				model children inventory whose item definition meet the required input.
				<br/>Note: Only extended attributes of the YFS_ITEM table are supported.</Description>
            </Element>
            <Attributes>
                <Attribute DataType="Flag" Name="GetUnpublishedItems">
                    <Description>Indicates if unpublished items needs to be considered.</Description>
                </Attribute>
                <Attribute DataType="Flag" Name="IgnoreInvalidItems">
                    <Description>
                        <div>
					This flag indicates whether invalid items should be considered.
					Valid values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
					Default value: <b>
                                <code>N</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <PrimaryInformation ColorCode="" ItemType="" ManufacturerItem=""
            ManufacturerName="" ProductLine="" SizeCode="">
            <yfc:doc>
                <Element EntityName="Item"/>
            </yfc:doc>
        </PrimaryInformation>
        <ClassificationCodes CommodityCode="" ECCNNo=""
            HarmonizedCode="" NAICSCode="" NMFCCode="" StorageType=""
            TaxProductCode="" UNSPSC="" VelocityCode="">
            <yfc:doc>
                <Element EntityName="Item"/>
            </yfc:doc>
        </ClassificationCodes>
    </ItemFilter>
</Item>
