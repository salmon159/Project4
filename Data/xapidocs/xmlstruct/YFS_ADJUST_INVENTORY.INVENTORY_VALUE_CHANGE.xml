<Cost AverageCost="" ChangeInInvValue="" CostCurrency=""
    InventoryValue="" ItemID="" OrganizationCode=""
    PostingClassification="" PrimarySupplier="" ProductClass=""
    Quantity="" ReasonCode="" ReasonText="" Reference_1=""
    Reference_2="" Reference_3="" Reference_4="" Reference_5=""
    ShipNode="" TransactionType="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/ADJUST_INVENTORY/INVENTORY_VALUE_CHANGE">
    <yfc:includeInParent IncludeXMLChildNodes="Y"
        IncludeXMLFileName="MasterInventoryValueChange.xml" IncludeXMLPath="/Cost"/>
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="AverageCost">
                <Description>Current Average Cost of the item. This is the new average cost.	</Description>
            </Attribute>
            <Attribute DataType="Money" Name="ChangeInInvValue">
                <Description>Change in Inventory Value. Amount by which the inventory value is increased due to the receipt.</Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="CostCurrency">
                <Description>Currency in which the unit cost of the order line shipped is specified.</Description>
            </Attribute>
            <Attribute DataType="String" Name="InventoryValue">
                <Description>Current Inventory value of the item. This includes the change in inventory value due to this transaction.</Description>
            </Attribute>
            <Attribute DataType="ItemID" Name="ItemID">
                <Description>Item Id of the item getting received.</Description>
            </Attribute>
            <Attribute EntityName="Inventory_Item"
                Name="OrganizationCode" NameInEntity="InventoryOrganizationCode"/>
            <Attribute DataType="Text-40" Name="PostingClassification">
                <Description>Posting Classification of the item received</Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="PrimarySupplier">
                <Description>Primary Supplier of the item received.</Description>
            </Attribute>
            <Attribute DataType="ProductClass" Name="ProductClass">
                <Description>Product Class of the item received.</Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="Quantity">
                <Description>Quantity received.</Description>
            </Attribute>
            <Attribute DataType="CommonCode" Name="ReasonCode">
                <Description>Reason Code. Reference Field.</Description>
            </Attribute>
            <Attribute DataType="Text-100" Name="ReasonText">
                <Description>Reason Text. Reference Field. </Description>
            </Attribute>
            <Attribute DataType="Text-50" Name="Reference_1">
                <Description>Reference Field. </Description>
            </Attribute>
            <Attribute DataType="Text-50" Name="Reference_2">
                <Description>Reference Field. </Description>
            </Attribute>
            <Attribute DataType="Text-50" Name="Reference_3">
                <Description>Reference Field. </Description>
            </Attribute>
            <Attribute DataType="Text-254" Name="Reference_4">
                <Description>Reference Field. </Description>
            </Attribute>
            <Attribute DataType="Text-254" Name="Reference_5">
                <Description>Reference Field. </Description>
            </Attribute>
            <Attribute DataType="ShipNode" Name="ShipNode">
                <Description>Ship node that received the item. </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="TransactionType">
                <Description>ADJUSTMENT. </Description>
            </Attribute>
            <Attribute DataType="UOM" Name="UnitOfMeasure">
                <Description>Unit Of Measure of the item received.</Description>
            </Attribute>
