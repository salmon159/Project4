<OrderApprovalRule DocumentType="" EffectiveEndDate=""
    EffectiveStartDate="" GreexRule="" IsActive="" IsOverride=""
    Message="" MessageCode="" OrderApprovalRuleKey=""
    OrganizationCode="" RuleID="" Type="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/manageOrderApprovalRule/output">
    <yfc:doc>
        <Attributes>
            <Attribute Name="IsOverride">
                <Description>
                    <div>
	                    Indicates whether approval is required for Overridden rule or Not.
	                    <br/>Valid Values: 
					<br/>
                        <b>
                            <code>X</code>
                        </b> - Approval is required to proceed with the transaction.
					<br/>
                        <b>
                            <code>Y</code>
                        </b> - Allow the transaction no approval required
					<br/>
                        <b>
                            <code>Z</code>
                        </b> -Do not allow the transaction.
					<br/>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ApprovalGroupList>
        <ApprovalGroup ApprovalGroupId="" ApprovalGroupKey=""
            ApprovalGroupName="" DocumentType="" OrganizationCode="" yfc:DTDOccurrence="ZeroOrMany"/>
    </ApprovalGroupList>
</OrderApprovalRule>
