<!--getReceiptLineList Input XML-->
<ReceiptLine AvailableForTranQuantity="" BatchNo="" CaseId=""
    CountryOfOrigin="" DispositionCode="" DispositionLocation=""
    EnterpriseCode="" FifoNo="" InspectedBy="" InspectionComments=""
    InspectionDate="" IsInspectionPending="" ItemID="" LotAttribute1=""
    LotAttribute2="" LotAttribute3="" LotKeyReference="" LotNumber=""
    ManufacturingDate="" MaximumRecords="" NetWeight="" NetWeightUom=""
    OrderHeaderKey="" OrderLineKey="" OrderNo="" OrderReleaseKey=""
    OrigReceiptLineKey="" OriginalQuantity="" PalletId="" PrimeLineNo=""
    ProductClass="" Quantity="" ReadFromHisotry="" ReadFromHistory=""
    ReceiptHeaderKey="" ReceiptLineKey="" ReceiptLineNo="" ReleaseNo=""
    RelevantItemLinesOnly="" RevisionNo="" SerialNo="" ShipByDate=""
    ShipmentContainerKey="" ShipmentLineKey="" SubLineNo=""
    TransactionId="" TransactionName="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getReceiptLineList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description> A CaseId. </Description>
            </Attribute>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>Specifies the Maximum Number of records to be returned. Default is 500.</Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description> A PalletId. </Description>
            </Attribute>
            <Attribute DataType="String" Name="ReadFromHisotry"/>
            <Attribute DataType="Text-1" Name="ReadFromHistory">
                <Description>
                    <div>
                    Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
                    <br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_RECEIPT_LINE table.
                    <br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_RECEIPT_LINE_H table (the history table).
                    <br/>
                        <b>
                            <code>B</code>
                        </b> - Returns records that satisfy the input criteria from both YFS_RECEIPT_LINE and YFS_RECEIPT_LINE_H tables. Records returned from the current table are first in the output.
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="RelevantItemLinesOnly">
                <Description>
                    <div>
                        <br/>Valid Values:
                    <br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns the relevant item receipt lines without the hierarchical information that satisfy the input criteria. This flag is relevant only if CaseId or PalletId is passed in the input and the passed CaseId/PalletId is not shipped out of warehouse.
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid"/>
        </Attributes>
    </yfc:doc>
</ReceiptLine>
