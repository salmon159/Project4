<ItemList xmlns="http://www.sterlingcommerce.com/documentation/YFS/getExternalPricesForItemListUE/output">
    <Item ItemID="" ItemKey="" OrganizationCode="" UnitOfMeasure="" yfc:DTDOccurrence="OneOrMany">
        <ComputedPrice AdditionalLinePriceTotal="" BundleTotal=""
            ListPrice="" RetailPrice="" UnitPrice="">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="YFS_getItemListForOrdering_output.xml" IncludeXMLPath="/ItemList/Item/ComputedPrice"/>
            <yfc:doc>
                <Element>
                    <Description>
						The price for the item, as computed by the user exit.
					</Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Money" Name="AdditionalLinePriceTotal">
                        <Description>
                            <div>
							The total price for all non-informational Additional Line Prices for the Item.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="BundleTotal">
                        <Description>
                            <div>The total cost of the Item's bundle based on the bundle pricing strategy.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="ListPrice">
                        <Description>
                            <div>The list price of the Item.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="RetailPrice">
                        <Description>
                            <div>The retail price of the Item.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Money" Name="UnitPrice">
                        <Description>
                            <div>The unit price of the Item, factoring in Adjustments and AdditionalLinePrices.
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <QuantityRangePriceList>
                <QuantityRangePrice BreakQtyHigh="" BreakQtyLow=""
                    RegionKey="" UnitPrice="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>Price for quantity range.
								</div>
                            </Description>
                        </Element>
                        <Attributes>
                            <Attribute DataType="Quantity" Name="BreakQtyHigh">
                                <Description>
                                    <div>Break high quantity for the quantity range.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="BreakQtyLow">
                                <Description>
                                    <div>Break low quantity for the quantity range.
								</div>
                                </Description>
                            </Attribute>
                            <Attribute EntityName="Region"
                                Name="RegionKey" NameInEntity="Region_Key">
                                <Description>
                                    <div>Link to YFS_REGION table. 
								</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="UnitPrice">
                                <Description>
                                    <div>The unit price of the quantity range.
								</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <Region RegionDescription="" RegionKey=""
                        RegionLevelName="" RegionName="">
                        <yfc:doc>
                            <Element EntityName="Region"/>
                        </yfc:doc>
                    </Region>
                </QuantityRangePrice>
            </QuantityRangePriceList>
        </ComputedPrice>
    </Item>
</ItemList>
