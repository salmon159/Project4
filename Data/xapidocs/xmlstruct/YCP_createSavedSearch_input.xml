<SavedSearch ApplicationCode="" Loginid="Required"
    SearchContext="Required" SearchData="" SearchName="Required"
    SearchViewId="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/createSavedSearch/input">
    <yfc:doc>
        <Attributes>
            <Attribute Name="SearchContext" NameInEntity="Search_Context">
                <Description>
                    <div>
				For the saved search to be used in a Console, the <code>SearchContext</code> should be the entity name for which the saved search is created.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="SearchData" NameInEntity="Search_Data">
                <Description>
                    <div>
					For the saved search to be used in a Console, the SearchData should contain the input XML to the List API of the SearchContext.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</SavedSearch>
