<?xml version="1.0" encoding="UTF-8"?>
<ShipmentProfileSummary xmlns="http://www.sterlingcommerce.com/documentation/WMS/getShipmentProfileSummary/output">
    <Profile NumberOfShipments="" NumberOfShipmentsNotWaved=""
        NumberOfShipmentsWaved="" ProfileId="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element EntityName="Shipment"/>
            <Attributes>
                <Attribute DataType="Integer" Name="NumberOfShipments">
                    <Description>   
						   Total Number of Shipments for that profile
						     </Description>
                </Attribute>
                <Attribute DataType="Integer" Name="NumberOfShipmentsNotWaved">
                    <Description>                  
						   Total Number of Shipments  not in a wave for that profile       
					        </Description>
                </Attribute>
                <Attribute DataType="Integer" Name="NumberOfShipmentsWaved">
                    <Description>                  
						    Total Number of Shipments in a wave for that profile        
					         </Description>
                </Attribute>
                <Attribute DataType="String" Name="ProfileId"/>
            </Attributes>
        </yfc:doc>
    </Profile>
</ShipmentProfileSummary>
