<WorkOrder EnterpriseCode="Conditional" NodeKey="Conditional"
    OverrideModificationRules="" QuantityRemoved=""
    ReasonCode="Required" ReasonText="" WorkOrderKey="Conditional"
    WorkOrderNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/VAS/cancelWorkOrder/input">
    <yfc:includeInParent IncludeXMLFileName="YFS_Work_Order_keys.xml" IncludeXMLPath="/WorkOrder"/>
    <yfc:doc>
        <Element EntityName="Work_Order">
            <Description>
                <div>The work order to be cancelled. Pass  <code>WorkOrderKey</code>
                    <i>or</i> pass <code>WorkOrderNo</code>, <code>NodeKey</code> and <code>EnterpriseCode</code> to identify the work order.
				<Validation>
                        <Condition>The passed attributes are unable to identify an existing work order.
					</Condition>
                        <Error Description="Invalid Work Order." ErrorCode="VAS00013"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Flag" Name="OverrideModificationRules">
                <Description>
                    <div>Pass <b>
                            <code>Y</code>
                        </b> to override the cancel work order modifcation type validation on the work order.
						<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="QuantityRemoved">
                <Description>
                    <div>Quantity to be removed from <code>WorkOrder</code> due to exceptions. 
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="ReasonCode">
                <Description>
                    <div>Cancellation Reason Code. 
					<Validation>
                            <Condition> It should be passed.
					</Condition>
                            <Error Description="Field Mandatory." ErrorCode="YCP0046"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</WorkOrder>
