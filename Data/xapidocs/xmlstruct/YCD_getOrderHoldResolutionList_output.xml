<OrderHoldTypes TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/getOrderHoldResolutionList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Number" Name="TotalNumberOfRecords">
                <Description>
   Number of Holds on the Order.
  </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderHoldType Enabled="" ExceptionKey="" HoldLevel="" HoldType=""
        HoldTypeDescription="" ReasonText="" Status="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Enabled">
                    <Description>
                        <div>If the current user can resolve this order hold, the value will be "Y" else "N".</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Inbox" Name="ExceptionKey" NameInEntity="Inbox_Key">
                    <Description>
                        <div>If there is a corresponding Alert for this Hold Type, the <code>InboxKey</code> identifier will be returned as <code>ExceptionKey</code>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="HoldLevel">
                    <Description>
                        <div>If the Order Hold record is associated to an order line, this will be "Line" otherwise this will be "Order".</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="HoldTypeDescription"/>
                <Attribute DataType="String" EntityName="Hold_Type"
                    Name="Status" NameInEntity="Status"/>
            </Attributes>
        </yfc:doc>
        <OrderLine OrderLineKey="" ProductClass="">
            <ItemDetails ItemID="">
                <yfc:doc>
                    <Element EntityName="Item"/>
                </yfc:doc>
                <PrimaryInformation DefaultProductClass=""
                    Description="" ExtendedDisplayDescription=""
                    ImageID="" ImageLabel="" ImageLocation="" ShortDescription=""/>
            </ItemDetails>
            <Item ItemID="" UnitOfMeasure=""/>
        </OrderLine>
    </OrderHoldType>
</OrderHoldTypes>
