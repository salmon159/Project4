<Coupon BatchNumber="" BatchSize="" CouponID="Required"
    OrganizationCode="Required" UserId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YPM/exportCouponCode/input">
    <yfc:doc>
        <Element EntityName="[NONE]">
            <Description>
                <div>
                   Export codes for a given coupon definition.  
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="PositiveQuantity" Name="BatchNumber">
                <Description>
                    <div>the batch number to export <code>1</code>.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="PositiveQuantity" Name="BatchSize">
                <Description>
                    <div>the number of coupons to export. Default value is <code>1000</code>.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="CouponId" Name="CouponID">
                <Description>
                    <div>Unique identifier of a coupon.
						<Validation>
                            <Condition>
                                <code>CouponID</code> must be passed.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="OrganizationCode">
                <Description>
                    <div>Pricing organization the <code>CouponID</code> belongs to.
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> must be passed.
                            </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not valid.
                            </Condition>
                            <Error
                                Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="UserId"/>
        </Attributes>
    </yfc:doc>
</Coupon>
