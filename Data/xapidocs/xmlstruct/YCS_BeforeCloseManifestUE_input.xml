<Manifest Carrier="" ManifestNumber="" PickUpSummaryNumber=""
    ShipNode="" ShipperAccountNumber="" xmlns="http://www.sterlingcommerce.com/documentation/YCS/BeforeCloseManifestUE/input">
    <yfc:doc>
        <Element EntityName="Manifest"/>
        <Attributes>
            <Attribute DataType="OrgCode" Name="Carrier">
                <Description>
                    <div> 
						A carrier by which this manifest will be transported.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" Name="ManifestNumber">
                <Description>
                    <div>
						An unique key identifier of the manifest.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" Name="PickUpSummaryNumber">
                <Description>
                    <div> 
						An unique number provided by the carrier personnel at shipment pickup. This usually comprises a book number and a page number. A pickup                   
						summary number is associated with the manifest using the <code>closeManifest </code> API. It should be 9 digits, of which first 7 digits represent the book number and the last 2 
						digits represent the page number.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="ShipNode" Name="ShipNode">
                <Description>
                    <div>Node at which Manifesting is taking place.</div>
                </Description>
            </Attribute>
            <Attribute DataType="DocumentNo" Name="ShipperAccountNumber">
                <Description>
                    <div>
						Provides the shipper's account number.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Manifest>
