<OrderInvoiceDetailList TotalNumberOfRecords="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderInvoiceDetailList/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="TotalNumberOfRecords"/>
        </Attributes>
    </yfc:doc>
    <OrderInvoiceDetail Charges="" ExtendedPrice="" IsHistory=""
        LineTotal="" OrderInvoiceDetailKey="" OrderLineKey=""
        PricingQty="" Quantity="" ShippedQty="" Tax="" UnitPrice="" isHistory="">
        <yfc:doc>
            <Element EntityName="Order_Invoice_Detail"/>
            <Attributes>
                <Attribute DataType="String" Name="IsHistory"/>
                <Attribute DataType="Key" Name="OrderInvoiceDetailKey">
                    <Description>Primary key attribute of YFS_ORDER_INVOICE_DETAIL.</Description>
                </Attribute>
                <Attribute DataType="UnitPrice" Name="UnitPrice">
                    <Description>Unit price of invoice line item.</Description>
                </Attribute>
                <Attribute DataType="Flag" Name="isHistory">
                    <Description>
                        <div>Indicates if data is retrieved from the history table or the regular table.
							<br/>Default: <b>
                                <code>N</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <InvoiceHeader ActualFreightCharge="" AmountCollected="Required"
            ChargedActualFreight="" CollectedExternally=""
            CollectedThroughAR="" DateInvoiced="" InvoiceNo=""
            InvoiceType="" LineSubTotal="" MasterInvoiceNo=""
            OrderInvoiceKey="" Reference1="" TotalAmount="" TotalTax="">
            <yfc:doc>
                <Element EntityName="Order_Invoice"/>
                <Attributes>
                    <Attribute DataType="PlusMinusMoney" Name="TotalTax">
                        <Description>Total tax amount in the invoice.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Order DeliveryCode="" DocumentType="" EnterpriseCode=""
                EntryType="" OrderHeaderKey="" OrderNo=""
                SellerOrganizationCode="" TermsCode="">
                <PriceInfo Currency=""/>
                <References>
                    <Reference Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                        <yfc:doc>
                            <Element EntityName="Reference_Table"/>
                            <Attributes>
                                <Attribute DataType="Text-40"
                                    EntityName="Reference_Table"
                                    Name="Name" NameInEntity="Name1">
                                    <Description>Reference name.</Description>
                                </Attribute>
                                <Attribute DataType="Text-254"
                                    EntityName="Reference_Table"
                                    Name="Value" NameInEntity="Value1">
                                    <Description>Reference value.</Description>
                                </Attribute>
                            </Attributes>
                        </yfc:doc>
                    </Reference>
                </References>
            </Order>
            <Shipment ManifestNo="" ReceivingNode="" SCAC="" ShipDate=""
                ShipNode="" ShipmentKey="" ShipmentNo=""/>
        </InvoiceHeader>
        <OrderLine OrderLineKey="" PrimeLineNo="" SubLineNo="">
            <Item ItemID="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Element EntityName="Item"/>
                    <Attributes>
                        <Attribute DataType="ProductClass" Name="ProductClass">
                            <Description>Product class of the item.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <LanguageDescriptionList>
                    <LanguageDescription Description=""
                        ExtendedDescription="" LocaleCode=""
                        ShortDescription="" yfc:DTDOccurrence="ZeroOrMany"/>
                </LanguageDescriptionList>
            </Item>
            <LinePriceInfo DiscountPercentage="" DiscountReference=""
                DiscountType="" InvoicedPricingQty="" ListPrice=""
                PricingQtyConversionFactor="" PricingUOM=""
                RetailPrice="" UnitPrice=""/>
            <References>
                <Reference Name="" Value="" yfc:DTDOccurrence="ZeroOrMany"/>
            </References>
        </OrderLine>
        <LineCharges>
            <LineCharge ChargeAmount="" ChargeCategory="" ChargeName=""
                ChargeNameKey="" ChargePerLine="" ChargePerUnit=""
                IsBillable="" IsDiscount="" IsShippingCharge="|N|Y"
                OriginalChargePerLine="" OriginalChargePerUnit=""
                Reference="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Line_Charges"/>
                    <Attributes>
                        <Attribute DataType="Key"
                            EntityName="Charge_Name"
                            Name="ChargeNameKey" NameInEntity="Charge_Name_Key">
                            <Description>Primary key for a charge name.</Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsBillable">
                            <Description>Describes if the charge is billable. Default: <code>N</code>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsDiscount">
                            <Description>Describes if the charge is discountable. Default: <code>N</code>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Flag" Name="IsShippingCharge">
                            <Description>Determines whether the charge is a shipping charge. This attribute value is system defined and cannot be overridden. The value for this attribute is set as <code>Y</code> only for Charge Name configured as <code>CHARGE_NAME_FOR_SHIPPING</code> in financial rule. For all other Charges this attribute is set to <code>N</code>.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </LineCharge>
        </LineCharges>
        <LineTaxes>
            <LineTax ChargeCategory="" ChargeName="" ChargeNameKey=""
                Reference1="" Reference2="" Reference3="" Tax=""
                TaxName="" TaxPercentage="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Tax_Breakup"/>
                    <Attributes>
                        <Attribute DataType="Key"
                            EntityName="Charge_Name"
                            Name="ChargeNameKey" NameInEntity="Charge_Name_Key">
                            <Description>Primary key for a charge name.</Description>
                        </Attribute>
                        <Attribute DataType="Text-100"
                            EntityName="Tax_Breakup" Name="Reference1" NameInEntity="Reference1">
                            <Description>Additional reference information. This field can be customized by the Enterprise.</Description>
                        </Attribute>
                        <Attribute DataType="Text-100"
                            EntityName="Tax_Breakup" Name="Reference2" NameInEntity="Reference2">
                            <Description>Additional reference information. This field can be customized by the Enterprise.</Description>
                        </Attribute>
                        <Attribute DataType="Text-100"
                            EntityName="Tax_Breakup" Name="Reference3" NameInEntity="Reference3">
                            <Description>Additional reference information. This field can be customized by the Enterprise.</Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </LineTax>
            <TaxSummary>
                <TaxSummaryDetail Tax="Required" TaxName="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Tax_Breakup"/>
                    </yfc:doc>
                </TaxSummaryDetail>
            </TaxSummary>
        </LineTaxes>
        <References>
            <Reference Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element EntityName="Reference_Table"/>
                </yfc:doc>
            </Reference>
        </References>
        <LineDetailTranQuantity PricingQty="" Quantity="" ShippedQty="" TransactionalUOM="">
            <yfc:doc>
                <Element EntityName="Order_Invoice_Detail"/>
                <Attributes>
                    <Attribute DataType="UOM" Name="TransactionalUOM">
                        <Description>UOM on which all the transactional quantities are specified.
						  </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </LineDetailTranQuantity>
    </OrderInvoiceDetail>
</OrderInvoiceDetailList>
