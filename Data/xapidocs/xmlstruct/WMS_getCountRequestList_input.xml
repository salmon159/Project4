<?xml version="1.0" encoding="UTF-8"?>
<CountRequest CancellationReasonCode="" CaseId="" CountProgramName=""
    CountRequestKey="" CountRequestNo="" DocumentType=""
    EnterpriseCode="" FinishNoLaterThan="" FromLocation=""
    ItemClassification1="" ItemClassification2="" ItemClassification3=""
    ItemID="" LocationId="" MaximumRecords="" Node="" PalletId=""
    PipelineKey="" Priority="" ProductClass="" ReasonText=""
    ReceiptHeaderKey="" RequestType="" RequestingUserId=""
    ShowSummaryAttributesOnly="" StartNoEarlierThan="" Status=""
    ToLocation="" UnitOfMeasure="" ZoneId="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getCountRequestList/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="YFS_Count_Request_master.xml"
        IncludeXMLPath="/CountRequest" RemoveDBHouseKeeping="Y"/>
    <yfc:doc>
        <Element EntityName="Count_Request"/>
        <Attributes>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description> 
				A CaseId 
					<Validation>
                        <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
							</Condition>
                        <Error
                            Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                    </Validation>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description> Maximum number of records to be fetched in the list.</Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description> 
				A PalletId 
					<Validation>
                        <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
							</Condition>
                        <Error
                            Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                    </Validation>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ShowSummaryAttributesOnly">
                <Description> Pass this Flag set to Y to get only the Header Attributes.</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Receipt ReceiptNo="">
        <yfc:doc>
            <Element EntityName="Receipt_Header"/>
        </yfc:doc>
    </Receipt>
    <Tasks>
        <Task IsSummaryTask="" Node="" OrganizationCode=""
            ParentTaskId="" PredecessorTaskId="" TaskId="" TaskKey=""
            TaskPriority="" TaskStatus="" TaskType="">
            <TaskReferences BatchNo="" ContainerNo="" DocumentType=""
                OrderHeaderKey="" OrderLineKey="" OrderNo=""
                OrderReleaseKey="" PrimeLineNo="" ReleaseNo=""
                ShipmentContainerKey="" ShipmentKey="" ShipmentNo=""
                SubLineNo="" WaveNo="" yfc:DTDOccurrence="ZeroOrMany"/>
        </Task>
    </Tasks>
</CountRequest>
