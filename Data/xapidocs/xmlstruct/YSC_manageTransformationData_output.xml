<TransformationHeader Description="" OrganizationCode="" TemplateName=""
    TransformationConfigKey="" TransformationHeaderKey="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/manageTransformationData/output">
    <yfc:doc>
        <Attributes>
            <Attribute Name="Description">
                <Description>
                    <div>This attribute corresponds  description for Template. 
										</div>
                </Description>
            </Attribute>
            <Attribute Name="OrganizationCode">
                <Description>
                    <div>Organization Code for an organization.
											<Validation>
                            <Condition>Must be valid Organization Code in the table <code>YFS_Organization</code>.
														</Condition>
                            <Error
                                Description="Invalid Organization Code." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="TemplateName">
                <Description>
                    <div>This attribute corresponds to  Template name. Eg <code>"Template for Government" </code>for <code>"Print Proposal" </code>. 
										</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <TransformationHeaderLocaleList>
        <TransformationHeaderLocale Country="" Description=""
            Language="" LocaleCode="" TemplateName="" Variant="">
            <yfc:doc>
                <Attributes>
                    <Attribute Name="Description">
                        <Description>
                            <div>This attribute corresponds  to locale specific description for Template. 
										</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="LocaleCode">
                        <Description>
                            <div>This attribute corresponds to Locale code like <code>en_US_EST</code>.
										</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="TemplateName">
                        <Description>
                            <div>This attribute corresponds to  Locale specific Template name. Eg <code>"Template for Government" </code>for <code>"Print Proposal" in en_US_EST </code>. 
										</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </TransformationHeaderLocale>
    </TransformationHeaderLocaleList>
</TransformationHeader>
