    ActualFreightCharge="" ActualWeight="" ActualWeightUOM=""
    AppliedWeight="" AppliedWeightUOM="" AstraCode=""
    BackOrderRemovedQuantity="" BarcodeDiscount="" BaseDropStatus=""
    BasicFreightCharge="" CarriageValue="" CaseId=""
    ContainerGrossWeight="" ContainerGrossWeightUOM=""
    ContainerHeight="" ContainerHeightUOM="" ContainerLength=""
    ContainerLengthUOM="" ContainerNetWeight="" ContainerNetWeightUOM=""
    ContainerNo="" ContainerSeqNo="" ContainerType="" ContainerWidth=""
    ContainerWidthUOM="" CustCarrierAccountNo="" CustomsValue=""
    DeclaredValue="" DeliverBy="" DeliveryDay="" DimmedFlag=""
    DiscountAmount="" ExportLicenseExpDate="" ExportLicenseNo=""
    FreightTerms="" GenerateContainerScm="" IsPackProcessComplete=""
    LocationId="" OverrideModificationRules="" OversizedFlag=""
    PalletId="" ReasonCode="" ReasonText="" RemoveFromShipmentLine=""
    RoutingCode="" ShipmentContainerKey="" ShipmentKey=""
    SpecialServicesSurcharge="" TrackingNo="" TransactionId=""
    Ucc128code="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/beforeChangeShipmentContainerUE/output">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Flag" Name="AcceptOutOfSequenceUpdates">
                <Description>
                    <div>Indicates whether to do updates which are not in sequence (sequence in pipeline).
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="ID-40" Name="Action">
                <Description>
                    <div>Indicates the action that is performed. 
					<br/>Default: <b>
                            <code>Create-Modify</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Create</code>
                        </b>, <b>
                            <code>Modify</code>
                        </b> or <b>
                            <code>Delete</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="BackOrderRemovedQuantity">
                <Description>
                    <div>Specify if removed quantity needs to be backordered.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Pipeline_Definition"
                Name="BaseDropStatus" NameInEntity="Drop_Status"/>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>Identifies the container that is processed by the API.
				</Description>
            </Attribute>
            <Attribute DataType="LPNType" Name="ContainerType">
                <Description>
                    <div>Identifies the type of the container that is processed by the API.
					<br/>Valid Values: <b>
                            <code>Case</code>
                        </b> or <b>
                            <code>Pallet</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="GenerateContainerScm">
                <Description>
                    <div>If set and if the container is not associated with a SCM then a SCM is generated and associated with the container speicifed in the input. 
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LocationId" Name="LocationId">
                <Description>The Location where the inventory updates for the container happens when deleted.
				</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="OverrideModificationRules">
                <Description>
                    <div>OverrideModificationRules is used to override the statuses of container that can be considered for various modifications. 
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>Identifies the container that is processed by the API.
				</Description>
            </Attribute>
            <Attribute EntityName="Location_Inventory_Audit"
                Name="ReasonCode" NameInEntity="Reason_Code"/>
            <Attribute EntityName="Location_Inventory_Audit"
                Name="ReasonText" NameInEntity="Reason_Text"/>
            <Attribute DataType="Flag" Name="RemoveFromShipmentLine">
                <Description>
                    <div>Specify if the quantity removed from container detail should also be removed from shipment line.
					<br/>Default: <b>
                            <code>N</code>
                        </b> or <b>
                            <code>Y</code>
                        </b> if BackOrderRemovedQuantity is Y
					<br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Shipment"
                Name="ShipmentKey" NameInEntity="Shipment_Key">
                <Description>
                    <div>
					Identifies the shipment to which the container belongs.
					<Validation>
                            <Condition> Must be a valid <code>ShipmentKey</code> in the table <code>YFS_SHIPMENT</code>.
						</Condition>
                            <Error Description="Invalid Shipment." ErrorCode="YFS10533"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid">
                <Description>
                    <div>
                        <Validation>
                            <br/>Default: <b>
                                <code>CREATE_CONTAINER</code>
                            </b> if <code>createShipmentContainer</code> API is called else  <b>
                                <code>CHANGE_CONTAINER</code>
                            </b>
                            <br/>Valid Values: Any derived instance of abstract transaction <b>
                                <code>CHANGE_CONTAINER_STATUS</code>
                            </b>
                            <Condition> Must be an instance of abstract transaction <code>CHANGE_CONTAINER_STATUS</code>.
						</Condition>
                            <Error Description="Invalid Transaction." ErrorCode="YCP0063"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Shipment SellerOrganizationCode="" ShipNode="" ShipmentNo=""/>
    <ContainerDetails Replace="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="Replace">
                    <Description>
                        <div>Indicates if information in the database must be replaced by the information specified in this node in this run of the API.
						<br/>Default: <b>
                                <code>N</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ContainerDetail Action="" FifoNo="" ItemID="" ProductClass=""
            Quantity="" ShipmentContainerKey="" ShipmentLineKey=""
            UnitOfMeasure="" isHistory="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="ID-40" Name="Action">
                        <Description>
                            <div>Indicates the action that is performed. 
							<br/>Default: <b>
                                    <code>Create-Modify</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Create</code>
                                </b>, <b>
                                    <code>Modify</code>
                                </b> or <b>
                                    <code>Delete</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="Quantity">
                        <Description>
                            <div>
                                <Validation>
                                    <Condition> Should be greater than <code>QuantityPlaced</code> in the corresponding <code>YFS_CONTAINER_DETAIL</code>.
									</Condition>
                                    <Error
                                    Description="Container Detail Quantity cannot go below Quantity Placed." ErrorCode="YDM00182"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>
                            <div>Indicates if data should be retrieved from the history table rather than the regular table. 
							<br/>Default: <b>
                                    <code>N</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ShipmentLine DocumentType="" ItemID="" OrderNo=""
                PrimeLineNo="" ReleaseNo="" ShipmentLineNo=""
                ShipmentSubLineNo="" SubLineNo="" UnitOfMeasure="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="DocumentType"/>
                    </Attributes>
                </yfc:doc>
                <ShipmentLineInvAttRequest BatchNo="" LotAttribute1=""
                    LotAttribute2="" LotAttribute3="" LotKeyReference=""
                    LotNumber="" ManufacturingDate="" RevisionNo="">
                    <yfc:doc>
                        <Element EntityName="Shipment_Line_Req_Tag"/>
                    </yfc:doc>
                </ShipmentLineInvAttRequest>
            </ShipmentLine>
            <ShipmentTagSerials Replace="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Flag" Name="Replace">
                            <Description>
                                <div>Indicates if information in the database must be replaced by the information specified in this node in this run of the API.
								<br/>Default: <b>
                                    <code>N</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <ShipmentTagSerial Action="" BatchNo=""
                    ContainerDetailKey="" LotAttribute1=""
                    LotAttribute2="" LotAttribute3="" LotKeyReference=""
                    LotNumber="" ManufacturingDate="" Quantity=""
                    RevisionNo="" SerialNo="" ShipByDate=""
                    ShipmentLineKey="" ShipmentTagSerialKey="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="ID-40" Name="Action">
                                <Description>
                                    <div>Indicates the action that is performed. 
									<br/>Default: <b>
                                    <code>Create-Modify</code>
                                    </b>
                                    <br/>Valid Values: <b>
                                    <code>Create</code>
                                    </b>, <b>
                                    <code>Modify</code>
                                    </b> or <b>
                                    <code>Delete</code>
                                    </b>
                                    </div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Quantity" Name="Quantity">
                                <Description>
                                    <div>
                                    <Validation>
                                    <Condition> Total tagged quantity cannot exceed <code>QuantityPlaced</code> in the corresponding <code>YFS_CONTAINER_DETAIL</code>.
											</Condition>
                                    <Error
                                    Description="Container Detail Tagged Quantity cannot exceed Quantity Placed." ErrorCode="YDM00182"/>
                                    </Validation>
                                    </div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </ShipmentTagSerial>
            </ShipmentTagSerials>
        </ContainerDetail>
    </ContainerDetails>
