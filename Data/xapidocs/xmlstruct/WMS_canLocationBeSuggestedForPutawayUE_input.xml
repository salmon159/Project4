<?xml version="1.0" encoding="UTF-8"?>
<CanLocationBeSuggested EnterpriseCode="Required"
    FromActivityGroup="Required" InventoryType="" Node="Required"
    OriginalInventoryType="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/canLocationBeSuggestedForPutawayUE/input">
    <yfc:doc>
        <Element EntityName="Activity_Constraint"/>
        <Attributes>
            <Attribute DataType="Text-40" Name="InventoryType">
                <Description> Type of inventory that is being considered for putaway 
					after honoring putaway configuration (SingleSku/MultiSku/UnknownSku) </Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="OriginalInventoryType">
                <Description>Type of inventory that was requested for putaway (SingleSku/MultiSku/UnknownSku) </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Source CaseId="" LocationId="" PalletId="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="LPN_Location"/>
            <Attributes>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description> A CaseId </Description>
                </Attribute>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description> A PalletId </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Inventory CountryOfOrigin="" FifoNo="" InventoryItemKey=""
            InventoryStatus="" InventoryTagKey="" PutawayQuantity=""
            ReceiptHeaderKey="" Segment="" SegmentType="" ShipByDate="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory"/>
            <yfc:doc>
                <Element EntityName="Location_Inventory"/>
                <Attributes>
                    <Attribute DataType="String" Name="PutawayQuantity">
                        <Description>The putaway quantity for item </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Receipt ReceiptNo="">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/Receipt"/>
                <yfc:doc>
                    <Element EntityName="Receipt_Header"/>
                </yfc:doc>
            </Receipt>
            <InventoryItem ItemID="Required" ProductClass="Required" UnitOfMeasure="Required">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="WMS_Inventory_master.xml" IncludeXMLPath="/Inventory/InventoryItem"/>
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Inventory_Item" Name="ItemID"/>
                        <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
                        <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
                    </Attributes>
                </yfc:doc>
            </InventoryItem>
        </Inventory>
    </Source>
    <Destination HoldQuantity="" LocationId="Required" ZoneId="Required" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Location"/>
            <Attributes>
                <Attribute DataType="String" Name="HoldQuantity">
                    <Description> The quantity that the location can hold after considering capacity constraints (Location Volume/Location Weight/Location Capacity / SKU Dedication Capacity etc)</Description>
                </Attribute>
                <Attribute Name="LocationId">
                    <Description>
							This is the location that put away has selected for suggesting after considering all the constraints. This location 
							belongs to the Node given in the <code>CanLocationBeSuggested</code> element.
						</Description>
                </Attribute>
                <Attribute Name="ZoneId">
                    <Description>
							ZoneId to which the location belongs. This zone belongs to the node given in the <code>CanLocationBeSuggested</code> element.
						</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Destination>
</CanLocationBeSuggested>
