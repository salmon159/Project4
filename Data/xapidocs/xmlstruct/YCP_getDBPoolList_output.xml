<DBPoolList xmlns="http://www.sterlingcommerce.com/documentation/YCP/getDBPoolList/output">
    <yfc:doc>
        <Element>
            <Description>
                <div>List of database shard connection information</div>
            </Description>
        </Element>
    </yfc:doc>
    <DBPool CanPersist="" PoolId="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element>
                <Description>
                    <div>Database shard information. This maps to either the PLT_DB_POOL table or the database configuration from jdbc.properties.</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="Boolean" Name="CanPersist">
                    <Description>
                        <div>Set to <code>Y</code> if this configuration is from a persistable
						source (like the PLT_DB_POOL table). Set to <code>N</code> if the configuration
						comes from property files.</div>
                    </Description>
                    <Options IsEnumeration="Y">
                        <Option
                            Description="Connection information comes from property files" Value="N"/>
                        <Option
                            Description="Connection information comes from the database" Value="Y"/>
                    </Options>
                </Attribute>
                <Attribute DataType="DatabasePoolId" Name="PoolId">
                    <Description>
                        <div>Unique identifier of the database shard</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <ParamList>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>List of connection parameters. Note if the connection data is from property files, some parameters may be missing.</div>
                    </Description>
                </Element>
            </yfc:doc>
            <Param IsSuppressed="" Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>Connection parameter</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Boolean" Name="IsSuppressed">
                            <Description>
                                <div>Flag to indicate whether the value was filtered from output.</div>
                            </Description>
                            <Options IsEnumeration="Y">
                                <Option
                                    Description="Value is not suppressed (default)"
                                    IsDefault="Y" Value="N"/>
                                <Option
                                    Description="The value attribute was suppressed" Value="Y"/>
                            </Options>
                        </Attribute>
                        <Attribute DataType="String" Name="Name">
                            <Description>
                                <div>Parameter name</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="String" Name="Value">
                            <Description>
                                <div>Parameter value. This attribute is not set if the value is suppressed for some reason.</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </Param>
        </ParamList>
    </DBPool>
</DBPoolList>
