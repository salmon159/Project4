<PrintDocument PrintDocumentId="" PrintJobName="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/determinePrintJobNameUE/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Text-40" Name="PrintDocumentId">
                <Description> The document that will be printed.
						</Description>
            </Attribute>
            <Attribute DataType="Text-40" Name="PrintJobName">
                <Description> The PrintJobName determined by the default logic.
						</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <LabelPreference BuyerOrganizationCode="" EnterpriseCode=""
        EquipmentType="" NoOfCopies="" Node="" SCAC="" SellerOrganizationCode=""/>
    <PrinterPreference OrganizationCode="Required" PrinterId=""
        UserId="" UsergroupId="" WorkStationId="">
        <yfc:doc>
            <Element EntityName="User_Printer_Preference"/>
            <Attributes>
                <Attribute EntityName="Device" Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
							The Organization for which the printer has to be determined.
							<Validation>
                                <Condition>
                                    <code>OrganizationCode</code> should be a valid Organization. </Condition>
                                <Error
                                    Description="Invalid Organization Code." ErrorCode="YCP0040"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Key" Name="PrinterId">
                    <Description> The device id of the printer. </Description>
                </Attribute>
                <Attribute DataType="EquipmentId" Name="WorkStationId">
                    <Description> The Work Station with which the printer is associated. </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </PrinterPreference>
    <KeyAttributes>
        <KeyAttribute Name="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="Name"/>
                </Attributes>
            </yfc:doc>
        </KeyAttribute>
    </KeyAttributes>
    <InputData APIName="" FlowName="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="APIName"/>
                <Attribute DataType="String" Name="FlowName"/>
            </Attributes>
        </yfc:doc>
        <Template/>
    </InputData>
</PrintDocument>
