<FreightCharge Carrier="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCS/getFreightCharge/input">
    <Shipment AccountType="" Carrier="Required" FromCity="" FromNode=""
        FromState="Required" FromZip="Required" Service="Required"
        ToCity="Required" ToCountry="Required" ToNode="" ToState="" ToZip="Required">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					Stores shipment information. A shipment is a collection of order lines which will be shipped as a unit. One or more orders from a buyer organization may be grouped into the same shipment to minimize transportation costs.
					</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="String" Name="AccountType">
                    <Description>
                        <div>Indicates the RE or CO. This attribute is only required when using internal logic to get the freight charge. If implementations are integrating with FXRS or ConnectShip then this attribute is not required.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="Carrier">
                    <Description>
                        <div>Indicates the carrier to which the package is manifested.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="FromCity">
                    <Description>
                        <div>Indicates the source city.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="FromNode">
                    <Description>
                        <div>Indicates the source ship node. If specified, the other source address attributes are derived from this node.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="FromState">
                    <Description>
                        <div>Indicates the source location state code.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="FromZip">
                    <Description>
                        <div>Indicates the source location postal code.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="Service">
                    <Description>
                        <div>Indicates the service.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ToCity">
                    <Description>
                        <div>Indicates the destination city name.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ToCountry">
                    <Description>
                        <div>Indicates the destination country code.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ToNode">
                    <Description>
                        <div>Indicates the destination node. If specified, the other destination address attributes are derived from this node.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ToState">
                    <Description>
                        <div>Indicates the destination state.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ToZip">
                    <Description>
                        <div>Indicates the destination zip code.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Shipment>
    <SpecialServices SpecialService="">
        <yfc:doc>
            <Element EntityName="Special_Service">
                <Description>
                    <div>
					Stores special services supported by various carriers that can be used when shipping packages. Examples of special services are signature required upon delivery of the package, Saturday pick-up and Saturday delivery.
					</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute Name="SpecialService">
                    <Description>
                        <div>Indicates the special service.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </SpecialServices>
    <Package DimensionUOM="" Height="Required" Length="Required"
        Weight="Required" WeightUOM="" Width="Required">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
					Indicates the details of the package.
					</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="String" Name="DimensionUOM">
                    <Description>
                        <div>Indicates the dimension UOM of the package.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="Height">
                    <Description>
                        <div>Indicates the height of the package.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="Length">
                    <Description>
                        <div>Indicates the length of the package.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="Weight">
                    <Description>
                        <div>Indicates the weight of the package.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="WeightUOM">
                    <Description>
                        <div>Indicates the weight UOM of the package.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="Width">
                    <Description>
                        <div>Indicates the width of the package.</div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Package>
    <ConnectionParameters SellerOrganizationCode="" ShipNode="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="String" Name="SellerOrganizationCode"/>
                <Attribute DataType="String" Name="ShipNode"/>
            </Attributes>
        </yfc:doc>
        <FedExParams FedExIPAddress="" FedExPortNo="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="FedExIPAddress"/>
                    <Attribute DataType="String" Name="FedExPortNo"/>
                </Attributes>
            </yfc:doc>
        </FedExParams>
        <ConnectShipParams ConnectShipURL="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="ConnectShipURL"/>
                </Attributes>
            </yfc:doc>
        </ConnectShipParams>
        <PierbridgeParams PBDBPassword="" PBDBURL="" PBDBUserName=""
            PBDefaultRateRequestUser="" PBServerURL="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="PBDBPassword"/>
                    <Attribute DataType="String" Name="PBDBURL"/>
                    <Attribute DataType="String" Name="PBDBUserName"/>
                    <Attribute DataType="String" Name="PBDefaultRateRequestUser"/>
                    <Attribute DataType="String" Name="PBServerURL"/>
                </Attributes>
            </yfc:doc>
        </PierbridgeParams>
    </ConnectionParameters>
</FreightCharge>
