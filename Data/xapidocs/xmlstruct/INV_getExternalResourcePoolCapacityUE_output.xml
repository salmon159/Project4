<ResourcePoolCapacity xmlns="http://www.sterlingcommerce.com/documentation/INV/getExternalResourcePoolCapacityUE/output">
    <ServiceSlots yfc:DTDOccurrence="REQUIRED">
        <!-- StartTime and EndTime attributes under ServiceSlot element are REQUIRED -->
        <ServiceSlot CapacityReference="" EndTime="Required"
            StartTime="Required" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="ID-24" Name="CapacityReference">
                        <Description>
                            <div>
						Capacity reference of service slot.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Time" Name="EndTime">
                        <Description>
                            <div>
						End time of service slot.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Time" Name="StartTime">
                        <Description>
                            <div>
						Start time of service slot.
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Dates yfc:DTDOccurrence="REQUIRED">
                <!-- Date attribute is REQUIRED -->
                <Date Capacity="" Date="Required" yfc:DTDOccurrence="OneOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="Capacity" Name="Capacity">
                                <Description>
                                    <div>
						Available capacity.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Date" Name="Date">
                                <Description>
                                    <div>
						Date for which capacity will be available.
						</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                    <AdditionalCapacityLimits>
                        <AdditionalCapacityLimit Capacity=""
                            CapacityUnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute DataType="Capacity" Name="Capacity">
                                    <Description>
                                    <div>The capacity available in the current unit of measure.</div>
                                    </Description>
                                    </Attribute>
                                    <Attribute DataType="ID-24" Name="CapacityUnitOfMeasure">
                                    <Description>
                                    <div>The unit of measure in which the capacity is allocated.</div>
                                    </Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                        </AdditionalCapacityLimit>
                    </AdditionalCapacityLimits>
                    <ServiceRequests>
                        <!-- ServiceRequestId attribute is REQUIRED -->
                        <ServiceRequest ServiceRequestId="Required">
                            <yfc:doc>
                                <Attributes>
                                    <Attribute DataType="ID-24" Name="ServiceRequestId">
                                    <Description>
                                    <div>
										Service Request Identification.
										</div>
                                    </Description>
                                    </Attribute>
                                </Attributes>
                            </yfc:doc>
                        </ServiceRequest>
                    </ServiceRequests>
                </Date>
            </Dates>
        </ServiceSlot>
    </ServiceSlots>
</ResourcePoolCapacity>
