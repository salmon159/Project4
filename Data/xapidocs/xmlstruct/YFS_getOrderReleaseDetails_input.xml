<!--getOrderReleaseDetail Input XML-->
<OrderReleaseDetail DocumentType="Conditional"
    EnterpriseCode="Conditional" OrderHeaderKey="Conditional"
    OrderNo="Conditional" OrderReleaseKey="Conditional"
    ReleaseNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getOrderReleaseDetails/input">
    <yfc:doc>
        <Element EntityName="Order_Release">
            <Description>
                <div>
			One of the following must be passed to identify the <code>OrderRelease</code>.
			<br/>- <code>OrderReleaseKey</code>
                    <br/>- <code>ReleaseNo</code> and <code>OrderHeaderKey</code>
                    <br/>- <code>ReleaseNo</code>, <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code>
                    <Validation>
                        <Condition>No release is found for information passed.
				</Condition>
                        <Error Description="No Record Found" ErrorCode="YFS10001"/>
                    </Validation>
                    <Validation>
                        <Condition>
                            <code>ReleaseNo</code> or <code>OrderReleaseKey</code> not passed.
					</Condition>
                        <Error
                            Description="Invalid input for getOrderReleaseDetail API." ErrorCode="YFS10412"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type"/>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No"/>
        </Attributes>
    </yfc:doc>
</OrderReleaseDetail>
