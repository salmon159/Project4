<!--manageAssociationrelationship Input XML-->
<AsscRelationshipList xmlns="http://www.sterlingcommerce.com/documentation/YCD/manageAssociationRelationship/input">
    <AsscRelationship AsscRelationshipKey="" AssociationType=""
        DocumentType="" EnterpriseCode="" Function=""
        InventoryChkRequired="" RelationshipType="" ShowInUI="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Assc_Relationship">
                <Description>
                    <div>
                        <Validation>
                            <Condition>
                                <code>AssociationType</code> is not provided.</Condition>
                            <Error Description="Missing AssociationType" ErrorCode="YCD00048"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>EnterpriseCode</code> is not provided.</Condition>
                            <Error Description="Missing EnterpriseCode" ErrorCode="YCD00050"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Function </code> should not be null when ShowInUI is Y.</Condition>
                            <Error Description="Missing Function" ErrorCode="YCD00051"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>RelationshipType</code> should not be null when ShowInUI is Y.</Condition>
                            <Error
                                Description="Missing RelationshipType" ErrorCode="YCD00049"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
        </yfc:doc>
    </AsscRelationship>
</AsscRelationshipList>
