<Order xmlns="http://www.sterlingcommerce.com/documentation/OMP/MasterOverallTotals">
    <OverallTotalsInEnterpriseCurrency AdditionalLinePriceTotal=""
        GrandCharges="" GrandDiscount="" GrandTax="" GrandTotal=""
        HdrCharges="" HdrDiscount="" HdrTax="" HdrTotal=""
        LineSubTotal="" RecurringAdditionalLinePriceTotal="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Money" Name="AdditionalLinePriceTotal">
                    <Description>Total Price for all Non-Informational and Non-Recurring Additional
					Line Prices for the Order.
				</Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandCharges">
                    <Description>
                        <div>The total amount in the enterprise's currency of charges applied on the order and lines.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandDiscount">
                    <Description>
                        <div>The total amount in the enterprise's currency of discounts applied on the order and lines.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandTax">
                    <Description>
                        <div>The total amount in the enterprise's currency of taxes applied on the order and lines.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandTotal">
                    <Description>
                        <div>
			  The grand total in the enterprise's currency for the order. Includes all line charges and taxes.
			  </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="HdrCharges">
                    <Description>
                        <div>The total amount in the enterprise's currency of charges applied directly to the order header.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="HdrDiscount">
                    <Description>
                        <div>The total amount in the enterprise's currency of discounts applied directly to the order header.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="HdrTax">
                    <Description>
                        <div>The total amount in the enterprise's currency of taxes applied directly to the order header.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="ComputedMoney" Name="HdrTotal">
                    <Description>
                        <div>The total amount in the enterprise's currency of the order excluding line totals.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="LineSubTotal">
                    <Description>
                        <div>The line sub total in the enterprise's currency for this order. This is the sum of each line's <code>ExtendedPrice</code>.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="RecurringAdditionalLinePriceTotal">
                    <Description>Total Price for all Non-Informational and Recurring Additional
					Line Prices for the Order.
				</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </OverallTotalsInEnterpriseCurrency>
