    <OverallTotals AdditionalLinePriceTotal="" GrandCharges=""
        GrandDiscount="" GrandShippingBaseCharge=""
        GrandShippingCharges="" GrandShippingDiscount=""
        GrandShippingTotal="" GrandTax="" GrandTotal="" HdrCharges=""
        HdrDiscount="" HdrShippingBaseCharge="" HdrShippingCharges=""
        HdrShippingDiscount="" HdrShippingTotal="" HdrTax="" HdrTotal=""
        LineSubTotal="" ManualDiscountPercentage=""
        PercentProfitMargin="" RecurringAdditionalLinePriceTotal="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Money" Name="AdditionalLinePriceTotal">
                    <Description>Total Price for all Non-Informational and Non-Recurring Additional
					Line Prices for the Order.
				</Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandCharges">
                    <Description>
                        <div>The total amount of charges applied on the order and lines.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandDiscount">
                    <Description>
                        <div>The total amount of discounts applied on the order and lines.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandShippingBaseCharge">
                    <Description>The amount that will be charged for shipping without discounts or charges for the entire order.</Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandShippingCharges">
                    <Description>The total of charges associated with shipping for the entire order.</Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandShippingDiscount">
                    <Description>The total of discounts associated with shipping for the entire order.</Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandShippingTotal">
                    <Description>The total amount for shipping the line including shipping discounts and charges for the entire order.</Description>
                </Attribute>
                <Attribute DataType="Money" Name="GrandTax">
                    <Description>
                        <div>The total amount of taxes applied on the order and lines.</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Order_Header" Name="GrandTotal" NameInEntity="Total_Amount"/>
                <Attribute DataType="Money" Name="HdrCharges">
                    <Description>
                        <div>The total amount of charges applied directly to the order header.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="HdrDiscount">
                    <Description>
                        <div>The total amount of discounts applied directly to the order header.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="HdrShippingBaseCharge">
                    <Description>The amount that will be charged for shipping without discounts or charges for the order header only.</Description>
                </Attribute>
                <Attribute DataType="Money" Name="HdrShippingCharges">
                    <Description>The total of charges associated with shipping for the order header only.</Description>
                </Attribute>
                <Attribute DataType="Money" Name="HdrShippingDiscount">
                    <Description>The total of discounts associated with shipping for the order header only.</Description>
                </Attribute>
                <Attribute DataType="Money" Name="HdrShippingTotal">
                    <Description>The total amount for shipping the line including shipping discounts and charges for the order header only.</Description>
                </Attribute>
                <Attribute DataType="Money" Name="HdrTax">
                    <Description>
                        <div>The total amount of taxes applied directly to the order header.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="ComputedMoney" Name="HdrTotal">
                    <Description>
                        <div>The total amount of the order excluding line totals.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="LineSubTotal">
                    <Description>
                        <div>The line sub total for this order. This is the sum of each line's <code>ExtendedPrice</code>.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Percentage" Name="ManualDiscountPercentage">
                    <Description>
                        <div>The percentage of the total for which manual discounts were applied.
			  <code>Sum of Manual Charges on Order and Lines / (Order Total - Sum of Manual Charges on Order and Lines)</code>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Percentage" Name="PercentProfitMargin">
                    <Description>
                        <div>The percentage of the amount being made on the product being sold.<code>
			  	( (Extended Price of all items on the order
			  	+ charges that are considered for profit margin calculation) - (Sum of Item Cost across all lines))/(Extended Price of all items on the order
			  	+ charges that are considered for profit margin calculation)</code>
                            <br/>If <code>ExtendedPrice+Charges</code> is zero, 
			  	<code>PercentProfitMargin</code> is not returned.</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Money" Name="RecurringAdditionalLinePriceTotal">
                    <Description>Total Price for all Non-Informational and Recurring Additional
					Line Prices for the Order.
				</Description>
                </Attribute>
            </Attributes>
            <Element>
                <Description>
                    <div>
		  Contains information about the price and total amount details for the order line.
		  </div>
                </Description>
            </Element>
        </yfc:doc>
        <OverallChargeTotals>
            <OverallChargeTotal ChargeCategory="" ChargeName=""
                GrandCharges="" GrandDiscount="" IsBillable=""
                IsDiscount="" IsManual="" IsRefundable=""
                IsShippingCharge="|Y|N" Reference="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute EntityName="Charge_Category"
                            Name="ChargeCategory" NameInEntity="ChargeCategory">
                            <Description>The charge category for the <code>OverallChargeTotal</code>.
							</Description>
                        </Attribute>
                        <Attribute EntityName="Charge_Name"
                            Name="ChargeName" NameInEntity="ChargeName">
                            <Description>The charge name for the <code>OverallChargeTotal</code>.
							</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="GrandCharges">
                            <Description>The total charges on the order for the
							<code>ChargeCategory</code> - <code>ChargeName</code> combination.
							</Description>
                        </Attribute>
                        <Attribute DataType="Money" Name="GrandDiscount">
                            <Description>The total discounts on the order for the
							<code>ChargeCategory</code> - <code>ChargeName</code> combination.
							</Description>
                        </Attribute>
                        <Attribute EntityName="Charge_Category" Name="IsBillable"/>
                        <Attribute EntityName="Charge_Category" Name="IsDiscount"/>
                        <Attribute EntityName="Header_Charges" Name="IsManual">
                            <Description>Determines whether the charge was manually created or created
							automatically through pricing engine.</Description>
                        </Attribute>
                        <Attribute EntityName="Charge_Category" Name="IsRefundable"/>
                        <Attribute DataType="Flag" Name="IsShippingCharge">
                            <Description>Determines whether the charge is a shipping charge. This attribute value is system defined and cannot be overridden. The value for this attribute is set as <code>Y</code> only for Charge Name configured as <code>CHARGE_NAME_FOR_SHIPPING</code> in financial rule. For all other Charges this attribute is set to <code>N</code>.
							</Description>
                        </Attribute>
                        <Attribute EntityName="Header_Charges" Name="Reference"/>
                    </Attributes>
                    <Element>
                        <Description>Represents the total charges and discounts on the order for the
						specified <code>ChargeCategory</code> and <code>ChargeName</code>.</Description>
                    </Element>
                </yfc:doc>
            </OverallChargeTotal>
        </OverallChargeTotals>
    </OverallTotals>
