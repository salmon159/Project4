<RestoreOrder DocumentType="Conditional" EnterpriseCode="Conditional"
    OrderHeaderKey="Conditional" OrderNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YFS/restoreOrder/input">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
                <div>
			The order entity which contains the description of the order. The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed.
			<Validation>
                        <Condition>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed and must be valid.
				</Condition>
                        <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                    </Validation>
                </div>
            </Description>
        </Element>
    </yfc:doc>
</RestoreOrder>
