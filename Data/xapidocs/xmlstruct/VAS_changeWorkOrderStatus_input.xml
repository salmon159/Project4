<WorkOrder BaseDropStatus="" EnterpriseCode="Conditional"
    NodeKey="Conditional" TaskQKey="Conditional" TransactionId=""
    WorkOrderKey="Conditional" WorkOrderNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/VAS/changeWorkOrderStatus/input">
    <yfc:includeInParent IncludeXMLFileName="YFS_Work_Order_keys.xml" IncludeXMLPath="/WorkOrder"/>
    <yfc:doc>
        <Element EntityName="Work_Order">
            <Description>
                <div>The work order which needs status change. If <code>TaskQKey</code> is not passed, then pass  <code>WorkOrderKey</code>
                    <i>or</i>
                    <code>WorkOrderNo</code> , <code>NodeKey</code> and <code>EnterpriseCode </code> to identify the work order.
				<Validation>
                        <Condition>
                            <code>TaskQKey</code> is not passed and passed attributes of <code>WorkOrder</code> are unable to identify an existing work order.
					</Condition>
                        <Error Description="Invalid object." ErrorCode="YDM00006"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Pipeline_Definition"
                Name="BaseDropStatus" NameInEntity="Drop_Status">
                <Description>
                    <div>Value passed is used to determine the actual drop status that the transaction assigns to the load. If <code>BaseStatus</code> is not passed, then the transaction must have only one base status defined as part of the setup.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Task_Queue" Name="TaskQKey" NameInEntity="Task_Q_Key">
                <Description>
                    <div>
					Identifies a task queue. Used to read the Work Order that needs a status change.
					</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid">
                <Description>
                    <div>Identifies a transaction. If <code>TransactionID</code> is passed, the transaction identified must be derived from the abstract transaction <code>CHANGE_WORK_ORDER_STATUS</code>. If <code>TaskQKey</code> is passed, any <code>TransactionID</code> passed is ignored.
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <WorkOrderStatusAudit AuditTransactionId="" AuditUserId=""
        Comments="" ReasonCode="" StatusDate="" WorkOrderKey="" WorkOrderStatusAuditKey="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Work_Order_Status_Audit_tran.xml" IncludeXMLPath="/WorkOrderStatusAudit"/>
        <yfc:doc>
            <Element EntityName="Work_Order_Status_Audit"/>
            <Attributes>
                <Attribute EntityName="Work_Order" Name="StatusDate" NameInEntity="StatusDate">
                    <Description>Date and time of the status change.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </WorkOrderStatusAudit>
</WorkOrder>
