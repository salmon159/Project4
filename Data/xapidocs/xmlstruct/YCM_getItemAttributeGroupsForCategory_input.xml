<ItemAttributeGroupType CategoryID="" CategoryKey="" CategoryPath=""
    ItemAttributeGroupType="Required" OrganizationCode=""
    ParentCategoryKey="" xmlns="http://www.sterlingcommerce.com/documentation/YCM/getItemAttributeGroupsForCategory/input">
    <yfc:doc>
        <Element>
            <Description>
                <div>
			Identifies the category and the <code>ItemAttributeGroupType</code> to retrieve <code>ItemAttributeGroup</code>s for.<br/>
			A <code>Category</code> can be identified by passing <code>CategoryKey</code>, or by passing <code>OrganizationCode</code> along with either <code>CategoryPath</code> or both <code>CategoryID</code> and <code>ParentCategoryKey</code>.
				<Validation>
                        <Condition>No category was identified.
					</Condition>
                        <Error Description="Category not found." ErrorCode="YCM0034"/>
                    </Validation>
                    <Validation>
                        <Condition>
                            <code>ItemAttributeGroupType</code> was not passed, or did not identify a valid <code>ItemAttributeGroupType</code>.
					</Condition>
                        <Error
                            Description="Item Attribute Group Type not found." ErrorCode="YCM75SP2_0003"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Category" Name="CategoryID" NameInEntity="Category_Id"/>
            <Attribute EntityName="Category" Name="CategoryKey" NameInEntity="Category_Key"/>
            <Attribute EntityName="Category" Name="CategoryPath" NameInEntity="Category_Path"/>
            <Attribute EntityName="Category" Name="OrganizationCode" NameInEntity="Organization_Code"/>
            <Attribute EntityName="Category" Name="ParentCategoryKey" NameInEntity="Parent_Category_Key"/>
        </Attributes>
    </yfc:doc>
</ItemAttributeGroupType>
