<!--managePriceMatch Output XML-->
<PriceMatch Competitor="" Currency="" EnterpriseCode=""
    ExpirationDate="" ItemID="" MinimumQuantity="" NewUnitPrice=""
    NoHassleOverridden="" Operation="" PriceMatchKey="" RegionCity=""
    RegionState="" ShippingCharges="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/YCD/managePriceMatchUE/output">
    <yfc:doc>
        <Element EntityName="Price_Match"/>
        <Attributes>
            <Attribute DataType="String" Name="Competitor"/>
            <Attribute Name="ExpirationDate">
                <Description>This is the date When the price offered by the competitor expires, it will default to the number of days to expire rule configured in the price match rule for the enterprise.
				</Description>
            </Attribute>
            <Attribute DataType="String" Name="NewUnitPrice"/>
            <Attribute DataType="String" Name="Operation">
                <Description>This is the operation which has to be performed. </Description>
            </Attribute>
            <Attribute DataType="String" Name="ShippingCharges"/>
        </Attributes>
    </yfc:doc>
</PriceMatch>
