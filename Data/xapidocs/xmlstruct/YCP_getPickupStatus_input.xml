<GetPickUpStatusInput TransactionId="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getPickupStatus/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid"/>
        </Attributes>
    </yfc:doc>
    <PipelineKeyList yfc:DTDOccurrence="REQUIRED">
        <PipelineKey OwnerKey="Conditional" PipelineId="Conditional"
            PipelineKey="Conditional" ProcessType="Conditional" yfc:DTDOccurrence="REQUIRED">
            <yfc:doc>
                <Element EntityName="Pipeline">
                    <Description>
                        <div>
				To uniquely identify a pipeline, pass either <code>PipelineKey</code>
                            <i>or</i>
                            <code>PipelineId</code>, <code>OwnerKey</code> and  <code>ProcessType</code>.
				<Validation>
                                <Condition>If neither  <code>PipelineKey</code>
                                    <i>nor</i>
                                    <code>PipelineId</code>, <code>OwnerKey</code> and  <code>ProcessType</code> is passed.
					</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Process_Type"
                        Name="ProcessType" NameInEntity="Process_Type"/>
                </Attributes>
            </yfc:doc>
        </PipelineKey>
    </PipelineKeyList>
</GetPickUpStatusInput>
