            <yfc:doc>
                <Element EntityName="Order_Header"/>
                <Attributes>
                    <Attribute DataType="Action" Name="Action">
                        <Description>
                            <div>
						Only applicable for published data of Delete Order event, which
						can be raised by the deleteOrder API.
						<code>Action</code>
						=
						<b>
                                    <code>DELETE</code>
                                </b>
						will be published to indicate that the order will be deleted.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="TimeStamp" Name="ActualPricingDate">
                        <Description>
                            <div>The date that will be used for pricing the order.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="CustomerDOB"/>
                    <Attribute DataType="Flag" Name="DefaultCustomerInformation">
                        <Description>
                            <div>
						Indicates if the billing, shipping, payment, tax, and currency
						information on the order should be defaulted from the customer
						information. When the flag is set to "Y", and a valid customer is
						found, the billing, shipping, and tax information on the order
						will be defaulted either from the customer or from the customer
						contact. The payment method on the order will only be defaulted if
						there is no payment method on the order. The currency will only be
						defaulted if the customer's
						<code>IsDefaultCurrency</code>
						is set to 'Y'.
						<br/>
						Default:
						<b>
                                    <code>N</code>
                                </b>
                                <br/>
						Valid Values:
						<b>
                                    <code>Y</code>
                                </b>
						or
						<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-2" Name="DriverDate">
                        <Description>Date used to driver order processing. '01' represents
					ReqShipDate, '02' represents ReqDeliveryDate.
				</Description>
                    </Attribute>
                    <Attribute DataType="Text-20" Name="EntryType" NameInEntity="Entry_Type">
                        <Description>
                            <div>
						The channel through which this order was created. If the value of EntryType on the order matches the value of the common code Entry_Type, 
						the common code's attributes are considered during order validation.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasChainedLines">
                        <Description>
                            <div>
						Indicates if this order has chained or related lines.
						<br/>
						Default:
						<b>
                                    <code>N</code>
                                </b>
                                <br/>
						Valid Values:
						<b>
                                    <code>Y</code>
                                </b>
						or
						<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasDeliveryLines">
                        <Description>
                            <div>
						Indicates if the order has any delivery lines.
						<br/>
						Default:
						<b>
                                    <code>N</code>
                                </b>
                                <br/>
						Valid Values:
						<b>
                                    <code>Y</code>
                                </b>
						or
						<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasPendingChanges">
                        <Description>
					If order has
					<code>pending changes</code>
					then this flag will be returned as "Y".
				</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasProductLines">
                        <Description>
                            <div>
						Indicates if the order has any product lines.
						<br/>
						Default:
						<b>
                                    <code>N</code>
                                </b>
                                <br/>
						Valid Values:
						<b>
                                    <code>Y</code>
                                </b>
						or
						<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="HasServiceLines">
                        <Description>
                            <div>
						Indicates if the order has any service lines.
						<br/>
						Default:
						<b>
                                    <code>N</code>
                                </b>
                                <br/>
						Valid Values:
						<b>
                                    <code>Y</code>
                                </b>
						or
						<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute Name="HoldFlag">
                        <Description>
                            <div>
						This flag determines whether an hold has been placed on the order
						when certain hold types criteria are met. If order is on hold,
						default configuration will stop the shceduling and release process
						for the order. The flag is reset to 'N' when all the hold types
						have been resolved on the order and all of its order lines. Based
						on hold type configuration, certain transactions may not be
						allowed to process. Note that there is no validation made on the
						hold applied but this flag can be reset by the application based
						on hold types existed on the order.
						<br/>
						Default:
						<b>
                                    <code>N</code>
                                </b>
                                <br/>
						Valid Values:
						<b>
                                    <code>Y</code>
                                </b>
						or
						<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="IsExpirationDateComputed">
                        <Description>
                            <div>
						This flag indicates whether the expiration date is a system
						recommended date or whether it is already determined for the
						buyer. The value is
						<code>Y</code>
						if expiration date is system recommended. This flag only applies
						to Quotes.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Status" Name="MaxOrderStatus">
                        <Description>Maximum order status based on all records that belong
					to an order.
				</Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="MaxOrderStatusDesc">
                        <Description>Description of maximum order status based on all
					records that belong to an order.
				</Description>
                    </Attribute>
                    <Attribute DataType="Status" Name="MinOrderStatus">
                        <Description>Minimum order status based on all records that belong
					to an order.
				</Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="MinOrderStatusDesc">
                        <Description>Description of minimum order status based on all
					records that belong to an order.
				</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="MultipleStatusesExist">
                        <Description>If order is in more than one status then this flag will
					be returned as "Y".
				</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="OverallStatus">
                        <Description>A description of the order's overall status determined by the <code>YFSGetOverallStatusUE</code>.
                </Description>
                    </Attribute>
                    <Attribute DataType="Text-24" Name="PendingChangesUserID">
                        <Description>This attribute indicates user, who performed pending
					changes on the order.
				</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="ProcessPaymentOnReturnOrder">
                        <Description>Indicates that payment methods should be copied onto
					the return order and that refunds should be processed there, as
					opposed to the original sales order. This is only valid for return
					orders.
				</Description>
                    </Attribute>
                    <Attribute EntityName="Approval_Plan" Name="Role" NameInEntity="User_Group_ID">
                        <Description>
                            <div>The User Group of the user who created the order.
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="ScacAndService">
                        <Description>Value of SCAC and Service code.
				</Description>
                    </Attribute>
                    <Attribute EntityName="SCAC_And_Service"
                        Name="ScacAndServiceKey" NameInEntity="Scac_And_Service_Key">
                        <Description>Link to YFS_SCAC_AND_SERVICE table.
				</Description>
                    </Attribute>
                    <Attribute DataType="Text-40" Name="SourcingClassification">
                        <Description>This attribute will be used to determine node for
					sourcing.
				</Description>
                    </Attribute>
                    <Attribute DataType="Text-100" Name="Status">
                        <Description>Status description of the order.
				</Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="isHistory">
                        <Description>
                            <div>
						Indicates if data should be retrieved from the history table
						rather than the regular table.
						<br/>
						Default:
						<b>
                                    <code>N</code>
                                </b>
                                <br/>
						Valid Values:
						<b>
                                    <code>Y</code>
                                </b>
						or
						<b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Order>
    </OrderList>
