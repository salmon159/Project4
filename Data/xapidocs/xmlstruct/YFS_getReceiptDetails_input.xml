<!--getReceiptDetails Input XML-->
<Receipt ReceiptHeaderKey="" ReceiptNo="" ReceivingNode=""
    ShipmentKey="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getReceiptDetails/input">
    <yfc:doc>
        <Element EntityName="Receipt_Header"/>
        <Attributes>
            <Attribute EntityName="Receipt_Header"
                Name="ReceiptHeaderKey" NameInEntity="Receipt_Header_Key">
                <Description>
                    <div>
						Primary key attribute of <code>YFS_RECEIPT_HEADER</code> table.
							<Validation>
                            <Condition>The input attributes passed should identify a receipt. Either <code>ReceiptHeaderKey</code> or <code>ReceiptNo</code>, <code>ReceivingNode</code> and <code>ShipmentKey</code> has to be passed to identify the receipt. Errors if not valid.
								</Condition>
                            <Error
                                Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Receipt_Header" Name="ReceiptNo" NameInEntity="Receipt_No">
                <Description>
                    <div>
						Unique key or business key, which by itself identifies a receipt along with <code>ReceivingNode</code>.
							<Validation>
                            <Condition>The input attributes passed should identify a receipt. Either <code>ReceiptHeaderKey</code> or <code>ReceiptNo</code>, <code>ReceivingNode</code> and <code>ShipmentKey</code> has to be passed to identify the receipt. Errors if not valid.
								</Condition>
                            <Error
                                Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Receipt_Header" Name="ReceivingNode" NameInEntity="ReceivingNode_Key">
                <Description>
                    <div>
						The Receiving Node.
							<Validation>
                            <Condition>The input attributes passed should identify a receipt. Either <code>ReceiptHeaderKey</code> or <code>ReceiptNo</code>, <code>ReceivingNode</code> and <code>ShipmentKey</code> has to be passed to identify the receipt. Errors if not valid.
								</Condition>
                            <Error
                                Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Receipt_Header" Name="ShipmentKey" NameInEntity="Shipment_Key">
                <Description>
                    <div>
						Primary key attribute of <code>YFS_SHIPMENT</code> table. Specifies the shipment being received.
							<Validation>
                            <Condition>The input attributes passed should identify a receipt. Either <code>ReceiptHeaderKey</code> or <code>ReceiptNo</code>, <code>ReceivingNode</code> and <code>ShipmentKey</code> has to be passed to identify the receipt. Errors if not valid.
								</Condition>
                            <Error
                                Description="Either ReceiptHeaderKey or ReceiptNo, ReceivingNode and ShipmentKey is mandatory." ErrorCode="OMP10043"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</Receipt>
