<Errors xmlns="http://www.sterlingcommerce.com/documentation/YFSError">
    <!-- One or more Error Elements -->
    <Error ErrorCode="" ErrorDescription="" ErrorRelatedMoreInfo="" yfc:DTDOccurrence="ZeroOrMany">
        <yfc:doc>
            <Element EntityName="Error_Code">
                <Description>Thrown by the app.
		  </Description>
            </Element>
            <Attributes>
                <Attribute DataType="CLOB" EntityName="[NONE]" Name="ErrorCode">
                    <Description>Link to YFS_ERROR_CODE table.
		  </Description>
                </Attribute>
                <Attribute DataType="CLOB" Name="ErrorDescription">
                    <Description>Description of the error.
		  </Description>
                </Attribute>
                <Attribute DataType="CLOB" Name="ErrorRelatedMoreInfo">
                    <Description>More information about the error added by the application.
		  </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <!-- One or More Error attribute elements -->
        <Attribute Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element>
                    <Description>This element provides additional information for error resolution. Important attributes at the moment the error occured are output as Name and Value.  The ErrorCode, ErrorDescription and ErrorRelatedMoreInfo are also output as Attribute elements.
		  		</Description>
                </Element>
                <Attributes>
                    <Attribute DataType="CLOB" Name="Name">
                        <Description>The XML attribute Name or the attribute name for providing additional information.
			  </Description>
                    </Attribute>
                    <Attribute DataType="CLOB" Name="Value">
                        <Description>The value of the attribute when the error occured.
			  </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Attribute>
        <Error ErrorCode="" ErrorDescription="" ErrorRelatedMoreInfo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="CLOB" Name="ErrorDescription"/>
                    <Attribute DataType="CLOB" Name="ErrorRelatedMoreInfo"/>
                </Attributes>
            </yfc:doc>
            <!-- One or More Error attribute elements -->
            <Attribute Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="CLOB" Name="Name"/>
                        <Attribute DataType="CLOB" Name="Value"/>
                    </Attributes>
                </yfc:doc>
            </Attribute>
            <!-- The stack trace as a text node -->
            <Stack>PCDATA</Stack>
        </Error>
        <!-- The stack trace as a text node -->
        <Stack>PCDATA<yfc:doc>
                <Element>
                    <Description>Stack trace of this error or exception as text node under this element.		  		</Description>
                </Element>
            </yfc:doc>
        </Stack>
    </Error>
</Errors>
