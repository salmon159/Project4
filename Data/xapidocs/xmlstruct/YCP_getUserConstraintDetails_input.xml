<User UserId="Conditional" UserKey="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getUserConstraintDetails/input">
    <yfc:doc>
        <Attributes>
            <Attribute EntityName="User" Name="UserId" NameInEntity="Loginid">
                <Description>
                    <div>
                        <Validation>
                            <Condition> Either <code>UserId</code> or <code>UserKey</code> must be passed. </Condition>
                            <Error
                                Description="Either UserKey or UserId is mandatory" ErrorCode="YCP0156"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute Name="UserKey">
                <Description>
                    <div>
                        <Validation>
                            <Condition> Either <code>UserId</code> or <code>UserKey</code> must be passed. </Condition>
                            <Error
                                Description="Either UserKey or UserId is mandatory" ErrorCode="YCP0156"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</User>
