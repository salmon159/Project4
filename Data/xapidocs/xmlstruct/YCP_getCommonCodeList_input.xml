<CommonCode ApplicationCode="" ApplicationVersion=""
    CallingOrganizationCode="" CodeLength="" CodeLongDescription=""
    CodeName="" CodeShortDescription="" CodeSymbol="" CodeType=""
    CodeValue="" CommonCodeKey="" DocumentType="" MeantForEnterprise=""
    MeantForInternal="" MeantForSupplier="" OrganizationCode=""
    QualifiedTagId="" SystemDefinedCode="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getCommonCodeList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="ApplicationCode">
                <Description>
                    <div>
				 Code to specify an application.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ApplicationVersion">
                <Description>
                    <div>
				 Application version identifier.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="CallingOrganizationCode">
                <Description>
                    <div>
			If CallingOrganizationCode is passed, then it will
			be used as the context organization. This, combined
			with the configuration data setup for common code,
			will result in a list filtered for access by the given organization.
			</div>
                </Description>
            </Attribute>
            <Attribute Name="CodeType" NameInEntity="Code_Type">
                <Description>
                    <div>
			A code type ( User or system defined) to which a common code belongs. 
			</div>
                    <Validation>
                        <Condition>
                            <code>CodeType</code> must be passed and must be valid.
				</Condition>
                        <Error Description="Invalid CodeType." ErrorCode="YCP0177"/>
                    </Validation>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <LocalizedAttributes/>
</CommonCode>
