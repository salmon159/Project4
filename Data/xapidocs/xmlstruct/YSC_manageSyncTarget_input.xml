<SyncTarget LongDescription="" Operation="" OrganizationCode=""
    ShortDescription="" SyncSubscriptionKey="" SyncTargetID=""
    SyncTargetKey="" TargetingOrg="" xmlns="http://www.sterlingcommerce.com/documentation/YSC/manageSyncTarget/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Operation" Name="Operation">
                <Description>
                    <div>
						Operation to take
						<br/>
						Valid Values:
						<br/>
                        <b>
                            <code>Manage</code>
                        </b>
						- Creates a new record if it does not exist or modifies an
						existing record if it does.
						<br/>
                        <b>
                            <code>Create</code>
                        </b>
						- Creates a new record.
						<br/>
                        <b>
                            <code>Modify</code>
                        </b>
						- Changes the value of an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b>
						- Deletes an existing record.
						<br/>
						The default behavior is
						<b>
                            <code>Manage</code>
                        </b>
						.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="OrgCode" Name="TargetingOrg">
                <Description>The Organization that is assigned to this SyncTarget</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</SyncTarget>
