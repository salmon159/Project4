<ReceivingDiscrepancies xmlns="http://www.sterlingcommerce.com/documentation/YFS/recordReceivingDiscrepancy/input">
    <ReceivingDiscrepancy DiscrQty="" DiscrType="" Operation=""
        OrderHeaderKey="" OrderLineKey="" ReasonEntryComplete=""
        ReceivingDiscrepancyKey="" ShipmentKey="">
        <yfc:doc>
            <Element EntityName="Receiving_Discrepancy"/>
            <Attributes>
                <Attribute DataType="Text-20" Name="Operation">
                    <Description>This attribute specifies the kind of operation to be done. Valid values are 'Create','Modify' and 'Delete'.</Description>
                </Attribute>
                <Attribute EntityName="Receiving_Discrepancy"
                    Name="OrderHeaderKey" NameInEntity="Order_Header_Key">
                    <Description>
                        <div>
						Reference to the <code>YFS_ORDER_HEADER</code> table. Identifies the order for which the discrepancy details are created.
							<Validation>
                                <Condition>
                                    <code>OrderHeaderKey</code> cannot be modified.
								</Condition>
                                <Error
                                    Description="Key attributes cannot be modified." ErrorCode="OMP10065"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Receiving_Discrepancy"
                    Name="OrderLineKey" NameInEntity="Order_Line_Key">
                    <Description>
                        <div>
						Reference to the <code>YFS_ORDER_LINE</code> table. Identifies the order line for which the discrepancy details are created.
							<Validation>
                                <Condition>
                                    <code>OrderLineKey</code> cannot be modified.
								</Condition>
                                <Error
                                    Description="Key attributes cannot be modified." ErrorCode="OMP10065"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Receiving_Discrepancy"
                    Name="ReceivingDiscrepancyKey" NameInEntity="Receiving_Discrepancy_Key">
                    <Description>
                        <div>
						Primary key attribute of <code>YFS_RECEIVING_DISCREPANCY</code> table. This element contains all the attributes which can identify a discrepancy being changed. A receipt can be uniquely identified by providing the <code>ReceivingDiscrepancyKey</code>.
							<Validation>
                                <Condition>
                                    <code>OrderLineKey</code> cannot be modified.
								</Condition>
                                <Error
                                    Description="Discrepancy record not found for the given Receiving Discrepancy Key." ErrorCode="OMP10068"/>
                                <Error
                                    Description="Cannot modify the discrepancy since discrepancy details are already invoiced." ErrorCode="OMP10069"/>
                                <Error
                                    Description="Cannot modify the discrepancy since the reason entry is completed." ErrorCode="OMP10070"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Receiving_Discrepancy"
                    Name="ShipmentKey" NameInEntity="Shipment_Key">
                    <Description>
                        <div>
						Reference to the <code>YFS_SHIPMENT</code> table. Identifies the shipment for which the discrepancy details are created.
							<Validation>
                                <Condition>
                                    <code>ShipmentKey</code> cannot be modified.
								</Condition>
                                <Error
                                    Description="Key attributes cannot be modified." ErrorCode="OMP10065"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Order DocumentType="" EnterpriseCode="" OrderNo=""/>
        <Shipment SellerOrganizationCode="" ShipNode="" ShipmentNo=""/>
        <OrderLine PrimeLineNo="" SubLineNo="">
            <Item ItemID="" ProductClass="" UnitOfMeasure="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="String" Name="ProductClass"/>
                    </Attributes>
                </yfc:doc>
            </Item>
        </OrderLine>
        <ReceivingDiscrepancyDtls Reset="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-1" Name="Reset">
                        <Description>This attribute specifies the kind of operation to be done. A value of 'Y' will replace the existing descrepancy details with the details given in the input.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <ReceivingDiscrepancyDtl DiscrReasonCode="" ReasonQty="">
                <yfc:doc>
                    <Attributes>
                        <Attribute Name="DiscrReasonCode" NameInEntity="Discr_Reason_Code">
                            <Description>
                                <div>
								This element contains information about the reasons for the discrepancy quantities. Based on the reason code given against the quantities, they are marked for adjustment invoicing. 
									<Validation>
                                    <Condition>Reason code should be a valid code for the given discrepancy type.
										</Condition>
                                    <Error
                                    Description="Discrepancy reason setup not found for the given reason code." ErrorCode="OMP10071"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </ReceivingDiscrepancyDtl>
        </ReceivingDiscrepancyDtls>
    </ReceivingDiscrepancy>
</ReceivingDiscrepancies>
