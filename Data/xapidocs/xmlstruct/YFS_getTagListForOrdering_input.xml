<Tag BatchNo="" LotAttribute1="" LotAttribute2="" LotAttribute3=""
    LotKeyReference="" LotNumber="" ManufacturingDate=""
    MaximumRecords="" RevisionNo="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/getTagListForOrdering/input">
    <yfc:doc>
        <Element EntityName="Inventory_Tag">
            <Description>
                <div>
				Stores tag information for inventory. This is a mandatory element.
				</div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>
                    <div>
					Maximum number of records to be returned.  
					</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <InventoryItem ItemID="" OrganizationCode="Required" ProductClass=""
        UnitOfMeasure="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Inventory_Item">
                <Description>
                    <div>
				The primary inventory table defining all inventory items. This is a mandatory element.
				</div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="OrgCode"
                    EntityName="Inventory_Item" Name="OrganizationCode" NameInEntity="InventoryOrganizationCode">
                    <Description>
                        <div>
					The <code>InventoryOrganizationCode</code> for this <code>OrganizationCode</code> passsed will be used by the API.
					<Validation>
                                <Condition>Must be a valid <code>OrganizationCode</code>.
						</Condition>
                                <Error
                                    Description="Invalid OrganizationCode passed for the Inventory Item." ErrorCode="YFS10395"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </InventoryItem>
</Tag>
