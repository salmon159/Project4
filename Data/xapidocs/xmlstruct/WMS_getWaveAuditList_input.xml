<?xml version="1.0" encoding="UTF-8"?>
<!-- WaveKey is mandatory-->
<WaveAudit WaveKey="Required" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getWaveAuditList/input">
    <yfc:doc>
        <Element EntityName="Wave_Audit"/>
        <Attributes>
            <Attribute EntityName="Wave_Audit" Name="WaveKey" NameInEntity="Wave_Key">
                <Description>
						Unique Identifier of the Wave that is being audited.
					</Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Wave Node="" WaveNo="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element EntityName="Wave"/>
            <Attributes>
                <Attribute DataType="Key" EntityName="Wave" Name="Node" NameInEntity="Node_Key">
                    <Description>               
						   Indicates the node or warehouse where wave(s) are created.                   
					</Description>
                </Attribute>
                <Attribute DataType="SequenceNo" EntityName="Wave"
                    Name="WaveNo" NameInEntity="Wave_No">
                    <Description>                  
						   The wave number is a one up number that system generates to uniquely identify the wave in a node.            
					</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </Wave>
</WaveAudit>
