<ResourcePoolCapacity CapacityOrganizationCode=""
    ConsiderSupplementalCapacity="" EndDate="" Mode="" Node=""
    ResourcePoolId="" ResourcePoolKey="" StartDate=""
    WorkOrderReference="" xmlns="http://www.sterlingcommerce.com/documentation/INV/getExternalResourcePoolCapacityUE/input">
    <yfc:doc>
        <Element EntityName="Res_Pool"/>
        <Attributes>
            <Attribute DataType="Flag" Name="ConsiderSupplementalCapacity">
                <Description>
                    <div>
				The <code>ConsiderSupplementalCapacity</code> flag indicates that supplemental capacity should also be considered as part of the returned capacity.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Date" Name="EndDate">
                <Description>
                    <div>
				End date.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Action" Name="Mode">
                <Description>
                    <div>
				This attribute indicates whether the call to the user exit is for inquiring or for reservation of capacity.			
				<br/>Valid Values: <b>
                            <code>Inquire</code>
                        </b> or  <b>
                            <code>Reserve</code>
                        </b>.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Date" Name="StartDate">
                <Description>
                    <div>
				Start date.
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="ID-24" Name="WorkOrderReference">
                <Description>
                    <div>
				The <code>WorkOrderReference</code> value is the Work Order Key. The capacity can be adjusted based on the work order information.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ServiceRequests>
        <ServiceRequest PreAllocatedCapacity="" PromisedApptEndDate=""
            PromisedApptStartDate="" RequiredCapacity="" ServiceRequestId="Required">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Capacity" Name="PreAllocatedCapacity">
                        <Description>
                            <div>
						Pre-allocated capacity of the service request.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="PromisedApptEndDate">
                        <Description>
                            <div>
						Promised appointment end date of the service request.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Date" Name="PromisedApptStartDate">
                        <Description>
                            <div>
						Promised appointment start date of the service request.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Capacity" Name="RequiredCapacity">
                        <Description>
                            <div>
						Required capacity of the service request.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="ID-24" Name="ServiceRequestId">
                        <Description>
                            <div>
						Service Request Identification.
						</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <AdditionalCapacityLimits>
                <AdditionalCapacityLimit CapacityUnitOfMeasure=""
                    PreAllocatedCapacity="" RequiredCapacity="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="ID-24" Name="CapacityUnitOfMeasure">
                                <Description>
                                    <div>The unit of measure in which the capacity is allocated.</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Capacity" Name="PreAllocatedCapacity">
                                <Description>
                                    <div>The capacity pre-allocated to the service request in the current unit of measure.</div>
                                </Description>
                            </Attribute>
                            <Attribute DataType="Capacity" Name="RequiredCapacity">
                                <Description>
                                    <div>The capacity that the service request requires in the current unit of measure.</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </AdditionalCapacityLimit>
            </AdditionalCapacityLimits>
            <CurrentCapacityAllocation Capacity="" CapacityReference=""
                Date="" EndTime="" Node="" StartTime="">
                <yfc:doc>
                    <Attributes>
                        <Attribute DataType="Capacity" Name="Capacity">
                            <Description>
                                <div>
						Capacity of current capacity allocation.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="ID-24" Name="CapacityReference">
                            <Description>
                                <div>
						Capacity reference of current capacity allocation.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Date" Name="Date">
                            <Description>
                                <div>
						Date of current capacity allocation.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Time" Name="EndTime">
                            <Description>
                                <div>
						End time of current capacity allocation.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="ID-24" Name="Node">
                            <Description>
                                <div>
						Node of current capacity allocation.
						</div>
                            </Description>
                        </Attribute>
                        <Attribute DataType="Time" Name="StartTime">
                            <Description>
                                <div>
						Start time of current capacity allocation.
						</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
            </CurrentCapacityAllocation>
            <ServiceSkills>
                <ServiceSkill ServiceSkillId="">
                    <yfc:doc>
                        <Element EntityName="Service_Skill"/>
                    </yfc:doc>
                </ServiceSkill>
            </ServiceSkills>
            <ServiceAddress AddressLine1="" AddressLine2=""
                AddressLine3="" AddressLine4="" AddressLine5=""
                AddressLine6="" AlternateEmailID="" Beeper="" City=""
                Company="" Country="" DayFaxNo="" DayPhone=""
                Department="" EMailID="" EveningFaxNo="" EveningPhone=""
                FirstName="" IsCommercialAddress="" JobTitle=""
                LastName="" MiddleName="" MobilePhone="" OtherPhone=""
                PersonID="" State="" Suffix="" TaxGeoCode="" Title="" ZipCode="">
                <yfc:doc>
                    <Element EntityName="Person_Info"/>
                </yfc:doc>
            </ServiceAddress>
        </ServiceRequest>
    </ServiceRequests>
</ResourcePoolCapacity>
