<?xml version="1.0" encoding="UTF-8"?>
<Shipment ConsiderShipmentsIncludedInwave="" EnterpriseCode=""
    MaximumRecords="" Node="Required" OrderBy="" ProfileId="" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getShipmentProfileSummary/input">
    <yfc:doc>
        <Element EntityName="Shipment"/>
        <Attributes>
            <Attribute DataType="Text-1" Name="ConsiderShipmentsIncludedInwave">
                <Description>
                    <div>
                        Default: <b>
                            <code>A</code>
                        </b>
                        <br/>Valid Values:
                        <br/>
                        <b>
                            <code>A</code>
                        </b> - Consider All Profiles
                        <br/>
                        <b>
                            <code>Y</code>
                        </b> - Consider Profiles included in a wave
						<br/>
                        <b>
                            <code>N</code>
                        </b> - Consider Profiles not included in a wave
                  </div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>
                    Specifies maximum number of records that must be fetched. 
                </Description>
            </Attribute>
            <Attribute DataType="Key" EntityName="Wave" Name="Node" NameInEntity="Node_Key">
                <Description>Indicates the node or warehouse.</Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="OrderBy">
                <Description>
                    <div>
                        Default: <b>
                            <code>H</code>
                        </b>
                        <br/>Valid Values:
                        <br/>
                        <b>
                            <code>H</code>
                        </b> - Returns records in descending order of Total No of shipments
                        <br/>
                        <b>
                            <code>L</code>
                        </b> - Returns records in ascending order of Total No of shipments.
                  </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ProfileId"/>
        </Attributes>
    </yfc:doc>
</Shipment>
