<ShortOrder AuditTransactionId="" DocumentType="" EnterpriseCode=""
    ModificationReasonCode="" ModificationReasonText=""
    ModificationReference1="" ModificationReference2=""
    ModificationReference3="" ModificationReference4=""
    OrderHeaderKey="" OrderNo="" OrderReleaseKey="" ReleaseNo=""
    SelectMethod="" ShortForAllAvailableQty="" xmlns="http://www.sterlingcommerce.com/documentation/YFS/shortOrder/input">
    <yfc:doc>
        <Element EntityName="Order_Header">
            <Description>
                <div>
		The order entity which contains the description of the order.
		<Validation>
                        <Condition>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed and must be valid.
			</Condition>
                        <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute EntityName="Order_Audit"
                Name="AuditTransactionId" NameInEntity="Audit_Transaction_Id"/>
            <Attribute EntityName="Order_Header" Name="DocumentType" NameInEntity="Document_Type">
                <Description>
                    <div>The document type used to describe the order. Part of the unique key for the order. Either <code>OrderHeaderKey</code>
                        <i>or</i>
                        <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> of the order must be passed.
					<Validation>
                            <Condition>Either <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code>
                                <i>or</i>
                                <code>OrderHeaderKey</code> of the order is not passed.
					</Condition>
                            <Error Description="Invalid Order" ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="EnterpriseCode" NameInEntity="Enterprise_Key">
                <Description>
                    <div>
					Value that identifies an Enterprise.
					<br/>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed to identify the <code>Order</code>.
					<Validation>
                            <Condition>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed and must be valid.
						</Condition>
                            <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonCode" NameInEntity="Reason_Code"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReasonText" NameInEntity="Reason_Text"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference1" NameInEntity="Reference_1"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference2" NameInEntity="Reference_2"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference3" NameInEntity="Reference_3"/>
            <Attribute EntityName="Order_Audit"
                Name="ModificationReference4" NameInEntity="Reference_4"/>
            <Attribute EntityName="Order_Header" Name="OrderHeaderKey" NameInEntity="Order_Header_Key">
                <Description>
                    <div>
					Unique key that identifies the order. This value is system-generated.
					<br/>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed to identify the <code>Order</code>.
					<Validation>
                            <Condition>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed and must be valid.
						</Condition>
                            <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Header" Name="OrderNo" NameInEntity="Order_No">
                <Description>
                    <div>
                        <br/>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed to identify the <code>Order</code>.
					<Validation>
                            <Condition>The <code>OrderHeaderKey</code> attribute OR the <code>EnterpriseCode</code>, <code>DocumentType</code> and <code>OrderNo</code> attributes must be passed and must be valid.
						</Condition>
                            <Error Description="YFS:Invalid Order." ErrorCode="YFS10003"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute EntityName="Order_Release" Name="OrderReleaseKey" NameInEntity="Order_Release_Key"/>
            <Attribute EntityName="Order_Release" Name="ReleaseNo" NameInEntity="Release_No"/>
            <Attribute DataType="Action" Name="SelectMethod">
                <Description>
                    <div>
				This attribute determines whether database select operations are carried out in the <b>
                            <code>WAIT</code>
                        </b> or <b>
                            <code>NO_WAIT</code> mode</b>.
				<br/>Default: <b>
                            <code>NO_WAIT</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>WAIT</code>
                        </b> or  <b>
                            <code>NO_WAIT</code> mode</b>.
			</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ShortForAllAvailableQty">
                <Description>
                    <div>
					This attribute determines whether to short all of the available quantities of all product lines of the specified Order.  If <code>ShortForAllAvailableQty</code> is passed as <b>
                            <code>Y</code>
                        </b>, then <code>OrderReleaseKey</code> and <code>ReleaseNo</code> will be ignored.
					Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <OrderLines>
        <OrderLine OrderLineKey="" OrderReleaseKey="" PrimeLineNo=""
            ReleaseNo="" SubLineNo="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Order_Line">
                    <Description>
                        <div>
				The order line whose available quantities will be shorted.
				<Validation>
                                <Condition>Not a product line.
					</Condition>
                                <Error Description="Invalid OrderLine." ErrorCode="YFS10009"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute EntityName="Order_Release"
                        Name="OrderReleaseKey" NameInEntity="Order_Release_Key"/>
                    <Attribute EntityName="Order_Release"
                        Name="ReleaseNo" NameInEntity="Release_No"/>
                </Attributes>
            </yfc:doc>
        </OrderLine>
    </OrderLines>
</ShortOrder>
