                    <Attributes>
                        <Attribute DataType="String" Name="ReceivingType"/>
                    </Attributes>
                </yfc:doc>
            </ClassificationCodes>
            <ContainerInformation CapacityVolume="" CapacityVolumeUom=""
                ContainerSize="" ContainerType="" EmptyCnrWeight=""
