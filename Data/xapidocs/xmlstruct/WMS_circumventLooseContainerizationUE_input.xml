                    <Attribute DataType="String" Name="Quantity"/>
                    <Attribute DataType="String" Name="SKUUnitSizeCode"/>
                    <Attribute DataType="String" Name="SourceSortSequence"/>
                    <Attribute DataType="String" Name="SourceZoneId"/>
                    <Attribute DataType="String" Name="TotalVolume"/>
                    <Attribute DataType="String" Name="TotalVolumeUOM"/>
                    <Attribute DataType="String" Name="TotalWeight"/>
                    <Attribute DataType="String" Name="TotalWeightUOM"/>
                    <Attribute DataType="String" Name="UnitOfMeasure"/>
                    <Attribute DataType="String" Name="Width"/>
                    <Attribute DataType="String" Name="WidthUOM"/>
                </Attributes>
            </yfc:doc>
        </ItemToBePacked>
    </ItemToBePackedList>
