                        <Attribute DataType="String" Name="MaxCnrWeightUom"/>
                    </Attributes>
                </yfc:doc>
            </ContainerInformation>
            <SkuUnitSizeCodeList>
                <SkuUnitSizeCode Quantity="" sizeCode="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="Quantity"/>
                            <Attribute DataType="String" Name="sizeCode"/>
                        </Attributes>
                    </yfc:doc>
                </SkuUnitSizeCode>
            </SkuUnitSizeCodeList>
        </Item>
    </ContainerList>
    <ItemToBePackedList>
        <ItemToBePacked ContainerCategory="" EnterpriseCode="" Height=""
            HeightUOM="" IdentifierKey="" ItemId="" Length=""
            LengthUOM="" ProductClass="" Quantity="" SKUUnitSizeCode=""
            SourceSortSequence="" SourceZoneId="" TotalVolume=""
            TotalVolumeUOM="" TotalWeight="" TotalWeightUOM=""
            UnitOfMeasure="" Width="" WidthUOM="">
