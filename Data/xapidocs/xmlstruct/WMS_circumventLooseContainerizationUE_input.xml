                <Attributes>
                    <Attribute DataType="String" Name="ContainerCategory"/>
                    <Attribute DataType="String" Name="EnterpriseCode"/>
                    <Attribute DataType="String" Name="Height"/>
                    <Attribute DataType="String" Name="HeightUOM"/>
                    <Attribute DataType="String" Name="IdentifierKey"/>
                    <Attribute DataType="String" Name="ItemId"/>
                    <Attribute DataType="String" Name="Length"/>
                    <Attribute DataType="String" Name="LengthUOM"/>
