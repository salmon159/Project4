<Responses xmlns="http://www.sterlingcommerce.com/documentation/YCP/createInitiatorResponse/input">
    <Response ApplicationAction="" ApplicationReference="" EnteredBy=""
        EnterpriseCode="" NegotiationHeaderKey="" NegotiationNo=""
        Notes="" OrganizationCode="Required" ReasonCode="" yfc:DTDOccurrence="REQUIRED">
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				One of the following must be passed and valid:
				<br/>- <code>NegotiationHeaderKey</code>
                        <br/>- <code>NegotiationNo</code> and <code>EnterpriseCode</code>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute EntityName="Negotiation_Hdr"
                    Name="EnterpriseCode" NameInEntity="Enterprise_Key"/>
                <Attribute EntityName="Negotiation_Hdr"
                    Name="NegotiationNo" NameInEntity="Negotiation_No"/>
                <Attribute EntityName="Response" Name="OrganizationCode" NameInEntity="Organization_Code">
                    <Description>
                        <div>
					The initiator organization.
						<Validation>
                                <Condition>
                                    <code>OrganizationCode</code> must be the initiator organization.
						</Condition>
                                <Error Description="" ErrorCode="YCP0025"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <!-- Either NegotiationHeaderKey or NegotiationNo and EnterpriseCode is required. -->
        <HeaderResponses>
            <HeaderResponse ForResponseNo="" ResponseAction="Required">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
                                <code>HeaderResponse</code> should not be passed if <code>HeaderTerms</code> are already accepted.
						</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute EntityName="Response_Hdr"
                            Name="ForResponseNo" NameInEntity="For_Response_No">
                            <Description>
                                <div>
							The Response number of the negotiator organization's last response.
								<Validation>
                                    <Condition>
                                    <code>ForResponseNo</code>  should be the negotiator's last response number. It should be <code>
                                    <b>0</b>
                                    </code> if the negotiator has not yet made any offer.
								</Condition>
                                    <Error Description="" ErrorCode="YCP0027"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Response_Hdr"
                            Name="ResponseAction" NameInEntity="Response_Action">
                            <Description>
                                <div>
							The action to be performed on the negotiation.
							<br/>If <code>ResponseAction</code> is <code>
                                    <b>Accept</b>
                                    </code>, <code>HeaderTerms</code> do not need to be passed.
							<br/>Valid Values:  <b>
                                    <code>Offer</code>
                                    </b> or <b>
                                    <code>Accept</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <HeaderTerms FreightTerms="" PaymentTerms=""/>
            </HeaderResponse>
        </HeaderResponses>
        <LineResponses>
            <yfc:doc>
                <Element>
                    <Description>
                        <div>
				One of the following is required:
				<br/>- <code>LineEntity</code> and <code>LineEntityKey</code>
                            <br/>- <code>PrimeLineNo</code> and <code>SubLineNo</code>
                        </div>
                    </Description>
                </Element>
            </yfc:doc>
            <LineResponse ApplicationAction="" ApplicationReference=""
                ForOptionNo="Required" ForResponseNo="Required"
                LineEntity="" LineEntityKey="" PrimeLineNo=""
                ResponseAction="Required" SubLineNo="" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Element>
                        <Description>
                            <div>
                                <code>LineResponse</code> should not be passed if <code>LineTerms</code> are already accepted.
						</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute EntityName="Response_Line"
                            Name="ForOptionNo" NameInEntity="For_Option_No">
                            <Description>
                                <div>
							Option number of the other organization's last response.
								<Validation>
                                    <Condition>
                                    <code>ForOptionNo</code> should be one of the option numbers from the negotiator's last response.
								</Condition>
                                    <Error Description="" ErrorCode="YCP0041"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Response_Line"
                            Name="ForResponseNo" NameInEntity="For_Response_No">
                            <Description>
                                <div>
							The Response number of the negotiator organization's last response.
								<Validation>
                                    <Condition>
                                    <code>ForResponseNo</code>  should be the negotiator's last response number. It should be <code>
                                    <b>0</b>
                                    </code> if the negotiator has not yet made any offer.
								</Condition>
                                    <Error Description="" ErrorCode="YCP0027"/>
                                    </Validation>
                                </div>
                            </Description>
                        </Attribute>
                        <Attribute EntityName="Response_Line"
                            Name="ResponseAction" NameInEntity="Response_Action">
                            <Description>
                                <div>
							The action to be performed on the negotiation.  
							<br/>If <code>ResponseAction</code> is <code>
                                    <b>Accept</b>
                                    </code> or <code>
                                    <b>Remove</b>
                                    </code>, <code>LineTerms</code> do not need to be passed.
							<br/>Valid Values:  <b>
                                    <code>Offer</code>
                                    </b>, 
								<b>
                                    <code>Remove</code>
                                    </b>, or <b>
                                    <code>Accept</code>
                                    </b>
                                </div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <!-- Either LineEntity and LineEntityKey or PrimeLineNo and SubLineNo is required -->
                <LineTerms>
                    <LineTermsDetail DeliveryDate="" FreightTerms=""
                        ItemID="Required" OptionNo="" Price=""
                        ProductClass="" Quantity="" SequenceNo=""
                        ShipDate="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany"/>
                </LineTerms>
            </LineResponse>
        </LineResponses>
    </Response>
</Responses>
