<Negotiation ApplicationAction="" ApplicationReference="" DocumentNo=""
    DocumentNoQryType="" DocumentType="" EnterpriseKey=""
    FreightTerms="" FromDocumentNo="" FromNegotiationExpirationDate=""
    FromNegotiationHeaderKey="" FromNegotiationNo="" HeaderEntity=""
    HeaderEntityKey="" HeaderTermsNegotiated="" InitiatorOrgCode=""
    MaximumRecords="" NegotiationExpirationDate=""
    NegotiationExpirationDateQryType="" NegotiationHeaderKey=""
    NegotiationHeaderKeyQryType="" NegotiationNo=""
    NegotiationNoQryType="" NegotiatorOrgCode="" PaymentTerms=""
    ReadFromHistory="" ReleaseNo="" Status="" ToDocumentNo=""
    ToNegotiationExpirationDate="" ToNegotiationHeaderKey=""
    ToNegotiationNo="" xmlns="http://www.sterlingcommerce.com/documentation/YCP/getNegotiationList/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Integer" Name="MaximumRecords">
                <Description>Specifies the Maximum Number of records to be returned. Default is 500.</Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="ReadFromHistory">
                <Description>
                    <div>
                    Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values:
                    <br/>
                        <b>
                            <code>N</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_NEGOTIATION_HDR table.
                    <br/>
                        <b>
                            <code>Y</code>
                        </b> - Returns records that satisfy the input criteria from the YFS_NEGOTIATION_HDR_H table (the history table).
                    <br/>
                        <b>
                            <code>B</code>
                        </b> - Returns records that satisfy the input criteria from both YFS_NEGOTIATION_HDR and YFS_NEGOTIATION_HDR_H tables. Records returned from the current table are first in the output.
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <NegotiationLine ApplicationAction="" ApplicationReference=""
        DeliveryDate="" FreightTerms="" FromDeliveryDate=""
        FromItemID="" FromPrice="" FromShipDate="" ItemID=""
        ItemIDQryType="" LineEntity="" LineEntityKey=""
        LineTermsNegotiated="" Price="" PriceQryType="" PrimeLineNo=""
        ProductClass="" ShipDate="" ShipDateQryType="" SubLineNo=""
        ToDeliveryDate="" ToItemID="" ToPrice="" ToShipDate="" UnitOfMeasure="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Date" Name="FromDeliveryDate">
                    <Description>Search negotiations with lines having delivery date after this date.</Description>
                </Attribute>
                <Attribute DataType="Date" Name="ToDeliveryDate">
                    <Description>Search negotiations with lines having delivery date before this date.</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </NegotiationLine>
</Negotiation>
