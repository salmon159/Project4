<?xml version="1.0" encoding="UTF-8"?>
<Request EnterpriseCode="" Node="Required" TableKey="Required"
    TableName="Required" xmlns="http://www.sterlingcommerce.com/documentation/WMS/getPlannedEffort/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="String" Name="EnterpriseCode">
                <Description>
                   EnterpriseCode for which Effort is being planned.
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="Node">
                <Description>
                   Ship Node for which Effort is being planned.
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="TableKey">
                <Description>
                   Identifies the table record for which Effort is being planned.
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="TableName">
                <Description>
                    Name of the Table for which Effort is being planned.
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <Attributes>
        <Attribute Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="Name">
                        <Description>
                            Name of a header level attribute
                        </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Value">
                        <Description>
                            Value of a header level attribute
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Attribute>
    </Attributes>
    <RequestLines>
        <RequestLine CountryOfOrigin="" ExpectedPickDate=""
            ExpectedShipmentDate="" ItemID="" KitCode="" ProductClass=""
            Quantity="" Segment="" SegmentType="" UnitOfMeasure="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="String" Name="CountryOfOrigin">
                        <Description>Country of origin for the item</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ExpectedPickDate">
                        <Description>ExpectedPickDate on the request line. This is required if conditions are defined for ExpectedPickDate.</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ExpectedShipmentDate">
                        <Description>Expected Shipment Date on the request line. This is required if conditions are defined for ExpectedShipmentDate.</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ItemID">
                        <Description>Identifies an item</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="KitCode">
                        <Description>The kit code of the item, if it is a Kit item.</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="ProductClass">
                        <Description>A product class</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Quantity">
                        <Description>Quantity on the request line</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="Segment">
                        <Description>Identifies an inventory segment </Description>
                    </Attribute>
                    <Attribute DataType="String" Name="SegmentType">
                        <Description>Identifies the type of inventory segment</Description>
                    </Attribute>
                    <Attribute DataType="String" Name="UnitOfMeasure">
                        <Description>Unit of measure</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Attributes>
                <Attribute Name="" Value="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="String" Name="Name">
                                <Description>
                                    Name of a line level attribute
                                </Description>
                            </Attribute>
                            <Attribute DataType="String" Name="Value">
                                <Description>
                                    Value of a line level attribute
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </Attribute>
            </Attributes>
        </RequestLine>
    </RequestLines>
</Request>
