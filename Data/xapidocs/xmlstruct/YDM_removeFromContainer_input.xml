<Container ActivityCode="" CODReturnTrackingNo="" CaseId=""
    ContainerNo="" ContainerScm="" ContainerSizeCode=""
    DoInventoryUpdatesOffline="" LoadKey="" OverrideModificationRules=""
    PalletId="" ShipmentKey="" xmlns="http://www.sterlingcommerce.com/documentation/YDM/removeFromContainer/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Activity" Name="ActivityCode">
                <Description>ActivityCode to be associated with Container while removing  a Container from it.
				</Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>
                    <div>
				The Container identifier from which <code>Inventory</code> or <code>Source</code> is to be removed. 
				<Validation>
                            <Condition>If container not found
				</Condition>
                            <Error
                                Description="Container does not exist." ErrorCode="YDM00157"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ContainerSizeCode"/>
            <Attribute DataType="Flag" Name="DoInventoryUpdatesOffline">
                <Description>
                    <div>Set to enable inventory update offline.
					<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="OverrideModificationRules">
                <Description>
                    <div>
						Pass as <b>
                            <code>Y</code>
                        </b> to override the validations for statuses of shipment container that can be considered for various modifications.
						<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>
                    <div>
				The Container identifier from which <code>Inventory</code> or <code>Source</code> is to be removed. 
				<Validation>
                            <Condition>If container not found
				</Condition>
                            <Error
                                Description="Container does not exist." ErrorCode="YDM00157"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <PackLocation LocationId="Conditional" Node="Required">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="LocationId" Name="LocationId">
                    <Description>
                        <div>A valid location in the Node
							<Validation>
                                <Condition> If <code>Node</code> is WMS Node Integrated with DCM and Location cannot be derived from <code>Source</code> or <code>Container</code> then Location must be an a valid <code>Location</code> in the Node.
								</Condition>
                                <Error
                                    Description="Location does not exist." ErrorCode="WMS00005"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="ShipNode" Name="Node">
                    <Description>
                        <div>Node at which Packing is taking place.
							<Validation>
                                <br/>
                                <Condition> Must be a valid. <code>Node</code>.
								</Condition>
                                <Error Description="Invalid ShipNode." ErrorCode="YFS10048"/>
                            </Validation>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </PackLocation>
    <Shipment SellerOrganizationCode="" ShipmentNo=""/>
    <Load LoadNo=""/>
    <Source CaseId="" LoadKey="" PalletId="" RetainContainer=""
        ShipmentKey="" TransactionId="">
        <yfc:doc>
            <Element>
                <Description>
                    <div>Specifies the Container or SKU to be removed from <code>Container</code>
                        <Validation>
                            <Condition> If both <code>CaseId</code> attribute and <code>Source</code> is passed 
				</Condition>
                            <Error
                                Description="Cannot remove both SKU and Container at same time." ErrorCode="YDM00176"/>
                        </Validation>
                    </div>
                </Description>
            </Element>
            <Attributes>
                <Attribute DataType="LPNNo" Name="CaseId">
                    <Description>
                        <div>The Container identifier which needs to removed from <code>Container</code>
                        </div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Load" Name="LoadKey" NameInEntity="LoadKey"/>
                <Attribute DataType="LPNNo" Name="PalletId">
                    <Description>
                        <div>
				The Container identifier which needs to removed from <code>Container</code>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="Flag" Name="RetainContainer">
                    <Description>Indicates if removed container should exist as an outbound container.
					</Description>
                </Attribute>
                <Attribute EntityName="Shipment" Name="ShipmentKey" NameInEntity="ShipmentKey"/>
                <Attribute EntityName="Transaction" Name="TransactionId" NameInEntity="Tranid">
                    <Description>If passed this would be used for the ChildContainer which is being removed from the Container.
					</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <Shipment SellerOrganizationCode="" ShipmentNo=""/>
        <Load LoadNo=""/>
        <Inventory CountryOfOrigin="" FifoNo="" InnerPackQuantity=""
            InventoryItemKey="" InventoryStatus="" InventoryTagKey=""
            NoOfInnerPacks="" Quantity="" ReceiptHeaderKey="" Segment=""
            SegmentType="" ShipByDate="">
            <yfc:doc>
                <Element EntityName="Location_Inventory">
                    <Description>
                        <div>
                            <Validation>
                                <br/>
                                <Condition> Either <code>Quantity</code> or <code>InnerPackQuantity</code> and <code>NoOfInnerPacks</code> must be passed.
					</Condition>
                                <Error
                                    Description="Quantity not passed in the input." ErrorCode="WMS00021"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="Quantity" Name="InnerPackQuantity">
                        <Description>
                            <div>Quantity contained in inner packs.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="NoOfInnerPacks">
                        <Description>
                            <div>Number of inner packs packed in the shipment container.
						</div>
                        </Description>
                    </Attribute>
                    <Attribute Name="Quantity">
                        <Description>
                            <div>
                                <Validation>
                                    <br/>
                                    <Condition>Quantity should not exceed placed ContainerDetail Quantity. Shipment line's OVER_SHIP_QUANTITY is decremented for Over picked quantity. 
							</Condition>
                                    <Error
                                    Description="Not enough Quantity to Remove." ErrorCode="YDM00180"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <Receipt ReceiptNo="">
                <yfc:doc>
                    <Element EntityName="Receipt_Header"/>
                </yfc:doc>
            </Receipt>
            <InventoryItem ItemID="Required" ProductClass="Required"
                UnitOfMeasure="Required" yfc:DTDOccurrence="REQUIRED">
                <yfc:doc>
                    <Element>
                        <Description>SKU to be removed
						</Description>
                    </Element>
                    <Attributes>
                        <Attribute EntityName="Inventory_Item" Name="ItemID"/>
                        <Attribute EntityName="Inventory_Item" Name="ProductClass"/>
                        <Attribute EntityName="Inventory_Item" Name="UnitOfMeasure"/>
                    </Attributes>
                </yfc:doc>
            </InventoryItem>
            <TagDetail BatchNo="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotExpirationDate="" LotKeyReference=""
                LotNumber="" ManufacturingDate="" RevisionNo="">
                <yfc:doc>
                    <Element EntityName="Inventory_Tag"/>
                    <Attributes>
                        <Attribute DataType="String" Name="LotExpirationDate"/>
                    </Attributes>
                </yfc:doc>
            </TagDetail>
            <RequestedTag BatchNo="" LotAttribute1="" LotAttribute2=""
                LotAttribute3="" LotKeyReference="" LotNumber=""
                ManufacturingDate="" RevisionNo="">
                <yfc:doc>
                    <Element EntityName="Shipment_Line_Req_Tag">
                        <Description>
							This is used to specify the <code>RequestedTag</code> if any to be used while packing. If this information is provided, API tries to fetch a shipment line with matching Requested Tag. Also, if <code>RequestedTag</code> is not given, preference will be given to a shipment line with blank requested tag and then a line with matching <code>RequestedTag</code>.
							<div>
                                <Validation>
                                    <br/>
                                    <Condition> In case of a matching shipment line cannot be found for the given input. 
									</Condition>
                                    <Error
                                    Description="Could not identify matching Shipment line." ErrorCode="YDM00239"/>
                                </Validation>
                            </div>
                        </Description>
                    </Element>
                </yfc:doc>
            </RequestedTag>
            <SerialList>
                <SerialDetail SecondarySerial1="" SecondarySerial2=""
                    SecondarySerial3="" SecondarySerial4=""
                    SecondarySerial5="" SecondarySerial6=""
                    SecondarySerial7="" SecondarySerial8=""
                    SecondarySerial9="" SerialNo="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Element EntityName="Global_Serial_Num"/>
                    </yfc:doc>
                </SerialDetail>
            </SerialList>
        </Inventory>
    </Source>
    <Audit ReasonText="" Reference1="" Reference2="" Reference3=""
        Reference4="" Reference5="">
        <yfc:doc>
            <Element EntityName="Location_Inventory_Audit"/>
        </yfc:doc>
    </Audit>
</Container>
