<Promise xmlns="http://www.sterlingcommerce.com/documentation/YFS/scheduleOrderLines/output">
    <PromiseLine DeliveryDate="" FromScheduleNo="" FromStatus=""
        OrderLineKey="" PrimeLineNo="" Quantity="" ShipDate=""
        ShipNode="" SubLineNo="" ToStatus="" ToTagNumber="" yfc:DTDOccurrence="OneOrMany">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Date" Name="DeliveryDate">
                    <Description>
                        <div>
							If driver date of the order is <b>
                                <code>DeliveryDate</code>
                            </b> then a <code>DeliveryDate</code> must be passed. <code>OrderLine</code> is scheduled to deliver on the passed <code>DeliveryDate</code>.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Count" Name="FromScheduleNo">
                    <Description>
                        <div>
						Indicates a schedule number allocate from. One line can be in multiple schedules. Pass a valid <code>FromScheduleNo</code> to allocate quantity in a particular schedule. 
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Status" Name="FromStatus">
                    <Description>
                        <div>
						Specifies a status that quantity should be allocated from. If <code>FromStatus</code> is passed, allocation is done only from that status. If <code>FromStatus</code> is not passed, allocation is made from any status.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Key" Name="OrderLineKey">
                    <Description>
                        <div>Unique identifier of the OrderLine. 
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LineNo" Name="PrimeLineNo">
                    <Description>
                        <div>
						Part of a key for the <code>OrderLine</code>. 
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Quantity" Name="Quantity">
                    <Description>
                        <div>
						The quantity to schedule for the <code>OrderLines</code>. If not passed, line quantity is not scheduled.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Date" Name="ShipDate">
                    <Description>
                        <div>
							If driver date of the order is <b>
                                <code>ShipDate</code>
                            </b> then a <code>ShipDate</code> must be passed. <code>OrderLine</code> is scheduled to ship on the passed <code>ShipDate</code>.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="ShipNode" Name="ShipNode">
                    <Description>
                        <div>
						Indicates the <code>ShipNode</code> this <code>OrderLine</code> should use.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="LineNo" Name="SubLineNo">
                    <Description>
                        <div>
						Part of a key for the <code>OrderLine</code>.
						</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Status" Name="ToStatus">
                    <Description>
                        <div>
							Indicates a status to which <code>OrderLines</code> are set, if the <code>OrderLines</code> are succesfully scheduled during this <code>scheduleOrderLines</code> call. If <code>ToStatus</code> is not passed, the Status is determined from the pipeline.
							<br/>Valid Values: An equivalent status of one of these statuses:  <b>
                                <code>1500</code>
                            </b>,<b>
                                <code>1600</code>
                            </b>,<b>
                                <code>1300</code>
                            </b>.<br>The value of this attribute is not populated in split line scenario.</br>
                        </div>
                    </Description>
                </Attribute>
                <Attribute DataType="String" Name="ToTagNumber"/>
            </Attributes>
        </yfc:doc>
        <Tag LotNumber="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Text-40" Name="LotNumber">
                        <Description>
                            <div>
			Lot Number
			</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Tag>
        <Result AssignedQty="" NoInventoryFoundQty=""
            NoStatusFoundQty="" RequestedQty="">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Quantity" Name="AssignedQty">
                        <Description>
                            <div>
					Quantity assigned for scheduling
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="NoInventoryFoundQty">
                        <Description>
                            <div>
					Quantity for which no inventory has been found during scheduling
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="NoStatusFoundQty">
                        <Description>
                            <div>
					Quantity for which no drop status has been found during scheduling
					</div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="RequestedQty">
                        <Description>
                            <div>
					Quantity requested for scheduling
					</div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Result>
    </PromiseLine>
</Promise>
