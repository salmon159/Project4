<ChargeCategory Action="" ChargeCategory="Required" ChargeCategoryKey=""
    ConsiderForProfitMargin="N|Y" Description="" DocumentType="Required"
    IsBillable="" IsDiscount="" IsRefundable=""
    OrganizationCode="Required" xmlns="http://www.sterlingcommerce.com/documentation/YCP/manageChargeCategory/input">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Text-8" Name="Action">
                <Description>
                    <div>
                        <br/>Valid Values:
						<br/>
                        <b>
                            <code>CREATE</code>
                        </b> - Creates a Record in YFS_CHARGE_CATEGORY table.
						<br/>
                        <b>
                            <code>MODIFY</code>
                        </b> - Modifies a record in YFS_CHARGE_CATEGORY table.
						<br/>
                        <b>
                            <code>DELETE</code>
                        </b> - Deletes a record in YFS_CHARGE_CATEGORY table.
						<br/>
                        <b>Default Action is <code>MODIFY</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="IsBillable">
                <Description>
                    <div>
                        <br/>Valid Values:
						<br/>
                        <b>
                            <code>Y</code>
                        </b> - Sets IS_BILLABLE field in record to Y
						<br/>
                        <b>
                            <code>N</code>
                        </b> - Sets IS_BILLABLE field in record to N
						<br/>
                        <b>Default value for<code>IsBillable</code> is <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="IsDiscount">
                <Description>
                    <div>
                        <br/>Valid Values:
						<br/>
                        <b>
                            <code>Y</code>
                        </b> - Sets IS_DISCOUNT field in record to Y
						<br/>
                        <b>
                            <code>N</code>
                        </b> - Sets IS_DISCOUNT field in record to N
						<br/>
                        <b>Default value for<code>IsDiscount</code> is <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-1" Name="IsRefundable">
                <Description>
                    <div>
                        <br/>Valid Values:
						<br/>
                        <b>
                            <code>Y</code>
                        </b> - Sets IS_REFUNDABLE field in record to Y
						<br/>
                        <b>
                            <code>N</code>
                        </b> - Sets IS_REFUNDABLE field in record to N
						<br/>
                        <b>Default value for<code>IsRefundable</code> is <code>Y</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
</ChargeCategory>
