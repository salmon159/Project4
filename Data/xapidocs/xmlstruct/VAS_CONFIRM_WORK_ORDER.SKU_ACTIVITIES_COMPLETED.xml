<WorkOrder AdditionalRequestedQuantity="" ApptStatusDescription=""
    CaseId="" ComputedRequestedQuantity="" CurrentApptSeq=""
    DelayAppointmentTaking="" DocumentType="" EnterpriseCode=""
    FinishNoLaterThan="" HoldFlag="" InvUpdateActivityCode=""
    IsMultiApptRequired="" IsPlanningComplete=""
    IsQuantityRequestedOverridden="" ItemID="" LevelOfService=""
    LocationId="" NextAlertTs="" NodeKey="" OpenWorkOrderFlag=""
    OrderHeaderKey="" OrderLineKey="" OrderNo="" PalletId=""
    PipeLineKey="" PreCallStatus="" PreCallStatusDescription=""
    PreferredResourcePoolId="" PreferredServiceResourceId="" Priority=""
    ProductAmount="" ProductClass="" PromisedApptEndDate=""
    PromisedApptStartDate="" ProviderOrganizationCode="" Purpose=""
    QuantityAllocated="" QuantityCompleted="" QuantityReleased=""
    QuantityRemoved="" QuantityRequested="" ReasonCode=""
    ReasonCodeDescription="" ReasonText="" Segment="" SegmentType=""
    SellerOrganizationCode="" SerialNo="" ServiceAmount=""
    ServiceComplexityLevel="" ServiceItemDescription=""
    ServiceItemGroupCode="" ServiceItemID="" ServiceSupervisorId=""
    ServiceUom="" ShipByDate="" ShipmentKey="" ShipmentNo=""
    StartNoEarlierThan="" StationId="" StationType="" Status=""
    StatusDate="" StatusDescription="" Timezone="" TotalAmount=""
    TotalVolume="" TotalWeight="" Uom="" VolumeUOM="" WeightUOM=""
    WorkOrderKey="" WorkOrderNo="" isHistory="" xmlns="http://www.sterlingcommerce.com/documentation/VAS/CONFIRM_WORK_ORDER/SKU_ACTIVITIES_COMPLETED">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="YFS_Work_Order_master.xml" IncludeXMLPath="/WorkOrder"/>
    <yfc:doc>
        <Element EntityName="Work_Order"/>
        <Attributes>
            <Attribute DataType="Text-100" Name="ApptStatusDescription">
                <Description>
                    <div>Description of appointment status.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="CaseId">
                <Description>
                    <div>Case for which the work order is processed.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="LineNo" Name="CurrentApptSeq">
                <Description>
                    <div>For work orders where multiple appointments are not required, this field denotes the current appointment sequence.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="LocationId"
                EntityName="Equipment_Detail" Name="LocationId" NameInEntity="Location_Id">
                <Description>
                    <div>The location associated with the VAS Station.</div>
                </Description>
            </Attribute>
            <Attribute DataType="LPNNo" Name="PalletId">
                <Description>
                    <div>Pallet for which the work order is processed.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-100" Name="PreCallStatusDescription">
                <Description>
                    <div>Description of Pre-call status.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-100" Name="ReasonCodeDescription">
                <Description>
                    <div>Description of Reason Code.
					</div>
                </Description>
            </Attribute>
            <Attribute Name="ServiceComplexityLevel">
                <Description>
                    <div>This field denotes the highest service level across all service items included in the work order.  For PS work orders only PS service items are considered.  Applicable only for PS or DS work orders.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-100" Name="ServiceItemDescription">
                <Description>
                    <div>Description of Service Item.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="EquipmentId" EntityName="Equipment"
                Name="StationId" NameInEntity="Equipment_Id">
                <Description>
                    <div>The <code>VAS Station</code> in which the last activity of the work order has taken place.</div>
                </Description>
            </Attribute>
            <Attribute DataType="EquipmentType" EntityName="Equipment"
                Name="StationType" NameInEntity="Equipment_Type">
                <Description>
                    <div>The <code>Station Type</code> of the VAS station.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-100" Name="StatusDescription">
                <Description>
                    <div>Description of WorkOrder status.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Money" Name="TotalAmount">
                <Description>
                    <div>The total value of the work order.
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="TotalVolume">
                <Description>
                    <div>The quantity requested in the volume unit of measure.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Quantity" Name="TotalWeight">
                <Description>
                    <div>The quantity requested in the weight unit of measure.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-24" Name="VolumeUOM">
                <Description>
                    <div>The volume unit of measure in which quantity needs to be requested.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-24" Name="WeightUOM">
                <Description>
                    <div>The weight unit of measure in which quantity needs to be requested.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="isHistory">
                <Description>
                    <div>Indicates if data should be retrieved from the history table rather than the regular table. 
				<br/>Default: <b>
                            <code>N</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <WorkOrderActivityDtl ActivityCode="" ActivityLocationId=""
        CaseId="" PalletId="" QuantityBeingConfirmed="" SerialNo="" ShipByDate="">
        <yfc:doc>
            <Element EntityName="Work_Order_Activity_Dtl"/>
            <Attributes>
                <Attribute DataType="Text-40" Name="CaseId">
                    <Description>
                        <div>Case identifier in which the work order activity is confirmed.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="PalletId">
                    <Description>
                        <div>Pallet identifier in which the work order activity is confirmed.
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Quantity" Name="QuantityBeingConfirmed">
                    <Description>
                        <div>Quantity confirmed in this API call. 
					</div>
                    </Description>
                </Attribute>
                <Attribute DataType="Text-40" Name="SerialNo">
                    <Description>
                        <div>SerialNo for which the work order activity is confirmed.
					</div>
                    </Description>
                </Attribute>
                <Attribute EntityName="Work_Order" Name="ShipByDate" NameInEntity="Ship_By_Date"/>
            </Attributes>
        </yfc:doc>
        <WorkOrderTag BatchNo="" LotAttribute1="" LotAttribute2=""
            LotAttribute3="" LotKeyReference="" LotNumber=""
            ManufacturingDate="" RevisionNo="" WorkOrderKey="" WorkOrderTagKey="">
            <yfc:includeInParent IncludeXMLChildNodes="N"
                IncludeXMLFileName="YFS_Work_Order_master.xml" IncludeXMLPath="/WorkOrder/WorkOrderTag"/>
            <yfc:doc>
                <Element EntityName="Work_Order_Tag">
                    <Description>
                        <div>
					Stores details of the inventory tag attributes of the work order item that is tag tracked. Tag information is stored only if the <code>Node</code> tracks tag information in inventory.	
					</div>
                    </Description>
                </Element>
            </yfc:doc>
        </WorkOrderTag>
        <SerialDetail SecondarySerial1="" SecondarySerial2=""
            SecondarySerial3="" SecondarySerial4="" SecondarySerial5=""
            SecondarySerial6="" SecondarySerial7="" SecondarySerial8=""
            SecondarySerial9="" SerialNo="">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Global_Serial_Num"
                        Name="SecondarySerial1" NameInEntity="Secondary_Serial_1"/>
                    <Attribute EntityName="Global_Serial_Num"
                        Name="SecondarySerial2" NameInEntity="Secondary_Serial_2"/>
                    <Attribute EntityName="Global_Serial_Num"
                        Name="SecondarySerial3" NameInEntity="Secondary_Serial_3"/>
                    <Attribute EntityName="Global_Serial_Num"
                        Name="SecondarySerial4" NameInEntity="Secondary_Serial_4"/>
                    <Attribute EntityName="Global_Serial_Num"
                        Name="SecondarySerial5" NameInEntity="Secondary_Serial_5"/>
                    <Attribute EntityName="Global_Serial_Num"
                        Name="SecondarySerial6" NameInEntity="Secondary_Serial_6"/>
                    <Attribute EntityName="Global_Serial_Num"
                        Name="SecondarySerial7" NameInEntity="Secondary_Serial_7"/>
                    <Attribute EntityName="Global_Serial_Num"
                        Name="SecondarySerial8" NameInEntity="Secondary_Serial_8"/>
                    <Attribute EntityName="Global_Serial_Num"
                        Name="SecondarySerial9" NameInEntity="Secondary_Serial_9"/>
                    <Attribute EntityName="Work_Order" Name="SerialNo" NameInEntity="Serial_No"/>
                </Attributes>
            </yfc:doc>
        </SerialDetail>
        <WorkOrderComponents>
            <WorkOrderComponent ComponentQuantity="" ItemID=""
                ProductClass="" Quantity="" Segment="" SegmentType=""
                SerialNo="" ShipByDate="" Uom=""
                WorkOrderComponentKey="" WorkOrderKey="">
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="YFS_Work_Order_Component_master.xml" IncludeXMLPath="/WorkOrderComponent"/>
                <yfc:doc>
                    <Element EntityName="Work_Order_Component">
                        <Description>
                            <div>
			This table stores information about all the components required by the work order.<br/>Components are not mandatory for work orders created for the <code>KITTING</code> service item group code.
			</div>
                        </Description>
                    </Element>
                    <Attributes>
                        <Attribute DataType="Quantity" Name="Quantity">
                            <Description>
                                <div>Quantity for which the component is confirmed.
							</div>
                            </Description>
                        </Attribute>
                    </Attributes>
                </yfc:doc>
                <SerialDetail SecondarySerial1="" SecondarySerial2=""
                    SecondarySerial3="" SecondarySerial4=""
                    SecondarySerial5="" SecondarySerial6=""
                    SecondarySerial7="" SecondarySerial8=""
                    SecondarySerial9="" SerialNo="">
                    <yfc:doc>
                        <Attributes>
                            <Attribute DataType="SerialNo" Name="SecondarySerial1"/>
                            <Attribute DataType="SerialNo" Name="SecondarySerial2"/>
                            <Attribute DataType="SerialNo" Name="SecondarySerial3"/>
                            <Attribute DataType="SerialNo" Name="SecondarySerial4"/>
                            <Attribute DataType="SerialNo" Name="SecondarySerial5"/>
                            <Attribute DataType="SerialNo" Name="SecondarySerial6"/>
                            <Attribute DataType="SerialNo" Name="SecondarySerial7"/>
                            <Attribute DataType="SerialNo" Name="SecondarySerial8"/>
                            <Attribute DataType="SerialNo" Name="SecondarySerial9"/>
                            <Attribute DataType="Text-40" Name="SerialNo"/>
                        </Attributes>
                    </yfc:doc>
                </SerialDetail>
                <WorkOrderComponentTag BatchNo="" LotAttribute1=""
                    LotAttribute2="" LotAttribute3="" LotKeyReference=""
                    LotNumber="" ManufacturingDate="" RevisionNo=""
                    WorkOrderComponentKey="" WorkOrderComponentTagKey="">
                    <yfc:doc>
                        <Element>
                            <Description>
                                <div>
					Stores details of the inventory tag attributes for work order components that are tag tracked. Tag information is stored only if the <code>Node</code> tracks tag information in inventory.	
					</div>
                            </Description>
                        </Element>
                    </yfc:doc>
                </WorkOrderComponentTag>
            </WorkOrderComponent>
        </WorkOrderComponents>
    </WorkOrderActivityDtl>
</WorkOrder>
