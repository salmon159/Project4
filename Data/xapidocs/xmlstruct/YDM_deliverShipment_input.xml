<!-- Either ShipmentNo or ShipmentKey is mandatory -->
<Shipment SellerOrganizationCode="Conditional" ShipNode="Conditional"
    ShipmentKey="Conditional" ShipmentNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/YDM/deliverShipment/input">
    <yfc:doc>
        <Element EntityName="Shipment">
            <Description>
                <div>
			The shipment being delivered. Must pass either <code>ShipmentKey</code> or <code>ShipmentNo</code>, <code>ShipNode</code> and <code>SellerOrganizationCode</code> to identify the shipment. The <code>deliverShipment</code> API invokes the c<code>hangeShipmentStatus</code> API, so the validations described there are also applied to this input data.
			</div>
            </Description>
        </Element>
    </yfc:doc>
    <ShipmentStatusAudit ReasonCode="" ReasonText="" ShipmentKey=""
        ShipmentStatusAuditKey="" StatusDate="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Shipment_Status_Audit_tran.xml"
            IncludeXMLPath="/ShipmentStatusAudit" RemoveDBHouseKeeping="Y"/>
        <yfc:doc>
            <Element EntityName="Shipment_Status_Audit"/>
            <Attributes>
                <Attribute EntityName="Shipment" Name="StatusDate" NameInEntity="StatusDate">
                    <Description>Date and time of the status change.
			</Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
    </ShipmentStatusAudit>
</Shipment>
