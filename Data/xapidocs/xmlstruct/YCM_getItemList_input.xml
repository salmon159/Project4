<Item AuthenticationKey="" CallingOrganizationCode=""
    CanUseAsServiceTool="" DisplayLocalizedFieldInLocale=""
    ExcludeChildItemsOfModelItems="" ExcludeItemsInPriceList=""
    FromGlobalItemID="" FromItemGroupCode="" FromItemID=""
    FromUnitOfMeasure="" GetUnpublishedItems="" GlobalItemID=""
    GlobalItemIDQryType="" IgnoreInvalidItems="" IgnoreOrdering=""
