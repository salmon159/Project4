        <yfc:doc>
            <Element>
                <Description>
                    <div>
				A list of ItemAttributeGroupType can be passed to be used as filter criteria to control the item attribute group types to return in the output XML.
				</div>
                </Description>
            </Element>
        </yfc:doc>
        <ItemAttributeGroupType ClassificationPurposeCode=""
            ItemAttributeGroupType="" yfc:DTDOccurrence="ZeroOrMany"/>
    </ItemAttributeGroupTypeList>
    <OrderBy>
        <yfc:doc>
            <Element>
                <Description>
                    <div>
				If OrderBy element is passed, items returned by the API are sorted in the same order as Attribute elements passed.
				</div>
                </Description>
            </Element>
        </yfc:doc>
        <Attribute Desc="" Name="Required" yfc:DTDOccurrence="OneOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="Flag" Name="Desc">
                        <Description>Sort in decending order.
						Valid values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
						Default value: <b>
                                <code>N</code>
                            </b>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Text-32" Name="Name">
                        <Description>Name of the attribute to use for sorting the results.</Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </Attribute>
    </OrderBy>
