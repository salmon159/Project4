    <Components>
        <Component ComponentItemID="Required" ComponentItemKey=""
            ComponentItemKeyQryType="" ComponentOrganizationCode=""
            ComponentUnitOfMeasure="" FromComponentItemKey=""
            ToComponentItemKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute EntityName="Item" Name="ComponentItemID" NameInEntity="Item_Id">
                        <Description>Along with ComponentOrganizationCode and ComponentUnitOfMeasure, it identifies a component item.</Description>
                    </Attribute>
                    <Attribute EntityName="Item" Name="ComponentItemKey" NameInEntity="Item_Key">
                        <Description>Component item's primary key</Description>
                    </Attribute>
                    <Attribute EntityName="Item"
                        Name="ComponentOrganizationCode" NameInEntity="Organization_Code">
                        <Description>Organization that maintains catalog.</Description>
                    </Attribute>
                    <Attribute EntityName="Item"
                        Name="ComponentUnitOfMeasure" NameInEntity="Uom">
                        <Description>Unit of measure for the Component Item</Description>
                    </Attribute>
                </Attributes>
                <Element EntityName="Kit_Item">
                    <Description>A Component Definition.  In order to correctly identify a component item, either <code>ComponentOrganizationCode</code>, <code>ComponentItemID</code> and <code>ComponentUnitOfMeasure</code> (if not blank) must be passed, or <code>ComponentItemKey</code> must be passed.</Description>
                </Element>
            </yfc:doc>
        </Component>
