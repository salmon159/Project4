    xmlns="http://www.sterlingcommerce.com/documentation/YCM/getItemList/input" yfc:ComplexQuerySupported="Y">
    <yfc:doc>
        <Attributes>
            <Attribute DataType="Key" Name="AuthenticationKey">
                <Description>Not used.</Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="CallingOrganizationCode" NameInEntity="Organization_Code">
                <Description>Calling Organization used to determine catalog organization.</Description>
            </Attribute>
            <Attribute DataType="Locale" Name="DisplayLocalizedFieldInLocale">
                <Description>
                    <div>
						The locale for which output should be returned in. Locale should be passed in the standard format of "language_country_variant" (e.g., "fr_FR", "en_US_CA", etc.).
					</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="ExcludeChildItemsOfModelItems">
                <Description>
                    <div>
			This flag indicates whether child items of an item with variations should be returned in the search results.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="String" Name="ExcludeItemsInPriceList">
                <Description>
                    <div>Items within this Pricelist will be excluded from the search results.</div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="GetUnpublishedItems">
                <Description>Indicates if unpublished items needs to be listed.</Description>
            </Attribute>
            <Attribute Name="GlobalItemID" NameInEntity="Global_Item_ID">
                <Description>Unique Global identifier used to cross reference an item with another catalog organization.</Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreInvalidItems">
                <Description>
                    <div>
			This flag indicates whether invalid items should be returned in the search results.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Flag" Name="IgnoreOrdering">
                <Description>
                    <div>
				By default, the output is ordered by <code>ORGANIZATION_CODE</code>, <code>ITEM_ID</code>, and <code>UNIT_OF_MEASURE</code>. Pass <code>IgnoreOrdering</code> as <b>
                            <code>Y</code>
                        </b> to omit ordering.
				</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Item"
                Name="InheritAttributesFromClassification" NameInEntity="Inherit_Attrs_From_Classification"/>
            <Attribute DataType="Flag" Name="IsForOrdering">
                <Description>
                    <div>
			This flag indicates whether the item search is for ordering purpose.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Text-20" Name="ItemGroupCode">
                <Description>
                    <div>
					Valid values: <b>
                            <code>PS</code>
                        </b> - Provided Service, <b>
                            <code>DS</code>
                        </b> - Delivery Service, <b>
                            <code>PROD</code>
                        </b> - Product Item
				</div>
                </Description>
            </Attribute>
            <Attribute DataType="Count" Name="MaximumRecords">
                <Description>Maximum records requested for output. Defaults to 5000.</Description>
            </Attribute>
            <Attribute EntityName="Organization" Name="OrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>
			If passed, items maintained by the <code>Organization</code> identified by the <code>OrganizationCode</code> are returned. This organization is the organization that maintains the catalog.
			</div>
                </Description>
            </Attribute>
            <Attribute EntityName="Item" Name="StartItemKey" NameInEntity="Item_Key">
                <Description>To start from a particular Item key.</Description>
            </Attribute>
            <Attribute EntityName="Organization"
                Name="UserOrganizationCode" NameInEntity="Organization_Code">
                <Description>
                    <div>If passed, items are returned for the catalog organization of the passed <code>UserOrganizationCode</code>. This attribute is considered only if <code>OrganizationCode</code> is not passed.
				<Validation>
                            <Condition>No valid organization identified.
					</Condition>
                            <Error
                                Description="Invalid Organization Code." ErrorCode="YCM0019"/>
                        </Validation>
                    </div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <ServiceType IsInStoreService="">
        <yfc:doc>
            <Element EntityName="Service_Type"/>
        </yfc:doc>
    </ServiceType>
    <ItemServiceAssoc ServiceItemGroupCode="" ServiceItemId=""
        ServiceOrganizationCode="" ServiceUOM="">
        <yfc:doc>
            <Element EntityName="Item_Service_Assoc"/>
        </yfc:doc>
    </ItemServiceAssoc>
    <CustomerInformation CustomerID="" CustomerLevel="" CustomerType=""
        Region="" RelationshipType="" Vertical="">
        <yfc:doc>
            <Attributes>
                <Attribute Name="CustomerID" NameInEntity="Customer_Id"/>
                <Attribute Name="CustomerLevel" NameInEntity="Customer_Level"/>
                <Attribute Name="CustomerType">
                    <Description>
                        <div> The type of customer.
							<br/>Valid Values:
							<br/>01 - for B2B customer
							<br/>02 - for B2C customer
							<br/>03 - for anonymous customer 
							<br/>04 - for everyone
						</div>
                    </Description>
                </Attribute>
                <Attribute Name="Region">
                    <Description>
                        <div> Region to which entitlement rule is assigned. </div>
                    </Description>
                </Attribute>
                <Attribute Name="RelationshipType" NameInEntity="Relationship_Type"/>
                <Attribute Name="Vertical" NameInEntity="Vertical"/>
            </Attributes>
            <Element EntityName="Entitle_Rule_Assignment">
                <Description>This is used to provide information about the customer for which items are being retrieved when entitlements need to be applied. CustomerID is used to get 
				customer information from the system. If both CustomerID and CustomerType are not populated, CustomerType is
				defaulted to <code>03 - Anonymous</code>. All other attributes are used as an override to apply corresponding entitlements. </Description>
            </Element>
        </yfc:doc>
    </CustomerInformation>
