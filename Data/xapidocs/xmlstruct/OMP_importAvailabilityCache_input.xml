<!--importAvailabilityCache Input XML-->
<InventoryItem InventoryItemKey="" InventoryOrganizationCode="Required"
    ItemID="" ProductClass="" UnitOfMeasure="" xmlns="http://www.sterlingcommerce.com/documentation/OMP/importAvailabilityCache/input">
    <yfc:includeInParent IncludeXMLChildNodes="N"
        IncludeXMLFileName="OMP_importAvailabilityCache_output.xml" IncludeXMLPath="/InventoryItem"/>
    <AvailabilityChanges yfc:DTDOccurrence="REQUIRED">
        <yfc:includeInParent IncludeXMLChildNodes="N"
            IncludeXMLFileName="OMP_importAvailabilityCache_output.xml" IncludeXMLPath="/InventoryItem/AvailabilityChanges"/>
        <AvailabilityChange AlertLevel="" AlertQuantity=""
            AlertRaisedOn="" AlertType="" DeliveryMethod=""
            DistributionRuleId="" EstimatedCost=""
            FirstFutureAvailableDate="" FutureAvailableDate=""
            FutureAvailableQuantity="" Latitude="" Longitude="" Node=""
            OnhandAvailableDate="" OnhandAvailableQuantity=""
            Operation="" OrganizationCode="" yfc:DTDOccurrence="OneOrMany">
            <yfc:includeInParent IncludeXMLChildNodes="Y"
                IncludeXMLFileName="OMP_importAvailabilityCache_output.xml" IncludeXMLPath="/InventoryItem/AvailabilityChanges/AvailabilityChange"/>
            <yfc:doc>
                <Element EntityName="Inventory_Alerts">
                    <Description>
                        <div>
                            <p>Stores information on Inventory Alert</p>
                            <p>The <code>Availability</code> element is uniquely defined by the following combinations of attributes:
	                		<ol>
                                    <li>
                                    <code>InventoryItemKey</code>,<code>OrganizationCode</code>,<code>DistributionRuleId</code>
                                    </li>
                                    <li>
                                    <code>InventoryItemKey</code>,<code>DeliveryMethod</code>,<code>Node</code>(ShipNode)</li>
                                </ol>
                            </p>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="String" Name="Operation">
                        <Description>
                            <div>
                                <p>Operation to perform. Default Value: <em>
                                    <code>Manage</code>
                                    </em>
                                </p>
                                <p>Valid Values:
								<ol>
                                    <li>
                                    <em>
                                    <code>Manage</code>
                                    </em> - Create/Update a inventory alert record</li>
                                    <!--<li><em><code>Delete</code></em> - Delete the specified inventory alert record.</li>-->
                                    </ol>
                                </p>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
            <AvailabilityInformation>
                <yfc:includeInParent IncludeXMLChildNodes="Y"
                    IncludeXMLFileName="MasterAvailabilityInformation.xml" IncludeXMLPath="/AvailabilityInformation"/>
                <AvailableInventory AvailableQuantity=""
                    EffectiveStartDate="" EffectiveUntilDate=""
                    IsOnhand="" MaxShipDate="" yfc:DTDOccurrence="ZeroOrMany">
                    <yfc:doc>
                        <Attributes>
                            <Attribute Name="AvailableQuantity">
                                <Description>
                                    <div>
							This indicates the available quantity. If inventory is not tracked, unplanned, or infinite,
							the value may be Java's Integer.MAX_VALUE, Java's Long.MAX_VALUE, or the 
							RealisticQuantityForInfiniteInventory property (if set).
						</div>
                                </Description>
                            </Attribute>
                            <Attribute Name="EffectiveStartDate">
                                <Description>
							The timestamp for which the ShipDate is effective from. Calculated based on the last RTAM execution time or the last <code>EffectiveUntilDate</code>.
					</Description>
                            </Attribute>
                            <Attribute Name="EffectiveUntilDate">
                                <Description>
                                    <div>
							The timestamp for which the ShipDate is effective until.This is calculated based on earliest timestamp of the computed date of each node.   The computed date of each node is computed based on the last cut-off time of the ship date and subtract back the minimum notification time based on the node's shipping calendar.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute Name="IsOnhand">
                                <Description>
                                    <div>
							This flag indicates whether the AvailableQuantity is based on on-hand supply.
						</div>
                                </Description>
                            </Attribute>
                            <Attribute Name="MaxShipDate">
                                <Description>
                                    <div>
							The date when this inventory is available. 
						</div>
                                </Description>
                            </Attribute>
                        </Attributes>
                    </yfc:doc>
                </AvailableInventory>
            </AvailabilityInformation>
        </AvailabilityChange>
    </AvailabilityChanges>
</InventoryItem>
