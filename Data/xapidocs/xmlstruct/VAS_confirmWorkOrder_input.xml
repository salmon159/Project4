<WorkOrder AttemptSuccessful="" EnterpriseCode="Conditional"
    NodeKey="Conditional" QuantityBeingConfirmed=""
    WorkOrderKey="Conditional" WorkOrderNo="Conditional" xmlns="http://www.sterlingcommerce.com/documentation/VAS/confirmWorkOrder/input">
    <yfc:includeInParent IncludeXMLFileName="YFS_Work_Order_keys.xml" IncludeXMLPath="/WorkOrder"/>
    <yfc:doc>
        <Element EntityName="Work_Order">
            <Description>
                <div>
			An existing <code>WorkOrder</code> is identified by using one of the following: 
			<code>WorkOrderKey </code>
                    <i>or</i>
                    <code>WorkOrderNo</code>, <code>NodeKey</code> and <code>EnterpriseCode </code>
                    <Validation>
                        <Condition>The passed attributes are unable to identify an existing work order.
				</Condition>
                        <Error Description="Invalid Work Order." ErrorCode="VAS00013"/>
                    </Validation>
                </div>
            </Description>
        </Element>
        <Attributes>
            <Attribute DataType="Flag" Name="AttemptSuccessful">
                <Description>
                    <div>Pass as <b>
                            <code>Y</code>
                        </b> to indicate that the service was executed successfully.
				<br/>Default: <b>
                            <code>Y</code>
                        </b>
                        <br/>Valid Values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
                    </div>
                </Description>
            </Attribute>
            <Attribute DataType="Number" Name="QuantityBeingConfirmed">
                <Description>
                    <div>Quantity being confirmed for work order.
				</div>
                </Description>
            </Attribute>
        </Attributes>
    </yfc:doc>
    <WorkOrderStatusAudit AuditTransactionId="" AuditUserId=""
        Comments="" ReasonCode="" WorkOrderKey="" WorkOrderStatusAuditKey="">
        <yfc:includeInParent
            IncludeXMLFileName="YFS_Work_Order_Status_Audit_tran.xml" IncludeXMLPath="/WorkOrderStatusAudit"/>
        <yfc:doc>
            <Element EntityName="Work_Order_Status_Audit"/>
        </yfc:doc>
    </WorkOrderStatusAudit>
    <WorkOrderAppointments>
        <WorkOrderAppointment AdditionalEquipmentUsed="" AlertSales=""
            AlertScheduler="" ApptSeq="" AttemptFailed=""
            AttentionRequired="" ExecutionBy="" ExecutionComments=""
            ExecutionDuration="" ExecutionEndTime=""
            ExecutionStartTime="" FailureReasonCode="" ItemsNotUsed=""
            NonInventoryItemsReturned="" VehicleNumber=""
            VehicleOdometerReading="" VehicleServiceRequest=""
            WorkOrderApptKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Work_Order_Appt">
                    <Description>
                        <div>
				This entity stores information about appointments taken on the work order.Appointments are used only for PS/DS work orders.
				<Validation>
                                <Condition>Appointment is not in OPEN status.
					</Condition>
                                <Error
                                    Description="Work order appointment modification not allowed." ErrorCode="VAS00058"/>
                            </Validation>
                        </div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="LineNo" Name="ApptSeq">
                        <Description>
                            <div>Appointment sequence number. Part of unique key. 
							<Validation>
                                    <Condition>Appointment sequence not found for work order.</Condition>
                                    <Error
                                    Description="No record found." ErrorCode="YFS10001"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Flag" Name="AttemptFailed">
                        <Description>
                            <div>
						If passed as <b>
                                    <code>Y</code>
                                </b>, the appointment status will be set as <code>FAILED</code>.  Otherwise, it will be set as <code>COMPLETE</code>. 
					<br/>Default: <b>
                                    <code>N</code>
                                </b>
                                <br/>Valid Values: <b>
                                    <code>Y</code>
                                </b> or <b>
                                    <code>N</code>
                                </b>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="WorkOrderApptKey">
                        <Description>
                            <div>Primary Key of YFS_WORK_ORDER_APPT. 
							<Validation>
                                    <Condition>Record does not exist.</Condition>
                                    <Error
                                    Description="No record found." ErrorCode="YFS10001"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </WorkOrderAppointment>
    </WorkOrderAppointments>
    <WorkOrderProdDeliveries AllProductsDelivered="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="AllProductsDelivered">
                    <Description>
                        <div>
                            <b>
                                <code>Y</code>
                            </b>: indicates that all requested quantities have been delivered. 
					<br/>Default: <b>
                                <code>N</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <WorkOrderProdDelivery LineNo="" QuantityDelivered=""
            WorkOrderProdDeliveryKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Attributes>
                    <Attribute DataType="LineNo" Name="LineNo">
                        <Description>
                            <div>The service line sequence number. Part of unique key. 
									<Validation>
                                    <Condition>Line not found for work order.</Condition>
                                    <Error
                                    Description="No record found." ErrorCode="YFS10001"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Quantity" Name="QuantityDelivered">
                        <Description>
                            <div>
								Additional product quantity that was delivered. For example, if prior to invoking this API, <code>QuantityDelivered</code>=3, then passing 4 in the input would now update the value to 7.
								<Validation>
                                    <Condition>Adding <code>QuantityDelivered</code> to quantity already delivered is greater than the requested quantity.</Condition>
                                    <Error
                                    Description="Cannot confirm more than the required quantity." ErrorCode="VAS00061"/>
                                </Validation>
                                <Validation>
                                    <Condition>Value is negative.</Condition>
                                    <Error
                                    Description="Cannot deliver negative quantities." ErrorCode="VAS00063"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="WorkOrderProdDeliveryKey">
                        <Description>
                            <div>Primary Key of YFS_WORK_ORDER_PROD_DEL. 
									<Validation>
                                    <Condition>Record does not exist.</Condition>
                                    <Error
                                    Description="No record found." ErrorCode="YFS10001"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </WorkOrderProdDelivery>
    </WorkOrderProdDeliveries>
    <WorkOrderServiceLines AllServicesComplete="">
        <yfc:doc>
            <Attributes>
                <Attribute DataType="Flag" Name="AllServicesComplete">
                    <Description>
                        <div>
                            <b>
                                <code>Y</code>
                            </b>: All work order service lines for Provided Service or Standalone Delivery Service order lines will be marked as complete (<code>IsComplete=Y</code>).  All others, including those passed in the input WorkOrderServiceLine XML nodes, will be ignored.
					<br/>Default: <b>
                                <code>N</code>
                            </b>
                            <br/>Valid Values: <b>
                                <code>Y</code>
                            </b> or <b>
                                <code>N</code>
                            </b>
                        </div>
                    </Description>
                </Attribute>
            </Attributes>
        </yfc:doc>
        <WorkOrderServiceLine LineNo="" WorkOrderServiceLineKey="" yfc:DTDOccurrence="ZeroOrMany">
            <yfc:doc>
                <Element EntityName="Work_Order_Service_Line">
                    <Description>
                        <div>
					This entity stores information about service lines included in the work order. Used only for PS/DS work orders.  Input service lines for Provided Service or Standalone Delivery Service order lines will be marked as complete (<code>IsComplete=Y</code>), others will be ignored.
				</div>
                    </Description>
                </Element>
                <Attributes>
                    <Attribute DataType="LineNo" Name="LineNo">
                        <Description>
                            <div>The service line sequence number. Part of unique key. 
							<Validation>
                                    <Condition>Line not found for work order.</Condition>
                                    <Error
                                    Description="No record found." ErrorCode="YFS10001"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                    <Attribute DataType="Key" Name="WorkOrderServiceLineKey">
                        <Description>
                            <div>Primary Key of YFS_WORK_ORDER_SERVICE_LINE. 
							<Validation>
                                    <Condition>Record does not exist.</Condition>
                                    <Error
                                    Description="No record found." ErrorCode="YFS10001"/>
                                </Validation>
                            </div>
                        </Description>
                    </Attribute>
                </Attributes>
            </yfc:doc>
        </WorkOrderServiceLine>
    </WorkOrderServiceLines>
</WorkOrder>
