<?xml version="1.0" encoding="UTF-8"?>
<ManageXmlList>
    <ManageXml Action="Manage" AttributeName="ExtnCountryCode"
        AttributeValue="/Order/Extn/@ExtnCountryCode" Xpath="/Order/OrderLines/OrderLine[*]/Extn"/>
    <ManageXml Action="Manage" AttributeName="PaymentKey"
        AttributeValue="/Order/PaymentMethods/PaymentMethod[@PaymentType='Encircle']/@PaymentKey"
        ElementName="TitanPaymentDetail" Xpath="/Order/OrderLines/OrderLine[*]/Extn/TitanPaymentDetailList/TitanPaymentDetail[@PaymentType='Encircle']"/>
    <ManageXml Action="Manage" AttributeName="ExtnEncircleNo"
        AttributeValue="/Order/Extn/@ExtnEncircleNo"
        ElementName="TitanPaymentDetail" Xpath="/Order/OrderLines/OrderLine[*]/Extn/TitanPaymentDetailList/TitanPaymentDetail[@PaymentType='Encircle']"/>
    <ManageXml Action="Manage" AttributeName="PaymentReference2"
        AttributeValue="/Order/PaymentMethods/PaymentMethod[@PaymentType='Encircle']/@PaymentReference2"
        ElementName="TitanPaymentDetail" Xpath="/Order/OrderLines/OrderLine[*]/Extn/TitanPaymentDetailList/TitanPaymentDetail[@PaymentType='Encircle']"/>
    <!-- Stamping Order Number to Order Level -->
    <ManageXml Action="Manage" AttributeName="OrderNo"
        AttributeValue="/Order/@OrderNo" Xpath="/Order/OrderLines/OrderLine[*]"/>
    <!-- Stamping Order Number to Order Level -->
    <!-- Stamping ExtnBrand To orderLine -->
    <ManageXml Action="Manage" AttributeName="ExtnBrand"
        AttributeValue="/Order/Extn/@ExtnBrand"
        ElementName="TitanPaymentDetail" Xpath="/Order/OrderLines/OrderLine[*]/Extn/TitanPaymentDetailList/TitanPaymentDetail[@PaymentType='Encircle']"/>
    <!-- Stamping ExtnBrand To orderLine -->
</ManageXmlList>
