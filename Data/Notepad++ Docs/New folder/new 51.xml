<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

    <xsl:template match="@*|node()">
        <xsl:copy>
            <xsl:apply-templates select="@*|node()"/>
        </xsl:copy>
    </xsl:template>
    <xsl:template match="TrackResponse">
    <TrackResponse>
        <xsl:apply-templates select="@*|node()"/>
        <CustomSQL>
        <xsl:variable name="awb" select="/TrackResponse/Shipment/Package/@TrackingNumber"/>
        <xsl:variable name="sqla" select="concat('select SCAC,shipment_no,order_no,AIRWAY_BILL_NO from yfs_shipment where AIRWAY_BILL_NO= &quot;',$awb)"/>
            <xsl:attribute name="Query">
                <xsl:value-of select="$sqla"/>
            </xsl:attribute>
        </CustomSQL>
        
        </TrackResponse>
    </xsl:template>
</xsl:stylesheet>



