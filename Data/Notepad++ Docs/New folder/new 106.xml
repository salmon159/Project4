<?xml version="1.0" encoding="UTF-8"?>
<ManageXmlList>
    <!-- EVEN last digit: set ShipNode to XTH, only if ShipNode is not WOC -->
    <ManageXml Action="Manage" AttributeName="ShipNode"
        AttributeValue="XTH"
        Condition="             /Order[@EnterpriseCode='TITAN_US']             /PaymentMethods[1]/PaymentMethod[1][                 string(number(substring(@PaymentReference6, string-length(@PaymentReference6), 1))) != 'NaN'                  and number(substring(@PaymentReference6, string-length(@PaymentReference6), 1)) mod 2 = 0             ]              /../../OrderLines/OrderLine[@ShipNode != 'WOC']" Xpath="/Order/OrderLines/OrderLine[@ShipNode != 'WOC']"/>
    <!-- ODD last digit: set ShipNode to XTD, only if ShipNode is not WOC -->
    <ManageXml Action="Manage" AttributeName="ShipNode"
        AttributeValue="XTH"
        Condition="             /Order[@EnterpriseCode='TITAN_US']             /PaymentMethods[1]/PaymentMethod[1][                 string(number(substring(@PaymentReference6, string-length(@PaymentReference6), 1))) != 'NaN'                  and number(substring(@PaymentReference6, string-length(@PaymentReference6), 1)) mod 2 = 1             ]              /../../OrderLines/OrderLine[@ShipNode != 'WOC']" Xpath="/Order/OrderLines/OrderLine[@ShipNode != 'WOC']"/>
</ManageXmlList>




